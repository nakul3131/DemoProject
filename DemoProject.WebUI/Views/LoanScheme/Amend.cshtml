@model DemoProject.Services.ViewModel.Account.Layout.LoanSchemeViewModel
@using DemoProject.Services.HtmlHelpers
@using DemoProject.Services.Constants
@using DemoProject.Services.ViewModel.Account.Layout
@using DemoProject.Services.ViewModel.Configuration

@{
    LayoutDetailViewModel detailViewModel = new LayoutDetailViewModel();
    HeaderPartialModel headerPartialModel = new HeaderPartialModel("LSCAMN");
}

<!doctype html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Loan Scheme - Amend</title>
</head>
<body>
    <div class="container-fluid">
        @*======= First Row - For Header =======*@
        <div class="row">
            <div class="offset-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @*======= Second Row - Data Entry Page =======*@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card d-block mb-3">
                    @using (Html.BeginForm("Amend", "LoanScheme", FormMethod.Post, htmlAttributes: new { @id = "form", @autocomplete = "off" }))
                    {
                        @Html.AntiForgeryToken()
                        {
                            <div class="card-body">
                                @*======= First Row - Mandatory Mark =======*@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <h6 class="mb-2 text-right text-muted"><b class="mandatory-mark">&nbsp; Mandatory Field</b></h6>
                                    </div>
                                </div>

                                @*======= Second Row - Page Title, Translation =======*@
                                <div class="row mt-2">
                                    @*Page Title*@
                                    <div class="col-12">
                                        <h5 class="font-weight-bold mt-1">Amend</h5>
                                        <hr />
                                    </div>
                                </div>

                                @*Submit Error*@
                                @Html.ValidationMessage("SubmitError")

                                @*======= Third Row - Data Entry Page =======*@
                                <div id="amend-view" class="row">
                                    <div class="col-12">
                                        @*NameOfScheme*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name Of Scheme")
                                            @Html.TextBoxFor(m => m.NameOfScheme, new { @id = "name-of-scheme", @class = "form-control mandatory-mark alpha-numeric", @placeholder = "Enter Name Of Scheme", required = "required", @maxlength = 100, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.NameOfScheme)
                                            <span id="name-of-scheme-error" class="error d-none">Name Of Scheme is already exist. </span>
                                        </div>

                                        @*TransNameOfScheme*@
                                        <div class="form-group">
                                            @Html.CustomLabel(detailViewModel.NameOfSchemeInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransNameOfScheme, new { @id = "trans-name-of-scheme", @class = "form-control mandatory-mark deny-multiple-space", @placeholder = detailViewModel.NameOfSchemePlaceHolderInRegionalLanguage, required = "required", @maxlength = 100, })
                                            @Html.ValidationMessageFor(m => m.TransNameOfScheme)
                                        </div>

                                        @*Alias Name*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Alias Name")
                                            @Html.TextBoxFor(m => m.AliasName, new { @id = "alias-name", @class = "form-control alpha-numeric", @placeholder = "Enter Alias Name", @maxlength = 10, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.AliasName)
                                        </div>

                                        @*TransAliasName*@
                                        <div class="form-group">
                                            @Html.CustomLabel(detailViewModel.AliasNameInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransAliasName, new { @id = "trans-alias-name", @class = "form-control deny-multiple-space", @placeholder = detailViewModel.AliasNamePlaceHolderInRegionalLanguage, @maxlength = 10, })
                                            @Html.ValidationMessageFor(m => m.TransAliasName)
                                        </div>

                                        @*NameOnReport*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name On Report")
                                            @Html.TextBoxFor(m => m.NameOnReport, new { @id = "name-on-report", @class = "form-control alpha-numeric", @placeholder = "Enter Name On Report", @maxlength = 100, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.NameOnReport)
                                        </div>

                                        @*TransNameOnReport*@
                                        <div class="form-group">
                                            @Html.CustomLabel(detailViewModel.NameOnReportInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransNameOnReport, new { @id = "trans-name-on-report", @class = "form-control deny-multiple-space", @placeholder = detailViewModel.NameOnReportPlaceHolderInRegionalLanguage, @maxlength = 100, })
                                            @Html.ValidationMessageFor(m => m.TransNameOnReport)
                                        </div>

                                        @*LoanType*@
                                        <div class="form-group read-only">
                                            @Html.CustomLabel("Loan Type")
                                            <span id="loan-type-id-info" class="ml-5 text-primary d-none">** Loan Type Should Not Be Changed.</span>
                                            @Html.DropDownListFor(m => m.SchemeLoanAccountParameterViewModel.LoanTypeId, new SelectList(detailViewModel.OrganizationLoanTypeDropdownList, "Value", "Text"), "--- Select Loan Type ---", new { @id = "loan-type-id", @class = "form-control mandatory-mark", required = "required" })
                                            @Html.ValidationMessageFor(m => m.SchemeLoanAccountParameterViewModel.LoanTypeId)
                                        </div>

                                        @*ActivationDate*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Activation Date")
                                            @Html.TextBoxFor(m => m.ActivationDate, "{0:yyyy-MM-dd}", new { @id = "activation-date-scheme", @class = "form-control activation-date mandatory-mark deny-next-financial-year-date deny-past-date", @placeholder = "Enter Activation Date", required = "required", @Type = "date" })
                                            @Html.ValidationMessageFor(m => m.ActivationDate)
                                        </div>

                                        @*ExpiryDate*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Expiry Date")
                                            @Html.TextBoxFor(m => m.ExpiryDate, "{0:yyyy-MM-dd}", new { @id = "expiry-date-scheme", @class = "form-control expiry-date allow-future-date-upto-50y", @Type = "date" })
                                            @Html.ValidationMessageFor(m => m.ExpiryDate)
                                        </div>

                                        @*Note*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Note")
                                            @Html.TextAreaFor(m => m.Note, new { @id = "note", @class = "form-control default-none deny-multiple-space", @placeholder = "Enter Note", @maxlength = "1500", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.Note)
                                        </div>

                                        @*TransNote*@
                                        <div class="form-group">
                                            @Html.CustomLabel(detailViewModel.NoteInRegionalLanguage)
                                            @Html.TextAreaFor(m => m.TransNote, new { @id = "trans-note", @class = "form-control default-none deny-multiple-space", @placeholder = detailViewModel.NotePlaceHolderInRegionalLanguage, @maxlength = 1500, })
                                            @Html.ValidationMessageFor(m => m.TransNote)
                                        </div>

                                        @*======= Fourth Row - Accordians =======*@
                                        <div class="row mt-3">
                                            <div class="col-12">
                                                <div id="accordion" class="accordion md-accordion accordion-blocks">
                                                    @*Accordian 1 - Account Parameter*@
                                                    <div id="scheme-account-parameter-card" class="card mt-3">
                                                        <div class="card-header p-3" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-scheme-account-parameter" href="#collapse-scheme-account-parameter">
                                                            <div class="row">
                                                                <div class="col-6">
                                                                    <a class="card-title" href="#">
                                                                        <b>@Html.TextWithRegionalLanguage("Account Parameter")</b>
                                                                    </a>
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <span id="account-parameter-accordion-title-error" class="error accordion-title-error d-none">This Is Required, Please Add Valid Data.</span>

                                                                </div>
                                                                <div class="col-2">
                                                                    <a href="#collapse-scheme-account-parameter" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-scheme-account-parameter" data-target="#collapse-scheme-account-parameter">
                                                                        <i class="fa fa-angle-down toggle-icon"></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div id="collapse-scheme-account-parameter" class="panel-collapse collapse" aria-labelledby="scheme-account-parameter-card" data-parent="#accordion">
                                                            <div class="card-body">
                                                                <div class="row">
                                                                    <div class="col-12">
                                                                        @*EnableAlternateAccountNumber2*@
                                                                        <div class="row form-group mt-4">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Alternate Account Number 2")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableAlternateAccountNumber2, new { @id = "enable-alternate-account-number2", @class = "form-control switch-input" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*EnableAlternateAccountNumber3*@
                                                                        <div class="row form-group mt-4">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Alternate Account Number 3")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableAlternateAccountNumber3, new { @id = "enable-alternate-account-number3", @class = "form-control switch-input" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*EnableMaturityDateOverride*@
                                                                        <div class="row form-group">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Maturity Date Override")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left"></div>
                                                                                <label class="switch switch-left-right">
                                                                                    @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableMaturityDateOverride, new { @id = "enable-maturity-date-override", @class = "form-control switch-input show-block" })
                                                                                    <span class="slider round">
                                                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                    </span>
                                                                                </label>
                                                                            </div>
                                                                        </div>

                                                                        @*MinimumOverridePeriod*@
                                                                        <div id="maturity-date-override-block" class="d-none">
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Minimum Override Period")
                                                                                @Html.TextBoxFor(m => m.SchemeAccountParameterViewModel.MinimumOverridePeriod, new { @id = "minimum-override-period", @class = "form-control  mandatory-mark min-number real-number account-parameter-input", @placeholder = "Enter Minimum Override Period", required = "required", @type = "number", @min = 0, @max = 90 })
                                                                                @Html.ValidationMessageFor(m => m.SchemeAccountParameterViewModel.MinimumOverridePeriod)
                                                                            </div>

                                                                            @*MaximumOverridePeriod*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Maximum Override Period")
                                                                                @Html.TextBoxFor(m => m.SchemeAccountParameterViewModel.MaximumOverridePeriod, new { @id = "maximum-override-period", @class = "form-control mandatory-mark max-number real-number account-parameter-input", @placeholder = "Enter Maximum Override Period", required = "required", @type = "number", @min = 0, @max = 90 })
                                                                                @Html.ValidationMessageFor(m => m.SchemeAccountParameterViewModel.MaximumOverridePeriod)
                                                                            </div>
                                                                        </div>

                                                                        @*EnableNumberOfJointAccountHoldingLimit*@
                                                                        <div class="row form-group">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Number Of Joint Account Holding Limit")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableNumberOfJointAccountHoldingLimit, new { @id = "enable-number-of-joint-account-holding-limit", @class = "form-control switch-input show-block" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*Joint Account Details*@
                                                                        <div id="number-of-joint-account-holding-limit-block" class="d-none">
                                                                            @*MinimumJointAccountHolder*@
                                                                            <div class="form-group minimumJointAccountHolder">
                                                                                @Html.CustomLabel("Minimum Joint Account Holder")
                                                                                @Html.TextBoxFor(m => m.SchemeAccountParameterViewModel.MinimumJointAccountHolder, new { @id = "minimum-joint-account-holder", @class = "form-control  mandatory-mark min-number real-number account-parameter-input", @placeholder = "Enter Minimum Joint Account Holder", required = "required", @type = "number", @min = 0, @max = 90 })
                                                                                @Html.ValidationMessageFor(m => m.SchemeAccountParameterViewModel.MinimumJointAccountHolder)
                                                                            </div>

                                                                            @*MaximumJointAccountHolder*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Maximum Joint Account Holder")
                                                                                @Html.TextBoxFor(m => m.SchemeAccountParameterViewModel.MaximumJointAccountHolder, new { @id = "maximum-joint-account-holder", @class = "form-control  mandatory-mark max-number real-number account-parameter-input", @placeholder = "Enter Maximum Joint Account Holder", required = "required", @type = "number", @min = 0, @max = 90 })
                                                                                @Html.ValidationMessageFor(m => m.SchemeAccountParameterViewModel.MaximumJointAccountHolder)
                                                                            </div>

                                                                            @*DefaultJointAccountHolder*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Default Joint Account Holder")
                                                                                @Html.TextBoxFor(m => m.SchemeAccountParameterViewModel.DefaultJointAccountHolder, new { @id = "default-joint-account-holder", @class = "form-control  mandatory-mark mid-number real-number account-parameter-input", @placeholder = "Enter Default Joint Account Holder", required = "required", @type = "number", @min = 0, @max = 90 })
                                                                                @Html.ValidationMessageFor(m => m.SchemeAccountParameterViewModel.DefaultJointAccountHolder)
                                                                            </div>
                                                                        </div>

                                                                        @*EnableNumberOfNomineeLimit*@
                                                                        <div class="row form-group mt-4">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Number Of Nominee Limit")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableNumberOfNomineeLimit, new { @id = "enable-number-of-nominee-limit", @class = "form-control switch-input show-block" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*Nominee Details*@
                                                                        <div id="number-of-nominee-limit-block" class="d-none">
                                                                            @*MinimumNominee*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Minimum Nominee")
                                                                                @Html.TextBoxFor(m => m.SchemeAccountParameterViewModel.MinimumNominee, new { @id = "minimum-nominee", @class = "form-control  mandatory-mark real-number min-number account-parameter-input", @placeholder = "Enter Minimum Nominee", required = "required", @type = "number", @min = 0, @max = 90 })
                                                                                @Html.ValidationMessageFor(m => m.SchemeAccountParameterViewModel.MinimumNominee)
                                                                            </div>

                                                                            @*MaximumNominee*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Maximum Nominee")
                                                                                @Html.TextBoxFor(m => m.SchemeAccountParameterViewModel.MaximumNominee, new { @id = "maximum-nominee", @class = "form-control  mandatory-mark real-number max-number account-parameter-input", @placeholder = "Enter Maximum Nominee", required = "required", @type = "number", @min = 0, @max = 90 })
                                                                                @Html.ValidationMessageFor(m => m.SchemeAccountParameterViewModel.MaximumNominee)
                                                                            </div>

                                                                            @*DefaultNominee*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Default Nominee")
                                                                                @Html.TextBoxFor(m => m.SchemeAccountParameterViewModel.DefaultNominee, new { @id = "default-nominee", @class = "form-control  mandatory-mark  real-number mid-number account-parameter-input", @placeholder = "Enter Default Nominee", required = "required", @type = "number", @min = 0, @max = 90 })
                                                                                @Html.ValidationMessageFor(m => m.SchemeAccountParameterViewModel.DefaultNominee)
                                                                            </div>
                                                                        </div>

                                                                        @*EnableChequeBook*@
                                                                        <div id="enable-cheque-book-input" class="row form-group optional-input mt-4">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Cheque Book")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableChequeBook, new { @id = "enable-cheque-book", @class = "form-control switch-input show-block" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*EnableStandingInstruction*@
                                                                        <div id="enable-standing-instruction-input" class="row form-group optional-input mt-4">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Standing Instruction")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableStandingInstruction, new { @id = "enable-standing-instruction", @class = "form-control switch-input show-block" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*EnableAdditionalNoteInCustomerAccount*@
                                                                        <div class="row form-group mt-4">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Additional Note In Customer Account")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableAdditionalNoteInCustomerAccount, new { @id = "enable-additional-noteI-in-customer-account", @class = "form-control switch-input show-block" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*EnableDigitalCodeForCustomerAccount*@
                                                                        <div class="row form-group mt-4">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Digital Code For Customer Account")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableDigitalCodeForCustomerAccount, new { @id = "enable-digital-code-for-customer-account", @class = "form-control switch-input show-block" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*EnableTransferCustomerAccountsInOtherBranch*@
                                                                        <div class="row form-group mt-4">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Transfer Customer Accounts In Other Branch")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableTransferCustomerAccountsInOtherBranch, new { @id = "enable-transfer-customer-accounts-in-other-branch", @class = "form-control switch-input show-block" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*EnableSmsService*@
                                                                        <div class="row form-group mt-4 @((bool)(ViewBag.LoanSchemeParameter.EnableSmsServiceParameter) ? "" : "d-none")">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Sms Service")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableSmsService, new { @id = "enable-sms-service", @class = "form-control switch-input notice-schedule" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*EnableEmailService*@
                                                                        <div class="row form-group mt-4 @((bool)(ViewBag.LoanSchemeParameter.EnableEmailServiceParameter) ? "" : "d-none")">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Email Service")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableEmailService, new { @id = "enable-email-service", @class = "form-control switch-input notice-schedule" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*EnableDocumentUpload*@
                                                                        <div id="enable-document-upload-input" class="row form-group mt-4 @((bool)(ViewBag.LoanSchemeParameter.EnableDocumentParameter) ? "" : "disable-document-parameter d-none")">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Document Upload")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableDocumentUpload, new { @id = "enable-document-upload", @class = "form-control switch-input show-block" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*IsRequiredOrdinaryMembership*@
                                                                        <div class="row form-group mt-4">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Is Required Ordinary Membership")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeLoanAccountParameterViewModel.IsRequiredOrdinaryMembership, new { @id = "enable-required-ordinary-membership", @class = "form-control switch-input hide-block" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
