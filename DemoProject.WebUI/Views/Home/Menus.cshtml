@{
    Layout = "~/Views/Shared/_DashBoard.cshtml";
    ViewBag.Title = "Menus";
}
<style>
    .ui-autocomplete {
        max-height: 200px !important;
        overflow-y: auto !important;
        /* prevent horizontal scrollbar */
        overflow-x: hidden !important;
        /* add padding to account for vertical scrollbar */
        padding-right: 20px !important;
    }

    .tt {
        background-color: transparent;
        border-color: transparent transparent #f3f3f3;
        box-shadow: 0 0 30px rgba(0, 0, 0, 0.1);
    }

    /* Tabs*/
    a.active {
        border-bottom: 2px solid #55c57a !important;
    }

    .nav-link {
        color: rgb(110, 110, 110);
        font-weight: 500
    }

        .nav-link:hover {
            color: rgb(110, 110, 110);
        }

    .nav-pills .nav-link.active {
        border-radius: 0.5rem 0.5rem 0 0;
        color: #0094ff;
        background-color: transparent;
        border-color: transparent transparent #f3f3f3;
        border-bottom: 3px solid !important;
    }

    .nav-tabs .nav-link {
        border: 0px solid transparent !important;
    }
</style>

<div class="col-xs-12 " style="margin-top:2%;">
    <!-- Tab -->
    <div class="nav nav-fill " style="margin-left:18%">
        <ul class="nav nav-pills mb-3" id="pills-tab" role="tablist">
            <li class="nav-item"> <a class="nav-link active" id="pills-history-tab" data-toggle="pill" href="#pills-menu" role="tab" aria-controls="pills-history" aria-selected="false">Menu</a> </li>
            <li class="nav-item"> <a class="nav-link" id="pills-report-tab" data-toggle="pill" href="#pills-report" role="tab" aria-controls="pills-report" aria-selected="false">Report</a> </li>
            <li class="nav-item"> <a class="nav-link" id="pills-menu-tab" data-toggle="pill" href="#pills-customer-account-number" role="tab" aria-controls="pills-menu" aria-selected="true">Customer Account Number</a> </li>
        </ul> <!-- content -->
    </div>
    <div class="nav nav-tabs justify-content-left" id="tabs"  role="tablist" style="margin-top:-1.2%;">
    </div>
</div>

<div class="container">
    <div class="tab-content justify-content-center" id="nav-tabContent" style="margin-left:11%;">
        <!-- 1st card -->
        <div class="tab-pane fade show active" id="pills-menu" role="tabpanel" aria-labelledby="nav-menu-tab">
            <ul id="static-pagination-list" class="list-group mt-4" style="list-style-type: disc"></ul>
        </div>
        <div class="tab-pane fade p-3" id="pills-report" role="tabpanel" aria-labelledby="nav-history-tab">
            <h5 class="card-title">Tab Card 2 </h5>
            <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
            <a href="#" class="btn btn-primary">Go somewhere</a>
        </div>
        <div class="tab-pane fade p-3" id="pills-customer-account-number" role="tabpanel" aria-labelledby="nav-svg-tab">
            <h5 class="card-title">Tab Card 3 </h5>
            <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
            <a href="#" class="btn btn-primary">Go somewhere</a>
        </div>
        <nav aria-label="Page navigation ">
            <ul id="static-pagination" class="pagination"></ul>
        </nav>
    </div>
</div>
<script src="~/Scripts/Custom/CustomeMenuSearch.js"></script>
