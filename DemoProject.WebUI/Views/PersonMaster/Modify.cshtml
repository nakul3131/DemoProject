@model DemoProject.Services.ViewModel.PersonInformation.PersonMasterViewModel
@using DemoProject.Services.ViewModel.Configuration
@using DemoProject.Services.ViewModel.PersonInformation
@using DemoProject.Services.ViewModel.PersonInformation.Parameter
@using DemoProject.Services.HtmlHelpers

@{
    PersonDetailViewModel personDetailViewModel = new PersonDetailViewModel();
    PersonInformationParameterViewModel personInformationParameterViewModel = new PersonInformationParameterViewModel();
    personInformationParameterViewModel = ViewBag.PersonInformationParameter;
    HeaderPartialModel headerPartialModel = new HeaderPartialModel("PRSMOD");

    byte counter = 0;
    string photoId = "";
    string fileUploaderId = "";
}

<!doctype html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0" />

    <title>Person Master - Modify</title>
</head>
<body>
    <div class="container-fluid">

        @* ======= First Row - For Header ======= *@
        <div class="row">
            <div class="offset-lg-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @* ======= Second Row - Data Entry Page ======= *@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card d-block mb-3">
                    @using (Html.BeginForm(null, null, FormMethod.Post, new { enctype = "multipart/form-data", @id = "form", autocomplete = "off" }))
                    {
                        @Html.AntiForgeryToken()
                        {
                    <div class="card-body">

                        @*======= First Row - Mandatory Mark =======*@
                        <div class="row mt-2">
                            <div class="col-12">
                                <h6 class="mb-2 text-right text-muted"><b class="mandatory-mark">&nbsp; Mandatory Field</b></h6>
                            </div>
                        </div>

                        @*======= Second Row - Page Title =======*@
                        <div class="row mt-2">
                            @*Page Title*@
                            <div class="col-12">
                                <h5 class="font-weight-bold mt-1">Modify</h5>
                                <hr />
                            </div>
                        </div>

                        @*Submit Error*@
                        @Html.ValidationMessage("SubmitError")

                        @{
                            <h5 class="mt-4 text-left font-weight-bold">
                                @Model.FullName
                            </h5>
                        }


                        @*Person Information Number*@
                        <div class="form-group @(ViewBag.PersonInformationParameter.EnableAutoGeneratePersonInformationNumber ? "d-none": "" )" disabled>
                            @Html.CustomLabel("Person Information Number")
                            @Html.TextBoxFor(m => m.PersonInformationNumber, new { @id = "person-information-number", @class = "form-control title-case mandatory-mark", @placeholder = "Enter Person Information Number", @type = "number", required = "required" })
                            @Html.ValidationMessageFor(m => m.PersonInformationNumber)
                            <span id="person-information-number-error" class="error d-none">Person Information Number is already exist. </span>
                        </div>

                        @*PrefixId*@
                        <div class="form-group mt-4">
                            @Html.CustomLabel("Prefix")
                            @Html.DropDownListFor(m => m.PersonPrefixViewModel.PrefixId, new SelectList(personDetailViewModel.PrefixDropdownList, "Value", "Text", 0), "--- Select Prefix ---", new { @id = "prefix", @class = "form-control mandatory-mark", required = "required" })
                            @Html.ValidationMessageFor(m => m.PersonPrefixViewModel.PrefixId)
                        </div>

                        @*FirstName*@
                        <div class="form-group">
                            @Html.CustomLabel("First Name")
                            @Html.TextBoxFor(m => m.FirstName, new { @id = "first-name", @class = "form-control mandatory-mark title-case alpha-with-single-space", @placeholder = "Enter First Name", @minlength = 2, @maxlength = 50, onkeydown = "Maketranslation()", required = "required" })
                            @Html.ValidationMessageFor(m => m.FirstName)
                        </div>

                        @*TransFirstName*@
                        <div class="form-group">
                            @Html.CustomLabel(personDetailViewModel.FirstNameInRegionalLanguage)
                            @Html.TextBoxFor(m => m.TransFirstName, null, new { @id = "trans-first-name", @class = "form-control deny-multiple-space mandatory-mark", @placeholder = personDetailViewModel.FirstNamePlaceHolderInRegionalLanguage, @minlength = 2, @maxlength = 50, required = "required" })
                            @Html.ValidationMessageFor(m => m.TransFirstName)
                        </div>

                        @*MiddleName*@
                        <div class="form-group">
                            @Html.CustomLabel("Middle Name")
                            @Html.TextBoxFor(m => m.MiddleName, new { @id = "middle-name", @class = "form-control mandatory-mark title-case alpha-with-single-space", @placeholder = "Enter Middle Name", @minlength = 2, @maxlength = 50, onkeydown = "Maketranslation()", required = "required" })
                            @Html.ValidationMessageFor(m => m.MiddleName)
                        </div>

                        @*TransMiddleName*@
                        <div class="form-group">
                            @Html.CustomLabel(personDetailViewModel.MiddleNameInRegionalLanguage)
                            @Html.TextBoxFor(m => m.TransMiddleName, null, new { @id = "trans-middle-name", @class = "form-control deny-multiple-space mandatory-mark", @placeholder = personDetailViewModel.MiddleNamePlaceHolderInRegionalLanguage, @minlength = 2, @maxlength = 50, required = "required" })
                            @Html.ValidationMessageFor(m => m.TransMiddleName)
                        </div>

                        @*LastName*@
                        <div class="form-group">
                            @Html.CustomLabel("Last Name")
                            @Html.TextBoxFor(m => m.LastName, new { @id = "last-name", @class = "form-control mandatory-mark title-case alpha-with-single-space", @placeholder = "Enter Last Name", @minlength = 2, @maxlength = 50, onkeydown = "Maketranslation()", required = "required" })
                            @Html.ValidationMessageFor(m => m.LastName)
                        </div>

                        @*TransLastName*@
                        <div class="form-group">
                            @Html.CustomLabel(personDetailViewModel.LastNameInRegionalLanguage)
                            @Html.TextBoxFor(m => m.TransLastName, null, new { @id = "trans-last-name", @class = "form-control deny-multiple-space trans mandatory-mark", @placeholder = personDetailViewModel.LastNamePlaceHolderInRegionalLanguage, @minlength = 2, @maxlength = 50, required = "required" })
                            @Html.ValidationMessageFor(m => m.TransLastName)
                        </div>

                        @*DateOfBirth*@
                        <div class="form-group">
                            @Html.CustomLabel("Date Of Birth")
                            @Html.TextBoxFor(m => m.DateOfBirth, "{0:yyyy-MM-dd}", new { id = "birth-date", @class = "form-control mandatory-mark deny-future-date allow-past-date-upto-150y", @Type = "date", required = "required" })
                            @Html.ValidationMessageFor(m => m.DateOfBirth)
                        </div>

                        @*DateOfBirthOnDocument*@
                        <div class="form-group checked">
                            @Html.CustomLabel("Date Of Birth On Document")
                            @Html.TextBoxFor(m => m.DateOfBirthOnDocument, "{0:yyyy-MM-dd}", new { id = "document-birth-date", @class = "form-control mandatory-mark", @Type = "date", required = "required" })
                            <span id="document-birth-date-error" class="text-c-orenge d-none"><i class="fas fa-exclamation-triangle">&nbsp;&nbsp;Selected Date Of Birth On Your Document Appears To Be Significantly Different From Your Original Birthdate, Please Confirm If This Is Correct.</i></span>
                            @Html.ValidationMessageFor(m => m.DateOfBirthOnDocument)
                        </div>

                        @*MotherName*@
                        <div class="form-group">
                            @Html.CustomLabel("Mother Name")
                            @Html.TextBoxFor(m => m.MotherName, new { @id = "mother-name", @class = "form-control mandatory-mark title-case alpha-with-single-space", @placeholder = "Enter Mother Name", @minlength = 2, @maxlength = 50, onkeydown = "Maketranslation()", required = "required" })
                            @Html.ValidationMessageFor(m => m.MotherName)
                        </div>

                        @*TransMotherName*@
                        <div class="form-group">
                            @Html.CustomLabel(personDetailViewModel.MotherNameInRegionalLanguage)
                            @Html.TextBoxFor(m => m.TransMotherName, null, new { @id = "trans-mother-name", @class = "form-control deny-multiple-space trans mandatory-mark", @placeholder = personDetailViewModel.MotherNamePlaceHolderInRegionalLanguage, @minlength = 2, @maxlength = 50, required = "required" })
                            @Html.ValidationMessageFor(m => m.TransMotherName)
                        </div>

                        @*MothersMaidenName*@
                        <div class="form-group">
                            @Html.CustomLabel("Mothers Maiden Name")
                            @Html.TextBoxFor(m => m.MothersMaidenName, new { @id = "mothers-maiden-name", @class = "form-control title-case mandatory-mark alpha", @placeholder = "Enter Mothers Maiden Name", @minlength = 2, @maxlength = 50, onkeydown = "Maketranslation()", required = "required" })
                            @Html.ValidationMessageFor(m => m.MothersMaidenName)
                        </div>

                        @*TransMothersMaidenName*@
                        <div class="form-group">
                            @Html.CustomLabel(personDetailViewModel.MothersMaidenNameInRegionalLanguage)
                            @Html.TextBoxFor(m => m.TransMothersMaidenName, null, new { @id = "trans-mothers-maiden-name", @class = "form-control deny-multiple-space mandatory-mark", @placeholder = personDetailViewModel.MothersMaidenNamePlaceHolderInRegionalLanguage, @minlength = 2, @maxlength = 50, required = "required" })
                            @Html.ValidationMessageFor(m => m.TransMothersMaidenName)
                        </div>

                        @*ActivationDate*@
                        <div class="form-group">
                            @Html.CustomLabel("Activation Date")
                            @Html.TextBoxFor(m => m.PersonPrefixViewModel.ActivationDate, "{0:yyyy-MM-dd}", new { @id = "activation-date-person-prefix", @class = "form-control activation-date mandatory-mark deny-next-financial-year-date deny-past-date", @type = "date", required = "required" })
                            @Html.ValidationMessageFor(m => m.PersonPrefixViewModel.ActivationDate)
                        </div>

                        @*Note*@
                        <div class="form-group">
                            @Html.CustomLabel("Note")
                            @Html.TextAreaFor(m => m.Note, new { @id = "note", @class = "form-control title-case deny-multiple-space default-none", @placeholder = "Enter Note", @maxlength = 1300, onkeydown = "Maketranslation()" })
                            @Html.ValidationMessageFor(m => m.Note)
                        </div>

                        @*TransNote*@
                        <div class="form-group">
                            @Html.CustomLabel(personDetailViewModel.NoteInRegionalLanguage)
                            @Html.TextAreaFor(m => m.TransNote, new { @id = "trans-note", @class = "form-control deny-multiple-space default-none", @placeholder = personDetailViewModel.NotePlaceHolderInRegionalLanguage, @maxlength = 1300, })
                            @Html.ValidationMessageFor(m => m.TransNote)
                        </div>
                        @* ======= Third Row - Data Entry Page ======= *@

                        @*======= Fourth Row - Accordians =======*@
                        <div id="accordion" class="accordion md-accordion accordion-blocks">

                            @*Accordian 1 - Address Details*@
                            <div id="heading-address-details" class="card mt-3">
                                <div class="card-header p-3" id="collapse-address-details-validations" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-address-details" data-target="#collapse-address-details">
                                    <div class="row">
                                        <div class="col-md-5">
                                            <a class="card-title" href="#">
                                                <b>@Html.TextWithRegionalLanguage("Address Details")</b>
                                            </a>
                                        </div>
                                        <div class="col-md-5">
                                            <span id="address-details-accordion-error" class="error accordion-title-error d-none">This Is Required, Please Add Valid Data.</span>
                                        </div>
                                        <div class="col-2">
                                            <a href="#collapse-address-details" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-address-details" data-target="#collapse-address-details">
                                                <i class="fa fa-angle-up"></i>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <div id="collapse-address-details" class="collapse" aria-labelledby="heading-address-details" data-parent="#accordion">
                                    <div class="card-body">
                                        <div class="row">
                                            <div class="col-12">
                                                <div class="modal fade" id="person-address-modal" tabindex="-1" data-backdrop="static" role="dialog" aria-labelledby="modal-new-event" aria-hidden="true">
                                                    <div class="modal-dialog modal-dialog-centered modal-dialog-zoom modal-lg" role="document">
                                                        <div class="modal-content">
                                                            <div class="modal-header">
                                                                <h4 class="modal-title text-center">Add</h4>
                                                                <button type="button" class="close-person-address-modal" data-dismiss="modal" aria-label="Close">
                                                                    <span aria-hidden="true">&times;</span>
                                                                </button>
                                                            </div>
                                                            <div class="modal-body">
                                                                @*AddressTypePrmKey*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Address Type")
                                                                    @Html.DropDownListFor(m => m.PersonAddressViewModel.AddressTypeId, new SelectList(personDetailViewModel.AddressTypeDropdownList, "Value", "Text", 0), "--- Select Address Type ---", new { @id = "address-type-id", @class = "form-control mandatory-mark modal-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.PersonAddressViewModel.AddressTypeId)
                                                                    <span id="address-type-id-error" class="error modal-input-error d-none">Please Select Valid Address Type. </span>
                                                                </div>

                                                                @*FlatDoorNo*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Flat Door No")
                                                                    @Html.TextBoxFor(m => m.PersonAddressViewModel.FlatDoorNo, new { @id = "flat-door-no", @class = "form-control mandatory-mark title-case alpha-numeric modal-input", @placeholder = "Enter Flat Door No", @minlength = 3, @maxlength = 50, onkeydown = "Maketranslation()", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.PersonAddressViewModel.FlatDoorNo)
                                                                    <span id="flat-door-no-error" class="error modal-input-error d-none">Please Enter Valid Flat Door No. </span>
                                                                </div>

                                                                @*TransFlatDoorNo*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel(personDetailViewModel.FlatDoorNoInRegionalLanguage)
                                                                    @Html.TextBoxFor(m => m.PersonAddressViewModel.TransFlatDoorNo, null, new { @id = "trans-flat-door-no", @class = "form-control deny-multiple-space mandatory-mark modal-input", @placeholder = personDetailViewModel.FlatDoorNoPlaceHolderInRegionalLanguage, @minlength = 3, @maxlength = 50, required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.PersonAddressViewModel.TransFlatDoorNo)
                                                                    <span id="trans-flat-door-no-error" class="error modal-input-error d-none">Please Enter Valid Flat Door No. </span>
                                                                </div>

                                                                @*NameOfBuilding*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Name Of Building")
                                                                    @Html.TextBoxFor(m => m.PersonAddressViewModel.NameOfBuilding, new { @id = "building-name", @class = "form-control mandatory-mark title-case alpha-numeric modal-input", @placeholder = "Enter Name Of Building", @minlength = 3, @maxlength = 100, onkeydown = "Maketranslation()", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.PersonAddressViewModel.NameOfBuilding)
                                                                    <span id="building-name-error" class="error modal-input-error d-none">Please Enter Valid Name Of Building. </span>
                                                                </div>

                                                                @*TransNameOfBuilding*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel(personDetailViewModel.NameOfBuildingInRegionalLanguage)
                                                                    @Html.TextBoxFor(m => m.PersonAddressViewModel.TransNameOfBuilding, new { @id = "trans-building-name", @class = "form-control deny-multiple-space mandatory-mark modal-input", @placeholder = personDetailViewModel.NameOfBuildingPlaceHolderInRegionalLanguage, @minlength = 3, @maxlength = 100, required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.PersonAddressViewModel.TransNameOfBuilding)
                                                                    <span id="trans-building-name-error" class="error modal-input-error d-none">Please Enter Valid Name Of Building. </span>
                                                                </div>

                                                                @*NameOfRoad*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Name Of Road")
                                                                    @Html.TextBoxFor(m => m.PersonAddressViewModel.NameOfRoad, new { @id = "road-name", @class = "form-control mandatory-mark title-case alpha-numeric modal-input", @placeholder = "Enter Name Of Road", @minlength = 3, @maxlength = 100, onkeydown = "Maketranslation()", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.PersonAddressViewModel.NameOfRoad)
                                                                    <span id="road-name-error" class="error modal-input-error d-none">Please Enter Valid Name Of Road. </span>
                                                                </div>

                                                                @*TransNameOfRoad*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel(personDetailViewModel.NameOfRoadInRegionalLanguage)
                                                                    @Html.TextBoxFor(m => m.PersonAddressViewModel.TransNameOfRoad, new { @id = "trans-road-name", @class = "form-control deny-multiple-space mandatory-mark modal-input", @placeholder = personDetailViewModel.NameOfRoadPlaceHolderInRegionalLanguage, @minlength = 3, @maxlength = 100, required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.PersonAddressViewModel.TransNameOfRoad)
                                                                    <span id="trans-road-name-error" class="error modal-input-error d-none">Please Enter Valid Name Of Road. </span>
                                                                </div>

                                                                @*NameOfArea*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Name Of Area")
                                                                    @Html.TextBoxFor(m => m.PersonAddressViewModel.NameOfArea, new { @id = "area-name", @class = "form-control mandatory-mark title-case alpha-numeric modal-input", @placeholder = "Enter Name Of Area", @minlength = 3, @maxlength = 100, onkeydown = "Maketranslation()", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.PersonAddressViewModel.NameOfArea)
                                                                    <span id="area-name-error" class="error modal-input-error d-none">Please Enter Valid Name Of Area. </span>
                                                                </div>

                                                                @*TransNameOfArea*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel(personDetailViewModel.NameOfAreaInRegionalLanguage)
                                                                    @Html.TextBoxFor(m => m.PersonAddressViewModel.TransNameOfArea, new { @id = "trans-area-name", @class = "form-control deny-multiple-space mandatory-mark modal-input", @placeholder = personDetailViewModel.NameOfAreaPlaceHolderInRegionalLanguage, @minlength = 3, @maxlength = 100, required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.PersonAddressViewModel.TransNameOfArea)
                                                                    <span id="trans-area-name-error" class="error modal-input-error d-none">Please Enter Valid Name Of Area. </span>
                                                                </div>

                                                                @*City*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("City")
                                                                    @Html.DropDownListFor(m => m.PersonAddressViewModel.CityId, new SelectList(personDetailViewModel.CityDropdownList, "Value", "Text", 0), "--- Select City ---", new { @id = "city-id", @class = "form-control mandatory-mark modal-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.PersonAddressViewModel.CityId)
                                                                    <span id="city-id-error" class="error modal-input-error d-none">Please Select Valid City. </span>
                                                                </div>

                                                                @*ResidenceType*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Residence Type")
                                                                    @Html.DropDownListFor(m => m.PersonAddressViewModel.ResidenceTypeId, new SelectList(personDetailViewModel.ResidenceTypeDropdownList, "Value", "Text", 0), "--- Select Residence Type ---", new { @id = "residence-type-id", @class = "form-control mandatory-mark modal-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.PersonAddressViewModel.ResidenceTypeId)
                                                                    <span id="residence-type-id-error" class="error modal-input-error d-none">Please Select Valid Residence Type. </span>
                                                                </div>

                                                                @*ResidenceOwnership*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Ownership Type")
                                                                    @Html.DropDownListFor(m => m.PersonAddressViewModel.OwnershipTypeId, new SelectList(personDetailViewModel.OwnershipTypeDropdownList, "Value", "Text", 0), " --- Select Ownership Type ---", new { @id = "ownership-type-id", @class = "form-control mandatory-mark modal-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.PersonAddressViewModel.OwnershipTypeId)
                                                                    <span id="ownership-type-id-error" class="error modal-input-error d-none">Please Select Valid Ownership Type.</span>
                                                                </div>

                                                                @*Note*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Note")
                                                                    @Html.TextAreaFor(m => m.PersonAddressViewModel.Note, new { @id = "note-address", @class = "form-control modal-input title-case deny-multiple-space default-none", @placeholder = "Enter Note", @maxlength = 1300, onkeydown = "Maketranslation()" })
                                                                    @Html.ValidationMessageFor(m => m.PersonAddressViewModel.Note)
                                                                </div>

                                                                @*TransNote*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel(personDetailViewModel.NoteInRegionalLanguage)
                                                                    @Html.TextAreaFor(m => m.PersonAddressViewModel.TransNote, new { @id = "trans-note-address", @class = "form-control modal-input deny-multiple-space default-none", @placeholder = personDetailViewModel.NotePlaceHolderInRegionalLanguage, @maxlength = 1300 })
                                                                    @Html.ValidationMessageFor(m => m.PersonAddressViewModel.TransNote)
                                                                </div>

                                                                @*ReasonForModification*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Reason For Modification")
                                                                    @Html.TextAreaFor(m => m.ReasonForModification, new { @id = "reason-for-modification-address", @class = "form-control default-none title-case modal-input", @placeholder = "Enter Reason For Modification", @maxlength = 1300 })
                                                                    @Html.ValidationMessageFor(m => m.ReasonForModification)
                                                                </div>
                                                            </div>
                                                            <div class="modal-footer">
                                                                <button type="button" id="btn-add-person-address-modal" class="btn btn-success ">Add</button>
                                                                <button type="button" id="btn-update-person-address-modal" class="btn btn-primary">Update</button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="card-body">

                                                    <table id="tbl-person-address" class="table compact table-striped table-bordered" cellspacing="0" width="100">
                                                        <thead class="text-center">
                                                            <tr>
                                                                <th><input type="checkbox" name="check_all" id="select-all-person-address" style="margin-left:11px;" /></th>
                                                                <th class="d-none">AddressTypeID</th>
                                                                <th>AddressType</th>
                                                                <th>FlatDoorNo</th>
                                                                <th>TransFlatDoorNo</th>
                                                                <th>NameOfBuilding</th>
                                                                <th>TransNameOfBuilding</th>
                                                                <th>NameOfRoad</th>
                                                                <th>TransNameOfRoad</th>
                                                                <th>NameOfArea</th>
                                                                <th>TransNameOfArea</th>
                                                                <th class="d-none">CityID</th>
                                                                <th>City</th>
                                                                <th class="d-none">ResidenceTypeID</th>
                                                                <th>ResidenceType</th>
                                                                <th class="d-none">OwnershipTypeId</th>
                                                                <th>OwnershipType</th>
                                                                <th>Note</th>
                                                                <th>TransNote</th>
                                                                <th>ReasonForModification</th>
                                                                <th class="d-none">personAddressPrmKey</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody class="text-center">
                                                            @{
                                                                IEnumerable<PersonAddressViewModel> personAddressViewModelList = (IEnumerable<PersonAddressViewModel>)HttpContext.Current.Session["Address"];

                                                                foreach (PersonAddressViewModel viewModel in personAddressViewModelList)
                                                                {
                                                                    <tr>
                                                                        <td><input type="checkbox" name="check_all" class="checks" /></td>
                                                                        <td class="d-none">@viewModel.AddressTypeId</td>
                                                                        <td>@viewModel.NameOfAddressType</td>
                                                                        <td>@viewModel.FlatDoorNo</td>
                                                                        <td>@viewModel.TransFlatDoorNo</td>
                                                                        <td>@viewModel.NameOfBuilding</td>
                                                                        <td>@viewModel.TransNameOfBuilding</td>
                                                                        <td>@viewModel.NameOfRoad</td>
                                                                        <td>@viewModel.TransNameOfRoad</td>
                                                                        <td>@viewModel.NameOfArea</td>
                                                                        <td>@viewModel.TransNameOfArea</td>
                                                                        <td class="d-none">@viewModel.CityId</td>
                                                                        <td>@viewModel.NameOfCenter</td>
                                                                        <td class="d-none">@viewModel.ResidenceTypeId</td>
                                                                        <td>@viewModel.NameOfResidenceType</td>
                                                                        <td class="d-none">@viewModel.OwnershipTypeId</td>
                                                                        <td>@viewModel.NameOfOwnershipType</td>
                                                                        <td>@viewModel.Note</td>
                                                                        <td>@viewModel.TransNote</td>
                                                                        <td>@viewModel.ReasonForModification</td>
                                                                        <td class="d-none">@viewModel.PersonAddressPrmKey</td>
                                                                    </tr>
                                                                }
                                                            }
                                                        </tbody>
                                                    </table>

                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>

                            @*Accordian 2 - Person Additional Details*@
                            <div id="heading-person-additional-details" class="card mt-3">
                                <div class="card-header" id="person-additional-details-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-person-additional-details" data-target="#collapse-person-additional-details">
                                    <div class="row">
                                        <div class="col-md-5">
                                            <a class="card-title" href="#">
                                                <b>@Html.TextWithRegionalLanguage("Additional Details")</b>
                                            </a>
                                        </div>
