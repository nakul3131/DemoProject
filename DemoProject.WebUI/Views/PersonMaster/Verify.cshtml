@model DemoProject.Services.ViewModel.PersonInformation.PersonMasterViewModel
@using DemoProject.Services.ViewModel.Configuration
@using DemoProject.Services.ViewModel.PersonInformation
@using DemoProject.Services.ViewModel.PersonInformation.Parameter
@using DemoProject.Services.HtmlHelpers

@{
    PersonDetailViewModel personDetailViewModel = new PersonDetailViewModel();
    PersonInformationParameterViewModel personInformationParameterViewModel = new PersonInformationParameterViewModel();
    personInformationParameterViewModel = ViewBag.PersonInformationParameter;
    HeaderPartialModel headerPartialModel = new HeaderPartialModel("PRSVRF");

    var serializer = new System.Web.Script.Serialization.JavaScriptSerializer();
    byte counter = 0;
    string photoId = "";
    string fileUploaderId = "";
}

<!doctype html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0" />

    <title>Person Master - Verify</title>
</head>
<body>
    <div class="container-fluid">

        @* ======= First Row - For Header ======= *@
        <div class="row">
            <div class="offset-lg-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @* ======= Second Row - Data Entry Page ======= *@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card d-block mb-3">
                    @using (Html.BeginForm(null, null, FormMethod.Post, new { enctype = "multipart/form-data", @id = "form", autocomplete = "off" }))
                    {
                        @Html.AntiForgeryToken()
                        {
                            <div class="card-body">
                                @*======= First Row - Mandatory Mark =======*@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <h6 class="mb-2 text-right text-muted"><b class="mandatory-mark">&nbsp; Mandatory Field</b></h6>
                                    </div>
                                </div>

                                @*======= Second Row - Page Title =======*@
                                <div class="row mt-2">
                                    @*Page Title*@
                                    <div class="col-12">
                                        <h5 class="font-weight-bold mt-1">Verify</h5>
                                        <hr />
                                    </div>
                                </div>

                                @*Submit Error*@
                                @Html.ValidationMessage("SubmitError")

                                @*======= Third Row - Data Entry Page =======*@
                                <div class="row">
                                    <div class="col-12">
                                        <div id="verify-view">
                                            @{
                                                <h5 class="mt-4 text-left font-weight-bold">
                                                    @Model.FullName
                                                </h5>
                                            }

                                            @*MakerRemark*@
                                            <div class="form-group mt-5">
                                                @Html.CustomLabel("Maker Remark")
                                                @Html.TextBoxFor(m => m.MakerRemark, new { @id = "maker-remark", @class = "form-control mandatory-mark read-only" })
                                                @Html.ValidationMessageFor(m => m.MakerRemark)
                                            </div>

                                            <div class="read-only">
                                                @*Person Information Number*@
                                                <div class="form-group @(ViewBag.PersonInformationParameter.EnableAutoGeneratePersonInformationNumber ? "d-none": "" )">
                                                    @Html.CustomLabel("Person Information Number")
                                                    @Html.TextBoxFor(m => m.PersonInformationNumber, new { @id = "person-information-number", @class = "form-control mandatory-mark", @placeholder = "Enter Person Information Number", @type = "number" })
                                                    @Html.ValidationMessageFor(m => m.PersonInformationNumber)
                                                    <span id="person-information-number-error" class="error d-none">Person Information Number is already exist. </span>
                                                </div>

                                                @*PrefixId*@
                                                <div class="form-group mt-4">
                                                    @Html.CustomLabel("Prefix")
                                                    @Html.DropDownListFor(m => m.PersonPrefixViewModel.PrefixId, new SelectList(personDetailViewModel.PrefixDropdownList, "Value", "Text", 0), "--- Select Prefix ---", new { @id = "prefix", @class = "form-control mandatory-mark" })
                                                    @Html.ValidationMessageFor(m => m.PersonPrefixViewModel.PrefixId)
                                                </div>

                                                @*FirstName*@
                                                <div class="form-group">
                                                    @Html.CustomLabel("First Name")
                                                    <span id="first-name-info" class="ml-5"></span>
                                                    @Html.TextBoxFor(m => m.FirstName, new { @id = "first-name", @class = "form-control mandatory-mark", @placeholder = "Enter First Name" })
                                                    @Html.ValidationMessageFor(m => m.FirstName)
                                                </div>

                                                @*TransFirstName*@
                                                <div class="form-group">
                                                    @Html.CustomLabel(personDetailViewModel.FirstNameInRegionalLanguage)
                                                    @Html.TextBoxFor(m => m.TransFirstName, null, new { @id = "trans-first-name", @class = "form-control mandatory-mark", @placeholder = personDetailViewModel.FirstNamePlaceHolderInRegionalLanguage })
                                                    @Html.ValidationMessageFor(m => m.TransFirstName)
                                                </div>

                                                @*MiddleName*@
                                                <div class="form-group">
                                                    @Html.CustomLabel("Middle Name")
                                                    <span id="middle-name-info" class="ml-5"></span>
                                                    @Html.TextBoxFor(m => m.MiddleName, new { @id = "middle-name", @class = "form-control mandatory-mark", @placeholder = "Enter Middle Name" })
                                                    @Html.ValidationMessageFor(m => m.MiddleName)
                                                </div>

                                                @*TransMiddleName*@
                                                <div class="form-group">
                                                    @Html.CustomLabel(personDetailViewModel.MiddleNameInRegionalLanguage)
                                                    @Html.TextBoxFor(m => m.TransMiddleName, null, new { @id = "trans-middle-name", @class = "form-control deny-multiple-space trans mandatory-mark", @placeholder = personDetailViewModel.MiddleNamePlaceHolderInRegionalLanguage })
                                                    @Html.ValidationMessageFor(m => m.TransMiddleName)
                                                </div>

                                                @*LastName*@
                                                <div class="form-group">
                                                    @Html.CustomLabel("Last Name")
                                                    <span id="last-name-info" class="ml-5"></span>
                                                    @Html.TextBoxFor(m => m.LastName, new { @id = "last-name", @class = "form-control mandatory-mark", @placeholder = "Enter Last Name" })
                                                    @Html.ValidationMessageFor(m => m.LastName)
                                                </div>

                                                @*TransLastName*@
                                                <div class="form-group">
                                                    @Html.CustomLabel(personDetailViewModel.LastNameInRegionalLanguage)
                                                    @Html.TextBoxFor(m => m.TransLastName, null, new { @id = "trans-last-name", @class = "form-control mandatory-mark", @placeholder = personDetailViewModel.LastNamePlaceHolderInRegionalLanguage })
                                                    @Html.ValidationMessageFor(m => m.TransLastName)
                                                </div>

                                                @*DateOfBirth*@
                                                <div class="form-group">
                                                    @Html.CustomLabel("Date Of Birth")
                                                    @Html.TextBoxFor(m => m.DateOfBirth, "{0:yyyy-MM-dd}", new { id = "birth-date", @class = "form-control mandatory-mark deny-future-date allow-past-date-upto-150y", @Type = "date", required = "required" })
                                                    @Html.ValidationMessageFor(m => m.DateOfBirth)
                                                </div>

                                                @*DateOfBirthOnDocument*@
                                                <div class="form-group checked">
                                                    @Html.CustomLabel("Date Of Birth On Document")
                                                    @Html.TextBoxFor(m => m.DateOfBirthOnDocument, "{0:yyyy-MM-dd}", new { id = "document-birth-date", @class = "form-control mandatory-mark", @Type = "date", required = "required" })
                                                    <span id="document-birth-date-error" class="text-c-orenge d-none"><i class="fas fa-exclamation-triangle">&nbsp;&nbsp;Selected Date Of Birth On Your Document Appears To Be Significantly Different From Your Original Birthdate, Please Confirm If This Is Correct.</i></span>
                                                    @Html.ValidationMessageFor(m => m.DateOfBirthOnDocument)
                                                </div>

                                                @*MotherName*@
                                                <div class="form-group">
                                                    @Html.CustomLabel("Mother Name")
                                                    @Html.TextBoxFor(m => m.MotherName, new { @id = "mother-name", @class = "form-control mandatory-mark", @placeholder = "Enter Mother Name" })
                                                    @Html.ValidationMessageFor(m => m.MotherName)
                                                </div>

                                                @*TransMotherName*@
                                                <div class="form-group">
                                                    @Html.CustomLabel(personDetailViewModel.MotherNameInRegionalLanguage)
                                                    @Html.TextBoxFor(m => m.TransMotherName, null, new { @id = "trans-mother-name", @class = "form-control mandatory-mark", @placeholder = personDetailViewModel.MotherNamePlaceHolderInRegionalLanguage })
                                                    @Html.ValidationMessageFor(m => m.TransMotherName)
                                                </div>

                                                @*MothersMaidenName*@
                                                <div class="form-group">
                                                    @Html.CustomLabel("Mothers Maiden Name")
                                                    @Html.TextBoxFor(m => m.MothersMaidenName, new { @id = "mothers-maiden-name", @class = "form-control mandatory-mark", @placeholder = "Enter Mothers Maiden Name" })
                                                    @Html.ValidationMessageFor(m => m.MothersMaidenName)
                                                </div>

                                                @*TransMothersMaidenName*@
                                                <div class="form-group">
                                                    @Html.CustomLabel(personDetailViewModel.MothersMaidenNameInRegionalLanguage)
                                                    @Html.TextBoxFor(m => m.TransMothersMaidenName, null, new { @id = "trans-mothers-maiden-name", @class = "form-control mandatory-mark", @placeholder = personDetailViewModel.MothersMaidenNamePlaceHolderInRegionalLanguage })
                                                    @Html.ValidationMessageFor(m => m.TransMothersMaidenName)
                                                </div>

                                                @*ActivationDate*@
                                                <div class="form-group">
                                                    @Html.CustomLabel("Activation Date")
                                                    @Html.TextBoxFor(m => m.PersonPrefixViewModel.ActivationDate, "{0:yyyy-MM-dd}", new { @id = "activation-date-person-prefix", @class = "form-control mandatory-mark", @type = "date" })
                                                    @Html.ValidationMessageFor(m => m.PersonPrefixViewModel.ActivationDate)
                                                </div>

                                                @*Note*@
                                                <div class="form-group">
                                                    @Html.CustomLabel("Note")
                                                    @Html.TextAreaFor(m => m.Note, new { @id = "note", @class = "form-control", @placeholder = "Enter Note" })
                                                    @Html.ValidationMessageFor(m => m.Note)
                                                </div>

                                                @*TransNote*@
                                                <div class="form-group">
                                                    @Html.CustomLabel(personDetailViewModel.NoteInRegionalLanguage)
                                                    @Html.TextAreaFor(m => m.TransNote, new { @id = "trans-note", @class = "form-control", @placeholder = personDetailViewModel.NotePlaceHolderInRegionalLanguage })
                                                    @Html.ValidationMessageFor(m => m.TransNote)
                                                </div>
                                            </div>

                                            @*======= - Accordians =======*@
                                            <div id="accordion" class="accordion md-accordion accordion-blocks">
                                                @*Accordian 1 - Address Details*@
                                                <div id="heading-address-details" class="card mt-3">
                                                    <div class="card-header p-3" id="collapse-address-details-validations" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-address-details" data-target="#collapse-address-details">
                                                        <div class="row">
                                                            <div class="col-md-5">
                                                                <a class="card-title" href="#">
                                                                    <b>@Html.TextWithRegionalLanguage("Address Details")</b>
                                                                </a>
                                                            </div>
                                                            <div class="col-md-5">
                                                                <span id="address-details-accordion-error" class="error accordion-title-error d-none">This Is Required, Please Add Valid Data.</span>
                                                            </div>
                                                            <div class="col-2">
                                                                <a href="#collapse-address-details" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-address-details" data-target="#collapse-address-details">
                                                                    <i class="fa fa-angle-up"></i>
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div id="collapse-address-details" class="collapse" aria-labelledby="heading-address-details" data-parent="#accordion">
                                                        <div class="card-body">
                                                            <div class="row">
                                                                <div class="col-12">
                                                                    <div class="card-body">

                                                                        <table id="tbl-person-address" class="table compact table-striped table-bordered" cellspacing="0" width="100">
                                                                            <thead class="text-center read-only">
                                                                                <tr>
                                                                                    <th><input type="checkbox" name="check_all" id="select-all-person-address" style="margin-left:11px;" /></th>
                                                                                    <th class="d-none">AddressTypeID</th>
                                                                                    <th>AddressType</th>
                                                                                    <th>FlatDoorNo</th>
                                                                                    <th>TransFlatDoorNo</th>
                                                                                    <th>NameOfBuilding</th>
                                                                                    <th>TransNameOfBuilding</th>
                                                                                    <th>NameOfRoad</th>
                                                                                    <th>TransNameOfRoad</th>
                                                                                    <th>NameOfArea</th>
                                                                                    <th>TransNameOfArea</th>
                                                                                    <th class="d-none">CityID</th>
                                                                                    <th>City</th>
                                                                                    <th class="d-none">ResidenceTypeID</th>
                                                                                    <th>ResidenceType</th>
                                                                                    <th class="d-none">OwnershipTypeId</th>
                                                                                    <th>OwnershipType</th>
                                                                                    <th>Note</th>
                                                                                    <th>TransNote</th>
                                                                                    <th>ReasonForModification</th>
                                                                                    <th class="d-none">personAddressPrmKey</th>
                                                                                </tr>
                                                                            </thead>
                                                                            <tbody class="text-center read-only">
                                                                                @{
                                                                                    IEnumerable<PersonAddressViewModel> personAddressViewModelList = (IEnumerable<PersonAddressViewModel>)HttpContext.Current.Session["Address"];

                                                                                    foreach (PersonAddressViewModel viewModel in personAddressViewModelList)
                                                                                    {
                                                                                        <tr>
                                                                                            <td><input type="checkbox" name="check_all" class="checks" /></td>
                                                                                            <td class="d-none">@viewModel.AddressTypeId</td>
                                                                                            <td>@viewModel.NameOfAddressType</td>
                                                                                            <td>@viewModel.FlatDoorNo</td>
                                                                                            <td>@viewModel.TransFlatDoorNo</td>
                                                                                            <td>@viewModel.NameOfBuilding</td>
                                                                                            <td>@viewModel.TransNameOfBuilding</td>
                                                                                            <td>@viewModel.NameOfRoad</td>
                                                                                            <td>@viewModel.TransNameOfRoad</td>
                                                                                            <td>@viewModel.NameOfArea</td>
                                                                                            <td>@viewModel.TransNameOfArea</td>
                                                                                            <td class="d-none">@viewModel.CityId</td>
                                                                                            <td>@viewModel.NameOfCenter</td>
                                                                                            <td class="d-none">@viewModel.ResidenceTypeId</td>
                                                                                            <td>@viewModel.NameOfResidenceType</td>
                                                                                            <td class="d-none">@viewModel.OwnershipTypeId</td>
                                                                                            <td>@viewModel.NameOfOwnershipType</td>
                                                                                            <td>@viewModel.Note</td>
                                                                                            <td>@viewModel.TransNote</td>
                                                                                            <td>@viewModel.ReasonForModification</td>
                                                                                            <td class="d-none">@viewModel.PersonAddressPrmKey</td>
                                                                                        </tr>
                                                                                    }
                                                                                }
                                                                            </tbody>
                                                                        </table>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </div>
                                                </div>

                                                @*Accordian 2 - Person Additional Details*@
                                                <div id="heading-person-additional-details" class="card mt-3">
                                                    <div class="card-header" id="person-additional-details-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-person-additional-details" data-target="#collapse-person-additional-details">
                                                        <div class="row">
                                                            <div class="col-md-5">
                                                                <a class="card-title" href="#">
                                                                    <b>@Html.TextWithRegionalLanguage("Additional Details")</b>
                                                                </a>
                                                            </div>
                                                            <div class="col-md-5">
                                                                <span id="person-additional-details-accordion-error" class="error accordion-title-error d-none">This Is Required, Please Add Valid Data.</span>
                                                            </div>
                                                            <div class="col-2">
                                                                <a href="#collapse-person-additional-details" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-person-additional-details" data-target="#collapse-person-additional-details">
                                                                    <i class="fa fa-angle-up"></i>
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div id="collapse-person-additional-details" class="collapse" aria-labelledby="heading-person-additional-details" data-parent="#accordion">
                                                        <div class="card-body read-only">
                                                            <div class="row">
                                                                <div class="col-12">
                                                                    @*PersonCategory*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("Person Category")
                                                                        @Html.DropDownListFor(m => m.PersonAdditionalDetailViewModel.PersonCategoryId, new SelectList(personDetailViewModel.PersonCategoryDropdownList, "Value", "Text", 0), "--- Select Person Category ---", new { @id = "person-category-id", @class = "form-control mandatory-mark additional-details-input", required = "required" })
                                                                        @Html.ValidationMessageFor(m => m.PersonAdditionalDetailViewModel.PersonCategoryId)
                                                                    </div>

                                                                    @*Gender*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("Gender")
                                                                        @Html.DropDownListFor(m => m.PersonAdditionalDetailViewModel.GenderId, new SelectList(personDetailViewModel.GenderDropdownList, "Value", "Text"), "--- Select Gender ---", new { @id = "gender-id", @class = "form-control mandatory-mark additional-details-input", required = "required" })
                                                                        @Html.ValidationMessageFor(m => m.PersonAdditionalDetailViewModel.GenderId)
                                                                    </div>

                                                                    @*BirthCity*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("Birth City")
                                                                        @Html.DropDownListFor(m => m.PersonAdditionalDetailViewModel.BirthCityId, new SelectList(personDetailViewModel.VillageTownCityDropdownList, "Value", "Text", 0), "--- Select Birth City ---", new { @id = "birth-city-id", @class = "form-control mandatory-mark additional-details-input", required = "required" })
                                                                        @Html.ValidationMessageFor(m => m.PersonAdditionalDetailViewModel.BirthCityId)
                                                                    </div>

                                                                    @*MaritalStatus*@
                                                                    <div id="marital-status-input" class="form-group">
                                                                        @Html.CustomLabel("Marital Status")
                                                                        @Html.DropDownListFor(m => m.PersonAdditionalDetailViewModel.MaritalStatusId, new SelectList(personDetailViewModel.MaritalStatusDropdownList, "Value", "Text"), "--- Select Marital Status ---", new { @id = "marital-status-id", @class = "form-control mandatory-mark additional-details-input", required = "required" })
                                                                        @Html.ValidationMessageFor(m => m.PersonAdditionalDetailViewModel.MaritalStatusId)
                                                                    </div>

                                                                    @*LifePartnerName*@
                                                                    <div id="married-status-input" class="d-none">
                                                                        <div class="form-group">
                                                                            @Html.CustomLabel("Life Partner Name")
                                                                            @Html.TextBoxFor(m => m.PersonAdditionalDetailViewModel.LifePartnerName, new { @id = "life-partner-name", @class = "form-control alpha title-case mandatory-mark additional-details-input", @placeholder = "Enter Life Partner Name", onkeydown = "Maketranslation()", required = "required" })
                                                                            @Html.ValidationMessageFor(m => m.PersonAdditionalDetailViewModel.LifePartnerName)
                                                                        </div>

                                                                        @*TransLifePartnerName*@
                                                                        <div class="form-group">
                                                                            @Html.CustomLabel(personDetailViewModel.LifePartnerNameInRegionalLanguage)
                                                                            @Html.TextBoxFor(m => m.PersonAdditionalDetailViewModel.TransLifePartnerName, null, new { @id = "trans-life-partner-name", @class = "form-control deny-multiple-space mandatory-mark additional-details-input", @placeholder = personDetailViewModel.LifePartnerNamePlaceHolderInRegionalLanguage, required = "required" })
                                                                            @Html.ValidationMessageFor(m => m.PersonAdditionalDetailViewModel.TransLifePartnerName)
                                                                        </div>

                                                                        @*LifePartnerMaidenName*@
                                                                        <div class="form-group">
                                                                            @Html.CustomLabel("Maiden Name")
                                                                            @Html.TextBoxFor(m => m.PersonAdditionalDetailViewModel.LifePartnerMaidenName, new { @id = "life-partner-maiden-name", @class = "form-control alpha mandatory-mark title-case additional-details-input", @placeholder = "Enter Life Partner Maiden Name", onkeydown = "Maketranslation()", required = "required" })
                                                                            @Html.ValidationMessageFor(m => m.PersonAdditionalDetailViewModel.LifePartnerMaidenName)
                                                                        </div>

                                                                        @*TransLifePartnerMaidenName*@
                                                                        <div class="form-group">
                                                                            @Html.CustomLabel(personDetailViewModel.LifePartnerMaidenNameInRegionalLanguage)
                                                                            @Html.TextBoxFor(m => m.PersonAdditionalDetailViewModel.TransLifePartnerMaidenName, null, new { @id = "trans-life-partner-maiden-name", @class = "form-control deny-multiple-space mandatory-mark additional-details-input", @placeholder = personDetailViewModel.LifePartnerMaidenNamePlaceHolderInRegionalLanguage, required = "required" })
                                                                            @Html.ValidationMessageFor(m => m.PersonAdditionalDetailViewModel.TransLifePartnerMaidenName)
                                                                        </div>

                                                                        @*DateOfMarriage*@
                                                                        <div class="form-group">
                                                                            @Html.CustomLabel("Date Of Marriage")
                                                                            @Html.TextBoxFor(m => m.PersonAdditionalDetailViewModel.DateOfMarriage, "{0:yyyy-MM-dd}", new { @id = "date-of-marriage", @class = "form-control mandatory-mark valid-marriage-date-range additional-details-input", required = "required", @Type = "date", onclick = "ValidMarriageDateRange('birth-date')" })
                                                                            @Html.ValidationMessageFor(m => m.PersonAdditionalDetailViewModel.DateOfMarriage)
                                                                        </div>
                                                                    </div>


                                                                    @*IsResident*@
                                                                    <div class="row form-group mt-4">
                                                                        <div class="col-10">
                                                                            @Html.CustomLabel("Is Resident")
                                                                        </div>
                                                                        <div class="col-2">
                                                                            <div class="text-left">
                                                                                <label class="switch switch-left-right">
                                                                                    @Html.CheckBoxFor(m => m.PersonAdditionalDetailViewModel.IsResident, new { @id = "is-resident", @class = "form-control" })
                                                                                    <span class="slider round">
                                                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                    </span>
                                                                                </label>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    @*HasOtherCountryResidentStatus*@
                                                                    <div class="row form-group">
                                                                        <div class="col-10">
                                                                            @Html.CustomLabel("Has Other Country Resident Status")
                                                                        </div>
                                                                        <div class="col-2">
                                                                            <div class="text-left">
                                                                                <label class="switch switch-left-right">
                                                                                    @Html.CheckBoxFor(m => m.PersonAdditionalDetailViewModel.HasOtherCountryResidentStatus, new { @id = "has-other-country-resident-status", @class = "form-control" })
                                                                                    <span class="slider round">
                                                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                    </span>
                                                                                </label>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    @*BloodGroup*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("Blood Group")
                                                                        @Html.DropDownListFor(m => m.PersonAdditionalDetailViewModel.BloodGroupId, new SelectList(personDetailViewModel.BloodGroupDropdownList, "Value", "Text", 0), "--- Select Blood Group ---", new { @id = "blood-group-id", @class = "form-control mandatory-mark additional-details-input", required = "required" })
                                                                        @Html.ValidationMessageFor(m => m.PersonAdditionalDetailViewModel.BloodGroupId)
                                                                    </div>

                                                                    @*PovertyStatus*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("Poverty Status")
                                                                        @Html.DropDownListFor(m => m.PersonAdditionalDetailViewModel.PovertyStatusId, new SelectList(personDetailViewModel.PovertyStatusDropdownList, "Value", "Text"), "--- Select Poverty Status ---", new { @id = "poverty-status-id", @class = "form-control mandatory-mark additional-details-input", required = "required" })
                                                                        @Html.ValidationMessageFor(m => m.PersonAdditionalDetailViewModel.PovertyStatusId)
                                                                    </div>

                                                                    @*PhysicalStatus*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("Physical Status")
                                                                        @Html.DropDownListFor(m => m.PersonAdditionalDetailViewModel.PhysicalStatusId, new SelectList(personDetailViewModel.PhysicalStatusDropdownList, "Value", "Text"), "--- Select Physical Status ---", new { @id = "physical-status-id", @class = "form-control mandatory-mark additional-details-input", required = "required" })
                                                                        @Html.ValidationMessageFor(m => m.PersonAdditionalDetailViewModel.PhysicalStatusId)
                                                                    </div>

                                                                    @*CastCategoryPrmKey*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("Cast Category")
                                                                        @Html.DropDownListFor(m => m.PersonAdditionalDetailViewModel.CastCategoryId, new SelectList(personDetailViewModel.CastCategoryDropdownList, "Value", "Text"), "--- Select Cast Category ---", new { @id = "cast-category-id", @class = "form-control mandatory-mark additional-details-input", required = "required" })
                                                                        @Html.ValidationMessageFor(m => m.PersonAdditionalDetailViewModel.CastCategoryId)
                                                                    </div>

                                                                    @*EducationalQualificationPrmKey / Literacy of Client*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("Educational Qualification")
                                                                        @Html.DropDownListFor(m => m.PersonAdditionalDetailViewModel.EducationalQualificationId, new SelectList(personDetailViewModel.EducationQualificationDropdownList, "Value", "Text"), "--- Select Educational Qualification ---", new { @id = "educational-qualification-id", @class = "form-control mandatory-mark additional-details-input", required = "required" })
