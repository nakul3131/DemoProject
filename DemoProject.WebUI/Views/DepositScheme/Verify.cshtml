@model DemoProject.Services.ViewModel.Account.Layout.DepositSchemeViewModel
@using DemoProject.Services.HtmlHelpers
@using DemoProject.Services.Constants
@using DemoProject.Services.ViewModel.Account.Layout
@using DemoProject.Services.ViewModel.Configuration

@{
    LayoutDetailViewModel detailViewModel = new LayoutDetailViewModel();
    HeaderPartialModel headerPartialModel = new HeaderPartialModel("DSSVRF");
}

<!doctype html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>DepositScheme - Verify</title>

</head>
<body>
    <div class="container-fluid">
        @*======= First Row - For Header =======*@
        <div class="row">
            <div class="offset-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @*======= Second Row - Data Entry Page =======*@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card d-block mb-3">
                    @using (Html.BeginForm(null, null, FormMethod.Post, new { @id = "form", @autocomplete = "off" }))
                    {
                        @Html.AntiForgeryToken()
                        {
                            <div class="card-body">
                                @*======= First Row - Mandatory Mark =======*@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <h6 class="mb-2 text-right text-muted"><b class="mandatory-mark">&nbsp; Mandatory Field</b></h6>
                                    </div>
                                </div>

                                @*======= Second Row - Page Title, Translation =======*@
                                <div class="row mt-2">
                                    @*Page Title*@
                                    <div class="col-12">
                                        <h5 class="font-weight-bold mt-1">Verify</h5>
                                        <hr />
                                    </div>
                                </div>

                                @*Submit Error*@
                                @Html.ValidationMessage("SubmitError")

                                @*======= Third Row - Data Entry Page =======*@
                                <div class="row">
                                    <div class="col-12">
                                        <div id="verify-view" class="read-only">
                                            @*MakerRemark*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Maker Remark")
                                                @Html.TextBoxFor(m => m.MakerRemark, new { @id = "name-of-center", @class = "form-control mandatory-mark" })
                                                @Html.ValidationMessageFor(m => m.MakerRemark)
                                            </div>

                                            @*NameOfScheme*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Name Of Scheme")
                                                @Html.TextBoxFor(m => m.NameOfScheme, new { @id = "name-of-scheme", @class = "form-control mandatory-mark", required = "required", @maxlength = "100" })
                                                @Html.ValidationMessageFor(m => m.NameOfScheme)
                                            </div>

                                            @*TransNameOfScheme*@
                                            <div class="form-group">
                                                @Html.CustomLabel(detailViewModel.NameOfSchemeInRegionalLanguage)
                                                @Html.TextBoxFor(m => m.TransNameOfScheme, new { @id = "trans-name-of-scheme", @class = "form-control mandatory-mark", required = "required", @maxlength = "100", @autocomplete = "off" })
                                                @Html.ValidationMessageFor(m => m.TransNameOfScheme)
                                            </div>

                                            @*Alias Name*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Alias Name")
                                                @Html.TextBoxFor(m => m.AliasName, new { @id = "alias-name", @class = "form-control", required = "required", @maxlength = "10" })
                                                @Html.ValidationMessageFor(m => m.AliasName)
                                            </div>

                                            @*TransAliasName*@
                                            <div class="form-group">
                                                @Html.CustomLabel(detailViewModel.AliasNameInRegionalLanguage)
                                                @Html.TextBoxFor(m => m.TransAliasName, new { @id = "trans-alias-name", @class = "form-control", required = "required", @maxlength = "10", @autocomplete = "off" })
                                                @Html.ValidationMessageFor(m => m.TransAliasName)
                                            </div>

                                            @*NameOnReport*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Name On Report")
                                                @Html.TextBoxFor(m => m.NameOnReport, new { @id = "name-on-report", @class = "form-control", required = "required", @maxlength = "100" })
                                                @Html.ValidationMessageFor(m => m.NameOnReport)
                                            </div>

                                            @*TransNameOnReport*@
                                            <div class="form-group">
                                                @Html.CustomLabel(detailViewModel.NameOnReportInRegionalLanguage)
                                                @Html.TextBoxFor(m => m.TransNameOnReport, new { @id = "trans-name-on-report", @class = "form-control", required = "required", @maxlength = "100", @autocomplete = "off" })
                                                @Html.ValidationMessageFor(m => m.TransNameOnReport)
                                            </div>

                                            @*ActivationDate*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Activation Date")
                                                @Html.TextBoxFor(m => m.ActivationDate, "{0:yyyy-MM-dd}", new { @id = "activation-date-scheme", @class = "form-control activation-date mandatory-mark deny-next-financial-year-date deny-past-date", required = "required" })
                                                @Html.ValidationMessageFor(m => m.ActivationDate)
                                            </div>

                                            @*ExpiryDate*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Expiry Date")
                                                @Html.TextBoxFor(m => m.ExpiryDate, "{0:yyyy-MM-dd}", new { @id = "expiry-date-scheme", @class = "form-control expiry-date deny-past-date-from-reference-date" })
                                                @Html.ValidationMessageFor(m => m.ExpiryDate)
                                            </div>

                                            @*Note*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Note")
                                                @Html.TextAreaFor(m => m.Note, new { @id = "note", @class = "form-control default-none deny-multiple-space", @maxlength = 1500 })
                                                @Html.ValidationMessageFor(m => m.Note)
                                            </div>

                                            @*TransNote*@
                                            <div class="form-group">
                                                @Html.CustomLabel(detailViewModel.NoteInRegionalLanguage)
                                                @Html.TextAreaFor(m => m.TransNote, new { @id = "trans-note", @class = "form-control", @maxlength = "1500", @autocomplete = "off" })
                                                @Html.ValidationMessageFor(m => m.TransNote)
                                            </div>

                                            @*DepositType*@
                                            <div class="form-group DepositType">
                                                @Html.CustomLabel("Deposit Type")
                                                <div>
                                                    @Html.ValidationMessageFor(m => m.SchemeDepositAccountParameterViewModel.DepositType)
                                                </div>
                                                <div class="row mt-2">
                                                    <div class="col-12 col-md-6 col-xl-6">
                                                        <div class="form-check form-check-inline">
                                                            @Html.RadioButtonFor(m => m.SchemeDepositAccountParameterViewModel.DepositType, StringLiteralValue.DemandDeposit, new { @name = "SchemeDepositAccountParameterViewModel.DepositType", @class = "deposit-type" })
                                                            <label class="form-check-label pl-2">@Html.TextWithRegionalLanguage("Demand Deposit")</label>
                                                        </div>
                                                    </div>
                                                    <div class="col-12 col-md-6 col-xl-6">
                                                        <div class="form-check form-check-inline">
                                                            @Html.RadioButtonFor(m => m.SchemeDepositAccountParameterViewModel.DepositType, StringLiteralValue.FixedDeposit, new { @name = "SchemeDepositAccountParameterViewModel.DepositType", @class = "deposit-type" })
                                                            <label class="form-check-label pl-2">@Html.TextWithRegionalLanguage("Fixed Deposit")</label>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row mt-2">
                                                    <div class="col-12 col-md-6 col-xl-6">
                                                        <div class="form-check form-check-inline">
                                                            @Html.RadioButtonFor(m => m.SchemeDepositAccountParameterViewModel.DepositType, StringLiteralValue.RecurringDeposit, new { @name = "SchemeDepositAccountParameterViewModel.DepositType", @class = "deposit-type" })
                                                            <label class="form-check-label pl-2">@Html.TextWithRegionalLanguage("Recurring Deposit")</label>
                                                        </div>
                                                    </div>
                                                    <div class="col-12 col-md-6 col-xl-6 d-none">
                                                        <div class="form-check form-check-inline">
                                                            @Html.RadioButtonFor(m => m.SchemeDepositAccountParameterViewModel.DepositType, StringLiteralValue.PublicProvidentFund, new { @name = "SchemeDepositAccountParameterViewModel.DepositType", @class = "deposit-type" })
                                                            <label class="form-check-label pl-2">@Html.TextWithRegionalLanguage("Public Provident Fund")</label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        @*======= Fourth Row - Accordians =======*@
                                        <div class="row mt-1">
                                            <div class="col-12">
                                                <div id="accordion" class="accordion md-accordion accordion-blocks">
                                                    @*EnableTenure*@
                                                    <div id="enable-tenure-input" class="row form-group mt-3 ml-1 demand-deposit read-only">
                                                        <div class="col-10">
                                                            @Html.CustomLabel("Enable Tenure")
                                                        </div>

                                                        <div class="col-2">
                                                            <div class="text-left">
                                                                <label class="switch switch-left-right">
                                                                    @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableTenure, new { @id = "enable-tenure", @class = "form-control switch-input show-block" })
                                                                    <span class="slider round">
                                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                    </span>
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    @*Accordian 1 MinimumTenure*@
                                                    <div id="tenure-block" class="card tenure-accordian d-none">
                                                        <div class="card-header" id="tenure-parameter-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-tenure-parameter" data-target="#collapse-tenure-parameter">
                                                            <div class="row">
                                                                <div class="col-6">
                                                                    <a class="card-title" href="#">
                                                                        <b>@Html.TextWithRegionalLanguage("Tenure Parameter")</b>
                                                                    </a>
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <span id="tenure-accordion-error" class="error accordion-title-error d-none">This Is Required Accordion, Please Add Valid Data.</span>
                                                                </div>
                                                                <div class="col-2">
                                                                    <a href="#collapse-tenure-parameter" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-tenure-parameter" data-target="#collapse-tenure-parameter">
                                                                        <i class="fa fa-angle-down toggle-icon"></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div id="collapse-tenure-parameter" class="collapse" aria-labelledby="heading-tenure-parameter" data-parent="#accordion">
                                                            <div class="card-body read-only">
                                                                <div class="row">
                                                                    <div class="col-12">
                                                                        @*TimePeriodUnitId*@
                                                                        <div class="form-group">
                                                                            @Html.CustomLabel("Time Period Unit")
                                                                            @Html.DropDownListFor(m => m.SchemeTenureViewModel.TimePeriodUnitId, new SelectList(detailViewModel.TimePeriodUnitDropdownList, "Value", "Text"), "-- Select Time Period Unit --", new { @id = "time-period-unit-id", @class = "form-control mandatory-mark", required = "required" })
                                                                            @Html.ValidationMessageFor(m => m.SchemeTenureViewModel.TimePeriodUnitId)
                                                                        </div>

                                                                        @*MinimumTenure*@
                                                                        <div class="form-group">
                                                                            @Html.CustomLabel("Minimum Tenure")
                                                                            @Html.TextBoxFor(m => m.SchemeTenureViewModel.MinimumTenure, new { @id = "minimum-tenure", @class = "form-control mandatory-mark", required = "required" })
                                                                            @Html.ValidationMessageFor(m => m.SchemeTenureViewModel.MinimumTenure)
                                                                        </div>

                                                                        @*MultiplesOf*@
                                                                        <div class="form-group">
                                                                            @Html.CustomLabel("Multiples Of")
                                                                            @Html.TextBoxFor(m => m.SchemeTenureViewModel.MultiplesOf, new { @id = "tenure-multiple-of", @class = "form-control mandatory-mark", required = "required" })
                                                                            @Html.ValidationMessageFor(m => m.SchemeTenureViewModel.MultiplesOf)
                                                                        </div>

                                                                        @*MaximumTenure*@
                                                                        <div class="form-group">
                                                                            @Html.CustomLabel("Maximum Tenure")
                                                                            @Html.TextBoxFor(m => m.SchemeTenureViewModel.MaximumTenure, new { @id = "maximum-tenure", @class = "form-control mandatory-mark", required = "required" })
                                                                            @Html.ValidationMessageFor(m => m.SchemeTenureViewModel.MaximumTenure)
                                                                        </div>

                                                                        @*DefaultTenure*@
                                                                        <div class="form-group">
                                                                            @Html.CustomLabel("Default Tenure")
                                                                            @Html.TextBoxFor(m => m.SchemeTenureViewModel.DefaultTenure, new { @id = "default-tenure", @class = "form-control mandatory-mark", required = "required" })
                                                                            @Html.ValidationMessageFor(m => m.SchemeTenureViewModel.DefaultTenure)
                                                                        </div>

                                                                        @*Note*@
                                                                        <div class="form-group">
                                                                            @Html.CustomLabel("Note")
                                                                            @Html.TextAreaFor(m => m.SchemeTenureViewModel.Note, new { @id = "note-tenure", @class = "form-control default-none deny-multiple-space", @maxlength = "1500" })
                                                                            @Html.ValidationMessageFor(m => m.Note)
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    @*EnableTenureList*@
                                                    <div class="row form-group mt-3 ml-1 tenure-list read-only @((bool)(ViewBag.DepositSchemeParameter.EnableTenureListParameter) ? "demand-deposit" : "d-none")">
                                                        <div class="col-10">
                                                            @Html.CustomLabel("Enable Tenure List")
                                                        </div>

                                                        <div class="col-2">
                                                            <div class="text-left read-only">
                                                                <label class="switch switch-left-right">
                                                                    @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableTenureList, new { @id = "enable-tenure-list", @class = "form-control switch-input show-block" })
                                                                    <span class="slider round">
                                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                    </span>
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    @*Accordian 2 DataTable -Tenure List*@
                                                    <div id="tenure-list-block" class="card scheme-tenure-list-accordian d-none">
                                                        <div class="card-header" id="collapse-scheme-tenure-list-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-scheme-tenure-list" data-target="#collapse-scheme-tenure-list">
                                                            <div class="row">
                                                                <div class="col-6">
                                                                    <a class="card-title" href="#">
                                                                        <b>@Html.TextWithRegionalLanguage("Tenure List")</b>
                                                                    </a>
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <span id="tenure-list-accordion-error" class="error accordion-title-error d-none"> This Is Required Data Table, Please Add Valid Data.</span>
                                                                </div>
                                                                <div class="col-2">
                                                                    <a href="#collapse-scheme-tenure-list" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-scheme-tenure-list" data-target="#collapse-scheme-tenure-list">
                                                                        <i class="fa fa-angle-down toggle-icon"></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div id="collapse-scheme-tenure-list" class="collapse" aria-labelledby="heading-scheme-tenure-list" data-parent="#accordion">
                                                            <div class="card-body">
                                                                <div class="row">
                                                                    <div class="col-12">
                                                                        <div class="card-body">
                                                                            <table id="tbl-scheme-tenure-list" class="table compact table-striped table-bordered" width="100">
                                                                                <thead class="text-center read-only">
                                                                                    <tr>
                                                                                        <th><input type="checkbox" name="select-all" id="select-all-scheme-tenure-list" /></th>
                                                                                        <th>Tenure</th>
                                                                                        <th class="d-none">TenureUnit</th>
                                                                                        <th>TenureUnitText</th>
                                                                                        <th>TenureText</th>
                                                                                        <th>Note</th>
                                                                                    </tr>
                                                                                </thead>
                                                                                <tbody class="text-center read-only">
                                                                                    @{
                                                                                        IEnumerable<SchemeTenureListViewModel> schemeTenureListViewModelList = (IEnumerable<SchemeTenureListViewModel>)HttpContext.Current.Session["SchemeTenureList"];
                                                                                        if (schemeTenureListViewModelList.Count() > 0)
                                                                                        {

                                                                                            foreach (SchemeTenureListViewModel viewModel in schemeTenureListViewModelList)
                                                                                            {
                                                                                                <tr>
                                                                                                    <td><input type="checkbox" name="check_all" class="checks" /></td>
                                                                                                    <td>@viewModel.Tenure</td>
                                                                                                    <td class="d-none">@viewModel.TenureUnit</td>
                                                                                                    <td>@viewModel.TenureUnitText</td>
                                                                                                    <td>@viewModel.TenureText</td>
                                                                                                    <td>@viewModel.Note</td>
                                                                                                </tr>
                                                                                            }
                                                                                        }
                                                                                    }
                                                                                </tbody>
                                                                            </table>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    @*EnableApplication*@
                                                    <div class="row form-group mt-4 ml-1 read-only  @((bool)(ViewBag.DepositSchemeParameter.EnableApplicationParameter) ? "" : "d-none")">
                                                        <div class="col-10">
                                                            @Html.CustomLabel("Enable Application")
                                                        </div>
                                                        <div class="col-2">
                                                            <div class="text-left">
                                                                <label class="switch switch-left-right">
                                                                    @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableApplication, new { @id = "enable-application", @class = "form-control switch-input show-block" })
                                                                    <span class="slider round">
                                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                    </span>
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    @*Accordian 3 - Scheme Application Parameter*@
                                                    <div id="application-block" class="card application-parameter-accordian d-none">
                                                        <div class="card-header p-3" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-application-parameter" href="#collapse-application-parameter">
                                                            <div class="row">
                                                                <div class="col-6">
                                                                    <a class="card-title" href="#">
                                                                        <b>@Html.TextWithRegionalLanguage("Application Number")</b>
                                                                    </a>
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <span id="application-number-accordion-error" class="error accordion-title-error d-none">This Is Required Accordion, Please Add Valid Data.</span>
                                                                </div>
                                                                <div class="col-2">
                                                                    <a href="#collapse-application-parameter" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-application-parameter" data-target="#collapse-application-parameter">
                                                                        <i class="fa fa-angle-down toggle-icon"></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div id="collapse-application-parameter" class="collapse" aria-labelledby="heading-application-parameter" data-parent="#accordion">
                                                            <div class="card-body read-only">
                                                                <div class="row">
                                                                    <div class="col-12">
                                                                        @*EnableApplicationNumberBranchwise*@
                                                                        <div class="row form-group mt-2">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Application Number Branchwise")
                                                                            </div>

                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeApplicationParameterViewModel.EnableApplicationNumberBranchwise, new { @id = "enable-application-number-branchwise", @class = "form-control switch-input hide-block" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*if EnableApplicationNumberBranchwise then Hide This Field*@
                                                                        <div id="application-number-branchwise-block" class="row">
                                                                            <div class="col-12">
                                                                                @*EnableAutoApplicationNumber*@
                                                                                <div class="row form-group mt-2">
                                                                                    <div class="col-10">
                                                                                        @Html.CustomLabel("Enable Auto Application Number")
                                                                                    </div>

                                                                                    <div class="col-2">
                                                                                        <div class="text-left">
                                                                                            <label class="switch switch-left-right">
                                                                                                @Html.CheckBoxFor(m => m.SchemeApplicationParameterViewModel.EnableAutoApplicationNumber, new { @id = "enable-auto-application-number", @class = "form-control switch-input show-block" })
                                                                                                <span class="slider round">
                                                                                                    <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                                </span>
                                                                                            </label>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*if EnableAutoApplicationNumber Then Hide This Field*@
                                                                        <div id="auto-application-number-block" class="d-none">
                                                                            @*EnableRegenerateUnusedApplicationNumber*@
                                                                            <div class="row form-group">
                                                                                <div class="col-10">
                                                                                    @Html.CustomLabel("Regenerate Unused Application Number")
                                                                                </div>

                                                                                <div class="col-2">
                                                                                    <div class="text-left">
                                                                                        <label class="switch switch-left-right">
                                                                                            @Html.CheckBoxFor(m => m.SchemeApplicationParameterViewModel.EnableRegenerateUnusedApplicationNumber, new { @id = "enable-regenerate-unused-application-number", @class = "form-control switch-input" })
                                                                                            <span class="slider round">
                                                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                            </span>
                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </div>

                                                                            @*EnableRandomApplicationNumber*@
                                                                            <div class="row form-group">
                                                                                <div class="col-10">
                                                                                    @Html.CustomLabel("Enable Random Application Number")
                                                                                </div>

                                                                                <div class="col-2">
                                                                                    <div class="text-left">
                                                                                        <label class="switch switch-left-right">
                                                                                            @Html.CheckBoxFor(m => m.SchemeApplicationParameterViewModel.EnableRandomApplicationNumber, new { @id = "enable-random-application-number", @class = "form-control switch-input" })
                                                                                            <span class="slider round">

                                                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                            </span>
                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </div>

                                                                            @*EnableCustomizeApplicationNumber*@
                                                                            <div class="row form-group">
                                                                                <div class="col-10">
                                                                                    @Html.CustomLabel("Enable Customize Application Number")
                                                                                </div>

                                                                                <div class="col-2">
                                                                                    <div class="text-left">
                                                                                        <label class="switch switch-left-right">
                                                                                            @Html.CheckBoxFor(m => m.SchemeApplicationParameterViewModel.EnableCustomizeApplicationNumber, new { @id = "enable-customize-application-number", @class = "form-control switch-input" })
                                                                                            <span class="slider round">
                                                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                            </span>
                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </div>

                                                                            @*EnableDigitalCodeForApplicationNumber*@
                                                                            <div class="row form-group">
                                                                                <div class="col-10">
                                                                                    @Html.CustomLabel("Enable Digital Code For Application Number")
                                                                                </div>

                                                                                <div class="col-2">
                                                                                    <div class="text-left">
                                                                                        <label class="switch switch-left-right">
                                                                                            @Html.CheckBoxFor(m => m.SchemeApplicationParameterViewModel.EnableDigitalCodeForApplicationNumber, new { @id = "enable-digital-code-for-application-number", @class = "form-control switch-input" })
                                                                                            <span class="slider round">
                                                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                            </span>
                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </div>


                                                                            @*ApplicationNumberMask*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Application Number Mask")
