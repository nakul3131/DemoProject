    <style>
        #logo {
            vertical-align: middle;
        }

        .navbar-brand span {
            font-size: 1.2rem;
            line-height: 1.5;
            font-weight: 400;
            display: inline-block;
            vertical-align: middle;
            padding: 0 0 0 4px;
            color: #ffffff;
        }

        .navbar-brand b {
            display: block;
            font-size: 90%;
            line-height: 1.2;
        }

        .navbar-brand img {
            margin-left: -5%;
        }

        .navbar-brand span {
            border-left: 2px solid white;
        }


        .center-div {
            width: 200px;
            height: 200px;
            position: absolute;
            left: 50%;
            top: 50%;
            margin-left: -30px;
            margin-top: -50px;
        }

        .spinner {
            position: fixed;
            z-index: 9999;
            height: 100%;
            width: 100%;
            top: 0;
            left: 0;
            background: black;
            filter: alpha(opacity=30);
            opacity: 0.5;
            -webkit-moz-opacity: 0.8;
        }

        .loader {
            width: 56px;
            height: 56px;
            border-radius: 50%;
            background: radial-gradient(farthest-side,#4773ff 94%,#0000) top/9px 9px no-repeat, conic-gradient(#0000 30%,#4773ff);
            -webkit-mask: radial-gradient(farthest-side,#0000 calc(100% - 9px),#000 0);
            animation: spinner-c7wet2 1s infinite linear;
        }

        @@keyframes spinner-c7wet2 {
            100% {
                transform: rotate(1turn);
            }
        }

        .loader11 {
            width: 56px;
            height: 56px;
            border-radius: 50%;
            border: 10px solid #15a0ec;
            background: url('../../Images/Login/Loading_icon.gif') 50% 50% no-repeat rgb(249,249,249);
        }

        @@keyframes spinner-bulqg1 {
            0% {
                clip-path: polygon(50% 50%, 0 0, 50% 0%, 50% 0%, 50% 0%, 50% 0%, 50% 0%);
            }

            12.5% {
                clip-path: polygon(50% 50%, 0 0, 50% 0%, 100% 0%, 100% 0%, 100% 0%, 100% 0%);
            }

            25% {
                clip-path: polygon(50% 50%, 0 0, 50% 0%, 100% 0%, 100% 100%, 100% 100%, 100% 100%);
            }

            50% {
                clip-path: polygon(50% 50%, 0 0, 50% 0%, 100% 0%, 100% 100%, 50% 100%, 0% 100%);
            }

            62.5% {
                clip-path: polygon(50% 50%, 100% 0, 100% 0%, 100% 0%, 100% 100%, 50% 100%, 0% 100%);
            }

            75% {
                clip-path: polygon(50% 50%, 100% 100%, 100% 100%, 100% 100%, 100% 100%, 50% 100%, 0% 100%);
            }

            100% {
                clip-path: polygon(50% 50%, 50% 100%, 50% 100%, 50% 100%, 50% 100%, 50% 100%, 0% 100%);
            }
        }

        @@keyframes spinner-oaa3wk {
            0% {
                transform: scaleY(1) rotate(0deg);
            }

            49.99% {
                transform: scaleY(1) rotate(135deg);
            }

            50% {
                transform: scaleY(-1) rotate(0deg);
            }

            100% {
                transform: scaleY(-1) rotate(-135deg);
            }
        }


        .loading {
            width: 100%;
            height: 100%;
            position: fixed;
            top: 0;
            right: 0;
            bottom: 0;
            left: 0;
            z-index: 9999;
            background-color: rgba(0,0,0,.3);
        }

        .loading-wheel {
            width: 20px;
            height: 20px;
            margin-top: -40px;
            margin-left: -35px;
            position: absolute;
            top: 50%;
            left: 50%;
            border-width: 30px;
            border-radius: 50%;
            -webkit-animation: spin 1s linear infinite;
        }

        .style-2 .loading-wheel {
            width: 56px;
            height: 56px;
            border-radius: 50%;
            border: 9px solid #007bff;
            animation: spinner-bulqg1 0.8s infinite linear alternate, spinner-oaa3wk 1.6s infinite linear;
            z-index: 9999;
        }

        .itm {
            position: absolute;
            background-color: #f3f3f3;
            margin-top: 8%;
            z-index: 9999;
            width: 460px;
            display: none;
            overflow-y: auto;
            overflow-x: auto;
            box-shadow: rgba(60, 64, 67, 0.3) 0px 1px 2px 0px, rgba(60, 64, 67, 0.15) 0px 1px 3px 1px;
        }

        .badge.even-larger-badge {
            font-size: 1.1em;
        }
    </style>


</head>
