    <script>
           // Hide Parameters From URL
           window.history.replaceState(null, null, location.href.split("?")[0]);
           document.getElementById("search").addEventListener("search", function(event) {
                $(".itm").hide();
            });

                $(document).on("keyup", "#search", function (){
                    debugger
                    var val =$("#search").val();


                    if  (!isNaN(parseFloat(val)))
                    {
                        $(".itm").show();
                        $("ul.list").html("");
                        $("ul.list").append("<li class='ml-2'>"+ val +"<i class='fas fa-equals icons ml-2'></i><span class='font-weight-bold mt-1 badge badge-pill badge-primary text-white even-larger-badge'>"+ eval(val) + "</span></li>");
                    }
                    else
                    {
                        $(".itm").hide();
                    }

                $("#search").autocomplete(
                {
                    source: function (request, response)
                    {
                        debugger;
                            $.ajax(
                            {
                                url: '@Url.Action("Menusearch", "HomeJQ")',
                                data: "{ '_inputString': '" + request.term + "'}",
                                dataType: "json",
                                contentType: "application/json; charset=utf-8",
                                type: "POST",
                                async: false,
                                cache: true,
                                dataFilter: function (data) { return data; },
                                success: function (data)
                                {
                                    debugger;
                                    response($.map(data.slice(0, 10), function (item)
                                    {
                                        return { label: item.QueryText, value: item.SearchQueryId,}
                                    }))
                                },
                                error: function (response)
                                {
                                    alert(response.responseText);
                                },

                                failure: function (response)
                                {
                                    alert(response.responseText);
                                }
                            });

                        },

                        minLength: 3,
                        delay: 0,
                        focus: function (event, ui)
                        {
                            event.preventDefault();
                            $(this).val(ui.item.label);
                        },

                        select: function (event,ui)
                        {

                            event.preventDefault();
                            $(this).val(ui.item.label);

                            var selectedObj = ui.item;
                            var searchQueryId = selectedObj.value;
                            var selectedObj = ui.item;
                            var label = selectedObj.label;
                            //window.location.href = "/Home/Menus?id=" + ui.item.value;
                            var url = "/Home/Menus";
                            localStorage.setItem('htmltest1', label);
                            localStorage.setItem('htmltest', searchQueryId);
                            window.location.href = url;
                        },
               });
            })


            $(document).ready(function (event)
            {
                $("#wrapper").hide();
                $("#menu-toggle").click(function (e)
                {
                    $("#wrapper").show();
                    $(".sidebar-nav").show();
                    $('#sidebar-nav1').addClass("d-none");
                    $('#sidebar-nav2').addClass("d-none");
                });
                $("#menu-Recent").click(function (e)
                {
                    $("#sidebar-nav1 > li").empty();
                    $.get('/Home/GetRouteMenu', function (data) {
                        $.each(data, function (i, val) {
                            debugger;
                            $("#sidebar-nav1 #applist").prepend('<li style="margin-left:-8%!important;">'+ val +'</li>');
                        })
                    })
                    $("#wrapper").show();
                    $(".sidebar-nav").hide();
                    $('#sidebar-nav1').removeClass("d-none");
                    $('#sidebar-nav1').removeAttr('style');
                });

                $('#recent-menu').click(function() {
                    debugger;
                    $('#sidebar-nav1').addClass("d-none");
                    $(".sidebar-nav").hide();
                    $('#sidebar-nav2').removeClass("d-none");
                    $('#sidebar-nav2').removeAttr('style');


                });
                $('#recent-menu1').click(function() {
                    debugger;
                    $('#sidebar-nav2').addClass("d-none");
                    $(".sidebar-nav").hide();
                    $('#sidebar-nav1').removeClass("d-none");
                    $('#sidebar-nav1').removeAttr('style');

                });

            })
            $("#wrapper").mouseleave(function(){
                $('#wrapper').hide();

            });

