@model DemoProject.Services.ViewModel.PersonInformation.Parameter.PersonInformationParameterViewModel
@using DemoProject.Services.Constants
@using DemoProject.Services.HtmlHelpers
@using DemoProject.Services.ViewModel.Configuration
@using DemoProject.Services.ViewModel.PersonInformation.Parameter

@{
    PersonInformationParameterDetailViewModel detailViewModel = new PersonInformationParameterDetailViewModel();
    HeaderPartialModel headerPartialModel = new HeaderPartialModel("PIPVRF");
}

<!doctype html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>PersonInformationParameter - Verify</title>
</head>
<body>
    <div class="container-fluid">
        @*======= First Row - For Header =======*@
        <div class="row">
            <div class="offset-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @*======= Second Row - Data Entry Page =======*@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card mb-3">
                    @using (Html.BeginForm("Verify", "PersonInformationParameter", FormMethod.Post, htmlAttributes: new { id = "form", @autocomplete = "off" }))
                    {
                        @Html.AntiForgeryToken()
                        {
                            <div class="card-body ">
                                @*======= First  Row - Mandatory Mark =======*@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <h6 class="mb-2 text-right text-muted"><b class="mandatory-mark">&nbsp; Mandatory Field</b></h6>
                                    </div>
                                </div>

                                @*======= Second Row - Page Title =======*@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <h5 class="font-weight-bold mt-1">Verify</h5>
                                        <hr />
                                    </div>
                                </div>

                                @*Submit Error*@
                                @Html.ValidationMessage("SubmitError")

                                @*======= Third Row - Data Entry Page =======*@
                                <div class="row">
                                    <div class="col-12 read-only">
                                        @*Effective Date*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Effective Date")
                                            @Html.TextBoxFor(m => m.EffectiveDate, "{0:yyyy-MM-dd}", new { @id = "effective-date", @class = "form-control deny-past-date deny-next-financial-year-date mandatory-mark", @Type = "date" })
                                            @Html.ValidationMessageFor(m => m.EffectiveDate)
                                        </div>

                                        @*EnablePersonInformationNumberBranchwise*@
                                        <div class="row form-group mt-5">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Person Information Number Branchwise")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnablePersonInformationNumberBranchwise, new { @id = "enable-person-information-number-branchwise", @class = "form-control switch-input person-number-input show-block", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*EnableAutoGeneratePersonInformationNumbers*@
                                        <div class="row form-group mt-4">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Auto Generate Person Information Numbers")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnableAutoGeneratePersonInformationNumber, new { @id = "enable-auto-person-number", @class = "form-control switch-input person-number-input show-block", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*Auto Generate Person Information Number*@
                                        <span id="auto-person-number-error" class="error accordion-title-error d-none">This Is Required, Please Add Valid Data.</span>
                                        <div id="auto-person-number-block" class="d-none">
                                            @*PersonInformationNumberMask*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Person Information Number Mask")
                                                @Html.ListBoxFor(m => m.MaskTypeCharacter, new MultiSelectList(detailViewModel.MaskTypeDropdownList, "Value", "Text"), new { @id = "person-information-number-mask", @class = "form-control mandatory-mark multi-select2 person-number-input", @multiple = "multiple", required = "required" })
                                                @Html.ValidationMessageFor(m => m.MaskTypeCharacter)
                                            </div>

                                            @*ChecksumAlgorithm*@
                                            <div class="form-group">
                                                <label class="font-weight-bold">Checksum Algorithm</label>
                                                @Html.DropDownListFor(m => m.ChecksumAlgorithmId, new SelectList(detailViewModel.ChecksumAlgorithmDropdownList, "Value", "Text", 0), "--- Select Checksum Algorithm ---", new { @id = "checksum-algorithm-id", @class = "form-control mandatory-mark person-number-input", required = "required" })
                                                @Html.ValidationMessageFor(m => m.ChecksumAlgorithmId)
                                            </div>

                                            @*StartPersonInformationNumber*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Start Person Information Number")
                                                @Html.TextBoxFor(m => m.StartPersonInformationNumber, new { @id = "start-person-information-number", @class = "form-control mandatory-mark real-number person-number-input", @placeholder = "Enter Start Person Information Number", @type = "number", @min = 1, @max = 9999999, required = "required" })
                                                @Html.ValidationMessageFor(m => m.StartPersonInformationNumber)
                                            </div>

                                            @*EndPersonInformationNumber*@
                                            <div class="form-group">
                                                @Html.CustomLabel("End Person Information Number")
                                                @Html.TextBoxFor(m => m.EndPersonInformationNumber, new { @id = "end-person-information-number", @class = "form-control mandatory-mark real-number person-number-input", @placeholder = "Enter End Person Information Number", @type = "number", @max = 999999999999999, required = "required" })
                                                @Html.ValidationMessageFor(m => m.EndPersonInformationNumber)
                                            </div>

                                            @*PersonInformationNumberIncrementBy*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Person Information Number Increment By")
                                                @Html.TextBoxFor(m => m.PersonInformationNumberIncrementBy, new { @id = "person-information-number-increment-by", @class = "form-control mandatory-mark real-number person-number-input", @placeholder = "Enter Person Information Number Increment By", @type = "number", required = "required", @min = 1 })
                                                @Html.ValidationMessageFor(m => m.PersonInformationNumberIncrementBy)
                                            </div>

                                            @*EnableRandomPersonInformationNumber*@
                                            <div class="row form-group">
                                                <div class="col-10">
                                                    @Html.CustomLabel("Enable Random Person Information Number")
                                                </div>
                                                <div class="col-2">
                                                    <div class="text-left">
                                                        <label class="switch switch-left-right">
                                                            @Html.CheckBoxFor(m => m.EnableRandomPersonInformationNumber, new { @id = "enable-random-person-information-number", @class = "form-control switch-input", required = "required" })
                                                            <span class="slider round">
                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                            </span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>

                                            @*EnableRegenerateUnusedPersonInformationNumber*@
                                            <div class="row form-group">
                                                <div class="col-10">
                                                    @Html.CustomLabel("Enable Regenerate Unused Person Information Number")
                                                </div>
                                                <div class="col-2">
                                                    <div class="text-left">
                                                        <label class="switch switch-left-right">
                                                            @Html.CheckBoxFor(m => m.EnableRegenerateUnusedPersonInformationNumber, new { @id = "enable-regenerate-unused-person-information-number", @class = "form-control switch-input", required = "required" })
                                                            <span class="slider round">
                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                            </span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        @*EnablePersonInformationDigitalCode*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Person Information Digital Code")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnablePersonInformationDigitalCode, new { @id = "enable-person-information-digital-code", @class = "form-control switch-input", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*EnablePowerOfAttorney*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Power Of Attorney")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnablePowerOfAttorney, new { @id = "enable-power-of-attorney", @class = "form-control switch-input", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*EnableChronicDisease*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Chronic Disease")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnableChronicDisease, new { @id = "enable-chronic-disease", @class = "form-control switch-input", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*EnableSocialMedia*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Social Media")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnableSocialMedia, new { @id = "enable-social-media", @class = "form-control switch-input", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*EnableInsuranceDetail*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Insurance Detail")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnableInsuranceDetail, new { @id = "enable-insurance-detail", @class = "form-control switch-input", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*EnableFamilyDetails*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Family Details")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnableFamilyDetails, new { @id = "enable-family-details", @class = "form-control switch-input", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*EnablePersonInformationGroup*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Person Information Group")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnablePersonInformationGroup, new { @id = "enable-person-information-group", @class = "form-control switch-input", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*EnableMobileOTPForVerification*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Mobile OTP Verification")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnableMobileOTPForVerification, new { @id = "enable-mobile-verification", @class = "form-control switch-input show-block", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*If Enable Mobile OTP Verification*@
                                        <div id="mobile-verification-block" class="d-none">
                                            @*VerificationMobileOTPDataType*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Mobile OTP Data Type")
                                                @Html.DropDownListFor(m => m.VerificationMobileOTPDataType, new SelectList(detailViewModel.DataTypeDropdownList, "Value", "Text"), "--- Select Mobile OTP Data Type ---", new { @id = "otp-data-type-id", @class = "form-control otp-moblie-input mandatory-mark", required = "required" })
                                                @Html.ValidationMessageFor(m => m.VerificationMobileOTPDataType)
                                            </div>

                                            @*VerificationMobileOTPLength*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Mobile OTP Length")
                                                @Html.TextBoxFor(m => m.VerificationMobileOTPLength, new { @id = "otp-length", @class = "form-control otp-moblie-input real-number mandatory-mark", @placeholder = "Enter Mobile OTP Length", type = "number", required = "required", @min = 3, @max = 15 })
                                                @Html.ValidationMessageFor(m => m.VerificationMobileOTPLength)
                                            </div>

                                            @*PrefixStringForVerificationMobileOTP*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Prefix String For Mobile OTP")
                                                @Html.TextBoxFor(m => m.PrefixStringForVerificationMobileOTP, new { @id = "otp-prefix", @class = "form-control otp-moblie-input mandatory-mark alpha-numeric", @placeholder = "Enter Prefix String For Mobile OTP", required = "required", @maxlength = 3 })
                                                @Html.ValidationMessageFor(m => m.PrefixStringForVerificationMobileOTP)
                                            </div>

                                            @*PostfixStringForVerificationMobileOTP*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Postfix String For Mobile OTP")
                                                @Html.TextBoxFor(m => m.PostfixStringForVerificationMobileOTP, new { @id = "otp-postfix", @class = "form-control otp-moblie-input mandatory-mark alpha-numeric", @placeholder = "Enter Postfix String For Mobile OTP", @maxlength = 3, required = "required" })
                                                @Html.ValidationMessageFor(m => m.PostfixStringForVerificationMobileOTP)
                                            </div>

                                            @*IncludedCharactersForVerificationMobileOTP*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Include (Special) Characters For Mobile OTP")
                                                @Html.TextBoxFor(m => m.IncludedCharactersForVerificationMobileOTP, new { @id = "otp-include-charcter", @class = "form-control otp-moblie-input deny-multiple-space special-characters mandatory-mark", @placeholder = "Enter Include (Special) Characters For Mobile OTP", @minlength = 1, @maxlength = 30, required = "required" })
                                                @Html.ValidationMessageFor(m => m.IncludedCharactersForVerificationMobileOTP)
                                            </div>

                                            @*ExcludedCharactersForVerificationMobileOTP*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Exclude Characters From Mobile OTP")
                                                @Html.TextBoxFor(m => m.ExcludedCharactersForVerificationMobileOTP, new { @id = "otp-excluded-charcter", @class = "form-control otp-moblie-input deny-multiple-space mandatory-mark", @placeholder = "Enter Exclude Characters From Mobile OTP", @maxlength = 30, required = "required" })
                                                @Html.ValidationMessageFor(m => m.ExcludedCharactersForVerificationMobileOTP)
                                            </div>

                                            @*VerificationMobileOTPExpiryTime*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Mobile OTP Expiry Time")
                                                @Html.TextBoxFor(m => m.VerificationMobileOTPExpiryTime, new { @id = "otp-expiry-time", @class = "form-control otp-moblie-input mandatory-mark", @placeholder = "Enter Mobile OTP Expiry Time", @Type = "time", required = "required" })
                                                @Html.ValidationMessageFor(m => m.VerificationMobileOTPExpiryTime)
                                            </div>

                                            @*MaximumResendForVerificationMobileOTP*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Maximum Mobile OTP Resend Limit")
                                                @Html.TextBoxFor(m => m.MaximumResendForVerificationMobileOTP, new { @id = "otp-resend-limit", @class = "form-control real-number otp-moblie-input mandatory-mark", @placeholder = "Enter Maximum Mobile OTP Resend Limit", required = "required", @type = "number", @min = 0, @max = 9 })
                                                @Html.ValidationMessageFor(m => m.MaximumResendForVerificationMobileOTP)
                                            </div>
                                        </div>
                                        @*EnableAdditionalIncomeDetail*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Additional Income Detail")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnableAdditionalIncomeDetail, new { @id = "enable-additional-income-detail", @class = "form-control switch-input", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*EnableBorrowingDetail*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Borrowing Detail")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnableBorrowingDetail, new { @id = "enable-borrowing-detail", @class = "form-control switch-input", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*EnableCourtCaseDetail*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Court Case Detail")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnableCourtCaseDetail, new { @id = "enable-court-case-detail", @class = "form-control switch-input", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*EnableCreditRating*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Credit Rating")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnableCreditRating, new { @id = "enable-credit-rating", @class = "form-control switch-input", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*EnableCommoditiesAsset*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Commodities Asset")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnableCommoditiesAsset, new { @id = "enable-commodities-asset", @class = "form-control switch-input", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*======= Rows - Accordians =======*@
                                <div class="row mt-1">
                                    <div class="col-12 ">
                                        <div id="accordion" class="accordion md-accordion accordion-blocks">

                                            @*EnableSMSAlert*@
                                            <div class="row form-group">
                                                <div class="col-10">
                                                    @Html.CustomLabel("Enable SMS Alert")
                                                </div>
                                                <div class="col-2 read-only">
                                                    <div class="text-left">
                                                        <label class="switch switch-left-right">
                                                            @Html.CheckBoxFor(m => m.EnableSMSAlert, new { @id = "enable-sms-alert", @class = "form-control switch-input show-block", required = "required" })
                                                            <span class="slider round">
                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                            </span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>

                                            @*Accordian 1 - SMS Alert*@
                                            <div id="sms-alert-block" class="card mt-1">
                                                @*enable-notice-type*@
                                                <div class="card-header" id="sms-alert-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-sms-alert" data-target="#collapse-sms-alert">
                                                    <div class="row">
                                                        <div class="col-6">
                                                            <a class="card-title" href="#">
                                                                <b>@Html.TextWithRegionalLanguage("SMS Alert")</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <span id="sms-alert-data-table-error" class="error accordion-title-error d-none">This Is Required, Please Add Valid Data.</span>

                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-sms-alert" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-sms-alert" data-target="#collapse-sms-alert">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div id="collapse-sms-alert" class="collapse" aria-labelledby="heading-sms-alert" data-parent="#accordion">
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                <div class="card-body">
                                                                    <table id="tbl-sms-alert" class="table compact table-striped table-bordered" width="100">
                                                                        <thead class="text-center read-only">
                                                                            <tr>
                                                                                <th><input type="checkbox" name="check_all" id="select-all-sms-alert" /></th>
                                                                                <th class="d-none">NoticeTypeId</th>
                                                                                <th>NameOfNoticeType</th>
                                                                                <th>EnableNoticeInRegionalLanguage</th>
                                                                                <th>MaximumResendsOnFailure</th>
                                                                                <th>ActivationDate</th>
                                                                                <th>ExpiryDate</th>
                                                                                <th class="d-none">CloseDate</th>
                                                                                <th>Note</th>
                                                                            </tr>
                                                                        </thead>

                                                                        <tbody class="text-center read-only">
                                                                            @{
                                                                                IEnumerable<PersonInformationParameterNoticeTypeViewModel> personInformationParameterNoticeTypeViewModelList = (IEnumerable<PersonInformationParameterNoticeTypeViewModel>)HttpContext.Current.Session["PersonInformationParameterNoticeType"];

                                                                                if (personInformationParameterNoticeTypeViewModelList != null)
                                                                                {
                                                                                    foreach (PersonInformationParameterNoticeTypeViewModel viewModel in personInformationParameterNoticeTypeViewModelList)
                                                                                    {
