@model DemoProject.Services.ViewModel.Security.UserRoles.RoleProfileViewModel
@using DemoProject.Services.ViewModel.Configuration
@using DemoProject.Services.ViewModel.Security.UserRoles
@using DemoProject.Services.HtmlHelpers

@{
    RoleProfileDetailViewModel detailViewModel = new RoleProfileDetailViewModel();
    HeaderPartialModel headerPartialModel = new HeaderPartialModel("RPFAUT");
}

<!doctype html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0" />

    <title>Role Profile - Verify</title>
</head>
<body>
    <div class="container-fluid">
        @*======= First Row - For Header =======*@
        <div class="row">
            <div class="offset-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @*======= Second Row - Data Entry Page =======*@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card mb-3">
                    @using (Html.BeginForm(null, null, FormMethod.Post, new { @id = "form", @autocomplete = "off" }))
                    {
                        @Html.AntiForgeryToken()
                        {
                            <div class="card-body">
                                @*======= First  Row - Mandatory Mark =======*@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <h6 class="mb-2 text-right text-muted"><b class="mandatory-mark"> &nbsp; Mandatory Field</b></h6>
                                    </div>
                                </div>

                                @*======= Second Row - Page Title =======*@
                                <div class="row mt-2">
                                    @*Page Title If Screen Large Then Column - 12 Otherwise Col - 6 For Verify And Remaining For Translation*@
                                    <div class="col-12">
                                        <h5 class="font-weight-bold mt-1">Verify</h5>
                                        <hr />
                                    </div>
                                </div>

                                @*Submit Error*@
                                @Html.ValidationMessage("SubmitError")

                                @*======= Third Row - Data Entry Page =======*@
                                <div class="row read-only">
                                    <div class="col-12">
                                        @*RoleProfileCode*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Role Profile Code")
                                            @Html.TextBoxFor(m => m.RoleProfileCode, new { @id = "role-profile-code", @class = "form-control mandatory-mark deny-multiple-space", @placeholder = "Enter Role Profile Code", required = "required", @maxlength = "20", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.RoleProfileCode)
                                        </div>

                                        @*NameOfRoleProfile*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name Of Role Profile")
                                            @Html.TextBoxFor(m => m.NameOfRoleProfile, new { @id = "name-of-role-profile", @class = "form-control mandatory-mark deny-multiple-space", @placeholder = "Enter Name Of Role Profile", required = "required", @maxlength = "50", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.NameOfRoleProfile)
                                        </div>

                                        @*NameOfRoleProfile*@
                                        <div class="form-group">
                                            @Html.CustomLabel(detailViewModel.NameOfRoleProfileInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransNameOfRoleProfile, null, new { @id = "trans-name-of-role-profile", @class = "form-control mandatory-mark deny-multiple-space", @placeholder = detailViewModel.NameOfRoleProfilePlaceHolderInRegionalLanguage, required = "required", @maxlength = "50" })
                                            @Html.ValidationMessageFor(m => m.TransNameOfRoleProfile)
                                        </div>

                                        @*AliasName*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Alias Name")
                                            @Html.TextBoxFor(m => m.AliasName, new { @id = "alias-name", @class = "form-control deny-multiple-space", @placeholder = "Enter Alias Name", @maxlength = "10", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.AliasName)
                                        </div>

                                        @*Alias Name*@
                                        <div class="form-group">
                                            @Html.CustomLabel(detailViewModel.AliasNameInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransAliasName, null, new { @id = "trans-alias-name", @class = "form-control deny-multiple-space", @placeholder = detailViewModel.AliasNamePlaceHolderInRegionalLanguage, @maxlength = "10" })
                                            @Html.ValidationMessageFor(m => m.TransAliasName)
                                        </div>

                                        @*NameOnReport*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name On Report")
                                            @Html.TextBoxFor(m => m.NameOnReport, new { @id = "name-on-report", @class = "form-control deny-multiple-space", @placeholder = "Enter Name On Report", @maxlength = "100", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.NameOnReport)
                                        </div>

                                        @*Name On Report*@
                                        <div class="form-group">
                                            @Html.CustomLabel(detailViewModel.NameOnReportInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransNameOnReport, null, new { @id = "trans-name-on-report", @class = "form-control deny-multiple-space", @placeholder = detailViewModel.NameOnReportPlaceHolderInRegionalLanguage, @maxlength = "100" })
                                            @Html.ValidationMessageFor(m => m.TransNameOnReport)
                                        </div>

                                        @*IsFixedRole*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Fixed Role")
                                            </div>
                                            <div class="text-right">
                                                <label class="switch switch-left-right">
                                                    @Html.CheckBoxFor(m => m.IsFixedRole, new { @id = "IsFixedRole", @class = "form-control switch-input", required = "required" })
                                                    <span class="slider round">
                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                    </span>
                                                </label>
                                            </div>
                                        </div>

                                        @*IsCentralizeRole*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Centralize Role")
                                            </div>
                                            <div class="text-right">
                                                <label class="switch switch-left-right">
                                                    @Html.CheckBoxFor(m => m.IsCentralizeRole, new { @id = "IsCentralizeRole", @class = "form-control switch-input", required = "required" })
                                                    <span class="slider round">
                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                    </span>
                                                </label>
                                            </div>
                                        </div>

                                        @*GeneralLedgerAccessType*@
                                        <div class="form-group">
                                            @Html.CustomLabel("General Ledger Access Type")
                                            <div class="row">
                                                <div class="col-12">
                                                    <div class="col-5 mt-1 d-flex justify-content-between">
                                                        <div class="form-check form-check-inline">
                                                            @Html.RadioButtonFor(m => m.GeneralLedgerAccessType, "A", new { @name = "AGeneralLedgerAccessType" })
                                                            <label class="form-check-label pl-2">@Html.TextWithRegionalLanguage("Allowed")</label>
                                                        </div>
                                                        <div class="form-check form-check-inline">
                                                            @Html.RadioButtonFor(m => m.GeneralLedgerAccessType, "D", new { @name = "DGeneralLedgerAccessType" })
                                                            <label class="form-check-label pl-2">@Html.TextWithRegionalLanguage("Disallowed")</label>
                                                        </div>
                                                    </div>
                                                    <span id="access-type-required-error" class="error d-none">Select Any One Access Type</span>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>

                                        @*BusinessOfficeAccessType*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Business Office Access Type")
                                            <div class="row">
                                                <div class="col-12">
                                                    <div class="col-5 mt-1 d-flex justify-content-between">
                                                        <div class="form-check form-check-inline">
                                                            @Html.RadioButtonFor(m => m.BusinessOfficeAccessType, "A", new { @name = "ABusinessOfficeAccessType" })
                                                            <label class="form-check-label pl-2">@Html.TextWithRegionalLanguage("Allowed")</label>
                                                        </div>
                                                        <div class="form-check form-check-inline">
                                                            @Html.RadioButtonFor(m => m.BusinessOfficeAccessType, "D", new { @name = "DBusinessOfficeAccessType" })
                                                            <label class="form-check-label pl-2">@Html.TextWithRegionalLanguage("Disallowed")</label>
                                                        </div>
                                                    </div>
                                                    <span id="business-access-type-required-error" class="error d-none">Select Any One Business Access Type</span>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>

                                        @*IsAllowAllAccessForBusinessOffice*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Allow All Access For Business Office")
                                            </div>
                                            <div class="text-right">
                                                <label class="switch switch-left-right">
                                                    @Html.CheckBoxFor(m => m.IsAllowAllAccessForBusinessOffice, new { @id = "enable-is-allow-all-access-for-business-office", @class = "form-control switch-input hide-block", required = "required" })
                                                    <span class="slider round">
                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                    </span>
                                                </label>
                                            </div>
                                        </div>

                                        @*IsAllowAllAccessForGeneralLedger*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Allow All Access For General Ledger")
                                            </div>
                                            <div class="text-right">
                                                <label class="switch switch-left-right">
                                                    @Html.CheckBoxFor(m => m.IsAllowAllAccessForGeneralLedger, new { @id = "enable-is-allow-all-access-for-general-ledger", @class = "form-control switch-input hide-block", required = "required" })
                                                    <span class="slider round">
                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                    </span>
                                                </label>
                                            </div>
                                        </div>

                                        @*IsAllowAllAccessForMenu*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Allow All Access For Menu")
                                            </div>
                                            <div class="text-right">
                                                <label class="switch switch-left-right">
                                                    @Html.CheckBoxFor(m => m.IsAllowAllAccessForMenu, new { @id = "enable-is-allow-all-access-for-menu", @class = "form-control switch-input hide-block", required = "required" })
                                                    <span class="slider round">
                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                    </span>
                                                </label>
                                            </div>
                                        </div>

                                        @*IsAllowAllAccessForSpecialPermission*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Allow All Access For Special Permission")
                                            </div>
                                            <div class="text-right">
                                                <label class="switch switch-left-right">
                                                    @Html.CheckBoxFor(m => m.IsAllowAllAccessForSpecialPermission, new { @id = "enable-is-allow-all-access-for-special-permission", @class = "form-control switch-input hide-block", required = "required" })
                                                    <span class="slider round">
                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                    </span>
                                                </label>
                                            </div>
                                        </div>

                                        @*IsAllowAllTransactions*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Allow All Transactions")
                                            </div>
                                            <div class="text-right">
                                                <label class="switch switch-left-right">
                                                    @Html.CheckBoxFor(m => m.IsAllowAllTransactions, new { @id = "enable-is-allow-all-access-for-transactions", @class = "form-control switch-input hide-block", required = "required" })
                                                    <span class="slider round">
                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                    </span>
                                                </label>
                                            </div>
                                        </div>

                                        @*ActivationDate*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Activation Date")
                                            @Html.TextBoxFor(m => m.ActivationDate, "{0:yyyy-MM-dd}", new { @id = "activation-date", @class = "form-control mandatory-mark deny-past-date", required = "required", onclick = "DenyPastDate()" })
                                            @Html.ValidationMessageFor(m => m.ActivationDate)
                                        </div>

                                        @*ExpiryDate*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Expiry Date")
                                            @Html.TextBoxFor(m => m.ExpiryDate, "{0:yyyy-MM-dd}", new { @id = "expiry-date", @class = "form-control deny-past-date-from-reference-date", onclick = "DenyPastDateFromReferenceDate('activation-date')" })
                                            @Html.ValidationMessageFor(m => m.ExpiryDate)
                                        </div>

                                        @*Note*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Note")
                                            @Html.TextAreaFor(m => m.Note, new { @id = "note", @class = "form-control", @placeholder = "Enter Note", @maxlength = "4000", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.Note)
                                        </div>

                                        @*Note*@
                                        <div class="form-group">
                                            @Html.CustomLabel(detailViewModel.NoteInRegionalLanguage)
                                            @Html.TextAreaFor(m => m.TransNote, new { @id = "trans-note", @class = "form-control deny-multiple-space", @placeholder = detailViewModel.NotePlaceHolderInRegionalLanguage, @maxlength = "4000" })
                                            @Html.ValidationMessageFor(m => m.TransNote)
                                        </div>
                                    </div>
                                </div>

                                <div class="row mt-3">
                                    <div class="col-12">
                                        <div id="accordion" class="accordion md-accordion accordion-blocks">

                                            @*Accordian 1 - Role Profile General Ledger*@
                                            <div id="is-allow-all-access-for-general-ledger-block" class="card mt-3">
                                                <div class="card-header " id="user-profile-general-ledger-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-user-profile-general-ledger" data-target="#collapse-user-profile-general-ledger">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <a class="card-title" href="#">
                                                                <b>@Html.TextWithRegionalLanguage("General Ledger")</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <span id="general-ledger-data-table-error" class="error d-none"> This Is Required, Please Add Valid Data..</span>
                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-user-profile-general-ledger" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-user-profile-general-ledger" data-target="#collapse-user-profile-general-ledger">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div id="collapse-user-profile-general-ledger" class="collapse" aria-labelledby="heading-user-profile-general-ledger" data-parent="#accordion">
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                
                                                                @*General Ledger Data Table*@
                                                                <div class="card-body">
                                                                    <table id="tbl-general-ledger" class="table compact  table-striped table-bordered text-nowrap" cellspacing="0" width="100">
                                                                        <thead class="text-center read-only">
                                                                            <tr>
                                                                                <th><input type="checkbox" name="select-all" id="select-all-general-ledger" /></th>
                                                                                <th class="d-none">GeneralLedgerID</th>
                                                                                <th>NameOfGeneralLedger</th>
                                                                                <th>ActivationDate</th>
                                                                                <th>ExpiryDate</th>
                                                                                <th class="d-none">CloseDate</th>
                                                                                <th>Note</th>
                                                                                <th class="d-none">ReasonForModification</th>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody class="text-center read-only">
                                                                            @{
                                                                                IEnumerable<RoleProfileGeneralLedgerViewModel> RoleProfileGeneralLedgerViewModelList = (IEnumerable<RoleProfileGeneralLedgerViewModel>)HttpContext.Current.Session["RoleProfileGeneralLedger"];

                                                                                foreach (RoleProfileGeneralLedgerViewModel viewModel in RoleProfileGeneralLedgerViewModelList)
                                                                                {
                                                                                    <tr>
                                                                                        <td><input type="checkbox" name="check_all" class="checks" /></td>
                                                                                        <td class="d-none">@viewModel.GeneralLedgerId</td>
                                                                                        <td>@viewModel.NameOfGL</td>
                                                                                        <td>@viewModel.ActivationDate</td>
                                                                                        <td>@viewModel.ExpiryDate</td>
                                                                                        <td class="d-none">@viewModel.CloseDate</td>
                                                                                        <td>@viewModel.Note</td>
                                                                                        <td class="d-none">@viewModel.ReasonForModification</td>
                                                                                    </tr>
                                                                                }
                                                                            }
                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            @*Accordian 2 -Role Profile Business Office*@
                                            <div id="is-allow-all-access-for-business-office-block" class="card mt-3">
                                                <div class="card-header " id="business-office-parameter-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-business-office-parameter" data-target="#collapse-business-office-parameter">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <a class="card-title" href="#">
                                                                <b>@Html.TextWithRegionalLanguage("Business Office")</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <span id="business-office-data-table-error" class="error d-none"> This Is Required, Please Add Valid Data.</span>
                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-business-office-parameter" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-business-office-parameter" data-target="#collapse-business-office-parameter">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div id="collapse-business-office-parameter" class="collapse" aria-labelledby="heading-business-office-parameter" data-parent="#accordion">
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                @*Business Office Data Table*@
                                                                <div class="card-body">
                                                                    <table id="tbl-business-office" class="table compact table-striped table-bordered " width="100">
                                                                        <thead class="text-center read-only">
                                                                            <tr>
                                                                                <th><input type="checkbox" name="select-all" id="select-all-business-office" /></th>
                                                                                <th class="d-none">BusinessOfficeID</th>
                                                                                <th>NameOfBusinessOffice</th>
                                                                                <th>Activation Date</th>
                                                                                <th>ExpiryDate</th>
                                                                                <th class="d-none">CloseDate</th>
                                                                                <th>Note</th>
                                                                                <th class="d-none">Reason For Modification</th>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody class="text-center read-only">
                                                                            @{
                                                                                IEnumerable<RoleProfileBusinessOfficeViewModel> RoleProfileBusinessOfficeViewModelList = (IEnumerable<RoleProfileBusinessOfficeViewModel>)HttpContext.Current.Session["RoleProfileBusinessOffice"];

                                                                                foreach (RoleProfileBusinessOfficeViewModel viewModel in RoleProfileBusinessOfficeViewModelList)
                                                                                {
                                                                                    <tr>
                                                                                        <td><input type="checkbox" name="check_all" class="checks" /></td>
                                                                                        <td class="d-none">@viewModel.BusinessOfficeId</td>
                                                                                        <td>@viewModel.NameOfBusinessOffice</td>
                                                                                        <td>@viewModel.ActivationDate</td>
                                                                                        <td>@viewModel.ExpiryDate</td>
                                                                                        <td class="d-none">@viewModel.CloseDate</td>
                                                                                        <td>@viewModel.Note</td>
                                                                                        <td class="d-none">@viewModel.ReasonForModification</td>
                                                                                    </tr>
                                                                                }
                                                                            }
                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>

                                            @*Accordian 3 - Role Profile  TransactionLimit*@
                                            <div id="is-allow-all-access-for-transactions-block" class="card mt-3">
                                                <div class="card-header " id="transaction-limit-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-transaction-limit" data-target="#collapse-transaction-limit">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <a class="card-title" href="#">
                                                                <b>@Html.TextWithRegionalLanguage("Transaction Limit")</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <span id="transaction-limit-data-table-error" class="error d-none">This Is Required, Please Add Valid Data.</span>
                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-transaction-limit" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-transaction-limit" data-target="#collapse-transaction-limit">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div id="collapse-transaction-limit" class="collapse" aria-labelledby="heading-transaction-limit" data-parent="#accordion">
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                @*Transaction Limit Data Table*@
                                                                <div class="card-body">
                                                                    <table id="tbl-transaction-limit" class="table compact table-striped table-bordered " width="100">
                                                                        <thead class="text-center read-only">
                                                                            <tr>
                                                                                <th><input type="checkbox" name="select-all" id="select-all-transaction-limit" /></th>
                                                                                <th class="d-none">GeneralLedgerId</th>
                                                                                <th>NameOfGeneralLedger</th>
                                                                                <th class="d-none">TransactionTypeId</th>
                                                                                <th>NameOfTransactionType</th>
                                                                                <th class="d-none">CurrencyId</th>
                                                                                <th>NameOfCurrency</th>
                                                                                <th>MinimumAmountLimitForTransaction</th>
                                                                                <th>MaximumAmountLimitForTransaction</th>
                                                                                <th>MaximumNumberOfBackDaysForTransaction</th>
                                                                                <th>MinimumAmountLimitForVerification</th>
                                                                                <th>MaximumAmountLimitForVerification</th>
                                                                                <th>MaximumNumberOfBackDaysForVerification</th>
                                                                                <th>MinimumAmountLimitForAutoVerification</th>
                                                                                <th>MaximumAmountLimitForAutoVerification</th>
                                                                                <th>MaximumNumberOfBackDaysForAutoVerification</th>
                                                                                <th>ActivationDate</th>
                                                                                <th>ExpiryDate</th>
                                                                                <th class="d-none">CloseDate</th>
                                                                                <th>Note</th>
                                                                                <th class="d-none">ReasonForModification</th>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody class="text-center read-only">
                                                                            @{
                                                                                IEnumerable<RoleProfileTransactionLimitViewModel> RoleProfileTransactionLimitViewModelList = (IEnumerable<RoleProfileTransactionLimitViewModel>)HttpContext.Current.Session["RoleProfileTransactionLimit"];

                                                                                foreach (RoleProfileTransactionLimitViewModel viewModel in RoleProfileTransactionLimitViewModelList)
                                                                                {
                                                                                    <tr>
                                                                                        <td><input type="checkbox" name="check_all" class="checks" /></td>
                                                                                        <td class="d-none">@viewModel.GeneralLedgerId</td>
                                                                                        <td>@viewModel.NameOfGL</td>
                                                                                        <td class="d-none">@viewModel.TransactionTypeId</td>
                                                                                        <td>@viewModel.NameOfTransactionType</td>
                                                                                        <td class="d-none">@viewModel.CurrencyId</td>
                                                                                        <td>@viewModel.NameOfCurrency</td>
                                                                                        <td>@viewModel.MinimumAmountLimitForTransaction</td>
                                                                                        <td>@viewModel.MaximumAmountLimitForTransaction</td>
                                                                                        <td>@viewModel.MaximumNumberOfBackDaysForTransaction</td>
                                                                                        <td>@viewModel.MinimumAmountLimitForVerification</td>
                                                                                        <td>@viewModel.MaximumAmountLimitForVerification</td>
                                                                                        <td>@viewModel.MaximumNumberOfBackDaysForVerification</td>
                                                                                        <td>@viewModel.MinimumAmountLimitForAutoVerification</td>
                                                                                        <td>@viewModel.MaximumAmountLimitForAutoVerification</td>
                                                                                        <td>@viewModel.MaximumNumberOfBackDaysForAutoVerification</td>
                                                                                        <td>@viewModel.ActivationDate</td>
