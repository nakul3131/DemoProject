@model DemoProject.Services.ViewModel.Security.UserRoles.RoleProfileViewModel
@using DemoProject.Services.ViewModel.Configuration
@using DemoProject.Services.ViewModel.Security.UserRoles
@using DemoProject.Services.HtmlHelpers

@{
    RoleProfileDetailViewModel detailViewModel = new RoleProfileDetailViewModel();
    HeaderPartialModel headerPartialModel = new HeaderPartialModel("RPFAMN");
}

<!doctype html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0" />

    <title>Role Profile - Amend</title>
</head>
<body>
    <div class="container-fluid">
        @*======= First Row - For Header =======*@
        <div class="row">
            <div class="offset-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @*======= Second Row - Data Entry Page =======*@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card mb-3">
                    @using (Html.BeginForm(null, null, FormMethod.Post, new { @id = "form", @autocomplete = "off" }))
                    {
                        @Html.AntiForgeryToken()
                        {
                            <div class="card-body">
                                @*======= First  Row - Mandatory Mark =======*@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <h6 class="mb-2 text-right text-muted"><b class="mandatory-mark"> &nbsp; Mandatory Field</b></h6>
                                    </div>
                                </div>

                                @*======= Second Row - Page Title =======*@
                                <div class="row mt-2">
                                    @*Page Title If Screen Large Then Column - 12 Otherwise Col - 6 For Amend And Remaining For Translation*@
                                    <div class="col-12">
                                        <h5 class="font-weight-bold mt-1">Amend</h5>
                                        <hr />
                                    </div>
                                </div>

                                @*Submit Error*@
                                @Html.ValidationMessage("SubmitError")

                                @*======= Third Row - Data Entry Page =======*@
                                <div class="row">
                                    <div class="col-12">
                                        @*CheckerRemark*@
                                        <div class="form-group read-only">
                                            @Html.CustomLabel("Checker Remark")
                                            @Html.TextBoxFor(m => m.CheckerRemark, new { @id = "name-of-center", @class = "form-control mandatory-mark " })
                                            @Html.ValidationMessageFor(m => m.CheckerRemark)
                                        </div>

                                        @*RoleProfileCode*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Role Profile Code")
                                            @Html.TextBoxFor(m => m.RoleProfileCode, new { @id = "role-profile-code", @class = "form-control alpha-numeric mandatory-mark ", @placeholder = "Enter Role Profile Code", required = "required", @maxlength = 20, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.RoleProfileCode)
                                        </div>

                                        @*NameOfRoleProfile*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name Of Role Profile")
                                            @Html.TextBoxFor(m => m.NameOfRoleProfile, new { @id = "name-of-role-profile", @class = "form-control alpha-numeric mandatory-mark ", @placeholder = "Enter Name Of Role Profile", required = "required", @maxlength = 50, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.NameOfRoleProfile)
                                        </div>

                                        @*NameOfRoleProfile*@
                                        <div class="form-group">
                                            @Html.CustomLabel(detailViewModel.NameOfRoleProfileInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransNameOfRoleProfile, null, new { @id = "trans-name-of-role-profile", @class = "form-control alpha-numeric mandatory-mark", @placeholder = detailViewModel.NameOfRoleProfilePlaceHolderInRegionalLanguage, required = "required", @maxlength = 50 })
                                            @Html.ValidationMessageFor(m => m.TransNameOfRoleProfile)
                                        </div>

                                        @*AliasName*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Alias Name")
                                            @Html.TextBoxFor(m => m.AliasName, new { @id = "alias-name", @class = "form-control default-none alpha-numeric", @placeholder = "Enter Alias Name", @maxlength = 10, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.AliasName)
                                        </div>

                                        @*TransAlias Name*@
                                        <div class="form-group">
                                            @Html.CustomLabel(detailViewModel.AliasNameInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransAliasName, null, new { @id = "trans-alias-name", @class = "form-control default-none alpha-numeric", @placeholder = detailViewModel.AliasNamePlaceHolderInRegionalLanguage, @maxlength = 10 })
                                            @Html.ValidationMessageFor(m => m.TransAliasName)
                                        </div>

                                        @*NameOnReport*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name On Report")
                                            @Html.TextBoxFor(m => m.NameOnReport, new { @id = "name-on-report", @class = "form-control default-none alpha-numeric", @placeholder = "Enter Name On Report", @maxlength = 100, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.NameOnReport)
                                        </div>

                                        @*TransName On Report*@
                                        <div class="form-group">
                                            @Html.CustomLabel(detailViewModel.NameOnReportInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransNameOnReport, null, new { @id = "trans-name-on-report", @class = "form-control default-none alpha-numeric", @placeholder = detailViewModel.NameOnReportPlaceHolderInRegionalLanguage, @maxlength = 100 })
                                            @Html.ValidationMessageFor(m => m.TransNameOnReport)
                                        </div>

                                        @*IsFixedRole*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Fixed Role")
                                            </div>
                                            <div class="text-right">
                                                <label class="switch switch-left-right">
                                                    @Html.CheckBoxFor(m => m.IsFixedRole, new { @id = "IsFixedRole", @class = "form-control switch-input", required = "required" })
                                                    <span class="slider round">
                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                    </span>
                                                </label>
                                            </div>
                                        </div>

                                        @*IsCentralizeRole*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Centralize Role")
                                            </div>
                                            <div class="text-right">
                                                <label class="switch switch-left-right">
                                                    @Html.CheckBoxFor(m => m.IsCentralizeRole, new { @id = "IsCentralizeRole", @class = "form-control switch-input", required = "required" })
                                                    <span class="slider round">
                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                    </span>
                                                </label>
                                            </div>
                                        </div>

                                        @*GeneralLedgerAccessType*@
                                        <div class="form-group">
                                            @Html.CustomLabel("General Ledger Access Type")
                                            <div class="row">
                                                <div class="col-12">
                                                    <div class="col-5 mt-1 d-flex justify-content-between">
                                                        <div class="form-check form-check-inline">
                                                            @Html.RadioButtonFor(m => m.GeneralLedgerAccessType, "A", new { @id = "Allowed", @name = "AGeneralLedgerAccessType", @class = "access-type" })
                                                            <label for="Allowed" class="form-check-label pl-2">@Html.TextWithRegionalLanguage("Allowed")</label>
                                                        </div>
                                                        <div class="form-check form-check-inline">
                                                            @Html.RadioButtonFor(m => m.GeneralLedgerAccessType, "D", new { @id = "Disallowed", @name = "DGeneralLedgerAccessType", @class = "access-type" })
                                                            <label for="Disallowed" class="form-check-label pl-2">@Html.TextWithRegionalLanguage("Disallowed")</label>
                                                        </div>
                                                    </div>
                                                    <span id="access-type-required-error" class="error d-none">Select Any One Access Type</span>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>

                                        @*BusinessOfficeAccessType*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Business Office Access Type")
                                            <div class="row">
                                                <div class="col-12">
                                                    <div class="col-5 mt-1 d-flex justify-content-between">
                                                        <div class="form-check form-check-inline">
                                                            @Html.RadioButtonFor(m => m.BusinessOfficeAccessType, "A", new { @id = "Allowedb", @name = "ABusinessOfficeAccessType", @class = "business-access-type" })
                                                            <label for="Allowedb" class="form-check-label pl-2">@Html.TextWithRegionalLanguage("Allowed")</label>
                                                        </div>
                                                        <div class="form-check form-check-inline">
                                                            @Html.RadioButtonFor(m => m.BusinessOfficeAccessType, "D", new { @id = "Disallowedb", @name = "DBusinessOfficeAccessType", @class = "business-access-type" })
                                                            <label for="Disallowedb" class="form-check-label pl-2">@Html.TextWithRegionalLanguage("Disallowed")</label>
                                                        </div>
                                                    </div>
                                                    <span id="business-access-type-required-error" class="error d-none">Select Any One Business Access Type</span>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>

                                        @*IsAllowAllAccessForBusinessOffice*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Allow All Access For Business Office")
                                            </div>
                                            <div class="text-right">
                                                <label class="switch switch-left-right">
                                                    @Html.CheckBoxFor(m => m.IsAllowAllAccessForBusinessOffice, new { @id = "enable-is-allow-all-access-for-business-office", @class = "form-control switch-input hide-block", required = "required" })
                                                    <span class="slider round">
                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                    </span>
                                                </label>
                                            </div>
                                        </div>

                                        @*IsAllowAllAccessForGeneralLedger*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Allow All Access For General Ledger")
                                            </div>
                                            <div class="text-right">
                                                <label class="switch switch-left-right">
                                                    @Html.CheckBoxFor(m => m.IsAllowAllAccessForGeneralLedger, new { @id = "enable-is-allow-all-access-for-general-ledger", @class = "form-control switch-input hide-block", required = "required" })
                                                    <span class="slider round">
                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                    </span>
                                                </label>
                                            </div>
                                        </div>

                                        @*IsAllowAllAccessForMenu*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Allow All Access For Menu")
                                            </div>
                                            <div class="text-right">
                                                <label class="switch switch-left-right">
                                                    @Html.CheckBoxFor(m => m.IsAllowAllAccessForMenu, new { @id = "enable-is-allow-all-access-for-menu", @class = "form-control switch-input hide-block", required = "required" })
                                                    <span class="slider round">
                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                    </span>
                                                </label>
                                            </div>
                                        </div>

                                        @*IsAllowAllAccessForSpecialPermission*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Allow All Access For Special Permission")
                                            </div>
                                            <div class="text-right">
                                                <label class="switch switch-left-right">
                                                    @Html.CheckBoxFor(m => m.IsAllowAllAccessForSpecialPermission, new { @id = "enable-is-allow-all-access-for-special-permission", @class = "form-control switch-input hide-block", required = "required" })
                                                    <span class="slider round">
                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                    </span>
                                                </label>
                                            </div>
                                        </div>

                                        @*IsAllowAllTransactions*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Allow All Transactions")
                                            </div>
                                            <div class="text-right">
                                                <label class="switch switch-left-right">
                                                    @Html.CheckBoxFor(m => m.IsAllowAllTransactions, new { @id = "enable-is-allow-all-access-for-transactions", @class = "form-control switch-input hide-block", required = "required" })
                                                    <span class="slider round">
                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                    </span>
                                                </label>
                                            </div>
                                        </div>

                                        @*ActivationDate*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Activation Date")
                                            @Html.TextBoxFor(m => m.ActivationDate, "{0:yyyy-MM-dd}", new { @id = "activation-date", @class = "form-control mandatory-mark deny-past-date set-current-date", required = "required", @Type = "date", onclick = "DenyPastDate()" })
                                            @Html.ValidationMessageFor(m => m.ActivationDate)
                                        </div>

                                        @*ExpiryDate*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Expiry Date")
                                            @Html.TextBoxFor(m => m.ExpiryDate, "{0:yyyy-MM-dd}", new { @id = "expiry-date", @class = "form-control deny-past-date-from-reference-date allow-future-date-upto-50y", @Type = "date", onclick = "DenyPastDateFromReferenceDate('activation-date')" })
                                            @Html.ValidationMessageFor(m => m.ExpiryDate)
                                        </div>

                                        @*Note*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Note")
                                            @Html.TextAreaFor(m => m.Note, new { @id = "note", @class = "form-control deny-multiple-space", @placeholder = "Enter Note", @maxlength = 4000, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.Note)
                                        </div>

                                        @*Note*@
                                        <div class="form-group">
                                            @Html.CustomLabel(detailViewModel.NoteInRegionalLanguage)
                                            @Html.TextAreaFor(m => m.TransNote, new { @id = "trans-note", @class = "form-control deny-multiple-space", @placeholder = detailViewModel.NotePlaceHolderInRegionalLanguage, @maxlength = 4000 })
                                            @Html.ValidationMessageFor(m => m.TransNote)
                                        </div>
                                    </div>
                                </div>

                                <div class="row mt-3">
                                    <div class="col-12">
                                        <div id="accordion" class="accordion md-accordion accordion-blocks">

                                            @*Accordian 1 - Role Profile General Ledger*@
                                            <div id="is-allow-all-access-for-general-ledger-block" class="card mt-3">
                                                <div class="card-header " id="user-profile-general-ledger-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-user-profile-general-ledger" data-target="#collapse-user-profile-general-ledger">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <a class="card-title" href="#">
                                                                <b>@Html.TextWithRegionalLanguage("General Ledger")</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <span id="general-ledger-data-table-error" class="error d-none"> This Is Required, Please Add Valid Data..</span>
                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-user-profile-general-ledger" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-user-profile-general-ledger" data-target="#collapse-user-profile-general-ledger">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div id="collapse-user-profile-general-ledger" class="collapse" aria-labelledby="heading-user-profile-general-ledger" data-parent="#accordion">
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                @*General Ledger / Popup Page*@
                                                                <div class="modal fade" id="general-ledger-modal" tabindex="-1" data-backdrop="static" role="dialog" aria-labelledby="modal-new-event" aria-hidden="true">
                                                                    <div class="modal-dialog modal-dialog-centered modal-dialog-zoom modal-md" role="document">
                                                                        <div class="modal-content">
                                                                            <div class="modal-header">
                                                                                <h4 class="modal-title text-center">Add</h4>
                                                                                <button type="button" id="close-general-ledger-modal" class="close" data-dismiss="modal" aria-label="Close">
                                                                                    <span aria-hidden="true">&times;</span>
                                                                                </button>
                                                                            </div>
                                                                            <div class="modal-body">
                                                                                @*General Ledger*@
                                                                                <div class="form-group">
                                                                                    @Html.CustomLabel("General Ledger")
                                                                                    <div class="mr-3">
                                                                                        @Html.ListBoxFor(m => m.RoleProfileGeneralLedgerViewModel.MultiGeneralLedgerId, new SelectList(detailViewModel.GeneralLedgerDropdownList, "Value", "Text"), new { @id = "general-ledger-id", @class = "form-control modal-input mandatory-mark", @multiple = "multiple" })
                                                                                        @Html.ValidationMessageFor(m => m.RoleProfileGeneralLedgerViewModel.MultiGeneralLedgerId)
                                                                                        <span id="user-profile-general-ledger-id-error" class="error modal-input-error d-none"> Please Select General Ledger Name.</span>
                                                                                    </div>
                                                                                </div>

                                                                                @*ActivationDate*@
                                                                                <div class="form-group">
                                                                                    @Html.CustomLabel("Activation Date")
                                                                                    @Html.TextBoxFor(m => m.RoleProfileGeneralLedgerViewModel.ActivationDate, "{0:yyyy-MM-dd}", new { @class = "form-control mandatory-mark activation-date deny-next-financial-year-date deny-past-date modal-input set-current-date", @id = "activation-date-general-ledger", @Type = "date", onclick = "DenyPastDate()", required = "required" })
                                                                                    @Html.ValidationMessageFor(m => m.RoleProfileGeneralLedgerViewModel.ActivationDate)
                                                                                    <span id="activation-date-general-ledger-error" class="error modal-input-error d-none"> Please Enter Activation Date.</span>
                                                                                </div>

                                                                                @*ExpiryDate*@
                                                                                <div class="form-group">
                                                                                    @Html.CustomLabel("Expiry Date")
                                                                                    @Html.TextBoxFor(m => m.RoleProfileGeneralLedgerViewModel.ExpiryDate, "{0:yyyy-MM-dd}", new { @class = "form-control expiry-date modal-input allow-future-date-upto-50y", @id = "expiry-date-general-ledger", @Type = "date" })
                                                                                    @Html.ValidationMessageFor(m => m.RoleProfileGeneralLedgerViewModel.ExpiryDate)
                                                                                    <span id="expiry-date-general-ledger-error" class="error modal-input-error d-none"> Please Enter Expiry Date.</span>
                                                                                </div>

                                                                                @*CloseDate*@
                                                                                <div class="form-group d-none">
                                                                                    @Html.CustomLabel("Close Date")
                                                                                    @Html.TextBoxFor(m => m.RoleProfileGeneralLedgerViewModel.CloseDate, "{0:yyyy-MM-dd}", new { @class = "form-control", @Type = "date", @id = "close-date-general-ledger" })
                                                                                    @Html.ValidationMessageFor(m => m.RoleProfileGeneralLedgerViewModel.CloseDate)
                                                                                </div>

                                                                                @*Note*@
                                                                                <div class="form-group">
                                                                                    @Html.CustomLabel("Note")
                                                                                    @Html.TextAreaFor(m => m.RoleProfileGeneralLedgerViewModel.Note, new { @id = "note-general-ledger", @class = "form-control deny-multiple-space modal-input", @placeholder = "Enter Note", @maxlength = 1500, onkeyup = "ToTextCapitalize()" })
                                                                                    @Html.ValidationMessageFor(m => m.RoleProfileGeneralLedgerViewModel.Note)
                                                                                </div>

                                                                                <div class="form-group d-none">
                                                                                    @Html.CustomLabel("Reason For Modification")
                                                                                    @Html.TextAreaFor(m => m.RoleProfileGeneralLedgerViewModel.ReasonForModification, new { @id = "reason-for-modification-general-ledger", @class = "form-control", @placeholder = "Enter Reason For Modification", @maxlength = 1500, onkeyup = "ToTextCapitalize()" })
                                                                                    @Html.ValidationMessageFor(m => m.RoleProfileGeneralLedgerViewModel.ReasonForModification)
                                                                                </div>
                                                                            </div>
                                                                            <div class="modal-footer">
                                                                                <button type="button" id="btn-add-general-ledger-modal" class="btn btn-success ">Add</button>
                                                                                <button type="button" id="btn-update-general-ledger-modal" class="btn btn-primary">Update</button>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                                @*General Ledger Data Table*@
                                                                <div class="card-body">
                                                                    <table id="tbl-general-ledger" class="table compact  table-striped table-bordered text-nowrap" cellspacing="0" width="100">
                                                                        <thead class="text-center">
                                                                            <tr>
                                                                                <th><input type="checkbox" name="check_all" id="select-all-general-ledger" /></th>
                                                                                <th class="d-none">GeneralLedgerID</th>
                                                                                <th>Name Of GeneralLedger</th>
                                                                                <th>Activation Date</th>
                                                                                <th>Expiry Date</th>
                                                                                <th class="d-none">CloseDate</th>
                                                                                <th>Note</th>
                                                                                <th class="d-none">ReasonForModification</th>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody class="text-center">
                                                                            @{
                                                                                IEnumerable<RoleProfileGeneralLedgerViewModel> viewModelList1 = (IEnumerable<RoleProfileGeneralLedgerViewModel>)HttpContext.Current.Session["RoleProfileGeneralLedger"];

                                                                                foreach (RoleProfileGeneralLedgerViewModel viewModel in viewModelList1)
                                                                                {
                                                                                    <tr>
                                                                                        <th><input type="checkbox" name="check_all" class="checks" /></th>
                                                                                        <td class="d-none">@viewModel.GeneralLedgerId</td>
                                                                                        <td>@viewModel.NameOfGL</td>
                                                                                        <td>@viewModel.ActivationDate</td>
                                                                                        <td>@viewModel.ExpiryDate</td>
                                                                                        <td class="d-none">@viewModel.CloseDate</td>
                                                                                        <td>@viewModel.Note</td>
                                                                                        <td class="d-none">@viewModel.ReasonForModification</td>
                                                                                    </tr>
                                                                                }
                                                                            }
                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            @*Accordian2 - Role Profile Business Office*@
                                            <div id="is-allow-all-access-for-business-office-block" class="card mt-3">
                                                <div class="card-header " id="business-office-parameter-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-business-office-parameter" data-target="#collapse-business-office-parameter">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <a class="card-title" href="#">
                                                                <b>@Html.TextWithRegionalLanguage("Business Office")</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <span id="business-office-data-table-error" class="error d-none"> This Is Required, Please Add Valid Data.</span>
                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-business-office-parameter" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-business-office-parameter" data-target="#collapse-business-office-parameter">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div id="collapse-business-office-parameter" class="collapse" aria-labelledby="heading-business-office-parameter" data-parent="#accordion">
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                @*Business Office Data Entry Modal / Popup Page*@
                                                                <div class="modal fade" id="business-office-modal" tabindex="-1" data-backdrop="static" role="dialog" aria-labelledby="modal-new-event" aria-hidden="true">
                                                                    <div class="modal-dialog modal-dialog-centered modal-dialog-zoom " role="document">
                                                                        <div class="modal-content">
                                                                            <div class="modal-header">
                                                                                <h4 class="modal-title  text-center" id="business-office-modal-title">Add</h4>
                                                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                                                    <span aria-hidden="true">&times;</span>
                                                                                </button>
                                                                            </div>

                                                                            <div class="modal-body">
                                                                                @*NameOfBusinessOffice*@
                                                                                <div class="form-group">
                                                                                    @Html.CustomLabel("Name Of Business Office")
