            traditional: true,
            success: function (data) {
                debugger
                $('#sub-menu-id').empty();
                var SubMenuList;
                for (var i = 0; i < data.length; i++) {
                    SubMenuList += "<option value=" + data[i].Value + ">" + data[i].Text + "</option>";
                }
                $('#sub-menu-id').append(SubMenuList);
            }
        });
    });


