@model DemoProject.Services.ViewModel.PersonInformation.PersonViewModel
@using DemoProject.Services.ViewModel.Configuration
@using DemoProject.Services.ViewModel.PersonInformation
@using DemoProject.Services.ViewModel.PersonInformation.Parameter
@using DemoProject.Services.HtmlHelpers
@{
    PersonDetailViewModel personDetailViewModel = new PersonDetailViewModel();
    PersonInformationParameterViewModel personInformationParameterViewModel = new PersonInformationParameterViewModel();
    personInformationParameterViewModel = ViewBag.PersonInformationParameter;
    HeaderPartialModel headerPartialModel = new HeaderPartialModel("PERAUT");

    var serializer = new System.Web.Script.Serialization.JavaScriptSerializer();
    byte counter = 0;
    string photoId = "";
    string fileUploaderId = "";

}

<!doctype html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0" />

    <title>Person - Verify</title>
</head>
<body>
    <div class="container-fluid">

        @* ======= First Row - For Header ======= *@
        <div class="row">
            <div class="offset-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @* ======= Second Row - Data Entry Page ======= *@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card d-block mb-3">
                    @using (Html.BeginForm("Verify", "Person", FormMethod.Post, new { enctype = "multipart/form-data", id = "form" }))
                    {
                        @Html.AntiForgeryToken()
                        {
                            <div class="card-body">
                                @* ======= First Row - Mandatory Mark ======= *@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <h6 class="mb-2 text-right text-muted">
                                            <b class="mandatory-mark"> &nbsp; Mandatory Field</b>
                                        </h6>
                                    </div>
                                </div>

                                @* ======= Second Row - Page Title, Translation ======= *@
                                <div class="row mt-2">
                                    @*Page Title*@
                                    <div class="col-12">
                                        <h5 class="font-weight-bold mt-1">Verify</h5>
                                        <hr />
                                    </div>
                                </div>

                                @*Submit Error*@
                                @Html.ValidationMessage("SubmitError")

                                @* ======= Third Row - Data Entry Page ======= *@
                                <div class="row">
                                    <div class="col-12">
                                        <div id="verify-view" class="read-only">

                                            @*MakerRemark*@
                                            <div class="form-group mt-5">
                                                @Html.CustomLabel("Maker Remark")
                                                @Html.TextBoxFor(m => m.MakerRemark, new { @id = "maker-remark", @class = "form-control mandatory-mark read-only" })
                                                @Html.ValidationMessageFor(m => m.MakerRemark)
                                            </div>

                                            @*Person Information Number*@
                                            <div class="form-group @(ViewBag.PersonInformationParameter.EnableAutoGeneratePersonInformationNumber ? " d-none": "" )">
                                                @Html.CustomLabel("Person Information Number")
                                                @Html.TextBoxFor(m => m.PersonInformationNumber, new { @id = "person-information-number", @class = "form-control mandatory-mark", @placeholder = "Enter Person Information Number", @type = "number", onkeyup = "ToTextCapitalize()", required = "required" })
                                                @Html.ValidationMessageFor(m => m.PersonInformationNumber)
                                                <span id="person-information-number-error" class="error d-none">Person Information Number is already exist. </span>
                                            </div>

                                            @*PersonType*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Person Type")
                                                @Html.DropDownListFor(m => m.PersonAdditionalDetailViewModel.PersonTypeId, new SelectList(personDetailViewModel.PersonTypeDropdownList, "Value", "Text", 0), "--- Select Person Type ---", new { @id = "person-type-id", @class = "form-control mandatory-mark", required = "required" })
                                                @Html.ValidationMessageFor(m => m.PersonAdditionalDetailViewModel.PersonTypeId)
                                            </div>

                                            @*PrefixId*@
                                            <div class="form-group individual">
                                                @Html.CustomLabel("Prefix")
                                                @Html.DropDownListFor(m => m.PersonPrefixViewModel.PrefixId, new SelectList(personDetailViewModel.PrefixDropdownList, "Value", "Text", 0), "--- Select Prefix ---", new { @class = "form-control mandatory-mark individual-main-input", required = "required" })
                                                @Html.ValidationMessageFor(m => m.PersonPrefixViewModel.PrefixId)
                                            </div>

                                            @*FirstName*@
                                            <div class="form-group individual">
                                                @Html.CustomLabel("First Name")
                                                <span id="first-name-info" class="ml-5"></span>
                                                @Html.TextBoxFor(m => m.FirstName, new { @id = "first-name", @class = "form-control mandatory-mark alpha-with-single-space individual-main-input", @placeholder = "Enter First Name", @maxlength = 50, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()", required = "required" })
                                                @Html.ValidationMessageFor(m => m.FirstName)
                                            </div>

                                            @*TransFirstName*@
                                            <div class="form-group individual">
                                                @Html.CustomLabel(personDetailViewModel.FirstNameInRegionalLanguage)
                                                @Html.TextBoxFor(m => m.TransFirstName, null, new { @id = "trans-first-name", @class = "form-control deny-multiple-space trans mandatory-mark individual-main-input", @placeholder = personDetailViewModel.FirstNamePlaceHolderInRegionalLanguage, @maxlength = 50, required = "required" })
                                                @Html.ValidationMessageFor(m => m.TransFirstName)
                                            </div>

                                            @*MiddleName*@
                                            <div class="form-group individual">
                                                @Html.CustomLabel("Middle Name")
                                                <span id="middle-name-info" class="ml-5"></span>
                                                @Html.TextBoxFor(m => m.MiddleName, new { @id = "middle-name", @class = "form-control mandatory-mark alpha-with-single-space individual-main-input", @placeholder = "Enter Middle Name", @maxlength = 50, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()", required = "required" })
                                                @Html.ValidationMessageFor(m => m.MiddleName)
                                            </div>

                                            @*TransMiddleName*@
                                            <div class="form-group individual">
                                                @Html.CustomLabel(personDetailViewModel.MiddleNameInRegionalLanguage)
                                                @Html.TextBoxFor(m => m.TransMiddleName, null, new { @id = "trans-middle-name", @class = "form-control deny-multiple-space trans mandatory-mark individual-main-input", @placeholder = personDetailViewModel.MiddleNamePlaceHolderInRegionalLanguage, @maxlength = 50, required = "required" })
                                                @Html.ValidationMessageFor(m => m.TransMiddleName)
                                            </div>

                                            @*LastName*@
                                            <div class="form-group individual">
                                                @Html.CustomLabel("Last Name")
                                                <span id="last-name-info" class="ml-5"></span>
                                                @Html.TextBoxFor(m => m.LastName, new { @id = "last-name", @class = "form-control mandatory-mark alpha-with-single-space individual-main-input", @placeholder = "Enter Last Name", @maxlength = 50, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()", required = "required" })
                                                @Html.ValidationMessageFor(m => m.LastName)
                                            </div>

                                            @*TransLastName*@
                                            <div class="form-group individual">
                                                @Html.CustomLabel(personDetailViewModel.LastNameInRegionalLanguage)
                                                @Html.TextBoxFor(m => m.TransLastName, null, new { @id = "trans-last-name", @class = "form-control deny-multiple-space trans mandatory-mark individual-main-input", @placeholder = personDetailViewModel.LastNamePlaceHolderInRegionalLanguage, @maxlength = 50, required = "required" })
                                                @Html.ValidationMessageFor(m => m.TransLastName)
                                            </div>

                                            @*DateOfBirth*@
                                            <div class="form-group individual">
                                                @Html.CustomLabel("Date Of Birth")
                                                @Html.TextBoxFor(m => m.DateOfBirth, "{0:yyyy-MM-dd}", new { id = "birth-date", @class = "form-control mandatory-mark deny-future-date allow-past-date-upto-150y individual-main-input", @Type = "date", required = "required" })
                                                @Html.ValidationMessageFor(m => m.DateOfBirth)
                                            </div>

                                            @*DateOfBirthOnDocument*@
                                            <div class="form-group checked individual">
                                                @Html.CustomLabel("Date Of Birth On Document")
                                                @Html.TextBoxFor(m => m.DateOfBirthOnDocument, "{0:yyyy-MM-dd}", new { id = "document-birth-date", @class = "form-control mandatory-mark individual-main-input", @Type = "date", required = "required" })
                                                <span id="document-birth-date-error" class="text-c-orenge d-none"><i class="fas fa-exclamation-triangle">&nbsp;&nbsp;Selected Date Of Birth On Your Document Appears To Be Significantly Different From Your Original Birthdate, Please Confirm If This Is Correct.</i></span>
                                                @Html.ValidationMessageFor(m => m.DateOfBirthOnDocument)
                                            </div>

                                            @*MotherName*@
                                            <div class="form-group individual">
                                                @Html.CustomLabel("Mother Name")
                                                @Html.TextBoxFor(m => m.MotherName, new { @id = "mother-name", @class = "form-control mandatory-mark alpha-with-single-space individual-main-input", @placeholder = "Enter Mother Name", required = "required", @maxlength = 50, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                                @Html.ValidationMessageFor(m => m.MotherName)
                                            </div>

                                            @*TransMotherName*@
                                            <div class="form-group individual">
                                                @Html.CustomLabel(personDetailViewModel.MotherNameInRegionalLanguage)
                                                @Html.TextBoxFor(m => m.TransMotherName, null, new { @id = "trans-mother-name", @class = "form-control deny-multiple-space trans mandatory-mark individual-main-input", @placeholder = personDetailViewModel.MotherNamePlaceHolderInRegionalLanguage, @maxlength = 50, required = "required" })
                                                @Html.ValidationMessageFor(m => m.TransMotherName)
                                            </div>

                                            @*MothersMaidenName*@
                                            <div class="form-group individual">
                                                @Html.CustomLabel("Mothers Maiden Name")
                                                @Html.TextBoxFor(m => m.MothersMaidenName, new { @id = "mothers-maiden-name", @class = "form-control mandatory-mark alpha individual-main-input", @placeholder = "Enter Mothers Maiden Name", required = "required", @maxlength = 50, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                                @Html.ValidationMessageFor(m => m.MothersMaidenName)
                                            </div>

                                            @*TransMothersMaidenName*@
                                            <div class="form-group individual">
                                                @Html.CustomLabel(personDetailViewModel.MothersMaidenNameInRegionalLanguage)
                                                @Html.TextBoxFor(m => m.TransMothersMaidenName, null, new { @id = "trans-mothers-maiden-name", @class = "form-control deny-multiple-space mandatory-mark individual-main-input", @placeholder = personDetailViewModel.MothersMaidenNamePlaceHolderInRegionalLanguage, @maxlength = 50, required = "required" })
                                                @Html.ValidationMessageFor(m => m.TransMothersMaidenName)
                                            </div>

                                            @*FullName*@
                                            <div class="form-group group">
                                                @Html.CustomLabel("Full Name")
                                                <span id="full-name-info" class="ml-5"></span>
                                                @Html.TextBoxFor(m => m.FullName, new { @id = "full-name", @class = "form-control mandatory-mark alpha group-main-input", @placeholder = "Enter Full Name", @maxlength = 50, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()", required = "required" })
                                                @Html.ValidationMessageFor(m => m.FullName)
                                            </div>

                                            @*TransFullName*@
                                            <div class="form-group group">
                                                @Html.CustomLabel(personDetailViewModel.LastNameInRegionalLanguage)
                                                @Html.TextBoxFor(m => m.TransFullName, null, new { @id = "trans-full-name", @class = "form-control deny-multiple-space trans mandatory-mark group-main-input", @placeholder = personDetailViewModel.LastNamePlaceHolderInRegionalLanguage, @maxlength = 50, required = "required" })
                                                @Html.ValidationMessageFor(m => m.TransFullName)
                                            </div>

                                            @*BusinessType*@
                                            <div class="form-group group">
                                                @Html.CustomLabel("Business Type")
                                                @Html.DropDownListFor(m => m.PersonGroupViewModel.BusinessType, new SelectList(personDetailViewModel.BusinessTypeDropdownList, "Value", "Text", 0), "--- Select Business Type ---", new { @id = "business-type", @class = "form-control mandatory-mark", required = "required" })
                                                @Html.ValidationMessageFor(m => m.PersonGroupViewModel.BusinessType)
                                            </div>

                                            @*DateOfEstablishment*@
                                            <div class="form-group checked group">
                                                @Html.CustomLabel("Date Of Establishment")
                                                @Html.TextBoxFor(m => m.PersonGroupViewModel.DateOfEstablishment, "{0:yyyy-MM-dd}", new { id = "date-of-establishment", @class = "form-control mandatory-mark deny-future-date allow-past-date-upto-150y group-main-input", required = "required", @Type = "date" })
                                                @Html.ValidationMessageFor(m => m.PersonGroupViewModel.DateOfEstablishment)
                                            </div>

                                            @*BusinessRegistrationNumber*@
                                            <div class="form-group group">
                                                @Html.CustomLabel("Corporate Identification Number")
                                                @Html.TextBoxFor(m => m.PersonGroupViewModel.BusinessRegistrationNumber, new { @id = "corporate-identification-number", @class = "form-control mandatory-mark alpha-numeric-with-dash-slash upper-case deny-multiple-space group-main-input", @placeholder = "Enter Corporate Identification Number", @minlength = 21, @maxlength = 21, required = "required" })
                                                @Html.ValidationMessageFor(m => m.PersonGroupViewModel.BusinessRegistrationNumber)
                                            </div>

                                            @*OtherRegistrationNumber*@
                                            <div class="form-group group">
                                                @Html.CustomLabel("Other Registration Number")
                                                @Html.TextBoxFor(m => m.PersonGroupViewModel.OtherRegistrationNumber, new { @id = "other-registration-number", @class = "form-control mandatory-mark default-none alpha-numeric-with-dash-slash deny-multiple-space group-main-input", @placeholder = "Enter Other Registration Number", @maxlength = 150, onkeyup = "ToTextCapitalize()", required = "required" })
                                                @Html.ValidationMessageFor(m => m.PersonGroupViewModel.OtherRegistrationNumber)
                                            </div>

                                            @*NatureOfBusiness*@
                                            <div class="form-group group">
                                                @Html.CustomLabel("Business Nature")
                                                @Html.DropDownListFor(m => m.PersonGroupViewModel.BusinessNature, new SelectList(personDetailViewModel.BusinessNatureDropdownList, "Value", "Text", 0), "--- Select Nature Of Business ---", new { @id = "business-nature", @class = "form-control mandatory-mark", required = "required" })
                                                @Html.ValidationMessageFor(m => m.PersonGroupViewModel.BusinessNature)
                                            </div>

                                            @*IndustryTypeHasAnyAssociatedCompaniesOrBusinesses*@
                                            <div class="row form-group mt-4 group">
                                                <div class="col-10">
                                                    @Html.CustomLabel("Has Any Associated Companies")
                                                </div>
                                                <div class="col-2">
                                                    <div class="text-left">
                                                        <label class="switch switch-left-right">
                                                            @Html.CheckBoxFor(m => m.PersonGroupViewModel.HasAnyAssociatedCompanies, new { @id = "enable-has-any-associated-companies", @class = "form-control show-block switch-input" })
                                                            <span class="slider round">
                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                            </span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>

                                            @*AssociatedCompaniesOrBusinessesList*@
                                            <div id="has-any-associated-companies-block" class="form-group d-none">
                                                @Html.CustomLabel("Associated Companies List")
                                                @Html.TextAreaFor(m => m.PersonGroupViewModel.AssociatedCompaniesList, new { @id = "associated-companies-list", @class = "form-control default-none deny-multiple-space", @placeholder = "Enter Associated Companies Or Businesses List", @maxlength = 1500, onkeyup = "ToTextCapitalize()" })
                                                @Html.ValidationMessageFor(m => m.PersonGroupViewModel.AssociatedCompaniesList)
                                            </div>

                                            @*ActivationDate*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Activation Date")
                                                @Html.TextBoxFor(m => m.PersonPrefixViewModel.ActivationDate, "{0:yyyy-MM-dd}", new { @id = "activation-date-person-prefix", @class = "form-control activation-date mandatory-mark", @type = "date", required = "required" })
                                                @Html.ValidationMessageFor(m => m.PersonPrefixViewModel.ActivationDate)
                                            </div>

                                            @*Note*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Note")
                                                @Html.TextAreaFor(m => m.Note, new { @id = "note", @class = "form-control default-none deny-multiple-space", @placeholder = "Enter Note", @maxlength = 1500, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                                @Html.ValidationMessageFor(m => m.Note)
                                            </div>

                                            @*TransNote*@
                                            <div class="form-group">
                                                @Html.CustomLabel(personDetailViewModel.NoteInRegionalLanguage)
                                                @Html.TextAreaFor(m => m.TransNote, new { @id = "trans-note", @class = "form-control default-none deny-multiple-space", @placeholder = personDetailViewModel.NotePlaceHolderInRegionalLanguage, @maxlength = 1500, })
                                                @Html.ValidationMessageFor(m => m.TransNote)
                                            </div>
                                        </div>

                                        <div class="row mt-3">
                                            <div class="col-12">
                                                <div id="accordion" class="accordion md-accordion accordion-blocks">

                                                    @*Accordian 1 - Address Details*@
                                                    <div id="heading-address-details" class="card mt-3">
                                                        <div class="card-header p-3" id="collapse-address-details-validations" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-address-details" data-target="#collapse-address-details">
                                                            <div class="row">
                                                                <div class="col-md-5">
                                                                    <a class="card-title" href="#">
                                                                        <b>@Html.TextWithRegionalLanguage("Address Details")</b>
                                                                    </a>
                                                                </div>
                                                                <div class="col-md-5">
                                                                    <span id="address-details-accordion-error" class="error accordion-title-error d-none">This Is Required, Please Add Valid Data.</span>
                                                                </div>
                                                                <div class="col-2">
                                                                    <a href="#collapse-address-details" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-address-details" data-target="#collapse-address-details">
                                                                        <i class="fa fa-angle-up"></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div id="collapse-address-details" class="collapse" aria-labelledby="heading-address-details" data-parent="#accordion">
                                                            <div class="card-body">
                                                                <div class="row">
                                                                    <div class="col-12">
                                                                        <div class="card-body">

                                                                            <table id="tbl-person-address" class="table compact table-striped table-bordered" cellspacing="0" width="100">
                                                                                <thead class="text-center read-only">
                                                                                    <tr>
                                                                                        <th><input type="checkbox" name="check_all" id="select-all-person-address" style="margin-left:11px;" /></th>
                                                                                        <th class="d-none">AddressTypeID</th>
                                                                                        <th>AddressType</th>
                                                                                        <th>FlatDoorNo</th>
                                                                                        <th>TransFlatDoorNo</th>
                                                                                        <th>NameOfBuilding</th>
                                                                                        <th>TransNameOfBuilding</th>
                                                                                        <th>NameOfRoad</th>
                                                                                        <th>TransNameOfRoad</th>
                                                                                        <th>NameOfArea</th>
                                                                                        <th>TransNameOfArea</th>
                                                                                        <th class="d-none">CityID</th>
                                                                                        <th>City</th>
                                                                                        <th class="d-none">ResidenceTypeID</th>
                                                                                        <th>ResidenceType</th>
                                                                                        <th class="d-none">OwnershipTypeId</th>
                                                                                        <th>OwnershipType</th>
                                                                                        <th>Note</th>
                                                                                        <th>TransNote</th>
                                                                                        <th class="d-none">ReasonForModification</th>
                                                                                        <th class="d-none">personAddressPrmKey</th>
                                                                                    </tr>
                                                                                </thead>
                                                                                <tbody class="text-center read-only">
                                                                                    @{
                                                                                        IEnumerable<PersonAddressViewModel> personAddressViewModelList = (IEnumerable<PersonAddressViewModel>)HttpContext.Current.Session["Address"];

                                                                                        foreach (PersonAddressViewModel viewModel in personAddressViewModelList)
                                                                                        {
                                                                                            <tr>
                                                                                                <td><input type="checkbox" name="check_all" class="checks" /></td>
                                                                                                <td class="d-none">@viewModel.AddressTypeId</td>
                                                                                                <td>@viewModel.NameOfAddressType</td>
                                                                                                <td>@viewModel.FlatDoorNo</td>
                                                                                                <td>@viewModel.TransFlatDoorNo</td>
                                                                                                <td>@viewModel.NameOfBuilding</td>
                                                                                                <td>@viewModel.TransNameOfBuilding</td>
                                                                                                <td>@viewModel.NameOfRoad</td>
                                                                                                <td>@viewModel.TransNameOfRoad</td>
                                                                                                <td>@viewModel.NameOfArea</td>
                                                                                                <td>@viewModel.TransNameOfArea</td>
                                                                                                <td class="d-none">@viewModel.CityId</td>
                                                                                                <td>@viewModel.NameOfCenter</td>
                                                                                                <td class="d-none">@viewModel.ResidenceTypeId</td>
                                                                                                <td>@viewModel.NameOfResidenceType</td>
                                                                                                <td class="d-none">@viewModel.OwnershipTypeId</td>
                                                                                                <td>@viewModel.NameOfOwnershipType</td>
                                                                                                <td>@viewModel.Note</td>
                                                                                                <td>@viewModel.TransNote</td>
                                                                                                <td class="d-none">@viewModel.ReasonForModification</td>
                                                                                                <td class="d-none">@viewModel.PersonAddressPrmKey</td>
                                                                                            </tr>
                                                                                        }
                                                                                    }
                                                                                </tbody>
                                                                            </table>

                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>

                                                        </div>
                                                    </div>

                                                    @*Accordian 2 - Person Contact Detail*@
                                                    <div id="heading-contact-details" class="card mt-3">
                                                        <div class="card-header p-3" id="collapse-contact-details-validations" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-contact-details" data-target="#collapse-contact-details">
                                                            <div class="row">
                                                                <div class="col-md-4">
                                                                    <a class="card-title" href="#">
                                                                        <b>@Html.TextWithRegionalLanguage("Contact Detail")</b>
                                                                    </a>
                                                                </div>
                                                                <div class="col-md-6">
                                                                    <span id="contact-error" class="error d-none">Person Contact Detail Required Data, Please Add Valid Data.</span>

                                                                </div>
                                                                <div class="col-2">
                                                                    <a href="#collapse-contact-details" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-contact-details" data-target="#collapse-contact-details">
                                                                        <i class="fa fa-angle-down toggle-icon"></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div id="collapse-contact-details" class="collapse" aria-labelledby="heading-contact-details" data-parent="#accordion">
                                                            <div class="card-body">
                                                                <div class="row">
                                                                    <div class="col-12">
                                                                        <table id="tbl-contact" class="table compact  table-striped table-bordered" width="100">
                                                                            <thead class="text-center read-only">
                                                                                <tr>
                                                                                    <th><input type="checkbox" name="check_all" id="selectAll" /></th>
                                                                                    <th class="d-none">ContactTypeId</th>
                                                                                    <th>ContactType</th>
                                                                                    <th>FieldValue</th>
                                                                                    <th>IsVerified</th>
                                                                                    <th>VerificationCode</th>
                                                                                    <th>Note</th>
                                                                                    <th class="d-none">ReasonForModification</th>
                                                                                    <th class="d-none">ContactDetailPrmKey</th>
                                                                                </tr>
                                                                            </thead>
                                                                            <tbody class="text-center read-only">
                                                                                @{
                                                                                    IEnumerable
                                                                                    <PersonContactDetailViewModel>
                                                                                        personContactDetailViewModelsList = (IEnumerable<PersonContactDetailViewModel>
                                                                                            )HttpContext.Current.Session["ContactDetail"];

                                                                                    foreach (PersonContactDetailViewModel viewModel in personContactDetailViewModelsList)
                                                                                    {
                                                                                        <tr>
                                                                                            <td><input type="checkbox" name="check_all" class="checks" /></td>
                                                                                            <td class="d-none">@viewModel.ContactTypeId</td>
                                                                                            <td>@viewModel.NameOfContactType</td>
                                                                                            <td>@viewModel.FieldValue</td>
                                                                                            <td>@viewModel.IsVerified</td>
                                                                                            <td>@viewModel.VerificationCode</td>
                                                                                            <td>@viewModel.Note</td>
                                                                                            <td class="d-none">@viewModel.ReasonForModification</td>
                                                                                            <td class="d-none">@viewModel.PersonContactDetailPrmKey</td>
                                                                                        </tr>
                                                                                    }
                                                                                }
                                                                            </tbody>
                                                                        </table>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    @* Accordian 3 - HomeBranch*@
                                                    <div id="heading-home-branch" class="card mt-3">
                                                        <div class="card-header" id="home-branch-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-home-branch" data-target="#collapse-home-branch">
                                                            <div class="row">
                                                                <div class="col-md-4">
                                                                    <a class="card-title" href="#">
                                                                        <b>@Html.TextWithRegionalLanguage("Home Branch")</b>
                                                                    </a>
                                                                </div>
                                                                <div class="col-md-6">
