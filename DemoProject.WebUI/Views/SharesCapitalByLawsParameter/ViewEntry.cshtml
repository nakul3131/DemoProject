                                            @Html.TextBoxFor(m => m.MembershipWaitingPeriodAfterExpulsion, new { @id = "membership-waiting-period-after-expulsion", @class = "form-control mandatory-mark", @placeholder = "Enter Period For Membership After Joint Membership Expulsion", @Type = "number", required = "required", @autocomplete = "off", @min = 365, @max = 1825 })
                                            @Html.ValidationMessageFor(m => m.MembershipWaitingPeriodAfterExpulsion)
                                        </div>

                                        @*MembershipWaitingPeriodAfterDisqualification*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Membership Waiting Period After Disqualification")
                                            @Html.TextBoxFor(m => m.MembershipWaitingPeriodAfterDisqualification, new { @id = "membership-waiting-period-after-disqualification", @class = "form-control mandatory-mark", @placeholder = "Enter Period For Membership After Joint Membership Disqualification", @Type = "number", required = "required", @autocomplete = "off", @min = 365, @max = 1825 })
                                            @Html.ValidationMessageFor(m => m.MembershipWaitingPeriodAfterDisqualification)
                                        </div>

                                        @*EstateOfDeceasedMemberLiablePeriod*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Estate Of Deceased Member Liable Period")
                                            @Html.TextBoxFor(m => m.EstateOfDeceasedMemberLiablePeriod, new { @id = "estate-of-deceased-member-liable-period", @class = "form-control mandatory-mark", @placeholder = "Enter Estate Of Deceased Member Liable For Period", @Type = "number", required = "required", @autocomplete = "off", @min = 730, @max = 36500 })
                                            @Html.ValidationMessageFor(m => m.EstateOfDeceasedMemberLiablePeriod)
                                        </div>

                                        @*EnableIndemnityBondForDuplicateShareCertificate*@
                                        <div class="row form-group mt-5">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Indemnity Bond For Duplicate Share Certificate")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-right">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnableIndemnityBondForDuplicateShareCertificate, new { @id = "enable-indemnity-bond-for-duplicate-share-certificate", @class = "form-control switch-input", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>

                                            <div id="enable-indemnity-bond-for-duplicate-share-certificate-err-div"></div>
                                        </div>

                                        @*DuplicateShareCertificateIndemnityBondStampAmount*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Duplicate Share Certificate Indemnity Bond Stamp Amount")
                                            @Html.TextBoxFor(m => m.DuplicateShareCertificateIndemnityBondStampAmount, new { @class = "form-control mandatory-mark", @placeholder = "Enter Duplicate Share Certificate Indemnity Bond Stamp Amount", @Type = "number", required = "required", @autocomplete = "off", @min = 10, @max = 1000 })
