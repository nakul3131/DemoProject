                                            @Html.ValidationMessageFor(m => m.DuplicateShareCertificateCharges)
                                        </div>

                                        @*SharesCapitalToNonMortgageLoanRatio*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Shares Capital To Non Mortgage Loan Ratio")
                                            @Html.TextBoxFor(m => m.SharesCapitalToNonMortgageLoanRatio, new { @id = "shares-non-mortgage-loan-ratio", @class = "form-control mandatory-mark", @placeholder = "Enter Shares Capital To Non Mortgage Loan Ratio", @Type = "number", required = "required", @autocomplete = "off", @min = 5, @max = 10 })
                                            @Html.ValidationMessageFor(m => m.SharesCapitalToNonMortgageLoanRatio)
                                        </div>

                                        @*SharesCapitalToMortgageLoanRatio*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Shares Capital To Mortgage Loan Ratio")
                                            @Html.TextBoxFor(m => m.SharesCapitalToMortgageLoanRatio, new { @id = "shares-mortgage-loan-ratio", @class = "form-control mandatory-mark", @placeholder = "Enter Shares Capital To Mortgage Loan Ratio", @Type = "number", required = "required", @autocomplete = "off", @min = 2.5, @max = 25 })
                                            @Html.ValidationMessageFor(m => m.SharesCapitalToMortgageLoanRatio)
                                        </div>

                                        @*Note*@        
                                        <div class="form-group">
                                            @Html.CustomLabel("Note")
                                            @Html.TextAreaFor(m => m.Note, new { @id = "note", @class = "form-control", @placeholder = "Enter Note", @autocomplete = "off", @maxlength = "1500", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.Note)
                                        </div>

                                        @*NameOfMaker*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name Of User")
                                            @Html.TextBoxFor(m => m.NameOfUser, new { @class = "form-control" })
                                        </div>

                                        @*EntryDateTime*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Entry Date Time")
                                            @Html.TextBoxFor(m => m.EntryDateTime, new { @class = "form-control" })
                                        </div>

                                    </div>
                                </div>
                            </div>

                            @*Save/Submit Button*@
                            <div class="card-footer bg-white">
                                @*Remark*@
                                <div class="form-group read-only">
                                    @Html.CustomLabel("Remark")
                                    @Html.TextBoxFor(m => m.Remark, new { @class = "form-control" })
                                </div>

                                <div class="d-flex justify-content-center mb-2">
                                    <button type="button" class="btn btn-success ml-3" onclick="window.location.href='@Url.Action("Index","SharesCapitalByLawsParameter")'"><h5><u>BackToList</u></h5></button>
                                </div>
                            </div>
                        }
                    }
                </div>
            </div>
        </div>
    </div>
</body>
</html>