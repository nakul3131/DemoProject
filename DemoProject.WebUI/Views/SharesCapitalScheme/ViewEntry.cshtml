                                            @*Accordian 14 - Limit Parameter*@
                                            <div id="limit-parameter-card" class="card mt-3 @((bool)(ViewBag.SharesCapitalSchemeParameter.EnableLimitParameter) ? "" : "d-none")">
                                                <div class="card-header " id="limit-parameter-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-limit-parameter" data-target="#collapse-limit-parameter">
                                                    <div class="row">
                                                        <div class="col-md-5">
                                                            <a class="card-title" href="#">
                                                                <b>@Html.TextWithRegionalLanguage("Limit Parameter")</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-5">
                                                            <span id="limit-accordion-error" class="error d-none">This Is Required Accordion, Please Add Valid Data.</span>

                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-limit-parameter" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-limit-parameter" data-target="#collapse-limit-parameter">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div id="collapse-limit-parameter" class="collapse" aria-labelledby="heading-limit-parameter" data-parent="#accordion">
                                                    <div class="card-body read-only">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                @*CashDepositLimit*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Cash Deposit Limit")
                                                                    @Html.TextBoxFor(m => m.SchemeLimitViewModel.CashDepositLimit, new { @id = "cash-deposit-limit", @class = "form-control mandatory-mark", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.SchemeLimitViewModel.CashDepositLimit)
                                                                </div>

                                                                @*CashWithdrawalLimit*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Cash Withdrawal Limit")
                                                                    @Html.TextBoxFor(m => m.SchemeLimitViewModel.CashWithdrawalLimit, new { @id = "cash-withdrawal-limit", @class = "form-control mandatory-mark", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.SchemeLimitViewModel.CashWithdrawalLimit)
                                                                </div>

                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Retail Account Turn Over Limit (Individual)")
                                                                    @Html.TextBoxFor(m => m.SchemeLimitViewModel.RetailAccountTurnOverLimit, new { @id = "retail-account-turn-over-limit", @class = "form-control mandatory-mark", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.SchemeLimitViewModel.RetailAccountTurnOverLimit)
                                                                    <span id="retail-account-turn-over-limit-error" class="error d-none">It Must Be Less Than Or Equal To Maximum Shares Holding Limit Amount By Shares Capital ByLaws Parameter</span>
                                                                </div>

                                                                @*CorporateAccountTurnOverLimit*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Corporate Account Turn Over Limit (Group)")
                                                                    @Html.TextBoxFor(m => m.SchemeLimitViewModel.CorporateAccountTurnOverLimit, new { @id = "corporate-account-turn-over-limit", @class = "form-control mandatory-mark", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.SchemeLimitViewModel.CorporateAccountTurnOverLimit)
                                                                    <span id="corporate-account-turn-over-limit-error" class="error d-none">It Must Be Less Than Or Equal To Maximum Shares Holding Limit Amount By Shares Capital ByLaws Parameter</span>
                                                                </div>

                                                                @*RetailHoldingAmountProportionToTotalAmount*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Retail Holding Portion To Total Amount (Individual)")
                                                                    @Html.TextBoxFor(m => m.SchemeLimitViewModel.RetailHoldingAmountProportionToTotalAmount, new { @id = "retail-holding-portion", @class = "form-control mandatory-mark", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.SchemeLimitViewModel.RetailHoldingAmountProportionToTotalAmount)
                                                                    <span id="retail-holding-portion-error" class="error d-none">It Must Be Less Than Or Equal To Maximum Shares Holding Limit Percentage By Shares Capital ByLaws Parameter</span>

                                                                </div>

                                                                @*CorporateHoldingAmountProportionToTotalAmount*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Corporate Holding Portion To Total Amount (Group)")
                                                                    @Html.TextBoxFor(m => m.SchemeLimitViewModel.CorporateHoldingAmountProportionToTotalAmount, new { @id = "corporate-holding-portion", @class = "form-control mandatory-mark", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.SchemeLimitViewModel.CorporateHoldingAmountProportionToTotalAmount)
                                                                    <span id="corporate-holding-portion-error" class="error d-none">It Must Be Less Than Or Equal To Maximum Shares Holding Limit Percentage By Shares Capital ByLaws Parameter</span>
                                                                </div>

                                                                @*TurnOverLimit*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Turn Over Limit")
                                                                    @Html.TextBoxFor(m => m.SchemeLimitViewModel.TurnOverLimit, new { @id = "turn-over-limit", @class = "form-control mandatory-mark", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.SchemeLimitViewModel.TurnOverLimit)
                                                                    <span id="turn-over-limit-error" class="error d-none">Please Enter Valid Turn Over Limit, Which Is Greater Than Ten Times Of Retail Account Turn Over Limit.</span>
                                                                </div>

                                                                @*Note*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Note")
                                                                    @Html.TextAreaFor(m => m.SchemeLimitViewModel.Note, new { @id = "note-limit", @class = "form-control deny-multiple-space default-none", @maxlength = "1500" })
                                                                    @Html.ValidationMessageFor(m => m.SchemeLimitViewModel.Note)
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            @*Accordian 15 - General Ledger*@
                                            <div id="general-ledger-card" class="card mt-3">
                                                <div class="card-header" id="general-ledger-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-general-ledger-parameter" data-target="#collapse-general-ledger">
                                                    <div class="row">
                                                        <div class="col-md-5">
                                                            <a class="card-title" href="#">
                                                                <b>@Html.TextWithRegionalLanguage("General Ledger")</b>
                                                            </a>
                                                        </div>

                                                        <div class="col-md-5">
                                                            <span id="general-ledger-accordion-error" class="error d-none">This Is Required Data Table, Please Add Valid Data.</span>
                                                        </div>

                                                        <div class="col-2">
                                                            <a href="#collapse-general-ledger" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-general-ledger-parameter" data-target="#collapse-general-ledger">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div id="collapse-general-ledger" class="collapse" aria-labelledby="heading-general-ledger" data-parent="#accordion">
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                @*General Ledger Data Table*@
                                                                <div class="card-body">
                                                                    <table id="tbl-general-ledger" class="table compact table-striped table-bordered" width="100">
                                                                        <thead class="text-center read-only">
                                                                            <tr>
                                                                                <th><input type="checkbox" name="check_all" id="select-all-general-ledger" /></th>
                                                                                <th class="d-none">GeneralLedgerId</th>
                                                                                <th>NameOfGeneralLedger</th>
                                                                                <th>ActivationDate</th>
                                                                                <th>ExpiryDate</th>
                                                                                <th class="d-none">CloseDate</th>
                                                                                <th>Note</th>
                                                                            </tr>
                                                                        </thead>

                                                                        <tbody class="text-center read-only">
                                                                            @{
                                                                                IEnumerable<SchemeGeneralLedgerViewModel> schemeGeneralLedgerViewModels = (IEnumerable<SchemeGeneralLedgerViewModel>)HttpContext.Current.Session["SchemeGeneralLedger"];

                                                                                if (schemeGeneralLedgerViewModels != null)
                                                                                {

                                                                                    foreach (SchemeGeneralLedgerViewModel viewModel in schemeGeneralLedgerViewModels)
                                                                                    {
                                                                                        <tr>
                                                                                            <td><input type="checkbox" name="check_all" class="checks" /></td>
                                                                                            <td class="d-none">@viewModel.GeneralLedgerId</td>
                                                                                            <td>@viewModel.NameOfGeneralLedger</td>
                                                                                            <td>@viewModel.ActivationDate.ToString("yyyy-MM-dd")</td>
                                                                                            <td>@(viewModel.ExpiryDate == null ? "" : Convert.ToDateTime(viewModel.ExpiryDate).ToString("yyyy-MM-dd"))</td>
                                                                                            <td class="d-none">@(viewModel.CloseDate == null ? "" : Convert.ToDateTime(viewModel.CloseDate).ToString("yyyy-MM-dd"))</td>
                                                                                            <td>@viewModel.Note</td>
                                                                                        </tr>
                                                                                    }
                                                                                }
                                                                            }
                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            @*Accordian 16 - Business Office*@
                                            <div id="business-office-card" class="card mt-3 @((bool)(ViewBag.SharesCapitalSchemeParameter.EnableBusinessOfficeParameter) ? "" : "d-none")">
                                                <div class="card-header " id="business-office-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-business-office-parameter" data-target="#collapse-business-office">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <a class="card-title" href="#">
                                                                <b>@Html.TextWithRegionalLanguage("Branch")</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <span id="business-office-error" class="error d-none">This Is Required Data Table, Please Add Valid Data.</span>

                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-business-office" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-business-office-parameter" data-target="#collapse-business-office">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div id="collapse-business-office" class="collapse" aria-labelledby="heading-business-office" data-parent="#accordion">
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                @*Business Office Data Table*@
                                                                <div class="card-body">
                                                                    <table id="tbl-business-office" class="table compact table-striped table-bordered " width="100">
                                                                        <thead class="text-center read-only">
                                                                            <tr>
                                                                                <th><input type="checkbox" name="select-all" id="select-all-business-office" /></th>
                                                                                <th class="d-none">BusinessOfficeID</th>
                                                                                <th>NameOfBranch</th>
                                                                                <th>ActivationDate</th>
                                                                                <th>ExpiryDate</th>
                                                                                <th class="d-none">CloseDate</th>
                                                                                <th>Note</th>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody class="text-center read-only">
                                                                            @{
                                                                                IEnumerable<SchemeBusinessOfficeViewModel> schemeBusinessOfficeViewModels = (IEnumerable<SchemeBusinessOfficeViewModel>)HttpContext.Current.Session["SchemeBusinessOffice"];

                                                                                if (schemeBusinessOfficeViewModels != null)
                                                                                {
                                                                                    foreach (SchemeBusinessOfficeViewModel viewModel in schemeBusinessOfficeViewModels)
                                                                                    {
                                                                                        <tr>
                                                                                            <td><input type="checkbox" name="check_all" class="checks" /></td>
                                                                                            <td class="d-none">@viewModel.BusinessOfficeId</td>
                                                                                            <td>@viewModel.NameOfBusinessOffice</td>
                                                                                            <td>@viewModel.ActivationDate.ToString("yyyy-MM-dd")</td>
                                                                                            <td>@(viewModel.ExpiryDate == null ? "" : Convert.ToDateTime(viewModel.ExpiryDate).ToString("yyyy-MM-dd"))</td>
                                                                                            <td class="d-none">@(viewModel.CloseDate == null ? "" : Convert.ToDateTime(viewModel.CloseDate).ToString("yyyy-MM-dd"))</td>
                                                                                            <td>@viewModel.Note</td>
                                                                                        </tr>
                                                                                    }
                                                                                }
                                                                            }
                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            @*NameOfUser*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Name Of User")
                                                @Html.TextBoxFor(m => m.NameOfUser, new { @class = "form-control" })
                                            </div>

                                            @*EntryDateTime*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Entry Date Time")
                                                @Html.TextBoxFor(m => m.EntryDateTime, new { @class = "form-control" })
                                            </div>

                                            @*Hiddent Field For SchemePrmKey*@
                                            @Html.HiddenFor(m => m.SchemePrmKey)

                                            @*Hiddent Field For SchemeTranslationPrmKey*@
                                            @Html.HiddenFor(m => m.SchemeTranslationPrmKey)

                                            @*Hiddent Field For SchemeApplicationParameterPrmKey*@
                                            @Html.HiddenFor(m => m.SchemeApplicationParameterViewModel.SchemeApplicationParameterPrmKey)

                                            @*Hiddent Field For SchemeSharesCapitalAccountParameterPrmKey*@
                                            @Html.HiddenFor(m => m.SchemeSharesCapitalAccountParameterViewModel.SchemeSharesCapitalAccountParameterPrmKey)

                                            @*Hiddent Field For SchemeCustomerAccountNumberPrmKey*@
                                            @Html.HiddenFor(m => m.SchemeCustomerAccountNumberViewModel.SchemeCustomerAccountNumberPrmKey)

                                            @*Hiddent Field For SchemeAccountParameterPrmKey*@
                                            @Html.HiddenFor(m => m.SchemeAccountParameterViewModel.SchemeAccountParameterPrmKey)

                                            @*Hiddent Field For SchemeSharesCapitalDividendParameterPrmKey*@
                                            @Html.HiddenFor(m => m.SchemeSharesCapitalDividendParameterViewModel.SchemeSharesCapitalDividendParameterPrmKey)

                                            @*Hiddent Field For SchemeAccountBankingChannelParameterPrmKey*@
                                            @Html.HiddenFor(m => m.SchemeAccountBankingChannelParameterViewModel.SchemeAccountBankingChannelParameterPrmKey)

                                            @*Hiddent Field For SchemeSharesCertificateParameterPrmKey*@
                                            @Html.HiddenFor(m => m.SchemeSharesCertificateParameterViewModel.SchemeSharesCertificateParameterPrmKey)

                                            @*Hiddent Field For SchemeEstimateTargetPrmKey*@
                                            @Html.HiddenFor(m => m.SchemeEstimateTargetViewModel.SchemeEstimateTargetPrmKey)

                                            @*Hiddent Field For SchemeLimitPrmKey*@
                                            @Html.HiddenFor(m => m.SchemeLimitViewModel.SchemeLimitPrmKey)

                                            @*Hiddent Field For SchemeClosingChargesPrmKey*@
                                            @Html.HiddenFor(m => m.SchemeClosingChargesViewModel.SchemeClosingChargesPrmKey)

                                            @*Hiddent Field For SchemeSharesTransferChargesPrmKey*@
                                            @Html.HiddenFor(m => m.SchemeSharesTransferChargesViewModel.SchemeSharesTransferChargesPrmKey)

                                            @*Hiddent Field For SchemeNoticeSchedulePrmKey*@
                                            @Html.HiddenFor(m => m.SchemeNoticeScheduleViewModel.SchemeNoticeSchedulePrmKey)

                                            @*Hiddent Field For SchemeReportFormatPrmKey*@
                                            @Html.HiddenFor(m => m.SchemeReportFormatViewModel.SchemeReportFormatPrmKey)

                                            @*Hiddent Field For SchemeGeneralLedgerPrmKey*@
                                            @Html.HiddenFor(m => m.SchemeGeneralLedgerViewModel.SchemeGeneralLedgerPrmKey)

                                            @*Hiddent Field For SchemeBusinessOfficePrmKey*@
                                            @Html.HiddenFor(m => m.SchemeBusinessOfficeViewModel.SchemeBusinessOfficePrmKey)

                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Save/Submit Button*@
                            @Html.OperationFooter("None", "Exit", "Home", "None")
                        }
                    }
                </div>
            </div>
        </div>
    </div>
</body>
</html>

