@model DemoProject.Services.ViewModel.Account.Layout.SharesCapitalSchemeViewModel
@using DemoProject.Services.HtmlHelpers
@using DemoProject.Services.Constants
@using DemoProject.Services.ViewModel.Account.Layout
@using DemoProject.Services.ViewModel.Configuration

@{
    LayoutDetailViewModel detailViewModel = new LayoutDetailViewModel();

    HeaderPartialModel headerPartialModel = new HeaderPartialModel("SCSCRT");
}

<!doctype html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>Shares Capital Scheme - Create</title>
</head>
<body>
    <div class="container-fluid">
        @*======= First Row - For Header =======*@
        <div class="row">
            <div class="offset-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @*======= Second Row - Data Entry Page =======*@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card d-block mb-3">
                    @using (Html.BeginForm("Create", "SharesCapitalScheme", FormMethod.Post, htmlAttributes: new { @autocomplete = "off" }))
                    {
                        @Html.AntiForgeryToken()
                        {
                            <div class="card-body">
                                @*======= First Row - Mandatory Mark =======*@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <h6 class="mb-2 text-right text-muted"><b class="mandatory-mark">&nbsp; Mandatory Field</b></h6>
                                    </div>
                                </div>

                                @*======= Second Row - Page Title, Translation =======*@
                                <div class="row mt-2">
                                    @*Page Title*@
                                    <div class="col-12">
                                        <h5 class="font-weight-bold mt-1">Create</h5>
                                        <hr />
                                    </div>
                                </div>

                                @*Submit Error*@
                                @Html.ValidationMessage("SubmitError")

                                @*======= Third Row - Data Entry Page =======*@
                                <div class="row">
                                    <div class="col-12">
                                        @*NameOfScheme*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name Of Scheme")
                                            @Html.TextBoxFor(m => m.NameOfScheme, new { @id = "name-of-scheme", @class = "form-control mandatory-mark alpha-numeric", @placeholder = "Enter Name Of Scheme", required = "required", @maxlength = 100, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.NameOfScheme)
                                            <span id="name-of-scheme-error" class="error modal-input-error d-none">Name Of Scheme Is Already Exist. </span>
                                        </div>

                                        @*TransNameOfScheme*@
                                        <div class="form-group">
                                            @Html.CustomLabel(detailViewModel.NameOfSchemeInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransNameOfScheme, new { @id = "trans-name-of-scheme", @class = "form-control mandatory-mark deny-multiple-space", @placeholder = detailViewModel.NameOfSchemePlaceHolderInRegionalLanguage, required = "required", @maxlength = 100 })
                                            @Html.ValidationMessageFor(m => m.TransNameOfScheme)
                                        </div>

                                        @*Alias Name*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Alias Name")
                                            @Html.TextBoxFor(m => m.AliasName, new { @id = "alias-name", @class = "form-control default-none alpha-numeric", @placeholder = "Enter Alias Name", required = "required", @maxlength = 10, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.AliasName)
                                        </div>

                                        @*TransAliasName*@
                                        <div class="form-group">
                                            @Html.CustomLabel(detailViewModel.AliasNameInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransAliasName, new { @id = "trans-alias-name", @class = "form-control default-none deny-multiple-space", @placeholder = detailViewModel.AliasNamePlaceHolderInRegionalLanguage, required = "required", @maxlength = 10, })
                                            @Html.ValidationMessageFor(m => m.TransAliasName)
                                        </div>

                                        @*NameOnReport*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name On Report")
                                            @Html.TextBoxFor(m => m.NameOnReport, new { @id = "name-on-report", @class = "form-control default-none alpha-numeric", @placeholder = "Enter Name On Report", required = "required", @maxlength = 100, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.NameOnReport)
                                        </div>

                                        @*TransNameOnReport*@
                                        <div class="form-group">
                                            @Html.CustomLabel(detailViewModel.NameOnReportInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransNameOnReport, new { @id = "trans-name-on-report", @class = "form-control default-none deny-multiple-space", @placeholder = detailViewModel.NameOnReportPlaceHolderInRegionalLanguage, required = "required", @maxlength = 100, })
                                            @Html.ValidationMessageFor(m => m.TransNameOnReport)
                                        </div>

                                        @*ActivationDate*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Activation Date")
                                            @Html.TextBoxFor(m => m.ActivationDate, "{0:yyyy-MM-dd}", new { @id = "activation-date-scheme", @class = "form-control mandatory-mark deny-past-date deny-next-financial-year-date activation-date set-current-date", required = "required", @Type = "date" })
                                            @Html.ValidationMessageFor(m => m.ActivationDate)
                                        </div>

                                        @*ExpiryDate*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Expiry Date")
                                            @Html.TextBoxFor(m => m.ExpiryDate, "{0:yyyy-MM-dd}", new { @id = "expiry-date-scheme", @class = "form-control expiry-date", @type = "date" })
                                            @Html.ValidationMessageFor(m => m.ExpiryDate)
                                        </div>

                                        @*Note*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Note")
                                            @Html.TextAreaFor(m => m.Note, new { @id = "note", @class = "form-control default-none deny-multiple-space", @placeholder = "Enter Note", @maxlength = 1500, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.Note)
                                        </div>

                                        @*TransNote*@
                                        <div class="form-group">
                                            @Html.CustomLabel(detailViewModel.NoteInRegionalLanguage)
                                            @Html.TextAreaFor(m => m.TransNote, new { @id = "trans-note", @class = "form-control default-none deny-multiple-space", @placeholder = detailViewModel.NotePlaceHolderInRegionalLanguage, @maxlength = 1500, })
                                            @Html.ValidationMessageFor(m => m.TransNote)
                                        </div>

                                        @*======= Fourth Row - Accordians =======*@
                                        <div class="row mt-3">
                                            <div class="col-12">
                                                <div id="accordion" class="accordion md-accordion accordion-blocks">
                                                    @*EnableApplication*@
                                                    <div class="row form-group mt-4 ml-1 @((bool)(ViewBag.SharesCapitalSchemeParameter.EnableApplicationParameter) ? "" : "d-none")">
                                                        <div class="col-10">
                                                            @Html.CustomLabel("Enable Application")
                                                        </div>
                                                        <div class="col-2 text-left">
                                                            <label class="switch switch-left-right">
                                                                @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableApplication, new { @id = "enable-application", @class = "form-control switch-input application-number-input show-block" })
                                                                <span class="slider round">
                                                                    <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                </span>
                                                            </label>
                                                        </div>
                                                    </div>

                                                    @*Enable-ClosingCharges*@
                                                    <div class="row form-group ml-1 mt-4 @((bool)(ViewBag.SharesCapitalSchemeParameter.EnableClosingChargesParameter) ? "" : "d-none")">
                                                        <div class="col-10">
                                                            @Html.CustomLabel("Enable Closing Charges")
                                                        </div>

                                                        <div class="col-2">
                                                            <div class="text-left">
                                                                <label class="switch switch-left-right">
                                                                    @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableClosingCharges, new { @id = "enable-closing-charges", @class = "form-control switch-input show-block" })
                                                                    <span class="slider round">
                                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                    </span>
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    @*EnableSharesTransferCharges*@
                                                    <div class="row form-group ml-1 mt-4 @((bool)(ViewBag.SharesCapitalSchemeParameter.EnableSharesTransferChargesParameter) ? " " : "d-none")">
                                                        <div class="col-10">
                                                            @Html.CustomLabel("Enable Transfer Charges")
                                                        </div>

                                                        <div class="col-2">
                                                            <div class="text-left">
                                                                <label class="switch switch-left-right">
                                                                    @Html.CheckBoxFor(m => m.SchemeSharesCapitalAccountParameterViewModel.EnableSharesTransferCharges, new { @id = "enable-transfer-charges", @class = "form-control switch-input show-block" })
                                                                    <span class="slider round">
                                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                    </span>
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    @*EnableDividend*@
                                                    <div class="row form-group ml-1 mt-4">
                                                        <div class="col-10">
                                                            @Html.CustomLabel("Enable Dividend")
                                                        </div>

                                                        <div class="col-2">
                                                            <div class="text-left">
                                                                <label class="switch switch-left-right">
                                                                    @Html.CheckBoxFor(m => m.SchemeSharesCapitalAccountParameterViewModel.EnableDividend, new { @id = "enable-dividend-parameter", @class = "form-control switch-input show-block" })
                                                                    <span class="slider round">
                                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                    </span>
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    @*Accordian 1 - Application*@
                                                    <div id="application-block" class="card mt-2 d-none">
                                                        <div class="card-header p-3" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-application-parameter" href="#collapse-application-parameter">
                                                            <div class="row">
                                                                <div class="col-md-5">
                                                                    <a class="card-title" href="#">
                                                                        <b>@Html.TextWithRegionalLanguage("Application")</b>
                                                                    </a>
                                                                </div>
                                                                <div class="col-md-5">
                                                                    <span id="application-number-accordion-title-error" class="error accordion-title-error d-none">This Is Required, Please Add Valid Data.</span>
                                                                </div>
                                                                <div class="col-2">
                                                                    <a href="#collapse-application-parameter" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-application-parameter" data-target="#collapse-application-parameter">
                                                                        <i class="fa fa-angle-down toggle-icon"></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <div id="collapse-application-parameter" class="collapse" aria-labelledby="heading-application-parameter" data-parent="#accordion">
                                                            <div class="card-body">
                                                                <div class="row">
                                                                    <div class="col-12">
                                                                        @*EnableApplicationNumberBranchwise*@
                                                                        <div class="row form-group mt-2 @(detailViewModel.HasApplicationNumberBranchWise ? "" : "d-none")">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Application Parameter Branchwise")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeApplicationParameterViewModel.EnableApplicationNumberBranchwise, new { @id = "enable-application-number-branchwise", @class = "form-control switch-input hide-block" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*Application Number Inputs Visibility Based On Toggle Switch Selection*@
                                                                        <div id="application-number-branchwise-block" class="row">
                                                                            <div class="col-12">
                                                                                @*EnableAutoApplicationNumber*@
                                                                                <div class="row form-group mt-2">
                                                                                    <div class="col-10">
                                                                                        @Html.CustomLabel("Enable Auto Application Number")
                                                                                    </div>
                                                                                    <div class="col-2">
                                                                                        <div class="text-left">
                                                                                            <label class="switch switch-left-right">
                                                                                                @Html.CheckBoxFor(m => m.SchemeApplicationParameterViewModel.EnableAutoApplicationNumber, new { @id = "enable-auto-application-number", @class = "form-control switch-input application-number-input show-block" })
                                                                                                <span class="slider round">
                                                                                                    <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                                </span>
                                                                                            </label>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*If Visible EnableAutoApplicationNumber Show This*@
                                                                        <div id="auto-application-number-block">
                                                                            @*Enable Regenerate Unused Application Number*@
                                                                            <div class="row form-group">
                                                                                <div class="col-10">
                                                                                    @Html.CustomLabel("Enable Regenerate Unused Application Number")
                                                                                </div>
                                                                                <div class="col-2">
                                                                                    <div class="text-left">
                                                                                        <label class="switch switch-left-right">
                                                                                            @Html.CheckBoxFor(m => m.SchemeApplicationParameterViewModel.EnableRegenerateUnusedApplicationNumber, new { @id = "enable-regenerate-unused-application-number", @class = "form-control switch-input" })
                                                                                            <span class="slider round">
                                                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                            </span>
                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </div>

                                                                            @*EnableRandomApplicationNumber*@
                                                                            <div class="row form-group">
                                                                                <div class="col-10">
                                                                                    @Html.CustomLabel("Enable Random Application Number")
                                                                                </div>
                                                                                <div class="col-2">
                                                                                    <div class="text-left">
                                                                                        <label class="switch switch-left-right">
                                                                                            @Html.CheckBoxFor(m => m.SchemeApplicationParameterViewModel.EnableRandomApplicationNumber, new { @id = "enable-random-application-number", @class = "form-control switch-input" })
                                                                                            <span class="slider round">
                                                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                            </span>
                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </div>

                                                                            @*EnableCustomizeApplicationNumber*@
                                                                            <div class="row form-group">
                                                                                <div class="col-10">
                                                                                    @Html.CustomLabel("Enable Customize Application Number")
                                                                                </div>
                                                                                <div class="col-2">
                                                                                    <div class="text-left">
                                                                                        <label class="switch switch-left-right">
                                                                                            @Html.CheckBoxFor(m => m.SchemeApplicationParameterViewModel.EnableCustomizeApplicationNumber, new { @id = "enable-customize-application-number", @class = "form-control switch-input" })
                                                                                            <span class="slider round">
                                                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                            </span>
                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </div>

                                                                            @*EnableDigitalCodeForApplicationNumber*@
                                                                            <div class="row form-group">
                                                                                <div class="col-10">
                                                                                    @Html.CustomLabel("Enable Digital Code For Application")
                                                                                </div>
                                                                                <div class="col-2">
                                                                                    <div class="text-left">
                                                                                        <label class="switch switch-left-right">
                                                                                            @Html.CheckBoxFor(m => m.SchemeApplicationParameterViewModel.EnableDigitalCodeForApplicationNumber, new { @id = "enable-digital-code-for-application-number", @class = "form-control switch-input" })
                                                                                            <span class="slider round">
                                                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                            </span>
                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </div>

                                                                            @*ApplicationNumberMask*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Application Number Mask")
                                                                                @Html.ListBoxFor(m => m.SchemeApplicationParameterViewModel.MaskTypeCharacterForApplication, new MultiSelectList(detailViewModel.MaskTypeDropdownList, "Value", "Text"), new { @id = "application-number-mask", @class = "form-control mandatory-mark multi-select2 application-number-input", @multiple = "multiple" })
                                                                                @Html.ValidationMessageFor(m => m.SchemeApplicationParameterViewModel.MaskTypeCharacterForApplication)
                                                                            </div>

                                                                            @*StartApplicationNumber*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Start Application Number")
                                                                                @Html.TextBoxFor(m => m.SchemeApplicationParameterViewModel.StartApplicationNumber, new { @id = "start-application-number", @class = "form-control mandatory-mark min-number real-number application-number-input", required = "required", @type = "number", @placeholder = "Enter Start Application Number", @min = 1, @max = 9999999 })
                                                                                @Html.ValidationMessageFor(m => m.SchemeApplicationParameterViewModel.StartApplicationNumber)
                                                                            </div>

                                                                            @*EndApplicationNumber*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("End Application Number")
                                                                                @Html.TextBoxFor(m => m.SchemeApplicationParameterViewModel.EndApplicationNumber, new { @id = "end-application-number", @class = "form-control mandatory-mark max-number real-number application-number-input", required = "required", @type = "number", @placeholder = "Enter End Application Number", @max = 999999999 })
                                                                                @Html.ValidationMessageFor(m => m.SchemeApplicationParameterViewModel.EndApplicationNumber)
                                                                            </div>

                                                                            @*ApplicationNumberIncrementBy*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Application Number Increment By")application-number-input
                                                                                @Html.TextBoxFor(m => m.SchemeApplicationParameterViewModel.ApplicationNumberIncrementBy, new { @id = "application-number-increment-by", @class = "form-control mandatory-mark mid-number real-number application-number-input", required = "required", @type = "number", @placeholder = "Enter Application Number Increment By", @min = 1 })
                                                                                @Html.ValidationMessageFor(m => m.SchemeApplicationParameterViewModel.ApplicationNumberIncrementBy)
                                                                            </div>
                                                                        </div>

                                                                        @*Note*@
                                                                        <div class="form-group">
                                                                            @Html.CustomLabel("Note")
                                                                            @Html.TextAreaFor(m => m.SchemeApplicationParameterViewModel.Note, new { @id = "note-application", @class = "form-control default-none deny-multiple-space application-number-input", @placeholder = "Enter Note", @maxlength = 1500, onkeyup = "ToTextCapitalize()" })
                                                                            @Html.ValidationMessageFor(m => m.SchemeApplicationParameterViewModel.Note)
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    @*Accordian 2 - Scheme Member Number*@
                                                    <div id="member-number-card" class="card mt-3">
                                                        <div class="card-header" id="member-number-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-member-number" data-target="#collapse-member-number">
                                                            <div class="row">
                                                                <div class="col-md-5">
                                                                    <a class="card-title" href="#">
                                                                        <b>@Html.TextWithRegionalLanguage("Member Number")</b>
                                                                    </a>
                                                                </div>
                                                                <div class="col-md-5">
                                                                    <span id="member-number-accordion-error" class="error accordion-title-error d-none">This Is Required, Please Add Valid Data.</span>
                                                                </div>
                                                                <div class="col-2">
                                                                    <a href="#collapse-member-number" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-member-number" data-target="#collapse-member-number">
                                                                        <i class="fa fa-angle-down toggle-icon"></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div id="collapse-member-number" class="collapse" aria-labelledby="heading-member-number" data-parent="#accordion">
                                                            <div class="card-body">
                                                                <div class="row">
                                                                    <div class="col-12">
                                                                        @*EnableMemberNumberBranchwise*@
                                                                        <div class="row form-group mt-2 @(detailViewModel.HasMemberNumberBranchWise ? "" : "d-none")">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Member Number Branchwise")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeSharesCapitalAccountParameterViewModel.EnableMemberNumberBranchwise, new { @id = "enable-member-number-branchwise", @class = "form-control switch-input hide-block" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*Member Number Inputs Visibility Based On Toggle Switch Selection*@
                                                                        <div id="member-number-branchwise-block" class="row">
                                                                            <div class="col-12">
                                                                                @*EnableAutoMemberNumber*@
                                                                                <div class="row form-group mt-2">
                                                                                    <div class="col-10">
                                                                                        @Html.CustomLabel("Enable Auto Member Number")
                                                                                    </div>
                                                                                    <div class="col-2">
                                                                                        <div class="text-left">
                                                                                            <label class="switch switch-left-right">
                                                                                                @Html.CheckBoxFor(m => m.SchemeSharesCapitalAccountParameterViewModel.EnableAutoMemberNumber, new { @id = "enable-auto-member-number", @class = "form-control switch-input member-number-input show-block" })
                                                                                                <span class="slider round">
                                                                                                    <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                                </span>
                                                                                            </label>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        <div id="auto-member-number-block">
                                                                            @*EnableReGenerateUnusedMemberNumber*@
                                                                            <div class="row form-group">
                                                                                <div class="col-10">
                                                                                    @Html.CustomLabel("Enable Regenerate Unused Member Number")
                                                                                </div>
                                                                                <div class="col-2">
                                                                                    <div class="text-left">
                                                                                        <label class="switch switch-left-right">
                                                                                            @Html.CheckBoxFor(m => m.SchemeSharesCapitalAccountParameterViewModel.EnableReGenerateUnusedMemberNumber, new { @id = "enable-regenerate-unused-member-number", @class = "form-control switch-input" })
                                                                                            <span class="slider round">
                                                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                            </span>
                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </div>

                                                                            @*EnableRandomMemberNumber*@
                                                                            <div class="row form-group">
                                                                                <div class="col-10">
                                                                                    @Html.CustomLabel("Enable Random Member Number")
                                                                                </div>
                                                                                <div class="col-2">
                                                                                    <div class="text-left">
                                                                                        <label class="switch switch-left-right">
                                                                                            @Html.CheckBoxFor(m => m.SchemeSharesCapitalAccountParameterViewModel.EnableRandomMemberNumber, new { @id = "enable-random-member-number", @class = "form-control switch-input" })
                                                                                            <span class="slider round">
                                                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                            </span>
                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </div>

                                                                            @*EnableCustomizeMemberNumber*@
                                                                            <div class="row form-group">
                                                                                <div class="col-10">
                                                                                    @Html.CustomLabel("Enable Customize Member Number")
                                                                                </div>
                                                                                <div class="col-2">
                                                                                    <div class="text-left">
                                                                                        <label class="switch switch-left-right">
                                                                                            @Html.CheckBoxFor(m => m.SchemeSharesCapitalAccountParameterViewModel.EnableCustomizeMemberNumber, new { @id = "enable-customize-member-number", @class = "form-control switch-input" })
                                                                                            <span class="slider round">
                                                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                            </span>
                                                                                        </label>
                                                                                    </div>
                                                                                </div>
