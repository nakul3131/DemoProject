@model DemoProject.Services.ViewModel.Enterprise.Office.BusinessOfficeViewModel
@using DemoProject.Services.HtmlHelpers
@using DemoProject.Services.ViewModel.Enterprise.Office
@using DemoProject.Services.ViewModel.Configuration

@{
    OfficeDetailViewModel officeDetailViewModel = new OfficeDetailViewModel();
    HeaderPartialModel headerPartialModel = new HeaderPartialModel("BUOAMN");
}

@*<!DOCTYPE html>*@
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>Business Office - Amend</title>
</head>
<body>
    <div class="container-fluid">
        @*======= First Row - For Header =======*@
        <div class="row">
            <div class="offset-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @*======= Second Row - Data Entry Page =======*@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card mb-3">
                    @using (Html.BeginForm("Amend", "BusinessOffice", FormMethod.Post, htmlAttributes: new { id = "form", @autocomplete = "off" }))
                    {
                        @Html.AntiForgeryToken()
                        {
                            <div class="card-body">
                                @*======= First Row - Mandatory Mark =======*@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <h6 class="mb-2 text-right text-muted"><b class="mandatory-mark">&nbsp; Mandatory Field</b></h6>
                                    </div>
                                </div>

                                @*======= Second Row - Page Title =======*@
                                <div class="row mt-2">
                                    @*Page Title*@
                                    <div class="col-12">
                                        <h5 class="font-weight-bold mt-1">Amend</h5>
                                        <hr />
                                    </div>
                                </div>

                                @*Submit Error*@
                                @Html.ValidationMessage("SubmitError")

                                @*======= Third Row - Data Entry Page =======*@
                                <div class="row">
                                    <div class="col-12">
                                        @*CheckerRemark*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Checker Remark")
                                            @Html.TextBoxFor(m => m.CheckerRemark, new { @class = "form-control read-only" })
                                            @Html.ValidationMessageFor(m => m.CheckerRemark)
                                        </div>

                                        @*BusinessOfficeCode*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Branch Code")
                                            @Html.TextBoxFor(m => m.BusinessOfficeCode, new { @class = "form-control alpha-numeric mandatory-mark", @placeholder = "Enter Branch Code", required = "required", @maxlength = 3, onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.BusinessOfficeCode)
                                        </div>

                                        @*AlternateBusinessOfficeCode*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Alternate Branch Code")
                                            @Html.TextBoxFor(m => m.AlternateBusinessOfficeCode, new { @id = "alternate-business-office-code", @class = "form-control alpha-numeric", @placeholder = "Enter Alternate Branch Code", @maxlength = 10, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.AlternateBusinessOfficeCode)
                                        </div>

                                        @*NameOfBusinessOffice*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name Of Branch")
                                            @Html.TextBoxFor(m => m.NameOfBusinessOffice, new { @id = "name-of-business-office", @class = "form-control alpha-numeric mandatory-mark", @placeholder = "Enter Name Of Branch", required = "required", @minlength = 2, @maxlength = 100, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.NameOfBusinessOffice)
                                        </div>

                                        @*TransNameOfBusinessOffice*@
                                        <div class="form-group">
                                            @Html.CustomLabel(officeDetailViewModel.NameOfBusinessOfficeInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransNameOfBusinessOffice, null, new { @id = "trans-name-of-business-office", @class = "form-control deny-multiple-space mandatory-mark", @placeholder = officeDetailViewModel.NameOfBusinessOfficePlaceHolderInRegionalLanguage, required = "required", @minlength = 2, @maxlength = 100, onkeydown = "Maketranslation()" })
                                            @Html.ValidationMessageFor(m => m.TransNameOfBusinessOffice)
                                        </div>

                                        @*AliasName*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Alias Name")
                                            @Html.TextBoxFor(m => m.AliasName, new { @id = "alias-name", @class = "form-control alpha-numeric", @placeholder = "Enter Alias Name", @maxlength = 10, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.AliasName)
                                        </div>

                                        @*TransAliasName*@
                                        <div class="form-group">
                                            @Html.CustomLabel(officeDetailViewModel.AliasNameInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransAliasName, null, new { @id = "trans-alias-name", @class = "form-control deny-multiple-space", @placeholder = officeDetailViewModel.AliasNamePlaceHolderInRegionalLanguage, @maxlength = 10 })
                                            @Html.ValidationMessageFor(m => m.TransAliasName)
                                        </div>

                                        @*NameOnReport*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name On Report")
                                            @Html.TextBoxFor(m => m.NameOnReport, new { @id = "name-report", @class = "form-control alpha-numeric", @placeholder = "Enter Name On Report", @maxlength = 100, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.NameOnReport)
                                        </div>

                                        @*TransNameOnReport*@
                                        <div class="form-group">
                                            @Html.CustomLabel(officeDetailViewModel.NameOnReportInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransNameOnReport, null, new { @id = "trans-name-report", @class = "form-control deny-multiple-space", @placeholder = officeDetailViewModel.NameOnReportPlaceHolderInRegionalLanguage, @maxlength = 100 })
                                            @Html.ValidationMessageFor(m => m.TransNameOnReport)
                                        </div>

                                        @*NameOfBusinessOfficeForThirdPartyInterface*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name Of Branch For Third Party Interface")
                                            @Html.TextBoxFor(m => m.NameOfBusinessOfficeForThirdPartyInterface, new { @id = "name-of-business-office-for-third-party-interface", @class = "form-control alpha-numeric", @placeholder = "Enter Name Of Branch For Third Party Interface", @maxlength = 100, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.NameOfBusinessOfficeForThirdPartyInterface)
                                        </div>

                                        @*OpeningDate*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Opening Date")
                                            @Html.TextBoxFor(m => m.OpeningDate, "{0:yyyy-MM-dd}", new { @id = "opening-date", @class = "form-control mandatory-mark deny-future-date deny-next-financial-year-date", @placeholder = "Enter Opening Date", required = "required", @Type = "date", onclick = "DenyFutureDate()" })
                                            @Html.ValidationMessageFor(m => m.OpeningDate)
                                        </div>

                                        @*ContactDetails*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Contact Details")
                                            @Html.TextAreaFor(m => m.ContactDetails, new { @id = "contact-details", @class = "form-control mandatory-mark alpha-numeric alpha-numeric", @placeholder = "Enter Contact Details", required = "required", @minlength = 7, @maxlength = 500, onkeydown = "Maketranslation()" })
                                            @Html.ValidationMessageFor(m => m.ContactDetails)
                                        </div>

                                        @*TransContactDetails*@
                                        <div class="form-group">
                                            @Html.CustomLabel(officeDetailViewModel.ContactDetailsInRegionalLanguage)
                                            @Html.TextAreaFor(m => m.TransContactDetails, new { @id = "trans-contact-details", @class = "form-control deny-multiple-space mandatory-mark", @placeholder = officeDetailViewModel.ContactDetailsPlaceHolderInRegionalLanguage, required = "required", @minlength = 7, @maxlength = 500 })
                                            @Html.ValidationMessageFor(m => m.TransContactDetails)
                                        </div>

                                        @*AddressDetails*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Address Details")
                                            @Html.TextAreaFor(m => m.AddressDetails, new { @id = "address-details", @class = "form-control alpha-numeric mandatory-mark", @placeholder = "Enter Address Details", required = "required", @minlength = 10, @maxlength = 1500, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.AddressDetails)
                                        </div>

                                        @*TransAddressDetails*@
                                        <div class="form-group">
                                            @Html.CustomLabel(officeDetailViewModel.AddressDetailsInRegionalLanguage)
                                            @Html.TextAreaFor(m => m.TransAddressDetails, new { @id = "trans-address-details", @class = "form-control deny-multiple-space mandatory-mark", @placeholder = officeDetailViewModel.AddressDetailsPlaceHolderInRegionalLanguage, required = "required", @minlength = 10, @maxlength = 1500 })
                                            @Html.ValidationMessageFor(m => m.TransAddressDetails)
                                        </div>

                                        @*IsFundBranch*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Fund Branch")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-right">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.IsFundBranch, new { @id = "is-fund-branch", @class = "form-control switch-input", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*EnableCorporateAccess*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Corporate Access")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-right">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnableCorporateAccess, new { @id = "enable-corporate-access", @class = "form-control switch-input", required = "required" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*LoanDirectDebitGenerationDays*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Loan Direct Debit Generation Days")
                                            @Html.TextBoxFor(m => m.LoanDirectDebitGenerationDays, new { @id = "loan-direct-debit-generation-days", @class = "form-control real-number mandatory-mark", @placeholder = "Enter Loan Direct Debit Generation Days", required = "required", @type = "number", @min = 1, @max = 90, onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.LoanDirectDebitGenerationDays)
                                        </div>

                                        @*ParentBusinessOfficePrmKey*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Parent Branch")
                                            @Html.DropDownListFor(m => m.ParentBusinessOfficeId, new SelectList(officeDetailViewModel.BusinessOfficeDropdownList, "Value", "Text"), "--- Select Parent Branch ---", new { @class = "form-control mandatory-mark", required = "required" })
                                            @Html.ValidationMessageFor(m => m.ParentBusinessOfficeId)
                                        </div>

                                        @*ClearingBusinessOfficePrmKey*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Clearing Branch")
                                            @Html.DropDownListFor(m => m.ClearingBusinessOfficeId, new SelectList(officeDetailViewModel.BusinessOfficeDropdownList, "Value", "Text"), "--- Select Clearing Branch ---", new { @class = "form-control mandatory-mark", required = "required" })
                                            @Html.ValidationMessageFor(m => m.ClearingBusinessOfficeId)
                                        </div>

                                        @*TransactionCodeForClearing*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Transaction Code For Clearing")
                                            @Html.TextBoxFor(m => m.TransactionCodeForClearing, new { @id = "transaction-code-for-clearing", @class = "form-control alpha-numeric mandatory-mark", @placeholder = "Enter Transaction Code For Clearing", required = "required", @minlength = 2, @maxlength = 15, onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.TransactionCodeForClearing)
                                        </div>

                                        @*RegionalOfficePrmKey*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Regional Office")
                                            @Html.DropDownListFor(m => m.RegionalOfficeId, new SelectList(officeDetailViewModel.BusinessOfficeDropdownList, "Value", "Text"), "--- Select Regional Office ---", new { @class = "form-control mandatory-mark", required = "required" })
                                            @Html.ValidationMessageFor(m => m.RegionalOfficeId)
                                        </div>

                                        @*BusinessOfficeStatusForCoreOperation*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Branch Status For Core Operation")
                                            @Html.DropDownListFor(m => m.BusinessOfficeStatusForCoreOperation, new List < SelectListItem >
                                                {
                                                new SelectListItem { Text = "Active", Value = "ACT" },
                                                new SelectListItem { Text = "Closed", Value = "CLS" },
                                                new SelectListItem { Text = "Inactive", Value = "INA" },
                                                new SelectListItem { Text = "Temporarily Blocked", Value = "BLC" },
                                                },
                                                "Select Branch Status For Core Operation", new { @class = "form-control mandatory-mark", required = "required"})
                                            @Html.ValidationMessageFor(m => m.BusinessOfficeStatusForCoreOperation)
                                        </div>

                                        @*Note*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Note")
                                            @Html.TextAreaFor(m => m.Note, new { @id = "note", @class = "form-control", @placeholder = "Enter Note", @maxlength = 1500, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.Note)
                                        </div>

                                        @*TransNote*@
                                        <div class="form-group">
                                            @Html.CustomLabel(officeDetailViewModel.NoteInRegionalLanguage)
                                            @Html.TextAreaFor(m => m.TransNote, new { @id = "trans-note", @class = "form-control", @placeholder = officeDetailViewModel.NotePlaceHolderInRegionalLanguage, @maxlength = 1500 })
                                            @Html.ValidationMessageFor(m => m.TransNote)
                                        </div>

                                        @*Hiddent Field For BusinessOfficePrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficePrmKey)

                                        @*Hiddent Field For BusinessOfficeModificationPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeModificationPrmKey)

                                        @*Hiddent Field For BusinessOfficeTranslationPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeTranslationPrmKey)

                                        @*Hiddent Field For BusinessOfficeCoopRegistrationPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeCoopRegistrationViewModel.BusinessOfficeCoopRegistrationPrmKey)

                                        @*Hiddent Field For BusinessOfficeCoopRegistrationTranslationPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeCoopRegistrationViewModel.BusinessOfficeCoopRegistrationTranslationPrmKey)

                                        @*Hiddent Field For BusinessOfficeDetailPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeDetailViewModel.BusinessOfficeDetailPrmKey)

                                        @*Hiddent Field For BusinessOfficeRBIRegistrationPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeRBIRegistrationViewModel.BusinessOfficeRBIRegistrationPrmKey)

                                        @*Hiddent Field For BusinessOfficeRBIRegistrationTranslationPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeRBIRegistrationViewModel.BusinessOfficeRBIRegistrationTranslationPrmKey)

                                        @*Hiddent Field For BusinessOfficePasswordPolicyPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficePasswordPolicyViewModel.BusinessOfficePasswordPolicyPrmKey)

                                        @*Hiddent Field For BusinessOfficeAccountNumberPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeAccountNumberViewModel.BusinessOfficeAccountNumberPrmKey)

                                        @*Hiddent Field For BusinessOfficeApplicationNumberPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeApplicationNumberViewModel.BusinessOfficeApplicationNumberPrmKey)

                                        @*Hiddent Field For BusinessOfficeSharesCertificateNumberPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeSharesCertificateNumberViewModel.BusinessOfficeSharesCertificateNumberPrmKey)

                                        @*Hiddent Field For BusinessOfficePersonInformationNumberPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficePersonInformationNumberViewModel.BusinessOfficePersonInformationNumberPrmKey)

                                        @*Hiddent Field For BusinessOfficeDepositCertificateNumberPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeDepositCertificateNumberViewModel.BusinessOfficeDepositCertificateNumberPrmKey)

                                        @*Hiddent Field For BusinessOfficePassbookNumberPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficePassbookNumberViewModel.BusinessOfficePassbookNumberPrmKey)

                                        @*Hiddent Field For BusinessOfficeMemberNumberPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeMemberNumberViewModel.BusinessOfficeMemberNumberPrmKey)

                                        @*Hiddent Field For BusinessOfficeCurrencyPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeCurrencyViewModel.BusinessOfficeCurrencyPrmKey)

                                        @*Hiddent Field For BusinessOfficeMenuPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeMenuViewModel.BusinessOfficeMenuPrmKey)

                                        @*Hiddent Field For BusinessOfficeSpecialPermissionPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeSpecialPermissionViewModel.BusinessOfficeSpecialPermissionPrmKey)

                                        @*Hiddent Field For BusinessOfficeTransactionLimitPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeTransactionLimitViewModel.BusinessOfficeTransactionLimitPrmKey)

                                        @*Hiddent Field For BusinessOfficeTransactionParameterPrmKey*@
                                        @Html.HiddenFor(m => m.BusinessOfficeTransactionParameterViewModel.BusinessOfficeTransactionParameterPrmKey)
                                    </div>
                                </div>

                                @*======= Forth Row - Accordians =======*@
                                <div class="row mt-3">
                                    <div class="col-12">
                                        <div id="accordion" class="accordion md-accordion accordion-blocks">

                                            @*Accordian 1 - BusinessOfficeDetail*@
                                            <div id="heading-business-office-detail" class="card mt-3">
                                                <div class="card-header p-3" id="business-office-detail-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-business-office-detail" data-target="#collapse-business-office-detail" href="#collapse-business-office-detail">
                                                    <div class="row">
                                                        <div class="col-6">
                                                            <a class="card-title" href="#">
                                                                <b>@Html.TextWithRegionalLanguage("Branch Detail")</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <span id="business-office-detail-accordion-error" class="error d-none"> This Is Required, Please Add Valid Data.</span>
                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-business-office-detail" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-business-office-detail" data-target="#collapse-business-office-detail">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div id="collapse-business-office-detail" class="collapse" aria-labelledby="heading-business-office-detail" data-parent="#accordion">
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                @*CenterPrmKey*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Center")
                                                                    @Html.DropDownListFor(m => m.BusinessOfficeDetailViewModel.CenterId, new SelectList(officeDetailViewModel.Centers, "Value", "Text"), "--- Select Center ---", new { @id = "center-id", @class = "form-control mandatory-mark business-office-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.CenterId)
                                                                </div>

                                                                @*LocalCurrencyPrmKey*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Local Currency")
                                                                    @Html.DropDownListFor(m => m.BusinessOfficeDetailViewModel.LocalCurrencyId, new SelectList(officeDetailViewModel.CurrencyDropdownList, "Value", "Text"), "--- Select Local Currency ---", new { @id = "local-currency-id", @class = "form-control mandatory-mark business-office-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.LocalCurrencyId)
                                                                </div>

                                                                @*OfficeSchedulePrmKey*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Office Schedule")
                                                                    @Html.DropDownListFor(m => m.BusinessOfficeDetailViewModel.OfficeScheduleId, new SelectList(officeDetailViewModel.OfficeSchedules, "Value", "Text"), "--- Select Office Schedule ---", new { @id = "office-schedule-dd", @class = "form-control mandatory-mark business-office-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.OfficeScheduleId)
                                                                </div>

                                                                @*BusinessOfficeTypePrmKey*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Branch Type")
                                                                    @Html.DropDownListFor(m => m.BusinessOfficeDetailViewModel.BusinessOfficeTypeId, new SelectList(officeDetailViewModel.BusinessOfficeTypes, "Value", "Text"), "--- Select Branch Type ---", new { @id = "business-office-type-id", @class = "form-control mandatory-mark business-office-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.BusinessOfficeTypeId)
                                                                </div>

                                                                @*BusinessNaturePrmKey*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Business Nature")
                                                                    @Html.DropDownListFor(m => m.BusinessOfficeDetailViewModel.BusinessNatureId, new SelectList(officeDetailViewModel.BusinessNatureDropdownList, "Value", "Text"), "--- Select Business Nature ---", new { @id = "business-nature-id", @class = "form-control mandatory-mark business-office-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.BusinessNatureId)
                                                                </div>

                                                                @*RegionalLanguage*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Regional Language")
                                                                    @Html.DropDownListFor(m => m.BusinessOfficeDetailViewModel.RegionalLanguageId, new SelectList(officeDetailViewModel.Languages, "Value", "Text"), "--- Select Regional Language ---", new { @id = "regional-language-id", @class = "form-control mandatory-mark business-office-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.RegionalLanguageId)
                                                                </div>

                                                                @*CommandAreaRadius*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Command Area Radius")
                                                                    @Html.TextBoxFor(m => m.BusinessOfficeDetailViewModel.CommandAreaRadius, new { @id = "command-area-radius", @class = "form-control mandatory-mark business-office-input", data_toggle = "tooltip", data_placement = "top", title = "Area In Kilometre KM", @type = "number", @min = 1, @max = 999, @placeholder = "Enter Command Area Radius", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.CommandAreaRadius)
                                                                </div>

                                                                @*PopulationOfTheCommandArea*@
                                                                <div class="form-group mt-5">
                                                                    @Html.CustomLabel("Population Of The Command Area")
                                                                    @Html.TextBoxFor(m => m.BusinessOfficeDetailViewModel.PopulationOfTheCommandArea, new { @id = "population-of-the-command-area", @class = "form-control mandatory-mark business-office-input", @type = "number", @min = 99, @max = 999999999, @placeholder = "Enter Population Of The Command Area", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.PopulationOfTheCommandArea)
                                                                </div>

                                                                @*GeneralLedgerPrmKey*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("General Ledger")
                                                                    @Html.DropDownListFor(m => m.BusinessOfficeDetailViewModel.GeneralLedgerId, new SelectList(officeDetailViewModel.BusinessOfficeGeneralLedgerDropdownList, "Value", "Text"), "--- Select General Ledger ---", new { @id = "office-general-ledger-id", @class = "form-control mandatory-mark business-office-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.GeneralLedgerId)
                                                                </div>

                                                                @*EnableAutoAuthorization*@
                                                                <div class="row form-group">
                                                                    <div class="col-10">
                                                                        @Html.CustomLabel("Enable Auto Authorization")
                                                                    </div>
                                                                    <div class="col-2">
                                                                        <div class="text-right">
                                                                            <label class="switch switch-left-right">
                                                                                @Html.CheckBoxFor(m => m.BusinessOfficeDetailViewModel.EnableAutoAuthorization, new { @id = "enable-auto-authorization", @class = "form-control switch-input", required = "required" })
                                                                                <span class="slider round">
                                                                                    <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                </span>
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                                @*GeneralLedgerGroupCode*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("General Ledger Group Code")
                                                                    @Html.TextBoxFor(m => m.BusinessOfficeDetailViewModel.GeneralLedgerGroupCode, new { @id = "general-ledger-group-code", @class = "form-control mandatory-mark business-office-input alpha-numeric", @placeholder = "Enter General Ledger Group Code", @maxlength = 3, onkeyup = "ToTextCapitalize()", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.GeneralLedgerGroupCode)
                                                                </div>

                                                                @*Note*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Note")
                                                                    @Html.TextAreaFor(m => m.BusinessOfficeDetailViewModel.Note, new { @class = "form-control deny-multiple-space", @placeholder = "Enter Note", @maxlength = 1500, onkeyup = "ToTextCapitalize()" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.Note)
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            @*Accordian 2 - CooperativeRegistration *@
