                                                        <div class="row">
                                                            <div class="col-12">
                                                                @*Passbook Data Table*@
                                                                <div class="card-body">
                                                                    <table id="tbl-passbook-number" class="table compact table-striped table-bordered" width="100">
                                                                        <thead class="text-center read-only">
                                                                            <tr>
                                                                                <th><input type="checkbox" name="select-all" id="select-all-passbook-number" /></th>
                                                                                <th class="d-none">GeneralLedgerId</th>
                                                                                <th>NameOfGeneralLedger</th>
                                                                                <th>EnableAutoPassbookNumber</th>
                                                                                <th>EnableReGenerateUnusedPassbookNumber</th>
                                                                                <th>EnableRandomPassbookNumber</th>
                                                                                <th>EnableCustomizePassbookNumber</th>
                                                                                <th>EnableDigitalCodeForPassbookNumber</th>
                                                                                <th class="d-none">PassbookNumberMask</th>
                                                                                <th>PassbookNumberMaskText</th>
                                                                                <th>StartPassbookNumber</th>
                                                                                <th>EndPassbookNumber</th>
                                                                                <th>PassbookNumberIncrementBy</th>
                                                                                <th>Note</th>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody class="text-center read-only">
                                                                            @{
                                                                                IEnumerable<BusinessOfficePassbookNumberViewModel> businessOfficePassbookNumberViewModelList = (IEnumerable<BusinessOfficePassbookNumberViewModel>)HttpContext.Current.Session["BusinessOfficePassbookNumber"];

                                                                                foreach (BusinessOfficePassbookNumberViewModel viewModel in businessOfficePassbookNumberViewModelList)
                                                                                {
                                                                                    <tr>
                                                                                        <td><input type="checkbox" name="check_all" class="checks" /></td>
                                                                                        <td class="d-none">@viewModel.GeneralLedgerId</td>
                                                                                        <td>@viewModel.NameOfGL</td>
                                                                                        <td>@viewModel.EnableAutoPassbookNumber</td>
                                                                                        <td>@viewModel.EnableReGenerateUnusedPassbookNumber</td>
                                                                                        <td>@viewModel.EnableRandomPassbookNumber</td>
                                                                                        <td>@viewModel.EnableCustomizePassbookNumber</td>
                                                                                        <td>@viewModel.EnableDigitalCodeForPassbookNumber</td>
                                                                                        <td class="d-none">@viewModel.PassbookNumberMask</td>
                                                                                        <td>@viewModel.PassbookNumberMask</td>
                                                                                        <td>@viewModel.StartPassbookNumber</td>
                                                                                        <td>@viewModel.EndPassbookNumber</td>
                                                                                        <td>@viewModel.PassbookNumberIncrementBy</td>
                                                                                        <td>@viewModel.Note</td>
                                                                                    </tr>
                                                                                }
                                                                            }
                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            @*Accordian 17 - Transaction Parameter*@
                                            <div id="heading-transaction-parameter" class="card mt-3">
                                                <div class="card-header " id="transaction-parameter-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-transaction-parameter" data-target="#collapse-transaction-parameter">
                                                    <div class="row">
                                                        <div class="col-6">
                                                            <a class="card-title" href="#">
                                                                <b>@Html.TextWithRegionalLanguage("Transaction Parameter")</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <span id="transaction-parameter-accordion-error" class="error d-none"> This Is Required Data Table, Please Add Valid Data.</span>
                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-transaction-parameter" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-transaction-parameter" data-target="#collapse-transaction-parameter">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div id="collapse-transaction-parameter" class="collapse" aria-labelledby="heading-transaction-parameter" data-parent="#accordion">
                                                    <div class="card-body read-only">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                @*EffectiveDate*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Effective Date")
                                                                    @Html.TextBoxFor(m => m.BusinessOfficeTransactionParameterViewModel.EffectiveDate, "{0:yyyy-MM-dd}", new { @id = "effective-date", @class = "form-control mandatory-mark deny-past-date deny-next-financial-year-date transaction-number-input set-current-date", required = "required", @type = "date", @Onclick = "DenyPastDate()" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeTransactionParameterViewModel.EffectiveDate)
                                                                </div>

                                                                @*EnableAutoTransactionNumber*@
                                                                <div class="row form-group mt-4">
                                                                    <div class="col-10">
                                                                        @Html.CustomLabel("Enable Auto Transaction Number")
                                                                    </div>
                                                                    <div class="col-2">
                                                                        <div class="text-left">
                                                                            <label class="switch switch-left-right">
                                                                                @Html.CheckBoxFor(m => m.BusinessOfficeTransactionParameterViewModel.EnableAutoGenerateTransactionNumber, new { @id = "enable-auto-transaction-number", @class = "form-control switch-input show-block" })
                                                                                <span class="slider round">
                                                                                    <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                </span>
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                                <div id="auto-transaction-number-block" class="d-none">
                                                                    @*EnableReGenerateUnusedTransactionNumber*@
                                                                    <div class="row form-group mt-4">
                                                                        <div class="col-10">
                                                                            @Html.CustomLabel("Enable Regenerate Unused Transaction Number")
                                                                        </div>
                                                                        <div class="col-2">
                                                                            <div class="text-left">
                                                                                <label class="switch switch-left-right">
                                                                                    @Html.CheckBoxFor(m => m.BusinessOfficeTransactionParameterViewModel.EnableRegenerateUnusedTransactionNumber, new { @id = "enable-regenerate-unused-transaction-number", @class = "form-control switch-input" })
                                                                                    <span class="slider round">
                                                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                    </span>
                                                                                </label>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    @*EnableDigitalCodeForTransactionNumber*@
                                                                    <div class="row form-group mt-4">
                                                                        <div class="col-10">
                                                                            @Html.CustomLabel("Enable Digital Code For Transaction Number")
                                                                        </div>
                                                                        <div class="col-2">
                                                                            <div class="text-left">
                                                                                <label class="switch switch-left-right">
                                                                                    @Html.CheckBoxFor(m => m.BusinessOfficeTransactionParameterViewModel.EnableTransactionDigitalCode, new { @id = "enable-digital-code-for-transaction-number", @class = "form-control switch-input" })
                                                                                    <span class="slider round">
                                                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                    </span>
                                                                                </label>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    @*TransactionNumberMask*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("Transaction Number Mask")
                                                                        @Html.ListBoxFor(m => m.BusinessOfficeTransactionParameterViewModel.MaskTypeCharacterForTransactionNumberMask, new MultiSelectList(officeDetailViewModel.MaskTypeDropdownList, "Value", "Text"), new { @id = "transaction-number-mask", @class = "form-control mandatory-mark format-types transaction-number-input", required = "required", @multiple = "multiple" })
                                                                        @Html.ValidationMessageFor(m => m.BusinessOfficeTransactionParameterViewModel.MaskTypeCharacterForTransactionNumberMask)
                                                                    </div>

                                                                    @*StartTransactionNumber*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("Start Transaction Number")
                                                                        @Html.TextBoxFor(m => m.BusinessOfficeTransactionParameterViewModel.StartTransactionNumber, new { @id = "start-transaction-number", @class = "form-control mandatory-mark real-number transaction-number-input", @placeholder = "Enter Start Transaction Number", required = "required", @type = "number", @min = 1, @max = 9999999 })
                                                                        @Html.ValidationMessageFor(m => m.BusinessOfficeTransactionParameterViewModel.StartTransactionNumber)
                                                                    </div>

                                                                    @*EndTransactionNumber*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("End Transaction Number")
                                                                        @Html.TextBoxFor(m => m.BusinessOfficeTransactionParameterViewModel.EndTransactionNumber, new { @id = "end-transaction-number", @class = "form-control mandatory-mark real-number transaction-number-input", @placeholder = "Enter End Transaction Number", required = "required", @type = "number", @min = 1, @max = 999999999 })
                                                                        @Html.ValidationMessageFor(m => m.BusinessOfficeTransactionParameterViewModel.EndTransactionNumber)
                                                                    </div>

                                                                    @*TransactionNumberIncrementBy*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("Transaction Number Increment By")
                                                                        @Html.TextBoxFor(m => m.BusinessOfficeTransactionParameterViewModel.TransactionNumberIncrementBy, new { @id = "transaction-number-increment-by", @class = "form-control mandatory-mark mid-number real-number transaction-number-input", @placeholder = "Enter Transaction Number Increment By", required = "required", @type = "number", @min = 1, @max = 999 })
                                                                        @Html.ValidationMessageFor(m => m.BusinessOfficeTransactionParameterViewModel.TransactionNumberIncrementBy)
                                                                    </div>

                                                                    @*TransactionNumberReset*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("Transaction Number Reset")
                                                                        <div class="tenure-unit">
                                                                            <div class="row mt-2">
                                                                                <div class="col-4">
                                                                                    <div class="form-check form-check-inline">
                                                                                        @Html.RadioButtonFor(m => m.BusinessOfficeTransactionParameterViewModel.TransactionNumberReset, "NVR", new { @id = "transaction-number-reset-nvr", @name = "ForNever", @class = "transaction-number-reset transaction-number-input", required = "required" })
                                                                                        <label for="transaction-number-reset-nvr" class="form-check-label pl-2">@Html.TextWithRegionalLanguage("For Never")</label>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-4">
                                                                                    <div class="form-check form-check-inline">
                                                                                        @Html.RadioButtonFor(m => m.BusinessOfficeTransactionParameterViewModel.TransactionNumberReset, "DLY", new { @id = "transaction-number-reset-dly", @name = "ForDaily", @class = "transaction-number-reset transaction-number-input", required = "required" })
                                                                                        <label for="transaction-number-reset-dly" class="form-check-label pl-2">@Html.TextWithRegionalLanguage("For Daily")</label>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-4">
                                                                                    <div class="form-check form-check-inline">
                                                                                        @Html.RadioButtonFor(m => m.BusinessOfficeTransactionParameterViewModel.TransactionNumberReset, "MON", new { @id = "transaction-number-reset-mon", @name = "ForMonthly", @class = "transaction-number-reset transaction-number-input", required = "required" })
                                                                                        <label for="transaction-number-reset-mon" class="form-check-label pl-2">@Html.TextWithRegionalLanguage("For Monthly")</label>
                                                                                    </div>
                                                                                </div>
                                                                            </div>

                                                                            <div class="row mt-2">
                                                                                <div class="col-4">
                                                                                    <div class="form-check form-check-inline">
                                                                                        @Html.RadioButtonFor(m => m.BusinessOfficeTransactionParameterViewModel.TransactionNumberReset, "QRT", new { @id = "transaction-number-reset-qrt", @name = "Quarterly", @class = "transaction-number-reset transaction-number-input", required = "required" })
                                                                                        <label for="transaction-number-reset-qrt" class="form-check-label pl-2">@Html.TextWithRegionalLanguage("Quarterly")</label>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-4">
                                                                                    <div class="form-check form-check-inline">
                                                                                        @Html.RadioButtonFor(m => m.BusinessOfficeTransactionParameterViewModel.TransactionNumberReset, "HLF", new { @id = "transaction-number-reset-hlf", @name = "HalfYearly", @class = "transaction-number-reset transaction-number-input", required = "required" })
                                                                                        <label for="transaction-number-reset-hlf" class="form-check-label pl-2">@Html.TextWithRegionalLanguage("Half Yearly")</label>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-4">
                                                                                    <div class="form-check form-check-inline">
                                                                                        @Html.RadioButtonFor(m => m.BusinessOfficeTransactionParameterViewModel.TransactionNumberReset, "YRL", new { @id = "transaction-number-reset-yrl", @name = "Yearly", @class = "transaction-number-reset transaction-number-input", required = "required" })
                                                                                        <label for="transaction-number-reset-yrl" class="form-check-label pl-2">@Html.TextWithRegionalLanguage("Yearly")</label>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        @Html.ValidationMessageFor(m => m.BusinessOfficeTransactionParameterViewModel.TransactionNumberReset)
                                                                    </div>

                                                                    @*ChecksumAlgorithmId*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("Checksum Algorithm")
                                                                        @Html.DropDownListFor(m => m.BusinessOfficeTransactionParameterViewModel.ChecksumAlgorithmId, new SelectList(officeDetailViewModel.ChecksumAlgorithmDropdownList, "Value", "Text", 0), "--- Select Checksum Algorithm ---", new { @id = "checksum-algorithm-id", @class = "form-control mandatory-mark transaction-number-input", required = "required" })
                                                                        @Html.ValidationMessageFor(m => m.BusinessOfficeTransactionParameterViewModel.ChecksumAlgorithmId)
                                                                    </div>
                                                                </div>

                                                                @*Note*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Note")
                                                                    @Html.TextAreaFor(m => m.BusinessOfficeTransactionParameterViewModel.Note, new { @class = "form-control default-none deny-multiple-space", @placeholder = "Enter Note", @maxlength = 1500, onkeyup = "ToTextCapitalize()" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeTransactionParameterViewModel.Note)
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            
                            @*Footer*@
                            @Html.OperationFooter("None", "Exit", "Home", "None")
                        }
                    }
                </div>
            </div>
        </div>
    </div>
</body>
</html>
<script src="~/Scripts/Custom/BusinessOffice.js"></script>