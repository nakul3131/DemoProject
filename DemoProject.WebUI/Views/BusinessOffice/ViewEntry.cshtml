@model DemoProject.Services.ViewModel.Enterprise.Office.BusinessOfficeViewModel
@using DemoProject.Services.HtmlHelpers
@using DemoProject.Services.ViewModel.Enterprise.Office
@using DemoProject.Services.ViewModel.Configuration

@{
    OfficeDetailViewModel officeDetailViewModel = new OfficeDetailViewModel();
    HeaderPartialModel headerPartialModel = new HeaderPartialModel("BUOVET");
}

<!DOCTYPE html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>Business Office - ViewEntry</title>
</head>
<body>
    <div class="container-fluid">
        @*======= First Row - For Header =======*@
        <div class="row">
            <div class="offset-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @*======= Second Row - Data Entry Page =======*@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card mb-3">
                    @using (Html.BeginForm("ViewEntry", "BusinessOffice", FormMethod.Post, htmlAttributes: new { id = "form", @autocomplete = "off" }))
                    {
                        @Html.AntiForgeryToken()
                        {
                            <div class="card-body">
                                @*======= First Row - Mandatory Mark =======*@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <h6 class="mb-2 text-right text-muted"><b class="mandatory-mark">&nbsp; Mandatory Field</b></h6>
                                    </div>
                                </div>

                                @*======= Second Row - Page Title =======*@
                                <div class="row mt-2">
                                    @*Page Title*@
                                    <div class="col-12">
                                        <h5 class="font-weight-bold mt-1">ViewEntry</h5>
                                        <hr />
                                    </div>
                                </div>

                                @*Submit Error*@
                                @Html.ValidationMessage("SubmitError")

                                @*======= Third Row - Data Entry Page =======*@
                                <div class="row read-only">
                                    <div class="col-12">
                                        @*BusinessOfficeCode*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Branch Code")
                                            @Html.TextBoxFor(m => m.BusinessOfficeCode, new { @class = "form-control mandatory-mark", @placeholder = "Enter Branch Code" })
                                            @Html.ValidationMessageFor(m => m.BusinessOfficeCode)
                                        </div>

                                        @*AlternateBusinessOfficeCode*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Alternate Branch Code")
                                            @Html.TextBoxFor(m => m.AlternateBusinessOfficeCode, new { @id = "alternate-business-office-code", @class = "form-control", @placeholder = "Enter Alternate Branch Code" })
                                            @Html.ValidationMessageFor(m => m.AlternateBusinessOfficeCode)
                                        </div>

                                        @*NameOfBusinessOffice*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name Of Branch")
                                            @Html.TextBoxFor(m => m.NameOfBusinessOffice, new { @id = "name-of-business-office", @class = "form-control mandatory-mark", @placeholder = "Enter Name Of Branch" })
                                            @Html.ValidationMessageFor(m => m.NameOfBusinessOffice)
                                        </div>

                                        @*TransNameOfBusinessOffice*@
                                        <div class="form-group">
                                            @Html.CustomLabel(officeDetailViewModel.NameOfBusinessOfficeInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransNameOfBusinessOffice, null, new { @id = "trans-name-of-business-office", @class = "form-control mandatory-mark", @placeholder = officeDetailViewModel.NameOfBusinessOfficePlaceHolderInRegionalLanguage })
                                            @Html.ValidationMessageFor(m => m.TransNameOfBusinessOffice)
                                        </div>

                                        @*AliasName*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Alias Name")
                                            @Html.TextBoxFor(m => m.AliasName, new { @id = "alias-name", @class = "form-control", @placeholder = "Enter Alias Name" })
                                            @Html.ValidationMessageFor(m => m.AliasName)
                                        </div>

                                        @*TransAliasName*@
                                        <div class="form-group">
                                            @Html.CustomLabel(officeDetailViewModel.AliasNameInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransAliasName, null, new { @id = "trans-alias-name", @class = "form-control", @placeholder = officeDetailViewModel.AliasNamePlaceHolderInRegionalLanguage })
                                            @Html.ValidationMessageFor(m => m.TransAliasName)
                                        </div>

                                        @*NameOnReport*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name On Report")
                                            @Html.TextBoxFor(m => m.NameOnReport, new { @id = "name-report", @class = "form-control ", @placeholder = "Enter Name On Report" })
                                            @Html.ValidationMessageFor(m => m.NameOnReport)
                                        </div>

                                        @*TransNameOnReport*@
                                        <div class="form-group">
                                            @Html.CustomLabel(officeDetailViewModel.NameOnReportInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.TransNameOnReport, null, new { @id = "trans-name-report", @class = "form-control", @placeholder = officeDetailViewModel.NameOnReportPlaceHolderInRegionalLanguage })
                                            @Html.ValidationMessageFor(m => m.TransNameOnReport)
                                        </div>

                                        @*NameOfBusinessOfficeForThirdPartyInterface*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name Of Branch For Third Party Interface")
                                            @Html.TextBoxFor(m => m.NameOfBusinessOfficeForThirdPartyInterface, new { @id = "name-of-business-office-for-third-party-interface", @class = "form-control", @placeholder = "Enter Name Of Branch For Third Party Interface" })
                                            @Html.ValidationMessageFor(m => m.NameOfBusinessOfficeForThirdPartyInterface)
                                        </div>

                                        @*OpeningDate*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Opening Date")
                                            @Html.TextBoxFor(m => m.OpeningDate, "{0:yyyy-MM-dd}", new { @class = "form-control mandatory-mark", @placeholder = "Enter Opening Date" })
                                            @Html.ValidationMessageFor(m => m.OpeningDate)
                                        </div>

                                        @*ContactDetails*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Contact Details")
                                            @Html.TextAreaFor(m => m.ContactDetails, new { @id = "contact-details", @class = "form-control mandatory-mark alpha-numeric", @placeholder = "Enter Contact Details" })
                                            @Html.ValidationMessageFor(m => m.ContactDetails)
                                        </div>

                                        @*TransContactDetails*@
                                        <div class="form-group">
                                            @Html.CustomLabel(officeDetailViewModel.ContactDetailsInRegionalLanguage)
                                            @Html.TextAreaFor(m => m.TransContactDetails, new { @id = "trans-contact-details", @class = "form-control mandatory-mark", @placeholder = officeDetailViewModel.ContactDetailsPlaceHolderInRegionalLanguage })
                                            @Html.ValidationMessageFor(m => m.TransContactDetails)
                                        </div>

                                        @*AddressDetails*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Address Details")
                                            @Html.TextAreaFor(m => m.AddressDetails, new { @id = "address-details", @class = "form-control mandatory-mark", @placeholder = "Enter Address Details" })
                                            @Html.ValidationMessageFor(m => m.AddressDetails)
                                        </div>

                                        @*TransAddressDetails*@
                                        <div class="form-group">
                                            @Html.CustomLabel(officeDetailViewModel.AddressDetailsInRegionalLanguage)
                                            @Html.TextAreaFor(m => m.TransAddressDetails, new { @id = "trans-address-details", @class = "form-control mandatory-mark", @placeholder = officeDetailViewModel.AddressDetailsPlaceHolderInRegionalLanguage })
                                            @Html.ValidationMessageFor(m => m.TransAddressDetails)
                                        </div>

                                        @*IsFundBranch*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Fund Branch")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-right">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.IsFundBranch, new { @id = "is-fund-branch", @class = "form-control switch-input" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*EnableCorporateAccess*@
                                        <div class="row form-group">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Corporate Access")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-right">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.EnableCorporateAccess, new { @id = "enable-corporate-access", @class = "form-control switch-input" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*LoanDirectDebitGenerationDays*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Loan Direct Debit Generation Days")
                                            @Html.TextBoxFor(m => m.LoanDirectDebitGenerationDays, new { @id = "loan-direct-debit-generation-days", @class = "form-control mandatory-mark", @placeholder = "Enter Loan Direct Debit Generation Days" })
                                            @Html.ValidationMessageFor(m => m.LoanDirectDebitGenerationDays)
                                        </div>

                                        @*ParentBusinessOfficePrmKey*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Parent Branch")
                                            @Html.DropDownListFor(m => m.ParentBusinessOfficeId, new SelectList(officeDetailViewModel.BusinessOfficeDropdownList, "Value", "Text"), "Select Parent Branch", new { @class = "form-control mandatory-mark" })
                                            @Html.ValidationMessageFor(m => m.ParentBusinessOfficeId)
                                        </div>

                                        @*ClearingBusinessOfficePrmKey*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Clearing Branch")
                                            @Html.DropDownListFor(m => m.ClearingBusinessOfficeId, new SelectList(officeDetailViewModel.BusinessOfficeDropdownList, "Value", "Text"), "Select Clearing Branch", new { @class = "form-control mandatory-mark" })
                                            @Html.ValidationMessageFor(m => m.ClearingBusinessOfficeId)
                                        </div>

                                        @*TransactionCodeForClearing*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Transaction Code For Clearing")
                                            @Html.TextBoxFor(m => m.TransactionCodeForClearing, new { @id = "transaction-code-for-clearing", @class = "form-control mandatory-mark", @placeholder = "Enter Transaction Code For Clearing" })
                                            @Html.ValidationMessageFor(m => m.TransactionCodeForClearing)
                                        </div>

                                        @*RegionalOfficePrmKey*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Regional Office")
                                            @Html.DropDownListFor(m => m.RegionalOfficeId, new SelectList(officeDetailViewModel.BusinessOfficeDropdownList, "Value", "Text"), "Select Regional Office", new { @class = "form-control mandatory-mark" })
                                            @Html.ValidationMessageFor(m => m.RegionalOfficeId)
                                        </div>

                                        @*BusinessOfficeStatusForCoreOperation*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Branch Status For Core Operation")
                                            @Html.DropDownListFor(m => m.BusinessOfficeStatusForCoreOperation, new List < SelectListItem >
                                                {
                                                new SelectListItem { Text = "Active", Value = "ACT" },
                                                new SelectListItem { Text = "Closed", Value = "CLS" },
                                                new SelectListItem { Text = "Inactive", Value = "INA" },
                                                new SelectListItem { Text = "Temporarily Blocked", Value = "BLC" },
                                                },
                                                "Select Branch Status For Core Operation", new { @class = "form-control mandatory-mark", required = "required"})
                                            @Html.ValidationMessageFor(m => m.BusinessOfficeStatusForCoreOperation)
                                        </div>

                                        @*Note*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Note")
                                            @Html.TextAreaFor(m => m.Note, new { @id = "note", @class = "form-control", @placeholder = "Enter Note", @maxlength = "1500" })
                                            @Html.ValidationMessageFor(m => m.Note)
                                        </div>

                                        @*TransNote*@
                                        <div class="form-group">
                                            @Html.CustomLabel(officeDetailViewModel.NoteInRegionalLanguage)
                                            @Html.TextAreaFor(m => m.TransNote, new { @id = "trans-note", @class = "form-control", @placeholder = officeDetailViewModel.NotePlaceHolderInRegionalLanguage })
                                            @Html.ValidationMessageFor(m => m.TransNote)
                                        </div>

                                        @*NameOfUser*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name Of User")
                                            @Html.TextBoxFor(m => m.NameOfUser, new { @class = "form-control" })
                                        </div>

                                        @*EntryDateTime*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Entry Date Time")
                                            @Html.TextBoxFor(m => m.EntryDateTime, new { @class = "form-control" })
                                        </div>
                                    </div>
                                </div>

                                @*======= Forth Row - Accordians =======*@
                                <div class="row mt-3">
                                    <div class="col-12">
                                        <div id="accordion" class="accordion md-accordion accordion-blocks">
                                            @*Accordian 1 - BusinessOfficeDetail*@
                                            <div id="heading-business-office-detail" class="card mt-3">
                                                <div class="card-header p-3" id="business-office-detail-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-business-office-detail" data-target="#collapse-business-office-detail" href="#collapse-business-office-detail">
                                                    <div class="row">
                                                        <div class="col-6">
                                                            <a class="card-title" href="#">
                                                                <b>@Html.TextWithRegionalLanguage("Branch Detail")</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <span id="business-office-detail-accordion-error" class="error d-none"> This Is Required, Please Add Valid Data.</span>
                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-business-office-detail" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-business-office-detail" data-target="#collapse-business-office-detail">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div id="collapse-business-office-detail" class="collapse" aria-labelledby="heading-business-office-detail" data-parent="#accordion">
                                                    <div class="card-body">
                                                        <div class="row read-only">
                                                            <div class="col-12">
                                                                @*CenterPrmKey*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Center")
                                                                    @Html.DropDownListFor(m => m.BusinessOfficeDetailViewModel.CenterId, new SelectList(officeDetailViewModel.Centers, "Value", "Text"), "Select City", new { @id = "center-id", @class = "form-control mandatory-mark business-office-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.CenterId)
                                                                </div>

                                                                @*LocalCurrencyPrmKey*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Local Currency")
                                                                    @Html.DropDownListFor(m => m.BusinessOfficeDetailViewModel.LocalCurrencyId, new SelectList(officeDetailViewModel.CurrencyDropdownList, "Value", "Text"), "Select Default Currency", new { @id = "local-currency-id", @class = "form-control mandatory-mark business-office-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.LocalCurrencyId)
                                                                </div>

                                                                @*OfficeSchedulePrmKey*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Office Schedule")
                                                                    @Html.DropDownListFor(m => m.BusinessOfficeDetailViewModel.OfficeScheduleId, new SelectList(officeDetailViewModel.OfficeSchedules, "Value", "Text"), "Select Office Schedule", new { @id = "office-schedule-dd", @class = "form-control mandatory-mark business-office-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.OfficeScheduleId)
                                                                </div>

                                                                @*BusinessOfficeTypePrmKey*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Branch Type")
                                                                    @Html.DropDownListFor(m => m.BusinessOfficeDetailViewModel.BusinessOfficeTypeId, new SelectList(officeDetailViewModel.BusinessOfficeTypes, "Value", "Text"), "Select Branch Type", new { @id = "business-office-type-id", @class = "form-control mandatory-mark business-office-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.BusinessOfficeTypeId)
                                                                </div>

                                                                @*BusinessNaturePrmKey*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Business Nature")
                                                                    @Html.DropDownListFor(m => m.BusinessOfficeDetailViewModel.BusinessNatureId, new SelectList(officeDetailViewModel.BusinessNatureDropdownList, "Value", "Text"), "Select Business Nature", new { @id = "business-nature-id", @class = "form-control mandatory-mark business-office-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.BusinessNatureId)
                                                                </div>

                                                                @*RegionalLanguage*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Regional Language")
                                                                    @Html.DropDownListFor(m => m.BusinessOfficeDetailViewModel.RegionalLanguageId, new SelectList(officeDetailViewModel.Languages, "Value", "Text"), "Select Regional Language", new { @id = "regional-language-id", @class = "form-control mandatory-mark business-office-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.RegionalLanguageId)
                                                                </div>

                                                                @*CommandAreaRadius*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Command Area Radius")
                                                                    @Html.TextBoxFor(m => m.BusinessOfficeDetailViewModel.CommandAreaRadius, new { @id = "command-area-radius", @class = "form-control mandatory-mark business-office-input", data_toggle = "tooltip", data_placement = "top", title = "Area In Kilometre KM", @min = 1, @max = 999, @placeholder = "Enter Command Area Radius", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.CommandAreaRadius)
                                                                </div>

                                                                @*PopulationOfTheCommandArea*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Population Of The Command Area")
                                                                    @Html.TextBoxFor(m => m.BusinessOfficeDetailViewModel.PopulationOfTheCommandArea, new { @id = "population-of-the-command-area", @class = "form-control mandatory-mark business-office-input", @min = 99, @max = 999999999, @placeholder = "Enter Population Of The Command Area", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.PopulationOfTheCommandArea)
                                                                </div>

                                                                @*GeneralLedgerPrmKey*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("General Ledger")
                                                                    @Html.DropDownListFor(m => m.BusinessOfficeDetailViewModel.GeneralLedgerId, new SelectList(officeDetailViewModel.BusinessOfficeGeneralLedgerDropdownList, "Value", "Text"), "Select General Ledger", new { @id = "office-general-ledger-id", @class = "form-control mandatory-mark business-office-input", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.GeneralLedgerId)
                                                                </div>

                                                                @*EnableAutoAuthorization*@
                                                                <div class="row form-group">
                                                                    <div class="col-10">
                                                                        @Html.CustomLabel("Enable Auto Authorization")
                                                                    </div>
                                                                    <div class="col-2">
                                                                        <div class="text-right">
                                                                            <label class="switch switch-left-right">
                                                                                @Html.CheckBoxFor(m => m.BusinessOfficeDetailViewModel.EnableAutoAuthorization, new { @id = "enable-auto-authorization", @class = "form-control switch-input", required = "required" })
                                                                                <span class="slider round">
                                                                                    <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                </span>
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                                @*GeneralLedgerGroupCode*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("General Ledger Group Code")
                                                                    @Html.TextBoxFor(m => m.BusinessOfficeDetailViewModel.GeneralLedgerGroupCode, new { @id = "general-ledger-group-code", @class = "form-control mandatory-mark business-office-input alpha-numeric", @placeholder = "Enter General Ledger Group Code", @maxlength = "3", onkeyup = "ToTextCapitalize()", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.GeneralLedgerGroupCode)
                                                                </div>

                                                                @*Note*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Note")
                                                                    @Html.TextAreaFor(m => m.BusinessOfficeDetailViewModel.Note, new { @class = "form-control", @placeholder = "Enter Note", @maxlength = "1500", onkeyup = "ToTextCapitalize()" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeDetailViewModel.Note)
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            @*Accordian 2 - CooperativeRegistration *@
                                            <div class="card @(officeDetailViewModel.IsRegisterUnderCooperative ? "d-block" : "d-none" ) mt-3">
                                                <div class="card-header collapsed p-3" id="heading-cooperative-registration" aria-expanded="false" data-toggle="collapse" href="#collapse-cooperative-registration">
                                                    @*<div class="card-header " id="special-permission-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-cooperative-registration" data-target="#collapse-cooperative-registration">*@
                                                    <div class="row">
                                                        <div class="col-6">
                                                            <a class="card-title" href="#">
                                                                <b>@Html.TextWithRegionalLanguage("Cooperative Registration")</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <span id="cooperative-registration-accordion-error" class="error d-none"> This Is Required, Please Add Valid Data.</span>
                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-cooperative-registration" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-cooperative-registration" data-target="#collapse-cooperative-registration">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                    @*</div>*@
                                                </div>
                                                <div id="collapse-cooperative-registration" class="collapse" aria-labelledby="heading-cooperative-registration" data-parent="#accordion">
                                                    <div class="card-body">
                                                        <div class="row read-only">
                                                            <div class="col-12">
                                                                @*CoopApprovalDate*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Approval Date")
                                                                    @Html.TextBoxFor(m => m.BusinessOfficeCoopRegistrationViewModel.ApprovalDate, "{0:yyyy-MM-dd}", new { @id = "coop-approval-date", @class = "form-control mandatory-mark deny-future-date cooperative-registration-input", @placeholder = "Enter Coop Approval Date", required = "required", onclick = "DenyFutureDate()" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeCoopRegistrationViewModel.ApprovalDate)
                                                                </div>

                                                                @*CoopRegistrationDate*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Registration Date")
                                                                    @Html.TextBoxFor(m => m.BusinessOfficeCoopRegistrationViewModel.RegistrationDate, "{0:yyyy-MM-dd}", new { @id = "coop-registration-date", @class = "form-control mandatory-mark deny-future-date cooperative-registration-input", @placeholder = "Enter Coop Registration Date", required = "required", onclick = "DenyFutureDate()" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeCoopRegistrationViewModel.RegistrationDate)
                                                                </div>

                                                                @*CoopRegistrationNumber*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Registration Number")
                                                                    @Html.TextBoxFor(m => m.BusinessOfficeCoopRegistrationViewModel.RegistrationNumber, new { @id = "coop-registration-number", @class = "form-control mandatory-mark cooperative-registration-input", @placeholder = "Enter Registration Number", required = "required", @minlength = 5, @maxlength = 50, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeCoopRegistrationViewModel.RegistrationNumber)
                                                                </div>

                                                                @*TransCoopRegistrationNumber*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel(officeDetailViewModel.RegistrationNumberInRegionalLanguage)
                                                                    @Html.TextBoxFor(m => m.BusinessOfficeCoopRegistrationViewModel.TransRegistrationNumber, new { @id = "trans-coop-registration-number", @class = "form-control deny-multiple-space mandatory-mark cooperative-registration-input", @placeholder = officeDetailViewModel.RegistrationNumberPlaceHolderInRegionalLanguage, required = "required", @minlength = 5, @maxlength = 50 })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeCoopRegistrationViewModel.TransRegistrationNumber)
                                                                </div>

                                                                @*CoopReferenceNumber*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Reference Number")
                                                                    @Html.TextBoxFor(m => m.BusinessOfficeCoopRegistrationViewModel.ReferenceNumber, new { @id = "coop-reference-number", @class = "form-control", @placeholder = "Enter Reference Number", @minlength = 4, @maxlength = 50, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeCoopRegistrationViewModel.ReferenceNumber)
                                                                </div>

                                                                @*TransCoopReferenceNumber*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel(officeDetailViewModel.ReferenceNumberInRegionalLanguage)
                                                                    @Html.TextBoxFor(m => m.BusinessOfficeCoopRegistrationViewModel.TransReferenceNumber, new { @id = "trans-coop-reference-number", @class = "form-control", @placeholder = officeDetailViewModel.ReferenceNumberPlaceHolderInRegionalLanguage, @minlength = 4, @maxlength = 50 })
                                                                    @Html.ValidationMessageFor(m => m.BusinessOfficeCoopRegistrationViewModel.TransReferenceNumber)
                                                                </div>

                                                                @*CoopNumericCode*@
                                                                <div class="form-group">
