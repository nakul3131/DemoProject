@model DemoProject.Services.ViewModel.Account.Customer.DepositCustomerAccountViewModel
@using DemoProject.Services.HtmlHelpers
@using DemoProject.Services.ViewModel.Account.Customer
@using DemoProject.Services.ViewModel.Configuration
@using DemoProject.Services.ViewModel.PersonInformation
@using DemoProject.Services.Constants

@{
    DepositCustomerAccountDetailViewModel depositCustomerAccountDetailViewModel = new DepositCustomerAccountDetailViewModel();

    HeaderPartialModel headerPartialModel = new HeaderPartialModel("CDAAMN");
}

<!doctype html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>Customer Deposit Account - Amend</title>

    <style>
        span .field-validation-error {
            margin-top: 20% !important
        }
    </style>
</head>
<body>
    <div class="container-fluid">
        @*======= First Row - For Header =======*@
        <div class="row">
            <div class="offset-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @*======= Second Row - Data Entry Page =======*@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card d-block mb-3">
                    @using (Html.BeginForm("Amend", "CustomerDepositAccount", FormMethod.Post, new { enctype = "multipart/form-data", @id = "form", @autocomplete = "off" }))
                    {
                        @Html.AntiForgeryToken()
                        {
                            <div class="card-body">
                                @*======= First Row - Mandatory Mark =======*@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <h6 class="mb-2 text-right text-muted"><b class="mandatory-mark">&nbsp; Mandatory Field</b></h6>
                                    </div>
                                </div>

                                @*======= Second Row - Page Title, Translation =======*@
                                <div class="row mt-2">
                                    @*Page Title*@
                                    <div class="col-12">
                                        <h5 class="font-weight-bold mt-1">Amend</h5>
                                        <hr />
                                    </div>
                                </div>

                                @*Submit Error*@
                                @Html.ValidationMessage("SubmitError")

                                @*======= Third Row - Data Entry Page =======*@
                                <div id="amend-view" class="row">
                                    <div class="col-12">
                                        @*CheckerRemark*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Checker Remark")
                                            @Html.TextBoxFor(m => m.CheckerRemark, new { @id = "name-of-center", @class = "form-control mandatory-mark read-only" })
                                            @Html.ValidationMessageFor(m => m.CheckerRemark)
                                        </div>

                                        @*BusinessOfficeId*@
                                        <div class="form-group @(depositCustomerAccountDetailViewModel.HasBranch ? "" : "d-none")">
                                            @Html.CustomLabel("Branch")
                                            <span id="business-office-change-info" class="ml-5 text-primary d-none"> ** If You Change The Business Office And Related Content, All Of Business Office Releated Content Is Removed.</span>
                                            @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.BusinessOfficeId, new SelectList(depositCustomerAccountDetailViewModel.AuthorizedBusinessOfficeDropdownList, "Value", "Text", 0), new { @id = "business-office-id", @class = "form-control mandatory-mark" })
                                            @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.BusinessOfficeId)
                                            <span id="business-office-id-error" class="error d-none"> Please Select Valid Business Office.</span>
                                        </div>

                                        @*DepositType*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Deposit Type")
                                            <span id="deposit-type-msg" class="ml-5 text-primary d-none"> ** If You Change The Deposit Type And Related Content, All Of Deposit Type Releated Content Is Removed.</span>
                                            @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.DepositType, new SelectList(depositCustomerAccountDetailViewModel.DepositTypeDropdownList, "Value", "Text", 0), "--- Select Deposit Type--- ", new { @id = "deposit-type-id", @class = "form-control mandatory-mark", required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.DepositType)
                                            <span id="deposit-type-id-error" class="error d-none">
                                                If You Change The Business Office And Related Content, All Of Business Office Releated Content Is Removed.
                                            </span>
                                        </div>

                                        @*GeneralLedgerId*@
                                        <div class="form-group">
                                            @Html.CustomLabel("General Ledger")
                                            <span id="general-ledger-change-info" class="ml-5 text-primary d-none">** If You Change The General Ledger And Related Content, All Of General Ledger Releated Content Is Removed.</span>
                                            @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.GeneralLedgerId, new SelectList(depositCustomerAccountDetailViewModel.GeneralLedgerDropdownList, "Value", "Text"), new { @id = "general-ledger-id", @class = "form-control mandatory-mark" })
                                            @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.GeneralLedgerId)
                                            <span id="general-ledger-id-error" class="error d-none"> Please Select General Ledger</span>
                                        </div>

                                        @*SchemeId*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Scheme")
                                            <span id="scheme-change-info" class="ml-5 text-primary d-none">** If You Change The Scheme And Related Content, All Of Scheme Releated Content Is Removed.</span>
                                            @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.SchemeId, new SelectList(depositCustomerAccountDetailViewModel.DepositSchemeDropdownList, "Value", "Text"), new { @id = "scheme-id", @class = "form-control mandatory-mark" })
                                            @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.SchemeId)
                                            <span id="scheme-id-error" class="error d-none">Scheme Setting Error : Something Went Wrong When Selecting Scheme.</span>
                                        </div>

                                        @*PersonId*@
                                        <div class="form-group" id="person-name">
                                            @Html.CustomLabel("Customer Name")
                                            <span id="person-change-info" class="ml-5 text-primary d-none"> ** You Are Changing Customer Name So Joint / Nominne Data Has Been Removed.</span>
                                            @Html.TextBox("person", null, new { @type = "search", @id = "person-id", @class = "form-control mandatory-mark deny-multiple-space text-capitalize", @placeholder = "Select Customer Name", required = "required" })
                                            @Html.TextBoxFor(m => m.CustomerAccountDetailViewModel.PersonId, new { @type = "search", @id = "person-id1", @class = "form-control mandatory-mark text-capitalize d-none", @placeholder = "Select Customer Name", required = "required" })
                                            <span id="person-id-value" class="@Model.CustomerAccountDetailViewModel.FullName"></span>
                                            <span id="person-id-error" class="error d-none"> Please Select Person Name.</span>                                            <span id="person-id-error" class="error d-none"> Please Select Person Name.</span>
                                        </div>

                                        @*Currency Name*@
                                        <div class="form-group @(depositCustomerAccountDetailViewModel.HasMultiCurrency ? "" : "d-none")">
                                            @Html.CustomLabel("Currency")
                                            @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.CurrencyId, new SelectList(depositCustomerAccountDetailViewModel.CurrencyDropdownList, "Value", "Text", 0), "--- Select Currency ---", new { @id = "currency-id", @class = "form-control mandatory-mark", required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.CurrencyId)
                                        </div>

                                        @*AccountOpeningDate*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Account Opening Date")
                                            @Html.TextBoxFor(m => m.AccountOpeningDate, "{0:yyyy-MM-dd}", new { @id = "account-opening-date", @class = "form-control mandatory-mark deny-future-date", required = "required", @Type = "date", min = depositCustomerAccountDetailViewModel.AllowLastPastDays })
                                            @Html.ValidationMessageFor(m => m.AccountOpeningDate)
                                        </div>

                                        @*Tenure*@
                                        <div class="form-group tenure term-deposit recurring-deposit">
                                            @Html.CustomLabel("Tenure")
                                            <div class="form-inline">
                                                <div class="input-group col">
                                                    @Html.TextBoxFor(m => m.Year, new { @id = "year", @class = "form-control default-zero", @placeholder = "Enter Year", @type = "number" })
                                                    <span class="input-group-addon" style="background-color:  #70A8FF; color: white;">Year</span>
                                                </div>
                                                <div class="input-group col">
                                                    @Html.TextBoxFor(m => m.Month, new { @id = "month", @class = "form-control default-zero", @placeholder = "Enter Month", @type = "number" })
                                                    <span class="input-group-addon" style="background-color:  #70A8FF; color: white;">Month</span>
                                                </div>
                                                <div class="input-group col">
                                                    @Html.TextBoxFor(m => m.Day, new { @id = "day", @class = "form-control default-zero", @placeholder = "Enter Day", @type = "number" })
                                                    <span class="input-group-addon" style="background-color:  #70A8FF; color: white;">Day</span>
                                                </div>
                                            </div>
                                        </div>

                                        @*MaturityDate*@
                                        <div class="form-group term-deposit recurring-deposit">
                                            @Html.CustomLabel("Maturity Date")
                                            @Html.TextBoxFor(m => m.CustomerDepositAccountViewModel.MaturityDate, "{0:yyyy-MM-dd}", new { @id = "maturity-date", @class = "form-control mandatory-mark deny-past-date-from-reference-date", required = "required", @Type = "date", onclick = "DenyPastDateFromReferenceDate('account-opening-date')" })
                                            @Html.ValidationMessageFor(m => m.CustomerDepositAccountViewModel.MaturityDate)
                                        </div>

                                        @*ApplicationNumber*@
                                        <div id="application-number-input" class="form-group">
                                            @Html.CustomLabel("Application Number")
                                            @Html.TextBoxFor(m => m.ApplicationNumber, new { @id = "application-number", @class = "form-control real-number mandatory-mark", @placeholder = "Enter Application Number", required = "required", @type = "number" })
                                            @Html.ValidationMessageFor(m => m.ApplicationNumber)
                                        </div>

                                        @*AccountNumber*@
                                        <div id="acc-number-grp" class="form-group">
                                            @Html.CustomLabel("Account Number")
                                            @Html.TextBoxFor(m => m.AccountNumber, new { @id = "account-number", @class = "form-control real-number mandatory-mark", @placeholder = "Enter Account Number", required = "required", @type = "number", @min = 1, @max = 999999999999999 })
                                            @Html.ValidationMessageFor(m => m.AccountNumber)
                                        </div>

                                        @*AlternateAccountNumber1*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Alternate Account Number 1")
                                            @Html.TextBoxFor(m => m.AlternateAccountNumber1, new { @id = "alternate-account-number-1", @class = "form-control mandatory-mark deny-multiple-space", @placeholder = "Enter Alternate Account Number 1", required = "required", @maxlength = 50, onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.AlternateAccountNumber1)
                                        </div>

                                        @*AlternateAccountNumber2*@
                                        <div id="account-number2" class="form-group">
                                            @Html.CustomLabel("Alternate Account Number 2")
                                            @Html.TextBoxFor(m => m.AlternateAccountNumber2, new { @id = "alternate-account-number-2", @class = "form-control mandatory-mark deny-multiple-space", @placeholder = "Enter Alternate Account Number 2", required = "required", @maxlength = 50, onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.AlternateAccountNumber2)
                                        </div>

                                        @*AlternateAccountNumber3*@
                                        <div id="account-number3" class="form-group ">
                                            @Html.CustomLabel("Alternate Account Number 3")
                                            @Html.TextBoxFor(m => m.AlternateAccountNumber3, new { @id = "alternate-account-number-3", @class = "form-control mandatory-mark deny-multiple-space", @placeholder = "Enter Alternate Account Number 3", required = "required", @maxlength = 50, onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.AlternateAccountNumber3)
                                        </div>

                                        @*Rate Of Interest*@
                                        <div class="form-group interest-rate-div">
                                            @Html.CustomLabel("Rate Of Interest")
                                            @Html.TextBoxFor(m => m.CustomerAccountInterestRateViewModel.RateOfInterest, new { @id = "rate-of-interest", @class = "form-control mandatory-mark", @placeholder = "Enter Rate Of Interest", required = "required", @type = "number", @min = 1, @max = 20 })
                                            @Html.ValidationMessageFor(m => m.CustomerAccountInterestRateViewModel.RateOfInterest)
                                        </div>

                                        <div class="recurring-deposit">
                                            @*DepositInstallmentAmount*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Deposit Installment Amount")
                                                @Html.TextBoxFor(m => m.CustomerDepositAccountViewModel.DepositInstallmentAmount, new { @id = "deposit-installment-amount", @class = "form-control mandatory-mark", @placeholder = "Enter Deposit Installment Amount", required = "required", @type = "number" })
                                                <span id="deposit-installment-amount-error" class="error d-none"></span>
                                                @Html.ValidationMessageFor(m => m.CustomerDepositAccountViewModel.DepositInstallmentAmount)
                                            </div>

                                            @*InstallmentFrequencyPrmKey*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Installment Frequency")
                                                @Html.DropDownListFor(m => m.CustomerDepositAccountViewModel.InstallmentFrequencyId, new SelectList(depositCustomerAccountDetailViewModel.InstallmentFrequencyDropdownList, "Value", "Text", 0), "--- Select Installment Frequency ---", new { @id = "installment-frequency-id", @class = "form-control mandatory-mark" })
                                                @Html.ValidationMessageFor(m => m.CustomerDepositAccountViewModel.InstallmentFrequencyId)
                                            </div>
                                        </div>

                                        @*PassbookNumber*@
                                        <div class="form-group passbook-number-field">
                                            @Html.CustomLabel("Passbook Number")
                                            @Html.TextBoxFor(m => m.PassbookNumber, new { @id = "passbook-number", @class = "form-control real-number mandatory-mark", @placeholder = "Enter Passbook Number", required = "required", @type = "number" })
                                            @Html.ValidationMessageFor(m => m.PassbookNumber)
                                        </div>

                                        @*AccountOperationMode*@
                                        <div class="form-group demand-deposit">
                                            @Html.CustomLabel("Account Operation Mode")
                                            @Html.DropDownListFor(m => m.CustomerDepositAccountViewModel.AccountOperationModeId, new SelectList(depositCustomerAccountDetailViewModel.AccountOperationModeDropdownList, "Value", "Text", 0), "--- Select Account Operation Mode ---", new { @id = "accountOperationMode-id", @class = "form-control mandatory-mark", required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerDepositAccountViewModel.AccountOperationModePrmKey)
                                        </div>

                                        @*EnableAutoDebit*@
                                        <div class="row form-group term-deposit recurring-deposit mt-4">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Auto Debit")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-right">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.CustomerAccountStandingInstructionViewModel.EnableAutoDebit, new { @id = "enable-auto-debit", @class = "form-control switch-input show-block" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*EnableAutoCloseOnMaturity*@
                                        <div id="auto-close-maturity-input" class="row auto-close-maturity term-deposit recurring-deposit form-group mt-4">
                                            <div class="col-10">
                                                @Html.CustomLabel("Enable Auto Close On Maturity")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-right">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.CustomerDepositAccountViewModel.EnableAutoCloseOnMaturity, new { @id = "enable-auto-close-on-maturity", @class = "form-control switch-input hide-block" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*IsPrivateCustomer*@
                                        <div class="row form-group mt-4">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is VIP Customer")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-right">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.IsPrivateCustomer, new { @id = "is-private-customer", @class = "form-control switch-input" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*Note*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Note")
                                            @Html.TextAreaFor(m => m.Note, new { @id = "note", @class = "form-control deny-multiple-space default-none", @placeholder = "Enter Note", @maxlength = 1500, onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.Note)
                                        </div>

                                        @*======= Fourth Row - Accordians =======*@
                                        <div class="row mt-3">
                                            <div class="col-12">
                                                <div id="accordion" class="accordion md-accordion accordion-blocks">
                                                    @*Accordian 1 - CustomerTermDepositAccountDetail*@
                                                    <div id="term-deposit-account-card" class="card term-deposit customer-term-deposit-account-detail mt-3">
                                                        <div class="card-header" id="collapse-term-deposit-account-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-term-deposit-account" data-target="#collapse-term-deposit-account">
                                                            <div class="row">
                                                                <div class="col-md-4">
                                                                    <a class="card-title" href="#">
                                                                        <b>@Html.TextWithRegionalLanguage("Term Deposit Detail")</b>
                                                                    </a>
                                                                </div>
                                                                <div class="col-md-6">
                                                                    <span id="term-deposit-account-error" class="error d-none">This Is Required, Please Add Valid Data.</span>
                                                                </div>
                                                                <div class="col-2">
                                                                    <a href="#collapse-term-deposit-account" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-term-deposit-account" data-target="#collapse-term-deposit-account">
                                                                        <i class="fa fa-angle-down toggle-icon"></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div id="collapse-term-deposit-account" class="collapse" aria-labelledby="heading-term-deposit-account" data-parent="#accordion">
                                                            <div class="card-body">
                                                                <div class="row">
                                                                    <div class="col-12">
                                                                        @*CertificateNumber*@
                                                                        <div id="certificate-input" class="form-group">
                                                                            @Html.CustomLabel("Certificate Number")
                                                                            @Html.TextBoxFor(m => m.CustomerTermDepositAccountDetailViewModel.CertificateNumber, new { @id = "certificate-number", @class = "form-control mandatory-mark term-deposit-account-input", @placeholder = "Enter Certificate Number Number", required = "required", @min = 1, @max = 999999999 })
                                                                            @Html.ValidationMessageFor(m => m.CustomerTermDepositAccountDetailViewModel.CertificateNumber)
                                                                        </div>

                                                                        @*DepositAmount*@
                                                                        <div class="form-group">
                                                                            @Html.CustomLabel("Deposit Amount")
                                                                            @Html.TextBoxFor(m => m.CustomerTermDepositAccountDetailViewModel.DepositAmount, new { @id = "deposit-amount", @class = "form-control mandatory-mark term-deposit-account-input", @placeholder = "Enter Deposit Amount Number", required = "required", @type = "number", @min = 0, @max = 9999999999 })
                                                                            <span id="deposit-amount-error" class="error d-none"></span>
                                                                            @Html.ValidationMessageFor(m => m.CustomerTermDepositAccountDetailViewModel.DepositAmount)
                                                                        </div>

                                                                        @*Number Of Split Deposits Accounts*@
                                                                        <div id="no-of-accounts-input" class="form-group d-none">
                                                                            @Html.CustomLabel("Number Of Split Deposits")
                                                                            @Html.TextBoxFor(m => m.CustomerTermDepositAccountDetailViewModel.NoOfDeposits, null, new { @id = "no-of-accounts", @class = "form-control mandatory-mark real-number term-deposit-account-input", @placeholder = "Enter No Of Accounts", required = "required", @type = "number", @min = 0, @max = 19 })
                                                                            @Html.ValidationMessageFor(m => m.CustomerTermDepositAccountDetailViewModel.NoOfDeposits)
                                                                            <span id="no-of-accounts-error" class="error d-none"></span>
                                                                        </div>

                                                                        <div class="no-of-inputs"></div>
                                                                        <div class="no-of-inputs-total"></div>
                                                                        <span id="deposit-split-amount-error" class="error d-none">Deposit Amount And Split Amount Mismatch</span>

                                                                        @*InterestPayoutFrequency*@
                                                                        <div id="interest-payout">
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Interest Payout Frequency")
                                                                                @Html.DropDownListFor(m => m.CustomerTermDepositAccountDetailViewModel.InterestPayoutFrequency, new SelectList(depositCustomerAccountDetailViewModel.InterestPayOutFrequencyDropdownList, "Value", "Text", 0), "--- Select Frequency ---", new { @id = "interest-payout-frequency", @class = "form-control mandatory-mark term-deposit-account-input", required = "required" })
                                                                                @Html.ValidationMessageFor(m => m.CustomerTermDepositAccountDetailViewModel.InterestPayoutFrequency)
                                                                            </div>

                                                                            @*InterestPayoutAmount*@
                                                                            <div id="interest-payout-amount-input" class="form-group">
                                                                                @Html.CustomLabel("Interest Payout Amount")
                                                                                @Html.TextBoxFor(m => m.CustomerTermDepositAccountDetailViewModel.InterestPayoutAmount, new { @id = "interest-payout-amount", @class = "form-control mandatory-mark term-deposit-account-input", @placeholder = "Enter Interest Payout Amount", required = "required", @type = "number", @min = 1, @max = 99999999 })
                                                                                @Html.ValidationMessageFor(m => m.CustomerTermDepositAccountDetailViewModel.InterestPayoutAmount)
                                                                            </div>

                                                                            @*InterestPayoutDay*@
                                                                            <div id="interest-payout-day-input" class="form-group">
                                                                                @Html.CustomLabel("Interest Payout Day")
                                                                                @Html.TextBoxFor(m => m.CustomerTermDepositAccountDetailViewModel.InterestPayoutDay, new { @id = "interest-payout-day", @class = "form-control mandatory-mark real-number term-deposit-account-input", @placeholder = "Enter Interest Payout Day", required = "required", @min = 1, @max = 31 })
                                                                                @Html.ValidationMessageFor(m => m.CustomerTermDepositAccountDetailViewModel.InterestPayoutDay)
                                                                            </div>
                                                                        </div>

                                                                        @* TotalInterestAmount *@
                                                                        <div class="form-group">
                                                                            @Html.CustomLabel("Total Interest Amount ")
                                                                            @Html.TextBoxFor(m => m.CustomerTermDepositAccountDetailViewModel.TotalInterestAmount, new { @id = "total-interest-amount", @class = "form-control mandatory-mark term-deposit-account-input", @placeholder = "Enter Total Interest Amount", required = "required", @type = "number", @min = 1, @max = 99999999 })
                                                                            @Html.ValidationMessageFor(m => m.CustomerTermDepositAccountDetailViewModel.TotalInterestAmount)
                                                                        </div>

                                                                        @* MaturityAmount*@
                                                                        <div class="form-group">
                                                                            @Html.CustomLabel("Maturity Amount")
                                                                            @Html.TextBoxFor(m => m.CustomerTermDepositAccountDetailViewModel.MaturityAmount, new { @id = "maturity-amount", @class = "form-control mandatory-mark read-only term-deposit-account-input", @placeholder = "Enter Maturity Amount", required = "required", @type = "number", @min = 1, @max = 9999999999 })
                                                                            @Html.ValidationMessageFor(m => m.CustomerTermDepositAccountDetailViewModel.MaturityAmount)
                                                                        </div>

                                                                        @*EnableAutoRenewalOnMaturity*@
                                                                        <div id="auto-renwal" class="row form-group mt-3">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Auto Renew On Maturity")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.CustomerTermDepositAccountDetailViewModel.EnableAutoRenewOnMaturity, new { @id = "enable-auto-renew-on-maturity", @class = "form-control switch-input show-block" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @* AutoRenewWaitingTimePeriod*@
                                                                        <div id="auto-renew-on-maturity-block" class="d-none">
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Auto Renew Waiting Time Period")
                                                                                @Html.TextBoxFor(m => m.CustomerTermDepositAccountDetailViewModel.AutoRenewWaitingTimePeriod, new { @id = "auto-renew-waiting-time-period", @class = "form-control mandatory-mark real-number term-deposit-account-input", @placeholder = "Enter Auto Renew Waiting Time Period", required = "required", @type = "number" })
                                                                                @Html.ValidationMessageFor(m => m.CustomerTermDepositAccountDetailViewModel.AutoRenewWaitingTimePeriod)
                                                                            </div>

                                                                            @*RenewTypePrmKey*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Renew Type")
