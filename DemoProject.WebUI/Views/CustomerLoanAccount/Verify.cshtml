                                                <div id="collapse-scheme-document" class="collapse" aria-labelledby="heading-scheme-document" data-parent="#accordion">
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                <div class="card-body">
                                                                    <table id="tbl-document" class="table compact table-striped table-bordered" width="100">
                                                                        <thead class="text-center read-only">
                                                                            <tr>
                                                                                <th><input type="checkbox" name="check_all" id="select-all-document" /></th>
                                                                                <th class="d-none">DocumenteId</th>
                                                                                <th>DocumentText</th>
                                                                                <th>SequenceNumber</th>
                                                                                <th>Photopath</th>
                                                                                <th>Photo</th>
                                                                                <th>FileCaption</th>
                                                                                <th>Note</th>
                                                                                <th class="d-none">ReasonForModification</th>
                                                                                <th class="d-none">FileName</th>
                                                                                <th class="d-none">CustomerAccountDocumentPrmKey</th>
                                                                                <th class="d-none">LocalStoragePath</th>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody class="text-center read-only">
                                                                            @{
                                                                                                                                                                                                                                                                IEnumerable<CustomerAccountDocumentViewModel> documentViewModelList = (IEnumerable<CustomerAccountDocumentViewModel>)HttpContext.Current.Session["Document"];

                                                                                                                                                                                                                                                                if (documentViewModelList != null)
                                                                                                                                                                                                                                                                {
                                                                                                                                                                                                                                                                    foreach (CustomerAccountDocumentViewModel viewModel in documentViewModelList)
                                                                                                                                                                                                                                                                    {
                                                                                                                                                                                                                                                                        counter += 1;
                                                                                                                                                                                                                                                                        photoId = "photo-id" + counter.ToString();
                                                                                                                                                                                                                                                                        fileId = "file-id" + counter.ToString();
                                                                                                                                                                                                                                                                        fileUploaderId = "data-table-document-file-uploader" + counter.ToString();

                                                                                <tr>
                                                                                    <td><input type="checkbox" name="check_all" class="checks" /></td>
                                                                                    <td class="d-none">@viewModel.DocumentId</td>
                                                                                    <td>@viewModel.NameOfDocument</td>
                                                                                    <td>@viewModel.SequenceNumber</td>
                                                                                    <td>
                                                                                        <input type="file" id=@fileUploaderId class="db-record" name="PhotoPath" disabled />
                                                                                    </td>
                                                                                    <td>
                                                                                        @if (viewModel.LocalStoragePath != "None")
                                                                                                                                                                                                                                                                                                {
                                                                                        <img id="@photoId" class="db-record" src="@Url.Content(viewModel.LocalStoragePath)" width="50" height="50" />
                                                                                                                                                                                                                                                                                                }
                                                                                                                                                                                                                                                                                                else
                                                                                                                                                                                                                                                                                                {
                                                                                                                                                                                                                                                                                                    var base64 = Convert.ToBase64String(viewModel.DocumentPhotoCopy);
                                                                                                                                                                                                                                                                                                    var imgsrc = string.Format("data:image/jpg;base64,{0}", base64);
                                                                                        <img src='@imgsrc' id=@photoId width="50" height="50" />
                                                                                                                                                                                                                                                                                                    string filename = Path.GetFileName(imgsrc);
                                                                                                                                                                                                                                                                                                }
                                                                                    </td>
                                                                                    <td>@viewModel.FileCaption</td>
                                                                                    <td>@viewModel.Note</td>
                                                                                    <td class="d-none">@viewModel.ReasonForModification</td>
                                                                                    <td class="d-none">@viewModel.NameOfFile</td>
                                                                                    <td class="d-none">@viewModel.CustomerAccountDocumentPrmKey</td>
                                                                                    <td class="d-none">@viewModel.LocalStoragePath</td>
                                                                                </tr>
                                                                                                                                                                                                                                                                    }
                                                                                                                                                                                                                                                                }
                                                                            }
                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
