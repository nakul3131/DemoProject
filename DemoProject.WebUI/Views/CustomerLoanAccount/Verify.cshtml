@model DemoProject.Services.ViewModel.Account.Customer.LoanCustomerAccountViewModel
@using DemoProject.Services.HtmlHelpers
@using DemoProject.Services.ViewModel.Account.Customer
@using DemoProject.Services.ViewModel.Configuration
@using DemoProject.Services.ViewModel.Account.Layout
@using DemoProject.Services.ViewModel.PersonInformation
@using DemoProject.Services.ViewModel.PersonInformation.Parameter

@{
        /*Review And Delete Those Variables*/
        byte counter = 0;
        string photoId = "";
        string fileId = "";
        string fileUploaderId = "";
        /*Review And Delete Those Variables*/

        LayoutDetailViewModel detailViewModel = new LayoutDetailViewModel();

        PersonDetailViewModel personDetailViewModel = new PersonDetailViewModel();
        LoanCustomerAccountDetailViewModel loanCustomerAccountDetailViewModel = new LoanCustomerAccountDetailViewModel();

        PersonInformationParameterViewModel personInformationParameterViewModel = new PersonInformationParameterViewModel();
        personInformationParameterViewModel = ViewBag.PersonInformationParameter;

        HeaderPartialModel headerPartialModel = new HeaderPartialModel("CLAVRF");
}

<!doctype html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>Loan Customer Account - Verify</title>
</head>
<body>
    <div class="container-fluid">
        @*======= First Row - For Header =======*@
        <div class="row">
            <div class="offset-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @*======= Second Row - Data Entry Page =======*@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card d-block mb-3">
                    @using (Html.BeginForm("Verify", "CustomerLoanAccount", FormMethod.Post, new { @id = "form", enctype = "multipart/form-data", @autocomplete = "off" }))
                                        {
                    @Html.AntiForgeryToken()
                                            {
                    <div class="card-body">
                        @*======= First Row - Mandatory Mark =======*@
                        <div class="row mt-2">
                            <div class="col-12">
                                <h6 class="mb-2 text-right text-muted"><b class="mandatory-mark">&nbsp; Mandatory Field</b></h6>
                            </div>
                        </div>

                        @*======= Second Row - Page Title, Translation =======*@
                        <div class="row mt-2">
                            @*Page Title*@
                            <div class="col-12">
                                <h5 class="font-weight-bold mt-1">Verify</h5>
                                <hr />
                            </div>
                        </div>

                        @*Submit Error*@
                        @Html.ValidationMessage("SubmitError")

                        @*======= Third Row - Data Entry Page =======*@
                        <div id="verify-view" class="row">
                            <div class="col-12">
                                <div class="read-only">
                                    @*MakerRemark*@
                                    <div class="form-group">
                                        @Html.CustomLabel("Maker Remark")
                                        @Html.TextBoxFor(m => m.MakerRemark, new { @id = "maker-remark", @class = "form-control mandatory-mark " })
                                        @Html.ValidationMessageFor(m => m.MakerRemark)
                                    </div>

                                    @*BusinessOfficeId*@
                                    <div class="form-group">
                                        @Html.CustomLabel("Branch")
                                        <span id="business-office-id-msg" class="ml-5"></span>
                                        @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.BusinessOfficeId, new SelectList(loanCustomerAccountDetailViewModel.BusinessOfficeDropdownList, "Value", "Text"), "--- Select Branch ---", new { @id = "business-office-id", @class = "form-control mandatory-mark", required = "required" })
                                        @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.BusinessOfficeId)
                                        <span id="business-office-id-error" class="error d-none"> Please Select Valid Business Office.</span>
                                    </div>

                                    @*LoanTypeId*@
                                    <div class="form-group">
                                        @Html.CustomLabel("Loan Type")
                                        <span id="loan-type-id-msg" class="ml-5"></span>
                                        @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.LoanTypeId, new SelectList(loanCustomerAccountDetailViewModel.LoanTypeDropdownList, "Value", "Text"), new { @id = "loan-type-id", @class = "form-control mandatory-mark", required = "required" })
                                        @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.LoanTypeId)
                                        <span id="loan-type-id-error" class="error d-none">
                                            If You Change The Business Office And Related Content, All Of Business Office Releated Content Is Removed.
                                        </span>
                                    </div>

                                    @*GeneralLedgerId*@
                                    <div class="form-group">
                                        @Html.CustomLabel("General Ledger")
                                        <span id="general-ledger-id-msg" class="ml-5"></span>
                                        @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.GeneralLedgerId, new SelectList(loanCustomerAccountDetailViewModel.GeneralLedgerDropdownList, "Value", "Text"), "Select General Ledger", new { @id = "general-ledger-id", @class = "form-control mandatory-mark", required = "required" })
                                        @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.GeneralLedgerId)
                                        <span id="general-ledger-id-error" class="error d-none"> Please Select General Ledger</span>
                                    </div>

                                    @*SchemeId*@
                                    <div class="form-group">
                                        @Html.CustomLabel("Scheme")
                                        <span id="scheme-change-info" class="ml-5 text-primary d-none">** If You Change The Scheme And Related Content, All Of Scheme Related Content Is Removed.</span>
                                        @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.SchemeId, new SelectList(loanCustomerAccountDetailViewModel.SchemeLoanAccountParameterDropdownList, "Value", "Text"), new { @id = "scheme-id", @class = "form-control mandatory-mark", required = "required" })
                                        @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.SchemeId)
                                        <span id="scheme-id-error" class="error d-none">Scheme Setting Error : Something Went Wrong When Selecting Scheme.</span>
                                    </div>

                                    @*MinuteOfMeetingAgenda*@
                                    <div id="minute-of-meeting-agenda-input" class="form-group optional-input">
                                        @Html.CustomLabel("Minute Of Meeting Agenda")
                                        @Html.DropDownListFor(m => m.CustomerLoanAccountViewModel.MinuteOfMeetingAgendaId, new SelectList(loanCustomerAccountDetailViewModel.MinuteOfMeetingAgendaDropdownList, "Value", "Text"), new { @id = "minute-of-meeting-agenda-id", @class = "form-control mandatory-mark", required = "required" })
                                        @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.MinuteOfMeetingAgendaId)
                                    </div>

                                    @*PersonId*@
                                    <div class="form-group">
                                        @Html.CustomLabel("Person")
                                        <span id="person-change-info" class="ml-5 text-primary d-none"> ** You Are Changing Customer Name So Joint / Nominne Data Has Been Removed.</span>
                                        @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.PersonId, new SelectList(loanCustomerAccountDetailViewModel.PersonDropdownList, "Value", "Text"), new { @id = "person-id", @class = "form-control mandatory-mark", required = "required" })
                                        @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.PersonId)
                                        <span id="person-id-error" class="error d-none"> Please Select Person Name.</span>
                                    </div>

                                    @*Currency Name*@
                                    <div class="form-group @(loanCustomerAccountDetailViewModel.HasMultiCurrency ? "" : "d-none")">
                                        @Html.CustomLabel("Currency")
                                        @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.CurrencyId, new SelectList(loanCustomerAccountDetailViewModel.CurrencyDropdownList, "Value", "Text", 0), "--- Select Currency ---", new { @id = "currency-id", @class = "form-control mandatory-mark", required = "required" })
                                        @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.CurrencyId)
                                    </div>

                                    @*AccountOpeningDate*@
                                    <div class="form-group">
                                        @Html.CustomLabel("Account Opening Date")
                                        @Html.TextBoxFor(m => m.AccountOpeningDate, "{0:yyyy-MM-dd}", new { @id = "account-opening-date", @class = "form-control mandatory-mark deny-future-date set-current-date", required = "required", @Type = "date" })
                                        @Html.ValidationMessageFor(m => m.AccountOpeningDate)
                                    </div>

                                    @*Tenure*@
                                    <div class="form-group tenure">
                                        @Html.CustomLabel("Tenure")
                                        <div class="form-inline row">
                                            <div class="input-group col">
                                                @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.Year, new { @id = "year", @class = "form-control default-zero", @placeholder = "Enter Year", @type = "number", required = "required", @min = 0 })
                                                <span class="input-group-addon" style="background-color:  #70A8FF; color: white;">Year</span>
                                            </div>
                                            <div class="input-group col">
                                                @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.Month, new { @id = "month", @class = "form-control default-zero", @placeholder = "Enter Month", @type = "number", required = "required", @min = 0 })
                                                <span class="input-group-addon" style="background-color:  #70A8FF; color: white;">Month</span>
                                            </div>
                                            <div class="input-group col">
                                                @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.Day, new { @id = "day", @class = "form-control default-zero", @placeholder = "Enter Day", @type = "number", required = "required", @min = 0 })
                                                <span class="input-group-addon" style="background-color:  #70A8FF; color: white;">Day</span>
                                            </div>
                                        </div>
                                    </div>

                                    @*MaturityDate*@
                                    <div class="form-group term-deposit recurring-deposit">
                                        @Html.CustomLabel("Maturity Date")
                                        @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.MaturityDate, "{0:yyyy-MM-dd}", new { @id = "maturity-date", @class = "form-control mandatory-mark deny-past-date-from-reference-date", required = "required", @Type = "date", onclick = "DenyPastDateFromReferenceDate('account-opening-date')" })
                                        @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.MaturityDate)
                                    </div>

                                    @*RateOfInterest*@
                                    <div class="form-group">
                                        @Html.CustomLabel("Rate Of Interest")
                                        @Html.TextBoxFor(m => m.CustomerAccountInterestRateViewModel.RateOfInterest, new { @id = "rate-of-interest", @class = "form-control mandatory-mark", @placeholder = "Enter Rate Of Interest", required = "required", @type = "number", @min = 1, @max = 99 })
                                        @Html.ValidationMessageFor(m => m.CustomerAccountInterestRateViewModel.RateOfInterest)
                                    </div>

                                    @*ApplicationNumber*@
                                    <div id="application-number-input" class="form-group">
                                        @Html.CustomLabel("Application Number")
                                        @Html.TextBoxFor(m => m.ApplicationNumber, new { @id = "application-number", @class = "form-control mandatory-mark", @placeholder = "Enter Application Number", required = "required", @type = "number" })
                                        @Html.ValidationMessageFor(m => m.ApplicationNumber)
                                    </div>

                                    @*AgreementNumber*@
                                    <div id="agreement-number-input" class="form-group">
                                        @Html.CustomLabel("Agreement Number")
                                        @Html.TextBoxFor(m => m.AgreementNumber, new { @id = "agreement-number", @class = "form-control real-number mandatory-mark", @placeholder = "Enter Agreement Number", required = "required", @type = "number" })
                                        @Html.ValidationMessageFor(m => m.AgreementNumber)
                                    </div>

                                    @*AccountNumber*@
                                    <div id="acc-number-input" class="form-group">
                                        @Html.CustomLabel("Account Number")
                                        @Html.TextBoxFor(m => m.AccountNumber, new { @id = "account-number", @class = "form-control mandatory-mark", @placeholder = "Enter Account Number", required = "required", @type = "number", @min = 1, @max = 999999999999999 })
                                        @Html.ValidationMessageFor(m => m.AccountNumber)
                                    </div>

                                    @*AlternateAccountNumber1*@
                                    <div class="form-group">
                                        @Html.CustomLabel("Alternate Account Number 1")
                                        @Html.TextBoxFor(m => m.AlternateAccountNumber1, new { @id = "alternate-account-number-1", @class = "form-control mandatory-mark alpha-numeric", @placeholder = "Enter Alternate Account Number 1", required = "required", @maxlength = 50, onkeyup = "ToTextCapitalize()" })
                                        @Html.ValidationMessageFor(m => m.AlternateAccountNumber1)
                                    </div>

                                    @*AlternateAccountNumber2*@
                                    <div id="account-number2" class="form-group">
                                        @Html.CustomLabel("Alternate Account Number 2")
                                        @Html.TextBoxFor(m => m.AlternateAccountNumber2, new { @id = "alternate-account-number-2", @class = "form-control alpha-numeric mandatory-mark", @placeholder = "Enter Alternate Account Number 2", required = "required", @maxlength = 50, onkeyup = "ToTextCapitalize()" })
                                        @Html.ValidationMessageFor(m => m.AlternateAccountNumber2)
                                    </div>

                                    @*AlternateAccountNumber3*@
                                    <div id="account-number3" class="form-group ">
                                        @Html.CustomLabel("Alternate Account Number 3")
                                        @Html.TextBoxFor(m => m.AlternateAccountNumber3, new { @id = "alternate-account-number-3", @class = "form-control alpha-numeric mandatory-mark", @placeholder = "Enter Alternate Account Number 3", required = "required", @maxlength = 50, onkeyup = "ToTextCapitalize()" })
                                        @Html.ValidationMessageFor(m => m.AlternateAccountNumber3)
                                    </div>

                                    @*PassbookNumber*@
                                    <div id="passbook-number-input" class="form-group">
                                        @Html.CustomLabel("Passbook Number")
                                        @Html.TextBoxFor(m => m.PassbookNumber, new { @id = "passbook-number", @class = "form-control real-number mandatory-mark", @placeholder = "Enter Passbook Number", required = "required", @type = "number" })
                                        @Html.ValidationMessageFor(m => m.PassbookNumber)
                                    </div>

                                    @*LoanReason*@
                                    <div class="form-group">
                                        @Html.CustomLabel("Loan Reason")
                                        @Html.DropDownListFor(m => m.CustomerLoanAccountViewModel.LoanReasonId, new SelectList(loanCustomerAccountDetailViewModel.LoanReasonDropdownList, "Value", "Text"), "--- Select Loan Reason ---", new { @id = "loan-reason-id", @class = "form-control mandatory-mark", required = "required" })
                                        @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.LoanReasonId)
                                    </div>

                                    @*DemandAmount*@
                                    <div id="demand-amount-input" class="form-group">
                                        @Html.CustomLabel("Demand Amount")
                                        @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.DemandAmount, new { @id = "demand-amount", @class = "form-control mandatory-mark", @placeholder = "Enter Demand Amount", required = "required", @type = "number" })
                                        @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.DemandAmount)
                                    </div>

                                    @*SanctionAmount*@
                                    <div class="form-group">
                                        @Html.CustomLabel("Sanction Amount")
                                        <div class="input-group">
                                            <div class="input-group-prepend"><span class="input-group-text">₹</span></div>
                                            @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.SanctionAmount, new { @id = "sanction-amount", @class = "form-control mandatory-mark", @placeholder = "Enter Sanction Amount", required = "required", @type = "number" })
                                        </div>
                                        @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.SanctionAmount)
                                    </div>

                                    @*DeductedSharesAmount*@
                                    <div class="form-group d-none" id="shares-amount-input">
                                        @Html.CustomLabel("Deducted Shares Amount")
                                        <div class="input-group">
                                            <div class="input-group-prepend"><span class="input-group-text">₹</span></div>
                                            @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.DeductedSharesAmount, new { @id = "deducted-shares-amount", @class = "form-control mandatory-mark read-only", @placeholder = "Enter Shares Amount", @type = "number", required = "required" })
                                        </div>
                                        @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.DeductedSharesAmount)
                                    </div>

                                    @*DeductionRemark*@
                                    <div class="form-group d-none read-only" id="deduction-remark-input">
                                        @Html.CustomLabel("Deduction Remark")
                                        @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.DeductionRemark, new { @id = "deduction-remark", @class = "form-control mandatory-mark alpha-numeric", @placeholder = "Enter Deduction Remark", required = "required" })
                                        @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.DeductionRemark)
                                    </div>

                                    <div id="swot-analysis-input">
                                        @*StrengthsFactors*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Strengths Factors")
                                            @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.StrengthsFactors, new { @id = "strengths-factors", @class = "form-control mandatory-mark title-case alpha-numeric", @placeholder = "Enter Strengths Factors", onkeydown = "Maketranslation()", @minLength = 4, @maxLength = 1500, required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.StrengthsFactors)
                                        </div>
                                        @*TransStrengthsFactors*@
                                        <div class="form-group">
                                            @Html.CustomLabel(personDetailViewModel.StrengthsFactorsInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.TransStrengthsFactors, new { @id = "trans-strengths-factors", @class = "form-control mandatory-mark", @placeholder = personDetailViewModel.StrengthsFactorsPlaceHolderInRegionalLanguage, @minLength = 4, @maxLength = 1500, required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.TransStrengthsFactors)
                                        </div>

                                        @*WeaknessesFactors*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Weaknesses Factors")
                                            @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.WeaknessesFactors, new { @id = "weaknesses-factors", @class = "form-control mandatory-mark title-case alpha-numeric", @placeholder = "Enter Weaknesses Factors", onkeydown = "Maketranslation()", @minLength = 4, @maxLength = 1500, required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.WeaknessesFactors)
                                        </div>

                                        @*TransWeaknessesFactors*@
                                        <div class="form-group">
                                            @Html.CustomLabel(personDetailViewModel.WeaknessesFactorsInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.TransWeaknessesFactors, new { @id = "trans-weaknesses-factors", @class = "form-control mandatory-mark", @placeholder = personDetailViewModel.WeaknessesFactorsPlaceHolderInRegionalLanguage, @minLength = 4, @maxLength = 1500, required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.TransWeaknessesFactors)
                                        </div>

                                        @*OpportunitiesFactors*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Opportunities Factors")
                                            @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.OpportunitiesFactors, new { @id = "opportunities-factors", @class = "form-control mandatory-mark title-case alpha-numeric", @placeholder = "Enter Opportunities Factors", onkeydown = "Maketranslation()", @minLength = 4, @maxLength = 1500, required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.OpportunitiesFactors)
                                        </div>

                                        @*TransOpportunitiesFactors*@
                                        <div class="form-group">
                                            @Html.CustomLabel(personDetailViewModel.OpportunitiesFactorsInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.TransOpportunitiesFactors, new { @id = "trans-opportunities-factors", @class = "form-control mandatory-mark", @placeholder = personDetailViewModel.OpportunitiesFactorsPlaceHolderInRegionalLanguage, @minLength = 4, @maxLength = 1500, required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.TransOpportunitiesFactors)
                                        </div>

                                        @*ThreatsFactors*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Threats Factors")
                                            @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.ThreatsFactors, new { @id = "threats-factors", @class = "form-control mandatory-mark title-case alpha-numeric", @placeholder = "Enter Threats Factors", onkeydown = "Maketranslation()", @minLength = 4, @maxLength = 1500, required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.ThreatsFactors)
                                        </div>

                                        @*TransThreatsFactors*@
                                        <div class="form-group">
                                            @Html.CustomLabel(personDetailViewModel.ThreatsFactorsInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.TransThreatsFactors, new { @id = "trans-threats-factors", @class = "form-control mandatory-mark", @placeholder = personDetailViewModel.ThreatsFactorsPlaceHolderInRegionalLanguage, @minLength = 4, @maxLength = 1500, required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.TransThreatsFactors)
                                        </div>
                                    </div>

                                    @*PastCreditHistory*@
                                    <div id="credit-history-input" class="form-group">
                                        <div class="form-group">
                                            @Html.CustomLabel("Past Credit History")
                                            @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.PastCreditHistory, new { @id = "past-credit-history", @class = "form-control mandatory-mark title-case alpha-numeric", @placeholder = "Enter Past Credit History", onkeydown = "Maketranslation()", @minLength = 4, @maxLength = 1500, required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.PastCreditHistory)
                                        </div>

                                        @*TransPastCreditHistory*@
                                        <div class="form-group">
                                            @Html.CustomLabel(personDetailViewModel.PastCreditHistoryInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.TransPastCreditHistory, new { @id = "trans-past-credit-history", @class = "form-control mandatory-mark", @placeholder = personDetailViewModel.PastCreditHistoryPlaceHolderInRegionalLanguage, @minLength = 4, @maxLength = 1500, required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.TransPastCreditHistory)
                                        </div>
                                    </div>

                                    @*LegalAndRegulatoryCompliance*@
                                    <div id="legal-regulatory-compliance-input" class="form-group">
                                        <div class="form-group">
                                            @Html.CustomLabel("Legal And Regulatory Compliance")
                                            @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.LegalAndRegulatoryCompliance, new { @id = "legal-and-regulatory-compliance", @class = "form-control mandatory-mark title-case alpha-numeric", @placeholder = "Enter Legal And Regulatory Compliance", @minLength = 4, @maxLength = 1500, onkeydown = "Maketranslation()", required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.LegalAndRegulatoryCompliance)
                                        </div>

                                        @*TransLegalAndRegulatoryCompliance*@
                                        <div class="form-group">
                                            @Html.CustomLabel(personDetailViewModel.LegalAndRegulatoryComplianceInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.CustomerLoanAccountViewModel.TransLegalAndRegulatoryCompliance, new { @id = "trans-legal-and-regulatory-compliance", @class = "form-control mandatory-mark", @placeholder = personDetailViewModel.LegalAndRegulatoryCompliancePlaceHolderInRegionalLanguage, @minLength = 4, @maxLength = 1500, required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.TransLegalAndRegulatoryCompliance)
                                        </div>
                                    </div>

                                    @*Occupation*@
                                    <div class="form-group">
                                        @Html.CustomLabel("Occupation")
                                        @Html.DropDownListFor(m => m.CustomerLoanAccountViewModel.OccupationId, new SelectList(personDetailViewModel.OccupationDropdownList, "Value", "Text", 0), "--- Select Occupation ---", new { @id = "occupation-id", @class = "form-control mandatory-mark additional-details-input", required = "required" })
                                        @Html.ValidationMessageFor(m => m.CustomerLoanAccountViewModel.OccupationId)
                                    </div>

                                    @*IsEmployee*@
                                    <div id="is-employee-input" class="row form-group mt-4">
                                        <div class="col-10">
                                            @Html.CustomLabel("Is Employee")
                                        </div>
                                        <div class="col-2">
                                            <div class="text-left">
                                                <label class="switch switch-left-right">
                                                    @Html.CheckBoxFor(m => m.CustomerLoanAccountViewModel.IsEmployee, new { @id = "enable-employee", @class = "form-control switch-input hide-block" })
                                                    <span class="slider round">
                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                    </span>
                                                </label>
                                            </div>
                                        </div>
                                    </div>

                                    @*Employer Details*@
                                    <div id="employee-block">
                                        @*NameOfEmployer*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name Of Employer")
                                            @Html.TextBoxFor(m => m.PersonEmploymentDetailViewModel.NameOfEmployer, new { @id = "employer-name", @class = "form-control mandatory-mark alpha employer-input title-case additional-details-input", @placeholder = "Enter Name Of Employer", @minlength = 2, @maxlength = 150, onkeydown = "Maketranslation()", required = "required" })
                                            @Html.ValidationMessageFor(m => m.PersonEmploymentDetailViewModel.NameOfEmployer)
                                        </div>

                                        @*TransNameOfEmployer*@
                                        <div class="form-group">
                                            @Html.CustomLabel(personDetailViewModel.NameOfEmployerInRegionalLanguage)
                                            @Html.TextBoxFor(m => m.PersonEmploymentDetailViewModel.TransNameOfEmployer, null, new { @id = "trans-employer-name", @class = "form-control deny-multiple-space mandatory-mark employer-input additional-details-input", @placeholder = personDetailViewModel.NameOfEmployerPlaceHolderInRegionalLanguage, @minlength = 2, @maxlength = 150, required = "required" })
                                            @Html.ValidationMessageFor(m => m.PersonEmploymentDetailViewModel.TransNameOfEmployer)
                                        </div>

                                        @*DateOfIncorporation*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Date Of Incorporation")
                                            @Html.TextBoxFor(m => m.PersonEmploymentDetailViewModel.DateOfIncorporation, "{0:yyyy-MM-dd}", new { @id = "date-of-incorporation", @class = "form-control mandatory-mark deny-future-date allow-past-date-upto-100y employer-input additional-details-input", @type = "date", required = "required" })
