                                                                            <div class="modal-dialog modal-dialog-centered modal-dialog-zoom modal-lg" role="document">
                                                                                <div class="modal-content">
                                                                                    <div class="modal-header">
                                                                                        <h4 class="modal-title  text-center">Add</h4>
                                                                                        <button type="button" class="close-guarantor-detail-modal" data-dismiss="modal" aria-label="Close">
                                                                                            <span aria-hidden="true">&times;</span>
                                                                                        </button>
                                                                                    </div>

                                                                                    <div class="modal-body">
                                                                                        <div id="guarantor-detail-change-info" class="ml-5 text-primary d-none">** If You Change Or Delete The Guarantor Detail Table Content Is Removed.</div>

                                                                                        @*GuarantorName*@
                                                                                        <div id="guarantor-person-input" class="form-group">
                                                                                            @Html.CustomLabel("Gurantor Name")
                                                                                            @Html.TextBoxFor(m => m.CustomerLoanAccountGuarantorDetailViewModel.PersonId, new { @type = "search", @id = "guarantor-name-id", @class = "form-control mandatory-mark deny-multiple-space text-capitalize modal-input", @placeholder = "Select Guarantor Name", required = "required" })
                                                                                            <span id="guarantor-name-id-error" class="error modal-input-error d-none">Please Select Valid Guarantor Name.</span>
                                                                                        </div>

                                                                                        @*SequenceNumber*@
                                                                                        <div class="form-group">
                                                                                            @Html.CustomLabel("Sequence Number")
                                                                                            @Html.TextBoxFor(m => m.CustomerLoanAccountGuarantorDetailViewModel.SequenceNumber, new { @id = "sequence-number-guarantor-detail", @class = "form-control mandatory-mark real-number modal-input", @placeholder = "Enter Sequence Number", required = "required", @type = "number", @min = 1, @max = 199 })
                                                                                            @Html.ValidationMessageFor(m => m.CustomerLoanAccountGuarantorDetailViewModel.SequenceNumber)
                                                                                            <span id="sequence-number-guarantor-detail-error" class="error modal-input-error d-none"> Please Enter Sequence Number.</span>
                                                                                        </div>

                                                                                        @*GuaranteePercentage*@
                                                                                        <div id="guarantee-percentage-input" class="form-group">
                                                                                            @Html.CustomLabel("Guarantee Percentage")
                                                                                            @Html.TextBoxFor(m => m.CustomerLoanAccountGuarantorDetailViewModel.GuaranteePercentage, new { @id = "guarantee-percentage", @class = "form-control mandatory-mark modal-input", @placeholder = "Enter Guarantee Percentage", required = "required", @type = "number", @min = 0, @max = 100 })
                                                                                            @Html.ValidationMessageFor(m => m.CustomerLoanAccountGuarantorDetailViewModel.GuaranteePercentage)
                                                                                            <span id="guarantee-percentage-error" class="error modal-input-error d-none"> Please Enter Guarantee Percentage.</span>
                                                                                        </div>

                                                                                        @*Note*@
                                                                                        <div class="form-group">
                                                                                            @Html.CustomLabel("Note")
                                                                                            @Html.TextAreaFor(m => m.CustomerLoanAccountGuarantorDetailViewModel.Note, new { @id = "note-account-guarantor-detail", @class = "form-control modal-input", @placeholder = "Enter Note", @maxlength = "1500", onkeyup = "ToTextCapitalize()" })
                                                                                            @Html.ValidationMessageFor(m => m.CustomerLoanAccountGuarantorDetailViewModel.Note)
                                                                                        </div>
                                                                                    </div>

                                                                                    <div class="modal-footer">
                                                                                        <button type="button" id="btn-add-guarantor-detail-modal" class="btn btn-success">Add</button>
                                                                                        <button type="button" id="btn-update-guarantor-detail-modal" class="btn btn-primary">Update</button>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*Currency Data Table*@
