                                                                        @Html.ValidationMessageFor(m => m.OrganizationGSTRegistrationDetailViewModel.StateId)
                                                                        <span id="state-id-error" class="error modal-input-error d-none">Please Select State.</span>
                                                                    </div>

                                                                    @*GST Registration Type*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("GST Registration Type")
                                                                        @Html.DropDownListFor(m => m.OrganizationGSTRegistrationDetailViewModel.GSTRegistrationTypeId, new SelectList(organizationDetailViewModel.GSTRegistrationTypeDropdownList, "Value", "Text", 0), "--- Select GST Registration Type ---", new { @id = "gst-registration-type-id", @class = "form-control mandatory-mark modal-input", required = "required" })
                                                                        @Html.ValidationMessageFor(m => m.OrganizationGSTRegistrationDetailViewModel.GSTRegistrationTypeId)
