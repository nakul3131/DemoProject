                                                                    </div>

                                                                    @*Applicable From*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("Applicable From")
                                                                        @Html.TextBoxFor(m => m.OrganizationGSTRegistrationDetailViewModel.ApplicableFrom, "{0:yyyy-MM-dd}", new { @id = "applicable-from", @class = "form-control mandatory-mark modal-input", @placeholder = "Enter Applicable From", @type = "date", @min = "2017-07-01", required = "required" })
                                                                        @Html.ValidationMessageFor(m => m.OrganizationGSTRegistrationDetailViewModel.ApplicableFrom)
                                                                        <span id="applicable-from-error" class="error modal-input-error d-none">Please Enter Applicable From.</span>
                                                                    </div>

                                                                    @*GST Return Periodicity*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("GST Return Periodicity")
                                                                        @Html.DropDownListFor(m => m.OrganizationGSTRegistrationDetailViewModel.GSTReturnPeriodicityId, new SelectList(organizationDetailViewModel.GSTReturnPeriodicityDropdownList, "Value", "Text", 0), "--- Select GST Return Periodicity ---", new { @id = "gst-return-periodicity-id", @class = "form-control mandatory-mark modal-input", required = "required" })
                                                                        @Html.ValidationMessageFor(m => m.OrganizationGSTRegistrationDetailViewModel.GSTReturnPeriodicityId)
                                                                        <span id="gst-return-periodicity-id-error" class="error modal-input-error d-none">Please Select Gst Return Periodicity.</span>
                                                                    </div>

                                                                    @*Is Applicable EWay Bill*@
