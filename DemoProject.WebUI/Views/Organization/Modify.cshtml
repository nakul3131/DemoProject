                                                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                    </span>
                                                                                </label>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    @*Threshold Limit*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("Threshold Limit")
                                                                        @Html.TextBoxFor(m => m.OrganizationGSTRegistrationDetailViewModel.ThresholdLimit, new { @id = "threshold-limit", @class = "form-control mandatory-mark modal-input", @placeholder = "Enter Threshold Limit", @type = "number", required = "required", @min = 50000, @max = 9999999 })
                                                                        @Html.ValidationMessageFor(m => m.OrganizationGSTRegistrationDetailViewModel.ThresholdLimit)
                                                                        <span id="threshold-limit-error" class="error modal-input-error d-none">Only 20 lakh or 40 lakh.</span>
                                                                    </div>

                                                                    @*GSTRegistration Number*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("GST Registration Number")
                                                                        @Html.TextBoxFor(m => m.OrganizationGSTRegistrationDetailViewModel.GSTRegistrationNumber, new { @id = "gst-registration-number", @class = "form-control modal-input", @placeholder = "Enter GST Registration Number", @minlength = 15, @maxlength = 15 })
                                                                        @Html.ValidationMessageFor(m => m.OrganizationGSTRegistrationDetailViewModel.GSTRegistrationNumber)
                                                                        <span id="gst-registration-number-error" class="error modal-input-error d-none">Please Enter gst-registration Number, Which is Less Than 15.</span>
                                                                    </div>

                                                                    @*Registration Date*@
                                                                    <div class="form-group">
                                                                        @Html.CustomLabel("Registration Date")
                                                                        @Html.TextBoxFor(m => m.OrganizationGSTRegistrationDetailViewModel.RegistrationDate, "{0:yyyy-MM-dd}", new { @id = "activation-date-registration-date", @class = "form-control mandatory-mark activation-date deny-future-date modal-input", @placeholder = "Enter Applicable From", @type = "date" })
                                                                        @Html.ValidationMessageFor(m => m.OrganizationGSTRegistrationDetailViewModel.RegistrationDate)
                                                                        <span id="activation-date-registration-date-error" class="error modal-input-error d-none">Please Enter Registration Date.</span>
                                                                    </div>

                                                                    @*Close Date*@
                                                                    <div class="form-group">
