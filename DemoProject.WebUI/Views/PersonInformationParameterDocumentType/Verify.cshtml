@model DemoProject.Services.ViewModel.Account.Layout.DepositSchemeViewModel
@using DemoProject.Services.HtmlHelpers
@using DemoProject.Services.ViewModel.Account.Layout
@using DemoProject.Services.ViewModel.Configuration

@{
    DepositSchemeViewModel model = new DepositSchemeViewModel();
    SchemeApplicationParameterViewModel schemeApplicationParameterViewModel = new SchemeApplicationParameterViewModel();
    SchemeCustomerAccountNumberViewModel schemeCustomerAccountNumberViewModel = new SchemeCustomerAccountNumberViewModel();
    SchemeDepositInstallmentParameterViewModel schemeDepositInstallmentParameterViewModel = new SchemeDepositInstallmentParameterViewModel();
    SchemeDepositAgentParameterViewModel schemeDepositAgentParameterViewModel = new SchemeDepositAgentParameterViewModel();
    SchemeDepositInterestPayoutFrequencyViewModel schemeDepositInterestPayoutFrequencyViewModel = new SchemeDepositInterestPayoutFrequencyViewModel();
    SchemeDepositInterestParameterViewModel schemeDepositInterestParameterViewModel = new SchemeDepositInterestParameterViewModel();
    SchemePaymentCardFeatureViewModel schemePaymentCardFeatureViewModel = new SchemePaymentCardFeatureViewModel();
    SchemeNumberOfTransactionLimitViewModel schemeNumberOfTransactionLimitViewModel = new SchemeNumberOfTransactionLimitViewModel();
    SchemeTransactionAmountLimitViewModel schemeTransactionAmountLimitViewModel = new SchemeTransactionAmountLimitViewModel();
    SchemeDemandDepositDetailViewModel schemeDemandDepositDetailViewModel = new SchemeDemandDepositDetailViewModel();
    SchemeDepositClosingModeViewModel schemeDepositClosingModeViewModel = new SchemeDepositClosingModeViewModel();
    SchemeFixedDepositParameterViewModel schemeFixedDepositParameterViewModel = new SchemeFixedDepositParameterViewModel();
    SchemeDepositPledgeLoanParameterViewModel schemeDepositPledgeLoanParameterViewModel = new SchemeDepositPledgeLoanParameterViewModel();
    SchemeAccountBankingChannelParameterViewModel schemeAccountBankingChannelParameterViewModel = new SchemeAccountBankingChannelParameterViewModel();
    SchemeLimitViewModel schemeLimitViewModel = new SchemeLimitViewModel();
    SchemeEstimateTargetViewModel schemeEstimateTargetViewModel = new SchemeEstimateTargetViewModel();
    SchemeReportFormatViewModel schemeReportFormatViewModel = new SchemeReportFormatViewModel();
    SchemeNoticeScheduleViewModel schemeNoticeScheduleViewModel = new SchemeNoticeScheduleViewModel();
    SchemeOtherFundSubscriptionViewModel schemeOtherFundSubscriptionViewModel = new SchemeOtherFundSubscriptionViewModel();
    SchemeDepositBalanceParameterViewModel schemeDepositBalanceParameterViewModel = new SchemeDepositBalanceParameterViewModel();
    SchemeChargesDetailViewModel schemeChargesDetailViewModel = new SchemeChargesDetailViewModel();
    SchemeTenureViewModel schemeTenureViewModel = new SchemeTenureViewModel();
    SchemeBusinessOfficeViewModel schemeBusinessOfficeViewModel = new SchemeBusinessOfficeViewModel();
    HeaderPartialModel headerPartialModel = new HeaderPartialModel("DSSVRF");
}

<!doctype html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>DepositScheme - Verify</title>

    <style>
        span .field-validation-error {
            margin-top: 20% !important
        }
    </style>
</head>
<body>
    <div class="container-fluid">
        @*======= First Row - For Header =======*@
        <div class="row">
            <div class="offset-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @*======= Second Row - Data Entry Page =======*@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card d-block mb-3">
                    @using (Html.BeginForm(null, null, FormMethod.Post, new { @id = "form" }))
                    {
                        @Html.AntiForgeryToken()
                        {
                            <div class="card-body">
                                @*======= First Row - Mandatory Mark =======*@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <h6 class="mb-2 text-right text-muted"><b class="mandatory-mark">&nbsp; Mandatory Field</b></h6>
                                    </div>
                                </div>

                                @*======= Second Row - Page Title, Translation =======*@
                                <div class="row mt-2">
                                    @*Page Title*@
                                    <div class="col-12">
                                        <h5 class="font-weight-bold mt-1">Verify</h5>
                                        <hr />
                                    </div>
                                </div>

                                @*Submit Error*@
                                @Html.ValidationMessage("SubmitError")

                                @*======= Third Row - Data Entry Page =======*@
                                <div class="row">
                                    <div class="col-12">
                                        <div class="read-only">
                                            @*MakerRemark*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Maker Remark")
                                                @Html.TextBoxFor(m => m.MakerRemark, new { @id = "name-of-center", @class = "form-control mandatory-mark " })
                                                @Html.ValidationMessageFor(m => m.MakerRemark)
                                            </div>

                                            @*NameOfScheme*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Name Of Scheme")
                                                @Html.TextBoxFor(m => m.NameOfScheme, new { @id = "name-of-scheme", @class = "form-control mandatory-mark", @placeholder = "Enter Name Of Scheme", required = "required", @maxlength = "100", @autocomplete = "off", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                                @Html.ValidationMessageFor(m => m.NameOfScheme)
                                            </div>

                                            @*TransNameOfScheme*@
                                            <div class="form-group">
                                                @Html.CustomLabel(model.NameOfSchemeInRegionalLanguage)
                                                @Html.TextBoxFor(m => m.TransNameOfScheme, new { @id = "trans-name-of-scheme", @class = "form-control mandatory-mark", @placeholder = model.NameOfSchemePlaceHolderInRegionalLanguage, required = "required", @maxlength = "100", @autocomplete = "off" })
                                                @Html.ValidationMessageFor(m => m.TransNameOfScheme)
                                            </div>

                                            @*Alias Name*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Alias Name")
                                                @Html.TextBoxFor(m => m.AliasName, new { @id = "alias-name", @class = "form-control", @placeholder = "Enter Alias Name", required = "required", @maxlength = "10", @autocomplete = "off", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                                @Html.ValidationMessageFor(m => m.AliasName)
                                            </div>

                                            @*TransAliasName*@
                                            <div class="form-group">
                                                @Html.CustomLabel(model.AliasNameInRegionalLanguage)
                                                @Html.TextBoxFor(m => m.TransAliasName, new { @id = "trans-alias-name", @class = "form-control", @placeholder = model.AliasNamePlaceHolderInRegionalLanguage, required = "required", @maxlength = "10", @autocomplete = "off" })
                                                @Html.ValidationMessageFor(m => m.TransAliasName)
                                            </div>

                                            @*NameOnReport*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Name On Report")
                                                @Html.TextBoxFor(m => m.NameOnReport, new { @id = "name-on-report", @class = "form-control", @placeholder = "Enter Name On Report", required = "required", @maxlength = "100", @autocomplete = "off", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                                @Html.ValidationMessageFor(m => m.NameOnReport)
                                            </div>

                                            @*TransNameOnReport*@
                                            <div class="form-group">
                                                @Html.CustomLabel(model.NameOnReportInRegionalLanguage)
                                                @Html.TextBoxFor(m => m.TransNameOnReport, new { @id = "trans-name-on-report", @class = "form-control", @placeholder = model.NameOnReportPlaceHolderInRegionalLanguage, required = "required", @maxlength = "100", @autocomplete = "off" })
                                                @Html.ValidationMessageFor(m => m.TransNameOnReport)
                                            </div>

                                            @*ActivationDate*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Activation Date")
                                                @Html.TextBoxFor(m => m.ActivationDate, "{0:yyyy-MM-dd}", new { @id = "activation-date-scheme", @class = "form-control mandatory-mark deny-past-date", @placeholder = "Enter Activation Date", required = "required", @autocomplete = "off", onclick = "DenyPastDate()" })
                                                @Html.ValidationMessageFor(m => m.ActivationDate)
                                            </div>

                                            @*ExpiryDate*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Expiry Date")
                                                @Html.TextBoxFor(m => m.ExpiryDate, "{0:yyyy-MM-dd}", new { @id = "expiry-date-scheme", @class = "form-control mandatory-mark deny-past-date-from-reference-date", @placeholder = "Enter Activation Date", @autocomplete = "off", onclick = "DenyPastDateFromReferenceDate('activation-date-scheme')" })
                                                @Html.ValidationMessageFor(m => m.ExpiryDate)
                                            </div>

                                            @*Note*@
                                            <div class="form-group">
                                                @Html.CustomLabel("Note")
                                                @Html.TextAreaFor(m => m.Note, new { @id = "note", @class = "form-control", @placeholder = "Enter Note", @maxlength = "1500", @autocomplete = "off", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                                @Html.ValidationMessageFor(m => m.Note)
                                            </div>

                                            @*TransNote*@
                                            <div class="form-group">
                                                @Html.CustomLabel(model.NoteInRegionalLanguage)
                                                @Html.TextAreaFor(m => m.TransNote, new { @id = "trans-note", @class = "form-control", @placeholder = model.NotePlaceHolderInRegionalLanguage, required = "required", @maxlength = "1500", @autocomplete = "off" })
                                                @Html.ValidationMessageFor(m => m.TransNote)
                                            </div>

                                            @*DepositType*@
                                            <div class="form-group DepositType">
                                                @Html.CustomLabel("Deposit Type")
                                                <div class="row mt-2">
                                                    <div class="col-12 col-md-6 col-xl-3">
                                                        <div class="form-check form-check-inline font-weight-bold">
                                                            @Html.RadioButtonFor(m => m.SchemeDepositAccountParameterViewModel.DepositType, "DMN", new { @name = "SchemeDepositAccountParameterViewModel.DepositType", @class = "deposit-type" })
                                                            <label class="form-check-label pl-2">Demand Deposit</label>
                                                        </div>
                                                    </div>
                                                    <div class="col-12 col-md-6 col-xl-3">
                                                        <div class="form-check form-check-inline font-weight-bold">
                                                            @Html.RadioButtonFor(m => m.SchemeDepositAccountParameterViewModel.DepositType, "FDP", new { @name = "SchemeDepositAccountParameterViewModel.DepositType", @class = "deposit-type" })
                                                            <label class="form-check-label pl-2">Fixed Deposit</label>
                                                        </div>
                                                    </div>
                                                    <div class="col-12 col-md-6 col-xl-3">
                                                        <div class="form-check form-check-inline font-weight-bold">
                                                            @Html.RadioButtonFor(m => m.SchemeDepositAccountParameterViewModel.DepositType, "REC", new { @name = "SchemeDepositAccountParameterViewModel.DepositType", @class = "deposit-type" })
                                                            <label class="form-check-label pl-2 font-weight-bold">Recurring Deposit</label>
                                                        </div>
                                                    </div>
                                                    <div class="col-12 col-md-6 col-xl-3">
                                                        <div class="form-check form-check-inline font-weight-bold">
                                                            @Html.RadioButtonFor(m => m.SchemeDepositAccountParameterViewModel.DepositType, "PPF", new { @name = "SchemeDepositAccountParameterViewModel.DepositType", @class = "deposit-type" })
                                                            <label class="form-check-label pl-2">Public Provident Fund</label>
                                                        </div>
                                                    </div>
                                                    @Html.ValidationMessageFor(m => m.SchemeDepositAccountParameterViewModel.DepositType)
                                                </div>
                                            </div>

                                            @*EnableTenure*@
                                            <div class="row form-group mt-4 tenure demand-deposit">
                                                <div class="col-10">
                                                    @Html.CustomLabel("Enable Tenure")
                                                </div>

                                                <div class="col-2">
                                                    <div class="text-left">
                                                        <label class="switch switch-left-right">
                                                            @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableTenure, new { @id = "enable-tenure", @class = "form-control switch-input" })
                                                            <span class="slider round">
                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                            </span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="tenure-accordian">
                                                @*MinimumTenure*@
                                                <div class="form-group">
                                                    @Html.CustomLabel("Minimum Tenure")
                                                    @Html.TextBoxFor(m => m.SchemeTenureViewModel.MinimumTenure, new { @id = "minimum-tenure", @class = "form-control mandatory-mark", @placeholder = "Enter Minimum Tenure", required = "required"})
                                                    @Html.ValidationMessageFor(m => m.SchemeTenureViewModel.MinimumTenure)
                                                </div>

                                                @*MultiplesOf*@
                                                <div class="form-group">
                                                    @Html.CustomLabel("Multiples Of")
                                                    @Html.TextBoxFor(m => m.SchemeTenureViewModel.MultiplesOf, new { @id = "multiples-of", @class = "form-control mandatory-mark", @placeholder = "Enter Multiples Of", required = "required"})
                                                    @Html.ValidationMessageFor(m => m.SchemeTenureViewModel.MultiplesOf)
                                                </div>

                                                @*MaximumTenure*@
                                                <div class="form-group">
                                                    @Html.CustomLabel("Maximum Tenure")
                                                    @Html.TextBoxFor(m => m.SchemeTenureViewModel.MaximumTenure, new { @id = "maximum-tenure", @class = "form-control mandatory-mark", @placeholder = "Enter Maximum Tenure", @max = 29999, required = "required"})
                                                    @Html.ValidationMessageFor(m => m.SchemeTenureViewModel.MaximumTenure)
                                                </div>

                                                @*DefaultTenure*@
                                                <div class="form-group">
                                                    @Html.CustomLabel("Default Tenure")
                                                    @Html.TextBoxFor(m => m.SchemeTenureViewModel.DefaultTenure, new { @id = "default-tenure", @class = "form-control mandatory-mark", @placeholder = "Enter Default Tenure", required = "required"})
                                                    @Html.ValidationMessageFor(m => m.SchemeTenureViewModel.DefaultTenure)
                                                </div>

                                                @*TimePeriodUnitId*@
                                                <div class="form-group">
                                                    @Html.CustomLabel("Time Period Unit")
                                                    @Html.DropDownListFor(m => m.SchemeTenureViewModel.TimePeriodUnitId, new SelectList(schemeTenureViewModel.TimePeriodUnitDropdownList, "Value", "Text"), "-- Select Time Period Unit --", new { @id = "time-period-unit-id", @class = "form-control mandatory-mark", required = "required" })
                                                    @Html.ValidationMessageFor(m => m.SchemeTenureViewModel.TimePeriodUnitId)
                                                </div>

                                                @*Note*@
                                                <div class="form-group">
                                                    @Html.CustomLabel("Note")
                                                    @Html.TextAreaFor(m => m.SchemeTenureViewModel.Note, new { @id = "note-tenure", @class = "form-control", @placeholder = "Enter Note", @maxlength = "1500", @autocomplete = "off", onkeyup = "ToTextCapitalize()" })
                                                    @Html.ValidationMessageFor(m => m.Note)
                                                </div>

                                            </div>
                                        </div>
                                        @*======= Fourth Row - Accordians =======*@
                                        <div class="row mt-1">
                                            <div class="col-12">
                                                <div id="accordion" class="accordion md-accordion accordion-blocks ">

                                                    @*Accordian 1 - Scheme Account Parameter*@
                                                    <div id="heading-account-parameter" class="card mt-3">
                                                        <a class="card-header p-3" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-account-parameter" href="#collapse-account-parameter">
                                                            <b>Account Parameter</b>
                                                        </a>
                                                        <div id="collapse-account-parameter" class="collapse" aria-labelledby="heading-account-parameter" data-parent="#accordion">
                                                            <div class="card-body">
                                                                <div class="row">
                                                                    <div class="col-12 read-only">
                                                                        @*EnableAlternateAccountNumber2*@
                                                                        <div class="row form-group mt-4">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Alternate Account Number2")
                                                                            </div>

                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableAlternateAccountNumber2, new { @id = "enable-alternate-account-number2", @class = "form-control switch-input" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*EnableAlternateAccountNumber3*@
                                                                        <div class="row form-group mt-4">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Alternate Account Number3")
                                                                            </div>

                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableAlternateAccountNumber3, new { @id = "enable-alternate-account-number3", @class = "form-control switch-input" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*EnableMaturityDate*@
                                                                        <div class="row form-group mt-4 maturity-date demand-deposit">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Maturity Date")
                                                                            </div>

                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableMaturityDate, new { @id = "enable-maturity-date", @class = "form-control switch-input" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        <div class="maturity-date-validation">

                                                                            @*EnableMaturityOnWorkingDay*@
                                                                            <div class="row form-group mt-4">
                                                                                <div class="col-10">
                                                                                    @Html.CustomLabel("Enable Maturity On Working Day")
                                                                                </div>

                                                                                <div class="col-2">
                                                                                    <div class="text-left">
                                                                                        <label class="switch switch-left-right">
                                                                                            @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableMaturityOnWorkingDay, new { @id = "enable-maturity-on-working-day", @class = "form-control switch-input" })
                                                                                            <span class="slider round">
                                                                                                <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                            </span>
                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </div>

                                                                            @*EnableMaturityDateOverride*@
                                                                            <div class="row form-group mt-4">
                                                                                <div class="col-10">
                                                                                    @Html.CustomLabel("Enable Maturity Date Override")
                                                                                </div>

                                                                                <div class="col-2">
                                                                                    <div class="text-left"></div>
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableMaturityDateOverride, new { @id = "enable-maturity-date-override", @class = "form-control switch-input" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>

                                                                            <div class="maturity-date-override-validation">
                                                                                @*MinimumOverridePeriod*@
                                                                                <div class="form-group mb-5">
                                                                                    @Html.CustomLabel("Minimum Override Period")
                                                                                    @Html.TextBoxFor(m => m.SchemeAccountParameterViewModel.MinimumOverridePeriod, new { @id = "minimum-override-period", @class = "form-control mb-5 mandatory-mark", rel = "txt-tooltip", data_toggle = "tooltip", data_placement = "auto", title = "Time Period In Days.", @placeholder = "Enter Minimum Override Period", @min = "0", @max = "90", @autocomplete = "off" })
                                                                                    @Html.ValidationMessageFor(m => m.SchemeAccountParameterViewModel.MinimumOverridePeriod)
                                                                                </div>

                                                                                @*MaximumOverridePeriod*@
                                                                                <div class="form-group">
                                                                                    @Html.CustomLabel("Maximum Override Period")
                                                                                    @Html.TextBoxFor(m => m.SchemeAccountParameterViewModel.MaximumOverridePeriod, new { @id = "maximum-override-period", @class = "form-control mandatory-mark", rel = "txt-tooltip", data_toggle = "tooltip", data_placement = "auto", title = "Time Period In Days.", @placeholder = "Enter Maximum Override Period", @max = "90", @autocomplete = "off" })
                                                                                    @Html.ValidationMessageFor(m => m.SchemeAccountParameterViewModel.MaximumOverridePeriod)
                                                                                </div>
                                                                            </div>

                                                                        </div>

                                                                        @*EnableNumberOfJointAccountHoldingLimit*@
                                                                        <div class="row form-group mt-4">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Number Of Joint Account Holding Limit")
                                                                            </div>

                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableNumberOfJointAccountHoldingLimit, new { @id = "enable-number-of-joint-account-holding-limit", @class = "form-control switch-input" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*Joint Account Details*@
                                                                        <div class="joint-account">
                                                                            @*MinimumJointAccountHolder*@
                                                                            <div class="form-group minimumJointAccountHolder">
                                                                                @Html.CustomLabel("Minimum Joint Account Holder")
                                                                                @Html.TextBoxFor(m => m.SchemeAccountParameterViewModel.MinimumJointAccountHolder, new { @id = "minimum-joint-account-holder", @class = "form-control mandatory-mark", @placeholder = "Enter Minimum Joint Account Holder", @min = 0, @max = 90 })
                                                                                @Html.ValidationMessageFor(m => m.SchemeAccountParameterViewModel.MinimumJointAccountHolder)
                                                                            </div>

                                                                            @*MaximumJointAccountHolder*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Maximum Joint Account Holder")
                                                                                @Html.TextBoxFor(m => m.SchemeAccountParameterViewModel.MaximumJointAccountHolder, new { @id = "maximum-joint-account-holder", @class = "form-control mandatory-mark", @placeholder = "Enter Maximum Joint Account Holder", @max = 90 })
                                                                                @Html.ValidationMessageFor(m => m.SchemeAccountParameterViewModel.MaximumJointAccountHolder)
                                                                            </div>

                                                                            @*DefaultJointAccountHolder*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Default Joint Account Holder")
                                                                                @Html.TextBoxFor(m => m.SchemeAccountParameterViewModel.DefaultJointAccountHolder, new { @id = "default-joint-account-holder", @class = "form-control mandatory-mark", @placeholder = "Enter Default Joint Account Holder"})
                                                                                @Html.ValidationMessageFor(m => m.SchemeAccountParameterViewModel.DefaultJointAccountHolder)
                                                                            </div>
                                                                        </div>

                                                                        @*EnableNumberOfNomineeLimit*@
                                                                        <div class="row form-group mt-4">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Number Of Nominee Limit")
                                                                            </div>
                                                                            <div class="col-2">
                                                                                <div class="text-left">
                                                                                    <label class="switch switch-left-right">
                                                                                        @Html.CheckBoxFor(m => m.SchemeAccountParameterViewModel.EnableNumberOfNomineeLimit, new { @id = "enable-number-of-nominee-limit", @class = "form-control switch-input" })
                                                                                        <span class="slider round">
                                                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                                                        </span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*Nominee Details*@
                                                                        <div class="nominee">
                                                                            @*MinimumNominee*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Minimum Nominee")
                                                                                @Html.TextBoxFor(m => m.SchemeAccountParameterViewModel.MinimumNominee, new { @id = "minimum-nominee", @class = "form-control mandatory-mark", @placeholder = "Enter Minimum Nominee", @min = 0, @max = 90 })
                                                                                @Html.ValidationMessageFor(m => m.SchemeAccountParameterViewModel.MinimumNominee)
                                                                            </div>

                                                                            @*MaximumNominee*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Maximum Nominee")
                                                                                @Html.TextBoxFor(m => m.SchemeAccountParameterViewModel.MaximumNominee, new { @id = "maximum-nominee", @class = "form-control mandatory-mark", @placeholder = "Enter Maximum Nominee", @max = 90 })
                                                                                @Html.ValidationMessageFor(m => m.SchemeAccountParameterViewModel.MaximumNominee)
                                                                            </div>

                                                                            @*DefaultNominee*@
                                                                            <div class="form-group">
                                                                                @Html.CustomLabel("Default Nominee")
                                                                                @Html.TextBoxFor(m => m.SchemeAccountParameterViewModel.DefaultNominee, new { @id = "default-nominee", @class = "form-control mandatory-mark", @placeholder = "Enter Default Nominee"})
                                                                                @Html.ValidationMessageFor(m => m.SchemeAccountParameterViewModel.DefaultNominee)
                                                                            </div>
                                                                        </div>

                                                                        @*EnableNomineeSharesHoldingPercentage*@
                                                                        <div class="row form-group mt-4 enable-nominee-shares-holding">
                                                                            <div class="col-10">
                                                                                @Html.CustomLabel("Enable Nominee Shares Holding Percentage")
                                                                            </div>

                                                                            <div class="col-2">
                                                                                <div class="text-left">
