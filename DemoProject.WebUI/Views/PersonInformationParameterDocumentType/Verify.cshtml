                                                                                <thead class="text-center">
                                                                                    <tr>
                                                                                        <th><input type="checkbox" name="check_all" id="selectAll-deposit-agent-incentive" /></th>
                                                                                        <th>MinimumCollectionAmount</th>
                                                                                        <th>MaximumCollectionAmount</th>
                                                                                        <th>IncentiveUnit</th>
                                                                                        <th>Incentive</th>
                                                                                        <th>RoundingMethod</th>
                                                                                        <th>Note</th>
                                                                                    </tr>
                                                                                </thead>
                                                                                <tbody class="text-center read-only">
                                                                                    @{
                                                                                        IEnumerable<SchemeDepositAgentIncentiveViewModel> SchemeDepositAgentIncentiveviewModelList = (IEnumerable<SchemeDepositAgentIncentiveViewModel>)HttpContext.Current.Session["SchemeDepositAgentIncentive"];

                                                                                        foreach (SchemeDepositAgentIncentiveViewModel viewModel in SchemeDepositAgentIncentiveviewModelList)
                                                                                        {
                                                                                    <tr class="hoverlightblue">
                                                                                        <th><input type="checkbox" name="check_all" id="select-all-scheme" /></th>
                                                                                        <td>@viewModel.MinimumCollectionAmount</td>
                                                                                        <td>@viewModel.MaximumCollectionAmount</td>
                                                                                        @if (viewModel.IncentiveUnit == "FLT")
                                                                                        {
                                                                                            <td>Flat Amount--->FLT</td>
                                                                                        }
                                                                                        else if (viewModel.IncentiveUnit == "PER")
                                                                                        {
                                                                                            <td>Percentage--->PER</td>
                                                                                        }

                                                                                        <td>@viewModel.Incentive</td>
                                                                                        @if (viewModel.RoundingMethod == "TRN")
                                                                                        {
                                                                                        <td>Truncate--->TRN</td>
                                                                                        }
                                                                                        else if (viewModel.RoundingMethod == "RUP")
                                                                                        {
                                                                                        <td>Round Up--->RUP</td>
                                                                                        }
                                                                                        else if (viewModel.RoundingMethod == "RDN")
                                                                                        {
                                                                                        <td>Round Down--->RDN</td>
                                                                                        }
                                                                                        else if (viewModel.RoundingMethod == "NON")
                                                                                        {
                                                                                        <td>Round Near--->NON</td>
                                                                                        }

                                                                                        <td>@viewModel.Note</td>
                                                                                    </tr>
                                                                                        }
                                                                                    }
                                                                                </tbody>
                                                                            </table>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
