@model DemoProject.Services.ViewModel.Security.Users.UserProfileViewModel
@using DemoProject.Services.ViewModel.Configuration
@using DemoProject.Services.ViewModel.Security.Users
@using DemoProject.Services.HtmlHelpers

@{
    UserProfileDetailViewModel userProfileDetailViewModel = new UserProfileDetailViewModel();
    HeaderPartialModel headerPartialModel = new HeaderPartialModel("UPLMOD");
}

<!doctype html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0" />

    <title>User Profile - Modify</title>
</head>
<body>
    <div class="container-fluid">
        @*======= First Row - For Header =======*@
        <div class="row">
            <div class="offset-lg-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @*======= Second Row - Data Entry Page =======*@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card mb-3">
                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()
                        {
                            <div class="card-body">
                                @*======= First  Row - Mandatory Mark =======*@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <h6 class="mb-2 text-right text-muted"><b class="mandatory-mark"> &nbsp; Mandatory Field</b></h6>
                                    </div>
                                </div>

                                @*======= Second Row - Page Title =======*@
                                <div class="row mt-2">
                                    @*Page Title If Screen Large Then Column - 12 Otherwise Col - 6 For Create And Remaining For Translation*@
                                    <div class="col-12">
                                        <h5 class="font-weight-bold mt-1">Modify</h5>
                                        <hr />
                                    </div>
                                </div>

                                @*Submit Error*@
                                @Html.ValidationMessage("SubmitError")

                                @*======= Third Row - Data Entry Page =======*@
                                <div class="row">
                                    <div class="col-12">
                                        @*Person Name*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Person")
                                            @Html.DropDownListFor(m => m.PersonId, new SelectList(userProfileDetailViewModel.PersonDropdownList, "Value", "Text", 0), "Select Person", new { @id = "person-id", @class = "form-control mandatory-mark", @autocomplete = "off", required = "required" })
                                            @Html.ValidationMessageFor(m => m.PersonId)
                                        </div>

                                        @*NameOfUserProfile*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Name Of User Profile")
                                            @Html.TextBoxFor(m => m.NameOfUserProfile, new { @id = "name-of-user-profile", @class = "form-control mandatory-mark text-capitalize", @maxlength = "50", @autocomplete = "off", @placeholder = "Enter Name Of User Profile", required = "required", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.NameOfUserProfile)
                                        </div>

                                        @*AliasName*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Alias Name")
                                            @Html.TextBoxFor(m => m.AliasName, new { @id = "alias-name", @class = "form-control text-capitalize", @maxlength = "10", @autocomplete = "off", @placeholder = "Enter Alias Name", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.AliasName)
                                        </div>

                                        @*NameOnReport*@
                                        <div class="form-group">
                                            @Html.CustomLabel(" Name On Report ")
                                            @Html.TextBoxFor(m => m.NameOnReport, new { @id = "name-on-report", @class = "form-control text-capitalize", @maxlength = "100", @autocomplete = "off", @placeholder = "Enter Name On Report", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.NameOnReport)
                                        </div>

                                        @*EmailId*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Email Id")
                                            @Html.TextBoxFor(m => m.EmailId, new { @id = "email-id", @class = "form-control text-capitalize", type = "email", @maxlength = "320", @autocomplete = "off", @placeholder = "Enter Email Id" })
                                            @Html.ValidationMessageFor(m => m.EmailId)
                                        </div>

                                        @*AlternateEmailId*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Alternate EmailId")
                                            @Html.TextBoxFor(m => m.AlternateEmailId, new { @id = "alternate-email-id", @class = "form-control text-capitalize", type = "email", @maxlength = "320", @autocomplete = "off", @placeholder = "Enter Email Id" })
                                            @Html.ValidationMessageFor(m => m.AlternateEmailId)
                                        </div>

                                        @*MobileNumber*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Mobile Number")
                                            @Html.TextBoxFor(m => m.MobileNumber, new { @id = "mobile-number", @class = "form-control text-capitalize", pattern = ".{10}", @maxlength = "320", @autocomplete = "off", @placeholder = "Enter Email Id", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.MobileNumber)
                                        </div>

                                        @*AlternateMobileNumber*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Alternate Mobile Number")
                                            @Html.TextBoxFor(m => m.AlternateMobileNumber, new { @id = "alternate-mobile-number", @class = "form-control text-capitalize", pattern = ".{10}", @maxlength = "320", @autocomplete = "off", @placeholder = "Enter Alternate Mobile Number", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.AlternateMobileNumber)
                                        </div>

                                        @*Activation Date*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Activation Date")
                                            @Html.TextBoxFor(m => m.ActivationDate, "{0:yyyy-MM-dd}", new { @class = "form-control mandatory-mark deny-past-date", @Type = "date", @id = "ActivationDate", @autocomplete = "off", @placeholder = "Enter Activation Date", onclick = "DenyPastDate()", required = "required" })
                                            @Html.ValidationMessageFor(m => m.ActivationDate)
                                        </div>

                                        @*Expiry Date*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Expiry Date")
                                            @Html.TextBoxFor(m => m.ExpiryDate, "{0:yyyy-MM-dd}", new { @class = "form-control deny-past-date-from-reference-date", @Type = "date", @id = "ExpiryDate", @autocomplete = "off", onclick = "DenyPastDateFromReferenceDate('ActivationDate')" })
                                            @Html.ValidationMessageFor(m => m.ExpiryDate)
                                        </div>

                                        @*CloseDate*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Close Date")
                                            @Html.TextBoxFor(m => m.CloseDate, "{0:yyyy-MM-dd}", new { @class = "form-control", @Type = "date", @id = "CloseDate", @autocomplete = "off" })
                                            @Html.ValidationMessageFor(m => m.CloseDate)
                                        </div>

                                        @*Note*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Note")
                                            @Html.TextAreaFor(m => m.Note, new { @id = "note", @class = "form-control text-capitalize", @autocomplete = "off", @maxlength = "1500", @placeholder = "Enter Note" })
                                            @Html.ValidationMessageFor(m => m.Note)
                                        </div>

                                        @*Role Profile*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Role Profile")
                                            <div class="mr-3">
                                                @Html.DropDownListFor(m => m.RoleProfileId, new SelectList(userProfileDetailViewModel.RoleProfileDropDownList, "Value", "Text"), "--- Select Role  Profile ---", new { @id = "role-profile-id", @class = "form-control mandatory-mark modal-input", required = "required" })
                                                @Html.ValidationMessageFor(m => m.RoleProfileId)
                                            </div>
                                        </div>

                                        @*IsShowUserProfileCurrency*@
                                        <div class="row form-group">
                                            <div class="col-8">
                                                @Html.CustomLabel("Is Show User Profile Currency")
                                            </div>
                                            <div class="col-4 text-right">
                                                <label class="switch switch-left-right">
                                                    @Html.CheckBoxFor(m => m.IsShowUserProfileCurrency, new { @class = "form-control switch-input", @id = "is-show-user-profile-currency", required = "required" })
                                                    <span class="slider round">
                                                        <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                    </span>
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-12">
                                        <div id="accordion" class="accordion md-accordion accordion-blocks">
                                            @*Accordian 1 - User Profile Home Business Office*@
                                            <div id="heading-home-business-office" class="card mt-3">
                                                <div class="card-header p-3" id="home-business-office-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-home-business-office" data-target="#collapse-home-business-office" href="#collapse-home-business-office">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <a class="card-title" href="#">
                                                                <b>Home Business Office</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <span id="home-business-office-accordion-error" class="error d-none"> Home Business Office Required Data, Please Add Valid Data.</span>
                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-home-business-office" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-home-business-office" data-target="#collapse-home-business-office">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div id="collapse-home-business-office" class="collapse" aria-labelledby="heading-home-business-office" data-parent="#accordion">
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                @*Home Branch Name*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Home Branch Name")
                                                                    @Html.DropDownListFor(m => m.UserProfileHomeBusinessOfficeViewModel.BusinessOfficeId, new SelectList(userProfileDetailViewModel.BusinessOfficeDropdownList, "Value", "Text"), "Select Business Office", new { @id = "home-branch", @class = "form-control mandatory-mark", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.UserProfileHomeBusinessOfficeViewModel.BusinessOfficeId)
                                                                </div>

                                                                @*Activation Date*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Activation Date")
                                                                    @Html.TextBoxFor(m => m.UserProfileHomeBusinessOfficeViewModel.ActivationDate, "{0:yyyy-MM-dd}", new { @class = "form-control mandatory-mark deny-past-date", @Type = "date", @id = "ActivationDate", @placeholder = "Enter Activation Date", onclick = "DenyPastDate()", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.UserProfileHomeBusinessOfficeViewModel.ActivationDate)
                                                                </div>

                                                                @*Expiry Date*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Expiry Date")
                                                                    @Html.TextBoxFor(m => m.UserProfileHomeBusinessOfficeViewModel.ExpiryDate, "{0:yyyy-MM-dd}", new { @class = "form-control deny-past-date-from-reference-date", @Type = "date", @id = "ExpiryDate", onclick = "DenyPastDateFromReferenceDate('ActivationDate')" })
                                                                    @Html.ValidationMessageFor(m => m.UserProfileHomeBusinessOfficeViewModel.ExpiryDate)
                                                                </div>

                                                                @*Note*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Note")
                                                                    @Html.TextAreaFor(m => m.UserProfileHomeBusinessOfficeViewModel.Note, new { @id = "note-user-profile-home-business-office", @class = "form-control text-capitalize deny-multiple-space", @maxlength = "1500", @placeholder = "Enter Note" })
                                                                    @Html.ValidationMessageFor(m => m.UserProfileHomeBusinessOfficeViewModel.Note)
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            @*Accordian 2 - User Profile Password Policy*@
                                            <div id="heading-password-policy" class="card mt-3">
                                                <div class="card-header p-3" id="password-policy-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-password-policy" data-target="#collapse-password-policy" href="#collapse-password-policy">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <a class="card-title" href="#">
                                                                <b>Password Policy</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <span id="password-policy-accordion-error" class="error d-none"> Password Policy Required Data, Please Add Valid Data.</span>
                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-password-policy" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-password-policy" data-target="#collapse-password-policy">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div id="collapse-password-policy" class="collapse" aria-labelledby="heading-password-policy" data-parent="#accordion">
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                @*Password Policy*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Password Policy")
                                                                    @Html.DropDownListFor(m => m.UserProfilePasswordPolicyViewModel.PasswordPolicyId, new SelectList(userProfileDetailViewModel.PasswordPolicyDropDownList, "Value", "Text"), "Select Password Policy", new { @id = "password-policy-id", @class = "form-control mandatory-mark", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.UserProfilePasswordPolicyViewModel.PasswordPolicyId)
                                                                </div>

                                                                @*ActivationDate*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Activation Date")
                                                                    @Html.TextBoxFor(m => m.UserProfilePasswordPolicyViewModel.ActivationDate, "{0:yyyy-MM-dd}", new { @class = "form-control mandatory-mark deny-past-date", @id = "activation-date-password-policy", @Type = "date", onclick = "DenyPastDate()", @placeholder = "Enter Activation Date", required = "required" })
                                                                    @Html.ValidationMessageFor(m => m.UserProfilePasswordPolicyViewModel.ActivationDate)
                                                                </div>

                                                                @*ExpiryDate*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Expiry Date")
                                                                    @Html.TextBoxFor(m => m.UserProfilePasswordPolicyViewModel.ExpiryDate, "{0:yyyy-MM-dd}", new { @class = "form-control deny-past-date-from-reference-date", @id = "expiry-date-password-policy", @Type = "date", onclick = "DenyPastDateFromReferenceDate('activation-date-password-policy')", @placeholder = "Enter Expiry Date" })
                                                                    @Html.ValidationMessageFor(m => m.UserProfilePasswordPolicyViewModel.ExpiryDate)
                                                                </div>

                                                                @*CloseDate*@
                                                                <div class="form-group d-none">
                                                                    @Html.CustomLabel("Close Date")
                                                                    @Html.TextBoxFor(m => m.UserProfilePasswordPolicyViewModel.CloseDate, "{0:yyyy-MM-dd}", new { @class = "form-control", @Type = "date", @id = "close-date-password-policy" })
                                                                    @Html.ValidationMessageFor(m => m.UserProfilePasswordPolicyViewModel.CloseDate)
                                                                </div>

                                                                @*Note*@
                                                                <div class="form-group">
                                                                    @Html.CustomLabel("Note")
                                                                    @Html.TextAreaFor(m => m.UserProfilePasswordPolicyViewModel.Note, new { @id = "note-password-policy", @class = "form-control deny-multiple-space", @placeholder = "Enter Note", @maxlength = "1500", onkeyup = "ToTextCapitalize()" })
                                                                    @Html.ValidationMessageFor(m => m.UserProfilePasswordPolicyViewModel.Note)
                                                                </div>

                                                                <div class="form-group d-none">
                                                                    @Html.CustomLabel("ReasonForModification")
                                                                    @Html.TextAreaFor(m => m.UserProfilePasswordPolicyViewModel.ReasonForModification, new { @id = "reason-for-modification-password-policy", @class = "form-control", @placeholder = "Enter Reason For Modification", @maxlength = "1500", onkeyup = "ToTextCapitalize()" })
                                                                    @Html.ValidationMessageFor(m => m.UserProfilePasswordPolicyViewModel.ReasonForModification)
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            @*Accordian 3 - User Profile Business Office*@
                                            <div id="heading-business-office-parameter" class="card mt-3">
                                                <div class="card-header" id="business-office-parameter-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-business-office-parameter" data-target="#collapse-business-office-parameter">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <a class="card-title" href="#">
                                                                <b>Business Office</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <span id="business-office-data-table-error" class="error d-none"> User Profile Business Office Data Table Required Data, Please Add Valid Data.</span>
                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-business-office-parameter" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-business-office-parameter" data-target="#collapse-business-office-parameter">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div id="collapse-business-office-parameter" class="collapse" aria-labelledby="heading-business-office-parameter" data-parent="#accordion">
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                @*Business Office Data Entry Modal / Popup Page*@
                                                                <div class="modal fade" id="business-office-modal" tabindex="-1" data-backdrop="static" role="dialog" aria-labelledby="modal-new-event" aria-hidden="true">
                                                                    <div class="modal-dialog modal-dialog-centered modal-dialog-zoom " role="document">
                                                                        <div class="modal-content">
                                                                            <div class="modal-header">
                                                                                <h4 class="modal-title  text-center" id="business-office-modal-title">Add</h4>
                                                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                                                    <span aria-hidden="true">&times;</span>
                                                                                </button>
                                                                            </div>

                                                                            <div class="modal-body">
                                                                                @*NameOfBusinessOffice*@
                                                                                <div class="form-group">
                                                                                    @Html.CustomLabel("Name Of Business Office")
                                                                                    @Html.DropDownListFor(m => m.UserProfileBusinessOfficeViewModel.MultiBusinessOfficeId, new MultiSelectList(userProfileDetailViewModel.BusinessOfficeDropdownList, "Value", "Text"), new { @id = "business-office-id", @class = "form-control mandatory-mark modal-input", @multiple = "multiple", required = "required" })
                                                                                    @Html.ValidationMessageFor(m => m.UserProfileBusinessOfficeViewModel.MultiBusinessOfficeId)
                                                                                    <span id="business-office-id-error" class="error modal-input-error d-none"> Please Select Name Of Business Office.</span>
                                                                                </div>

                                                                                @*ActivationDate*@
                                                                                <div class="form-group">
                                                                                    @Html.CustomLabel("Activation Date")
                                                                                    @Html.TextBoxFor(m => m.UserProfileBusinessOfficeViewModel.ActivationDate, "{0:yyyy-MM-dd}", new { @id = "activation-date-business-office", @class = "form-control mandatory-mark activation-date deny-past-date deny-next-financial-year-date modal-input set-current-date", @placeholder = "Enter Activation Date", required = "required", @Type = "date", onclick = "DenyPastDate()" })
                                                                                    @Html.ValidationMessageFor(m => m.UserProfileBusinessOfficeViewModel.ActivationDate)
                                                                                    <span id="activation-date-business-office-error" class="error modal-input-error d-none"> Please Select Activation Date.</span>
                                                                                </div>

                                                                                @*ExpiryDate*@
                                                                                <div class="form-group">
                                                                                    @Html.CustomLabel("Expiry Date")
                                                                                    @Html.TextBoxFor(m => m.UserProfileBusinessOfficeViewModel.ExpiryDate, "{0:yyyy-MM-dd}", new { @id = "expiry-date-business-office", @class = "form-control expiry-date modal-input", @placeholder = "Enter Expiry Date", @Type = "date", })
                                                                                    @Html.ValidationMessageFor(m => m.UserProfileBusinessOfficeViewModel.ExpiryDate)
                                                                                    <span id="expiry-date-business-office-error" class="error modal-input-error d-none"> Please Select Expiry Date.</span>
                                                                                </div>

                                                                                @*ClosedDate*@
                                                                                <div class="form-group d-none">
                                                                                    @Html.CustomLabel("Closed Date")
                                                                                    @Html.TextBoxFor(m => m.UserProfileBusinessOfficeViewModel.CloseDate, "{0:yyyy-MM-dd}", new { @id = "close-date-business-office", @class = "form-control deny-future-date-from-reference-date modal-input", @placeholder = "Enter Close Date", @Type = "date", onclick = "DenyFutureDateFromReferenceDate('activation-date-business-office')" })
                                                                                    @Html.ValidationMessageFor(m => m.UserProfileBusinessOfficeViewModel.CloseDate)
                                                                                </div>

                                                                                @*Note*@
                                                                                <div class="form-group">
                                                                                    @Html.CustomLabel("Note")
                                                                                    @Html.TextAreaFor(m => m.UserProfileBusinessOfficeViewModel.Note, new { @id = "note-business-office", @class = "form-control default-none deny-multiple-space modal-input", @placeholder = "Enter Note", @maxlength = 1500, onkeyup = "ToTextCapitalize()" })
                                                                                    @Html.ValidationMessageFor(m => m.UserProfileBusinessOfficeViewModel.Note)
                                                                                </div>

                                                                                @*ReasonForModification*@
                                                                                <div class="form-group d-none">
                                                                                    @Html.CustomLabel("ReasonForModification")
                                                                                    @Html.TextAreaFor(m => m.UserProfileBusinessOfficeViewModel.ReasonForModification, new { @id = "reason-for-modification-business-office", @class = "form-control", @placeholder = "Enter Reason For Modification", @maxlength = 1500, onkeyup = "ToTextCapitalize()" })
                                                                                    @Html.ValidationMessageFor(m => m.UserProfileBusinessOfficeViewModel.ReasonForModification)
                                                                                </div>

                                                                            </div>
                                                                            <div class="modal-footer">
                                                                                <button type="button" id="btn-add-business-office-modal" class="btn btn-success ">Add</button>
                                                                                <button type="button" id="btn-update-business-office-modal" class="btn btn-primary ">Update</button>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                                @*Business Office Data Table*@
                                                                <div class="card-body">
                                                                    <table id="tbl-business-office" class="table compact table-striped table-bordered " width="100">
                                                                        <thead class="text-center">
                                                                            <tr>
                                                                                <th><input type="checkbox" name="select-all" id="select-all-business-office" /></th>
                                                                                <th class="d-none">BusinessOfficeID</th>
                                                                                <th>NameOfBusinessOffice</th>
                                                                                <th>Activation Date</th>
                                                                                <th>ExpiryDate</th>
                                                                                <th class="d-none">CloseDate</th>
                                                                                <th>Note</th>
                                                                                <th class="d-none">Reason For Modification</th>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody class="text-center">
                                                                            @{
                                                                                IEnumerable<UserProfileBusinessOfficeViewModel> UserProfileBusinessOfficeviewModelList = (IEnumerable<UserProfileBusinessOfficeViewModel>)HttpContext.Current.Session["UserProfileBusinessOffice"];

                                                                                foreach (UserProfileBusinessOfficeViewModel viewModel in UserProfileBusinessOfficeviewModelList)
                                                                                {
                                                                                    <tr class="hoverlightblue">
                                                                                        <th><input type="checkbox" name="check_all" class="checks" /></th>
                                                                                        <td class="d-none">@viewModel.BusinessOfficeId</td>
                                                                                        <td>@viewModel.NameOfBusinessOffice</td>
                                                                                        <td>@viewModel.ActivationDate</td>
                                                                                        <td>@viewModel.ExpiryDate</td>
                                                                                        <td class="d-none">@viewModel.CloseDate</td>
                                                                                        <td>@viewModel.Note</td>
                                                                                        <td class="d-none">@viewModel.ReasonForModification</td>
                                                                                    </tr>
                                                                                }
                                                                            }
                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            @*Accordian 4 - Currency*@
                                            <div id="show-user-profile-currency-block" class="card mt-3 d-none">
                                                <div class="card-header " id="currency-validation" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-currency" data-target="#collapse-currency">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <a class="card-title" href="#">
                                                                <b>Currency</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <span id="currency-data-table-error" class="error d-none"> Currency Data Table Required Data, Please Add Valid Data.</span>
                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-currency" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-currency" data-target="#collapse-currency">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div id="collapse-currency" class="collapse" aria-labelledby="heading-currency" data-parent="#accordion">
