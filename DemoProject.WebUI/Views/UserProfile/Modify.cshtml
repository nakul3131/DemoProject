                                                                                    @Html.TextBoxFor(m => m.UserRoleProfileViewModel.ActivationDate, "{0:yyyy-MM-dd}", new { @class = "form-control mandatory-mark activation-date deny-next-financial-year-date deny-past-date modal-input set-current-date", @id = "activation-date-role-profile", @Type = "date", @placeholder = "Enter Activation Date", onclick = "DenyPastDate()", required = "required" })
                                                                                    @Html.ValidationMessageFor(m => m.UserRoleProfileViewModel.ActivationDate)
                                                                                    <span id="activation-date-role-profile-error" class="error modal-input-error d-none">Please Enter Activation Date.</span>
                                                                                </div>

                                                                                @*ExpiryDate*@
                                                                                <div class="form-group">
                                                                                    @Html.CustomLabel("Expiry Date")
                                                                                    @Html.TextBoxFor(m => m.UserRoleProfileViewModel.ExpiryDate, "{0:yyyy-MM-dd}", new { @class = "form-control expiry-date modal-input", @id = "expiry-date-role-profile", @Type = "date", @placeholder = "Enter Expiry Date" })
                                                                                    @Html.ValidationMessageFor(m => m.UserRoleProfileViewModel.ExpiryDate)
                                                                                    <span id="expiry-date-role-profile-error" class="error modal-input-error d-none">Please Enter Expiry Date.</span>
                                                                                </div>

                                                                                @*CloseDate*@
                                                                                <div class="form-group d-none">
                                                                                    @Html.CustomLabel("Close Date")
                                                                                    @Html.TextBoxFor(m => m.UserRoleProfileViewModel.CloseDate, "{0:yyyy-MM-dd}", new { @class = "form-control modal-input", @Type = "date", @id = "close-date-role-profile" })
                                                                                    @Html.ValidationMessageFor(m => m.UserRoleProfileViewModel.CloseDate)
                                                                                </div>

                                                                                @*Note*@
                                                                                <div class="form-group">
                                                                                    @Html.CustomLabel("Note")
                                                                                    @Html.TextAreaFor(m => m.UserRoleProfileViewModel.Note, new { @id = "note-role-profile", @class = "form-control deny-multiple-space modal-input", @placeholder = "Enter Note", @maxlength = 1500, onkeyup = "ToTextCapitalize()" })
                                                                                    @Html.ValidationMessageFor(m => m.UserRoleProfileViewModel.Note)
                                                                                </div>

                                                                                <div class="form-group d-none">
                                                                                    @Html.CustomLabel("ReasonForModification")
                                                                                    @Html.TextAreaFor(m => m.UserRoleProfileViewModel.ReasonForModification, new { @id = "reason-for-modification-role-profile", @class = "form-control", @placeholder = "Enter Reason For Modification", @maxlength = 1500, onkeyup = "ToTextCapitalize()" })
                                                                                    @Html.ValidationMessageFor(m => m.UserRoleProfileViewModel.ReasonForModification)
                                                                                </div>
                                                                            </div>
                                                                            <div class="modal-footer">
                                                                                <button type="button" id="btn-add-role-profile-modal" class="btn btn-success ">Add</button>
                                                                                <button type="button" id="btn-update-role-profile-modal" class="btn btn-primary">Update</button>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                                @*User Profile Role Profile / Popup Page*@
                                                                <div class="card-body">
                                                                    <table id="tbl-role-profile" class="table compact  table-striped table-bordered text-nowrap" cellspacing="0" width="100">
                                                                        <thead class="text-center">
                                                                            <tr>
                                                                                <th><input type="checkbox" name="check_all" id="select-all-role-profile" /></th>
                                                                                <th class="d-none">BusinessOfficeID</th>
                                                                                <th>Name Of Business Office</th>
                                                                                <th class="d-none">RoleProfileId</th>
                                                                                <th>Name Of RoleProfile</th>
                                                                                <th>Activation Date</th>
                                                                                <th>Expiry Date</th>
                                                                                <th class="d-none">CloseDate</th>
                                                                                <th>Note</th>
                                                                                <th class="d-none">ReasonForModification</th>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody class="text-center">
                                                                            @{
                                                                                IEnumerable<UserRoleProfileViewModel> UserRoleProfileviewModelList = (IEnumerable<UserRoleProfileViewModel>)HttpContext.Current.Session["UserRoleProfile"];

                                                                                foreach (UserRoleProfileViewModel viewModel in UserRoleProfileviewModelList)
                                                                                {
                                                                                    <tr class="hoverlightblue">
                                                                                        <th><input type="checkbox" name="check_all" class="checks" /></th>
                                                                                        <th class="d-none">@viewModel.BusinessOfficeId</th>
                                                                                        <th>@viewModel.NameOfBusinessOffice</th>
                                                                                        <td class="d-none">@viewModel.RoleProfileId</td>
                                                                                        <td>@viewModel.NameOfRoleProfile</td>
                                                                                        <td>@viewModel.ActivationDate</td>
                                                                                        <td>@viewModel.ExpiryDate</td>
                                                                                        <td class="d-none">@viewModel.CloseDate</td>
                                                                                        <td>@viewModel.Note</td>
                                                                                        <td class="d-none">@viewModel.ReasonForModification</td>
                                                                                    </tr>
                                                                                }
                                                                            }
                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                @*======= Fifth Row - Reason For Modification Input In Regional Language =======*@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <div class="form-group">
                                            @Html.CustomLabel("Reason For Modification")
                                            @Html.TextAreaFor(m => m.ReasonForModification, new { @id = "reason-for-modification", @class = "form-control", @placeholder = "Enter Reason For Modification", @maxlength = "1500", @autocomplete = "off", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.ReasonForModification)
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Hiddent Field For UserProfilePrmKey*@
                            @Html.HiddenFor(m => m.UserProfilePrmKey)

                            @*Hiddent Field For UserProfileModificationPrmKey*@
                            @Html.HiddenFor(m => m.UserProfileModificationPrmKey)

                            @*Hiddent Field For UserProfileBusinessOfficePrmKey*@
                            @Html.HiddenFor(m => m.UserProfileBusinessOfficeViewModel.UserProfileBusinessOfficePrmKey)

                            @*Hiddent Field For UserProfileCurrencyPrmKey*@
                            @Html.HiddenFor(m => m.UserProfileCurrencyViewModel.UserProfileCurrencyPrmKey)

                            @*Hiddent Field For UserProfileGeneralLedgerPrmKey*@
                            @Html.HiddenFor(m => m.UserProfileGeneralLedgerViewModel.UserProfileGeneralLedgerPrmKey)

                            @*Hiddent Field For UserProfileHomeBusinessOfficePrmKey*@
                            @Html.HiddenFor(m => m.UserProfileHomeBusinessOfficeViewModel.UserProfileHomeBusinessOfficePrmKey)

                            @*Hiddent Field For UserProfileLoginDevicePrmKey*@
                            @Html.HiddenFor(m => m.UserProfileLoginDevicePrmKey)

                            @*Hiddent Field For UserProfileMenuPrmKey*@
                            @Html.HiddenFor(m => m.UserProfileMenuViewModel.UserProfileMenuPrmKey)

                            @*Hiddent Field For UserProfilePasswordPolicyPrmKey*@
                            @Html.HiddenFor(m => m.UserProfilePasswordPolicyViewModel.UserProfilePasswordPolicyPrmKey)

                            @*Hiddent Field For UserProfileSpecialPermissionPrmKey*@
                            @Html.HiddenFor(m => m.UserProfileSpecialPermissionViewModel.UserProfileSpecialPermissionPrmKey)

                            @*Hiddent Field For UserProfileTransactionLimitPrmKey*@
                            @Html.HiddenFor(m => m.UserProfileTransactionLimitViewModel.UserProfileTransactionLimitPrmKey)

                            @*Hiddent Field For UserRoleProfilePrmKey*@
                            @Html.HiddenFor(m => m.UserRoleProfileViewModel.UserRoleProfilePrmKey)

                            @Html.OperationFooter("Modify", "Exit", "Home", Model.Remark)
                        }
                    }
                </div>
            </div>
        </div>
    </div>
    <link href="~/Content/css/jquery.multiselect.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.multiselect.js"></script>
</body>
</html>
<script>
    $('#multi-businessoffice, #currency-id, #general-ledger-id, #login-device-id, #special-permission-id, #transaction-type-id, #user-role-profile-id').multiselect({
        columns: 1,
        placeholder: 'Select Correct Name',
        search: true,
        selectAll: true,
        reload: true,
    });

   $('#sub-menu-id').select2({
        closeOnSelect: false
   })

    //Dropdown Select List For menu-add
    $("#model-menu-id").on("change", function () {
        debugger
        $.ajax({
            type: "post",
            url: Modelmenuurl,
            data: { MenuId: $('#model-menu-id').val() },
            datatype: "json",
            traditional: true,
            success: function (data) {
                debugger
                $('#main-menu-id').empty();
                //var MainMenuList = '<option value="All">--Select All Menu--</option>';
                var MainMenuList;
                for (var i = 0; i < data.length; i++) {
                    MainMenuList += "<option value=" + data[i].Value + ">" + data[i].Text + "</option>";
                }
                debugger;
                $('#main-menu-id').append(MainMenuList);
            }
        });
    })

    $("#main-menu-id").on("change", function () {
        debugger
        $.ajax({
            type: "post",
            url: "/RoleProfile/GetModel",
            data: { MenuId: $('#main-menu-id').val() },
            datatype: "json",
            traditional: true,
            success: function (data) {
                debugger
                $('#sub-menu-id').empty();
                //var SubMenuList = '<option value="0">--Select Sub Menu--</option>';
                var SubMenuList;
                for (var i = 0; i < data.length; i++) {
                    SubMenuList += "<option value=" + data[i].Value + ">" + data[i].Text + "</option>";
                }
                $('#sub-menu-id').append(SubMenuList);
            }
        });
    });

    var url = '@Url.Action("SaveDataTables", "UserProfile")';

    var getPersonDropdownByUserType = "@Url.Action("GetPersonDropdownByUserType", "UserProfile")";

    var Modelmenuurl = "@Url.Action("GetModel", "RoleProfile")";

    var uniqueNameOfRoleProfile = "@Url.Action("GetUniqueRoleProfileName", "RoleProfile")";
</script>
<script src="~/Scripts/Custom/UserProfile.js"></script>