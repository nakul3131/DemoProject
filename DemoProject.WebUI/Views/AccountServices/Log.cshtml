
@{
    Layout = "~/Views/Shared/_LoginSupportLayout.cshtml";
}

<!doctype html>

<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0" />
    <title>Last Login</title>
</head>
<body>
    <div class="container-fluid">
        <!-- First Row (<- Back Arrow and Title) -->
        <div class="row mt-5">
            <div class="col-1">
                <a class="text-center" href="javascript:void(0)" onclick="window.location.href='@Url.Action("Login", "Account")'">
                    <i class='fas fa-arrow-left btn-lg text-primary'></i>
                </a>
            </div>
            <div class="col-10">
                <h2 class="text-center text-primary">
                    <strong>Last Login Log</strong>
                </h2>
            </div>
        </div>
        <!-- Second Row (Page Description) -->
        <div class="row mt-5">
            <div class="offset-1 col-10 text-center">
                <figure>
                    <img class="img-fluid" src="~/Icons/Login/Log.jpg" alt="Last Login Log Image" />
                    <figcaption>
                        <h6 class="text-center line-height-3 mt-4">
                            All Users Displayed Their Last Login And Login History Including Date And Times That User's Account Used.
                        </h6>
                    </figcaption>
                </figure>
            </div>
        </div>
    </div>
</body>
</html>
