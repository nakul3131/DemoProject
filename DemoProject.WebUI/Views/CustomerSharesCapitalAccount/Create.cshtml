@model DemoProject.Services.ViewModel.Account.Customer.SharesCapitalCustomerAccountViewModel
@using DemoProject.Services.HtmlHelpers
@using DemoProject.Services.ViewModel.Account.Customer
@using DemoProject.Services.ViewModel.Configuration
@using DemoProject.Services.Constants

@{
    SharesCapitalCustomerAccountDetailViewModel sharesCapitalCustomerAccountDetailViewModel = new SharesCapitalCustomerAccountDetailViewModel();
    //CustomerAccountDetailViewModel customerAccountDetailViewModel = new CustomerAccountDetailViewModel();
    HeaderPartialModel headerPartialModel = new HeaderPartialModel("CUCCRT");
}

<!doctype html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>Customer Shares Capital Account - Create</title>
</head>
<body>
    <div class="container-fluid">
        @*======= First Row - For Header =======*@
        <div class="row">
            <div class="offset-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @*======= Second Row - Data Entry Page =======*@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card d-block mb-3">
                    @using (Html.BeginForm("Create", "CustomerSharesCapitalAccount", FormMethod.Post, htmlAttributes: new { @id = "form", @autocomplete = "off" }))
                    {
                        @Html.AntiForgeryToken()
                        {
                            <div class="card-body">
                                @*======= First Row - Mandatory Mark =======*@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <h6 class="mb-2 text-right text-muted"><b class="mandatory-mark">&nbsp; Mandatory Field</b></h6>
                                    </div>
                                </div>

                                @*======= Second Row - Page Title, Translation =======*@
                                <div class="row mt-2">
                                    @*Page Title*@
                                    <div class="col-12">
                                        <h5 class="font-weight-bold mt-1">Create</h5>
                                        <hr />
                                    </div>
                                </div>

                                @*Submit Error*@
                                @Html.ValidationMessage("SubmitError")

                                @*======= Third Row - Data Entry Page =======*@
                                <div class="row">
                                    <div class="col-12">
                                        @*BusinessOfficeId*@
                                        <div class="form-group @(sharesCapitalCustomerAccountDetailViewModel.HasBranch ? "" : "d-none")">
                                            @Html.CustomLabel("Branch")
                                            <span id="business-office-change-info" class="ml-5 text-primary d-none"> ** If You Change The Business Office And Related Content, All Of Business Office Releated Content Is Removed.</span>
                                            @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.BusinessOfficeId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.AuthorizedBusinessOfficeDropdownList, "Value", "Text", 0), "--- Select Branch ---", new { @id = "business-office-id", @class = "form-control mandatory-mark", required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.BusinessOfficeId)
                                            <span id="business-office-id-error" class="error d-none"> Please Select Valid Business Office.</span>
                                        </div>

                                        @*GeneralLedgerId*@
                                        <div class="form-group">
                                            @Html.CustomLabel("General Ledger")
                                            <span id="general-ledger-change-info" class="ml-5 text-primary d-none">** If You Change The General Ledger And Related Content, All Of General Ledger Releated Content Is Removed.</span>
                                            @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.GeneralLedgerId, new List<SelectListItem>(), "Select General Ledger", new { @id = "general-ledger-id", @class = "form-control mandatory-mark", required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.GeneralLedgerId)
                                            <span id="general-ledger-id-error" class="error d-none"> Please Select General Ledger</span>
                                        </div>

                                        @*SchemeId*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Scheme")
                                            <span id="scheme-change-info" class="ml-5 text-primary d-none">** If You Change The Scheme And Related Content, All Of Scheme Related Content Is Removed.</span>
                                            @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.SchemeId, new List<SelectListItem>(), "--- Select Scheme ---", new { @id = "scheme-id", @class = "form-control mandatory-mark", required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.SchemeId)
                                            <span id="scheme-id-error" class="error d-none">Scheme Setting Error : Something Went Wrong When Selecting Scheme.</span>
                                        </div>

                                        @*MinuteOfMeetingAgenda*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Minute Of Meeting Agenda")
                                            @Html.DropDownListFor(m => m.CustomerSharesCapitalAccountViewModel.MinuteOfMeetingAgendaId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.MinuteOfMeetingAgendaDropdownList, "Value", "Text"), "--- Select Minute Of Meeting Agenda ---", new { @id = "minute-of-meeting-agenda-id", @class = "form-control mandatory-mark", required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerSharesCapitalAccountViewModel.MinuteOfMeetingAgendaId)
                                        </div>

                                        @*PersonId*@
                                        <div class="form-group" id="person-name">
                                            @Html.CustomLabel("Customer Name")
                                            <span id="person-change-info" class="ml-5 text-primary d-none"> ** You Are Changing Customer Name So Joint / Nominne Data Has Been Removed.</span>
                                            @Html.TextBox("person", null, new { @type = "search", @id = "person-id", @class = "form-control mandatory-mark deny-multiple-space text-capitalize", @placeholder = "Select Customer Name", required = "required" })
                                            @Html.TextBoxFor(m => m.CustomerAccountDetailViewModel.PersonId, new { @type = "search", @id = "person-id1", @class = "form-control mandatory-mark text-capitalize d-none", @placeholder = "Select Customer Name", required = "required" })
                                            <span id="person-id-error" class="error d-none"> Please Select Person Name.</span>
                                        </div>

                                        @*CurrencyId*@
                                        <div class="form-group @(sharesCapitalCustomerAccountDetailViewModel.HasMultiCurrency ? "" : "d-none")">
                                            @Html.CustomLabel("Currency")
                                            @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.CurrencyId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.CurrencyDropdownList, "Value", "Text", 0), "--- Select Currency ---", new { @id = "currency-id", @class = "form-control mandatory-mark", required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.CurrencyId)
                                        </div>

                                        @*AccountOpeningDate*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Account Opening Date")
                                            @Html.TextBoxFor(m => m.AccountOpeningDate, "{0:yyyy-MM-dd}", new { @id = "account-opening-date", @class = "form-control mandatory-mark deny-future-date set-current-date", required = "required", @Type = "date", min = sharesCapitalCustomerAccountDetailViewModel.AllowLastPastDays })
                                            @Html.ValidationMessageFor(m => m.AccountOpeningDate)
                                        </div>

                                        @*ApplicationNumber*@
                                        <div id="application-number-input" class="form-group">
                                            @Html.CustomLabel("Application Number")
                                            @Html.TextBoxFor(m => m.ApplicationNumber, new { @id = "application-number", @class = "form-control real-number mandatory-mark", @placeholder = "Enter Application Number", required = "required", @type = "number" })
                                            @Html.ValidationMessageFor(m => m.ApplicationNumber)
                                        </div>

                                        @*AccountNumber*@
                                        <div id="account-number-input" class="form-group">
                                            @Html.CustomLabel("Account Number")
                                            @Html.TextBoxFor(m => m.AccountNumber, new { @id = "account-number", @class = "form-control mandatory-mark real-number", @placeholder = "Enter Account Number", required = "required", type = "number", @min = 1, @max = 999999999999999 })
                                            @Html.ValidationMessageFor(m => m.AccountNumber)
                                        </div>

                                        @*AlternateAccountNumber1*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Alternate Account Number 1")
                                            @Html.TextBoxFor(m => m.AlternateAccountNumber1, new { @id = "alternate-account-number-1", @class = "form-control mandatory-mark deny-multiple-space", @placeholder = "Enter Alternate Account Number 1", required = "required", @maxlength = 50, onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.AlternateAccountNumber1)
                                        </div>

                                        @*AlternateAccountNumber2*@
                                        <div id="account-number2" class="form-group">
                                            @Html.CustomLabel("Alternate Account Number 2")
                                            @Html.TextBoxFor(m => m.AlternateAccountNumber2, new { @id = "alternate-account-number-2", @class = "form-control mandatory-mark deny-multiple-space", @placeholder = "Enter Alternate Account Number 2", required = "required", @maxlength = 50, onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.AlternateAccountNumber2)
                                        </div>

                                        @*AlternateAccountNumber3*@
                                        <div id="account-number3" class="form-group ">
                                            @Html.CustomLabel("Alternate Account Number 3")
                                            @Html.TextBoxFor(m => m.AlternateAccountNumber3, new { @id = "alternate-account-number-3", @class = "form-control mandatory-mark deny-multiple-space", @placeholder = "Enter Alternate Account Number 3", required = "required", @maxlength = 50, onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.AlternateAccountNumber3)
                                        </div>

                                        @*MemberNumber*@
                                        <div id="member-number-grp" class="form-group">
                                            @Html.CustomLabel("Member Number")
                                            @Html.TextBoxFor(m => m.CustomerSharesCapitalAccountViewModel.MemberNumber, new { @id = "member-number", @class = "form-control mandatory-mark real-number", @placeholder = "Enter Member Number", type = "number", required = "required", @min = 1, @max = 999999999 })
                                            @Html.ValidationMessageFor(m => m.CustomerSharesCapitalAccountViewModel.MemberNumber)
                                        </div>

                                        @*IsPrivateCustomer*@
                                        <div class="row form-group mt-4">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Private Customer")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.IsPrivateCustomer, new { @id = "is-private-customer", @class = "form-control switch-input" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*IsOtherSocietyMember*@
                                        <div class="row form-group mt-4">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Other Society Member")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.CustomerSharesCapitalAccountViewModel.IsOtherSocietyMember, new { @id = "is-other-society-member", @class = "form-control switch-input" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*Note*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Note")
                                            @Html.TextAreaFor(m => m.Note, new { @id = "note-scheme", @class = "form-control default-none deny-multiple-space", @placeholder = "Enter Note", @maxlength = 1500, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.Note)
                                        </div>

                                        @*======= Fourth Row - Accordians =======*@
                                        <div class="row mt-3">
                                            <div class="col-12">
                                                <div id="accordion" class="accordion md-accordion accordion-blocks">

                                                    @*Accordian 1 -Address Details*@
                                                    <div id="address-details-card" class="card mt-3 address-details">
                                                        <div class="card-header p-3" id="collapse-address-details-validations" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-address-details" data-target="#collapse-address-details">
                                                            <div class="row">
                                                                <div class="col-6">
                                                                    <a class="card-title" href="#">
                                                                        <b>@Html.TextWithRegionalLanguage("Address Details")</b>
                                                                    </a>
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <span id="address-accordion-error" class="error d-none">This Is Required, Please Add Valid Data.</span>
                                                                </div>
                                                                <div class="col-2">
                                                                    <a href="#collapse-address-details" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-address-details" data-target="#collapse-address-details">
                                                                        <i class="fa fa-angle-down toggle-icon"></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div id="collapse-address-details" class="collapse" aria-labelledby="heading-address-details" data-parent="#accordion">
                                                            <div class="card-body">
                                                                <div class="row">
                                                                    <div class="col-12">
                                                                        <div class="modal fade" id="person-address-modal" tabindex="-1" data-backdrop="static" role="dialog" aria-labelledby="modal-new-event" aria-hidden="true">
                                                                            <div class="modal-dialog modal-dialog-centered modal-dialog-zoom modal-lg" role="document">
                                                                                <div class="modal-content">
                                                                                    <div class="modal-header">
                                                                                        <h4 class="modal-title text-center" id="person-address-text">Add</h4>
                                                                                        <button type="button" class="close-person-address-modal" data-dismiss="modal" aria-label="Close">
                                                                                            <span aria-hidden="true">&times;</span>
                                                                                        </button>
                                                                                    </div>
                                                                                    <div class="modal-body">
                                                                                        <div class="person-address-details">
                                                                                            @*AddressTypePrmKey*@
                                                                                            <div class="form-group">
                                                                                                @Html.CustomLabel("Address Type")
                                                                                                @Html.DropDownListFor(m => m.PersonAddressViewModel.AddressTypeId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.AddressTypeDropdownList, "Value", "Text", 0), "--- Select Address Type ---", new { @id = "address-type-id", @class = "form-control mandatory-mark modal-input", required = "required" })
                                                                                                @Html.ValidationMessageFor(m => m.PersonAddressViewModel.AddressTypeId)
                                                                                                <span id="address-type-id-error" class="error modal-input-error d-none"> Please Select Valid Address Type.</span>
                                                                                            </div>

                                                                                            @*FlatDoorNo*@
                                                                                            <div class="form-group">
                                                                                                @Html.CustomLabel("Flat Door No")
                                                                                                @Html.TextBoxFor(m => m.PersonAddressViewModel.FlatDoorNo, new { @id = "flat-door-no", @class = "form-control mandatory-mark modal-input alpha-numeric", @placeholder = "Enter Flat Door No", required = "required", @minlength = 3, @maxlength = 50, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                                                                                @Html.ValidationMessageFor(m => m.PersonAddressViewModel.FlatDoorNo)
                                                                                                <span id="flat-door-no-error" class="error modal-input-error d-none"> Please Enter Valid Flat Door No.</span>
                                                                                            </div>

                                                                                            @*TransFlatDoorNo*@
                                                                                            <div class="form-group">
                                                                                                @Html.CustomLabel(sharesCapitalCustomerAccountDetailViewModel.FlatDoorNoInRegionalLanguage)
                                                                                                @Html.TextBoxFor(m => m.PersonAddressViewModel.TransFlatDoorNo, null, new { @id = "trans-flat-door-no", @class = "form-control mandatory-mark deny-multiple-space modal-input", @placeholder = sharesCapitalCustomerAccountDetailViewModel.FlatDoorNoPlaceHolderInRegionalLanguage, @maxlength = 50, required = "required" })
                                                                                                @Html.ValidationMessageFor(m => m.PersonAddressViewModel.TransFlatDoorNo)
                                                                                                <span id="trans-flat-door-no-error" class="error modal-input-error d-none"> Please Enter Valid Flat Door No.</span>
                                                                                            </div>

                                                                                            @*NameOfBuilding*@
                                                                                            <div class="form-group">
                                                                                                @Html.CustomLabel("Name Of Building")
                                                                                                @Html.TextBoxFor(m => m.PersonAddressViewModel.NameOfBuilding, new { @id = "building-name", @class = "form-control mandatory-mark modal-input alpha-numeric", @placeholder = "Enter Name Of Building", @minlength = 3, @maxlength = 100, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()", required = "required" })
                                                                                                @Html.ValidationMessageFor(m => m.PersonAddressViewModel.NameOfBuilding)
                                                                                                <span id="building-name-error" class="error modal-input-error d-none"> Please Enter Valid Building Name.</span>
                                                                                            </div>

                                                                                            @*TransNameOfBuilding*@
                                                                                            <div class="form-group">
                                                                                                @Html.CustomLabel(sharesCapitalCustomerAccountDetailViewModel.NameOfBuildingInRegionalLanguage)
                                                                                                @Html.TextBoxFor(m => m.PersonAddressViewModel.TransNameOfBuilding, new { @id = "trans-building-name", @class = "form-control mandatory-mark deny-multiple-space modal-input", @placeholder = sharesCapitalCustomerAccountDetailViewModel.NameOfBuildingPlaceHolderInRegionalLanguage, required = "required", @maxlength = 100, })
                                                                                                @Html.ValidationMessageFor(m => m.PersonAddressViewModel.TransNameOfBuilding)
                                                                                                <span id="trans-building-name-error" class="error modal-input-error d-none"> Please Enter Valid Building Name.</span>
                                                                                            </div>

                                                                                            @*NameOfRoad*@
                                                                                            <div class="form-group">
                                                                                                @Html.CustomLabel("Name Of Road")
                                                                                                @Html.TextBoxFor(m => m.PersonAddressViewModel.NameOfRoad, new { @id = "road-name", @class = "form-control mandatory-mark modal-input alpha-numeric", @placeholder = "Enter Name Of Road", @minlength = 3, @maxlength = 100, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()", required = "required" })
                                                                                                @Html.ValidationMessageFor(m => m.PersonAddressViewModel.NameOfRoad)
                                                                                                <span id="road-name-error" class="error modal-input-error d-none"> Please Enter Valid Road name.</span>
                                                                                            </div>

                                                                                            @*TransNameOfRoad*@
                                                                                            <div class="form-group">
                                                                                                @Html.CustomLabel(sharesCapitalCustomerAccountDetailViewModel.NameOfRoadInRegionalLanguage)
                                                                                                @Html.TextBoxFor(m => m.PersonAddressViewModel.TransNameOfRoad, new { @id = "trans-road-name", @class = "form-control mandatory-mark modal-input deny-multiple-space", @placeholder = sharesCapitalCustomerAccountDetailViewModel.NameOfRoadPlaceHolderInRegionalLanguage, required = "required", @maxlength = 100 })
                                                                                                @Html.ValidationMessageFor(m => m.PersonAddressViewModel.TransNameOfRoad)
                                                                                                <span id="trans-road-name-error" class="error modal-input-error d-none"> Please Enter Valid Road Name.</span>
                                                                                            </div>

                                                                                            @*NameOfArea*@
                                                                                            <div class="form-group">
                                                                                                @Html.CustomLabel("Name Of Area")
                                                                                                @Html.TextBoxFor(m => m.PersonAddressViewModel.NameOfArea, new { @id = "area-name", @class = "form-control mandatory-mark modal-input alpha-numeric", @placeholder = "Enter Name Of Area", @minlength = 3, @maxlength = 100, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()", required = "required" })
                                                                                                @Html.ValidationMessageFor(m => m.PersonAddressViewModel.NameOfArea)
                                                                                                <span id="area-name-error" class="error modal-input-error d-none"> Please Enter Valid Area Name.</span>
                                                                                            </div>

                                                                                            @*TransNameOfArea*@
                                                                                            <div class="form-group">
                                                                                                @Html.CustomLabel(sharesCapitalCustomerAccountDetailViewModel.NameOfAreaInRegionalLanguage)
                                                                                                @Html.TextBoxFor(m => m.PersonAddressViewModel.TransNameOfArea, new { @id = "trans-area-name", @class = "form-control mandatory-mark modal-input deny-multiple-space", @placeholder = sharesCapitalCustomerAccountDetailViewModel.NameOfAreaPlaceHolderInRegionalLanguage, required = "required", @maxlength = 100 })
                                                                                                @Html.ValidationMessageFor(m => m.PersonAddressViewModel.TransNameOfArea)
                                                                                                <span id="trans-area-name-error" class="error modal-input-error d-none"> Please Enter Valid Area Name</span>
                                                                                            </div>

                                                                                            @*City*@
                                                                                            <div class="form-group">
                                                                                                @Html.CustomLabel("City")
                                                                                                @Html.DropDownListFor(m => m.PersonAddressViewModel.CityId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.CityDropdownList, "Value", "Text", 0), "--- Select City ---", new { @id = "city-id", @class = "form-control mandatory-mark modal-input", required = "required" })
                                                                                                @Html.ValidationMessageFor(m => m.PersonAddressViewModel.CityId)
                                                                                                <span id="city-id-error" class="error modal-input-error d-none"> Please Select City.</span>
                                                                                            </div>

                                                                                            @*ResidenceType*@
                                                                                            <div class="form-group">
                                                                                                @Html.CustomLabel("Residence Type")
                                                                                                @Html.DropDownListFor(m => m.PersonAddressViewModel.ResidenceTypeId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.ResidenceTypeDropdownList, "Value", "Text", 0), "--- Select Residence Type ---", new { @id = "residence-type-id", @class = "form-control mandatory-mark modal-input", required = "required" })
                                                                                                @Html.ValidationMessageFor(m => m.PersonAddressViewModel.ResidenceTypeId)
                                                                                                <span id="residence-type-id-error" class="error modal-input-error d-none">Please Select Residence Type</span>
                                                                                            </div>

                                                                                            @*Ownership Type*@
                                                                                            <div class="form-group">
                                                                                                @Html.CustomLabel("Ownership Type")
                                                                                                @Html.DropDownListFor(m => m.PersonAddressViewModel.OwnershipTypeId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.OwnershipTypeDropdownList, "Value", "Text", 0), " --- Select Ownership Type ---", new { @id = "residence-ownership-id", @class = "form-control mandatory-mark modal-input", required = "required" })
                                                                                                @Html.ValidationMessageFor(m => m.PersonAddressViewModel.OwnershipTypeId)
                                                                                                <span id="residence-ownership-id-error" class="error modal-input-error d-none"> Please Select Residence Ownership.</span>
                                                                                            </div>

                                                                                            @*Note*@
                                                                                            <div class="form-group">
                                                                                                @Html.CustomLabel("Note")
                                                                                                @Html.TextAreaFor(m => m.PersonAddressViewModel.Note, new { @id = "note-address", @class = "form-control default-none deny-multiple-space modal-input", @placeholder = "Enter Note", @maxlength = 1500, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                                                                                @Html.ValidationMessageFor(m => m.PersonAddressViewModel.Note)
                                                                                            </div>

                                                                                            @*TransNote*@
                                                                                            <div class="form-group">
                                                                                                @Html.CustomLabel(sharesCapitalCustomerAccountDetailViewModel.NoteInRegionalLanguage)
                                                                                                @Html.TextAreaFor(m => m.PersonAddressViewModel.TransNote, new { @id = "trans-note-address", @class = "form-control default-none deny-multiple-space modal-input", @placeholder = sharesCapitalCustomerAccountDetailViewModel.NotePlaceHolderInRegionalLanguage, @maxlength = 1500 })
                                                                                                @Html.ValidationMessageFor(m => m.PersonAddressViewModel.TransNote)
                                                                                            </div>

                                                                                            @*ReasonForModification*@
                                                                                            <div id="address-div" class="form-group d-none">
                                                                                                @Html.CustomLabel("Reason For Modification")
                                                                                                @Html.TextAreaFor(m => m.PersonAddressViewModel.ReasonForModification, new { @id = "reason-for-modification-address", @class = "form-control default-none deny-multiple-space", @placeholder = "Enter Reason For Modification", @maxlength = 1500, onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                                                                                @Html.ValidationMessageFor(m => m.PersonAddressViewModel.ReasonForModification)
                                                                                            </div>
                                                                                        </div>

                                                                                        @*IsVerified*@
                                                                                        <div class="row form-group mt-4">
                                                                                            <div class="col-8">
                                                                                                @Html.CustomLabel("Is Verified")
                                                                                            </div>
                                                                                            <div class="col-4">
                                                                                                <div class="text-right">
                                                                                                    <label class="switch switch-left-right">
                                                                                                        @Html.CheckBoxFor(m => m.PersonAddressViewModel.IsVerified, new { @id = "is-verified-address", @class = "form-control switch-input address-detail-new modal-switch-input" })
                                                                                                        <span class="slider round">
                                                                                                            <span class="on"><b>Yes</b></span><span class="off"><b class="mr-3">No</b></span>
                                                                                                        </span>
                                                                                                    </label>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>

                                                                                    <div class="modal-footer">
                                                                                        <button type="button" id="btn-add-person-address-modal" class="btn btn-success ">Add</button>
                                                                                        <button type="button" id="btn-update-person-address-modal" class="btn btn-primary">Update</button>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        <div class="card-body">
                                                                            <div class="table-responsive" style="min-height:200px;">
                                                                                <table id="tbl-person-address" class="table compact table-striped table-bordered" cellspacing="0" width="100">
                                                                                    <thead class="text-center">
                                                                                        <tr>
                                                                                            <th><input id="select-all-person-address" type="checkbox" name="select-all" style="margin-left:11px;" /></th>
                                                                                            <th class="d-none">AddressTypeID</th>
                                                                                            <th>AddressType</th>
                                                                                            <th>FlatDoorNo</th>
                                                                                            <th>FlatDoorNo</th>
                                                                                            <th>NameOfBuilding</th>
                                                                                            <th>NameOfBuilding</th>
                                                                                            <th>NameOfRoad</th>
