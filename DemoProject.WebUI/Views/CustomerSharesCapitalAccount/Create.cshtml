                                                                    </a>
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <span id="notice-data-table-error" class="error d-none">This Is Required, Please Add Valid Data.</span>
                                                                </div>
                                                                <div class="col-2">
                                                                    <a href="#collapse-notice-schedule" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-notice-schedule" data-target="#collapse-notice-schedule">
                                                                        <i class="fa fa-angle-down toggle-icon"></i>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <div id="collapse-notice-schedule" class="collapse" aria-labelledby="heading-notice-schedule" data-parent="#accordion">
                                                            <div class="card-body">
                                                                <div class="row">
                                                                    <div class="col-12">
                                                                        @*Notice Schedule Parameter Data Entry Modal / Popup Page*@
                                                                        <div class="modal fade" id="notice-schedule-modal" tabindex="-1" data-backdrop="static" role="dialog" aria-labelledby="modal-new-event" aria-hidden="true">
                                                                            <div class="modal-dialog modal-dialog-centered modal-dialog-zoom modal-md" role="document">
                                                                                <div class="modal-content">
                                                                                    <div class="modal-header">
                                                                                        <h4 class="modal-title text-center">Add</h4>
                                                                                        <button type="button" class="close-notice-schedule-modal" data-dismiss="modal" aria-label="Close">
                                                                                            <span aria-hidden="true">&times;</span>
                                                                                        </button>
                                                                                    </div>
                                                                                    <div class="modal-body">
                                                                                        @*NoticeTypeId*@
                                                                                        <div class="form-group">
                                                                                            @Html.CustomLabel("Alert / Notice Type")
                                                                                            @Html.DropDownListFor(m => m.CustomerAccountNoticeScheduleViewModel.NoticeTypeId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.NoticeTypeDropdownList, "Value", "Text", 0), "--- Select Notice Type ---", new { @id = "notice-type-id", @class = "form-control mandatory-mark  modal-input", required = "required" })
                                                                                            @Html.ValidationMessageFor(m => m.CustomerAccountNoticeScheduleViewModel.NoticeTypeId)
                                                                                            <span id="notice-type-id-error" class="error modal-input-error d-none">Please Select Valid Notice Type. </span>
                                                                                        </div>

                                                                                        @*CommunicationMediaId*@
                                                                                        <div class="form-group">
                                                                                            @Html.CustomLabel("Communication Media")
                                                                                            @Html.DropDownListFor(m => m.CustomerAccountNoticeScheduleViewModel.CommunicationMediaId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.CommunicationMediaDropdownList, "Value", "Text", 0), "--- Select Communication Media ---", new { @id = "comunication-media-id", @class = "form-control mandatory-mark modal-input" })
                                                                                            @Html.ValidationMessageFor(m => m.CustomerAccountNoticeScheduleViewModel.CommunicationMediaId)
                                                                                            <span id="comunication-media-id-error" class="error modal-input-error d-none">Please Select Valid Communication Media. </span>
                                                                                        </div>

                                                                                        @*ScheduleId*@
                                                                                        <div class="form-group">
                                                                                            @Html.CustomLabel("Schedule")
                                                                                            @Html.DropDownListFor(m => m.CustomerAccountNoticeScheduleViewModel.ScheduleId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.ScheduleDropdownList, "Value", "Text", 0), "--- Select Schedule ---", new { @id = "schedule-id", @class = "form-control mandatory-mark modal-input" })
                                                                                            @Html.ValidationMessageFor(m => m.CustomerAccountNoticeScheduleViewModel.ScheduleId)
                                                                                            <span id="schedule-id-error" class="error modal-input-error d-none">Please Select Valid Schedule. </span>
                                                                                        </div>

                                                                                        @*Note*@
                                                                                        <div class="form-group">
                                                                                            @Html.CustomLabel("Note")
                                                                                            @Html.TextAreaFor(m => m.CustomerAccountNoticeScheduleViewModel.Note, new { @id = "note-notice-type", @class = "form-control deny-multiple-space default-none modal-input", @placeholder = "Enter Note", @maxlength = 1500, onkeyup = "ToTextCapitalize()" })
                                                                                            @Html.ValidationMessageFor(m => m.CustomerAccountNoticeScheduleViewModel.Note)
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="modal-footer">
                                                                                        <button type="button" id="btn-add-notice-schedule-modal" class="btn btn-success ">Add</button>
                                                                                        <button type="button" id="btn-update-notice-schedule-modal" class="btn btn-primary ">Update</button>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        @*Notice Schedule Parameter Data Table*@
                                                                        <div class="card-body">
                                                                            <table id="tbl-notice-schedule" class="table compact table-striped table-bordered " width="100">
                                                                                <thead class="text-center">
                                                                                    <tr>
                                                                                        <th><input type="checkbox" name="select-all" id="select-all-notice-schedule" /></th>
                                                                                        <th class="hide">NoticeTypeId</th>
                                                                                        <th>NameOfNoticeType</th>
                                                                                        <th class="d-none">CommunicationMediaId</th>
                                                                                        <th>NameOfCommunicationMedia</th>
                                                                                        <th class="d-none">ScheduleId</th>
                                                                                        <th>Schedule</th>
                                                                                        <th>Note</th>
                                                                                    </tr>
                                                                                </thead>
                                                                                <tbody class="text-center"></tbody>
                                                                            </table>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Save/Submit Button*@
                            @Html.OperationFooter("Create", "Exit", "Cancel", "None")
                        }
                    }
                </div>
            </div>
        </div>
    </div>
</body>
</html>

