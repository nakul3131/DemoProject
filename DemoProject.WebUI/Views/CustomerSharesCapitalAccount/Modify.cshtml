@model DemoProject.Services.ViewModel.Account.Customer.SharesCapitalCustomerAccountViewModel
@using DemoProject.Services.HtmlHelpers
@using DemoProject.Services.ViewModel.Account.Customer
@using DemoProject.Services.ViewModel.Configuration
@using DemoProject.Services.Constants
@using DemoProject.Services.ViewModel.PersonInformation

@{
    SharesCapitalCustomerAccountDetailViewModel sharesCapitalCustomerAccountDetailViewModel = new SharesCapitalCustomerAccountDetailViewModel();
    CustomerAccountDetailViewModel customerAccountDetailViewModel = new CustomerAccountDetailViewModel();
    HeaderPartialModel headerPartialModel = new HeaderPartialModel("CUCMDF");
}

<!doctype html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>Customer Shares Capital Account - Modify</title>
</head>
<body>
    <div class="container-fluid">
        @*======= First Row - For Header =======*@
        <div class="row">
            <div class="offset-1 col-10">
                @Html.Partial("Header", headerPartialModel)
            </div>
        </div>

        @*======= Second Row - Data Entry Page =======*@
        <div class="row mt-3">
            <div class="offset-1 col-10">
                <div class="card d-block mb-3">
                   
                    @using (Html.BeginForm(null, null, FormMethod.Post, htmlAttributes: new { @id = "form" ,@autocomplete = "off"}))
                    {
                        @Html.AntiForgeryToken()
                        {
                            <div class="card-body">
                                @*======= First Row - Mandatory Mark =======*@
                                <div class="row mt-2">
                                    <div class="col-12">
                                        <h6 class="mb-2 text-right text-muted"><b class="mandatory-mark">&nbsp; Mandatory Field</b></h6>
                                    </div>
                                </div>

                                @*======= Second Row - Page Title, Translation =======*@
                                <div class="row mt-2">
                                    @*Page Title*@
                                    <div class="col-12">
                                        <h5 class="font-weight-bold mt-1">Modify</h5>
                                        <hr />
                                    </div>
                                </div>

                                @*Submit Error*@
                                @Html.ValidationMessage("SubmitError")

                                @*======= Third Row - Data Entry Page =======*@
                                <div class="row">
                                    <div class="col-12">
                                        @*CheckerRemark*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Checker Remark")
                                            @Html.TextBoxFor(m => m.CheckerRemark, new { @id = "name-of-center", @class = "form-control mandatory-mark " })
                                            @Html.ValidationMessageFor(m => m.CheckerRemark)
                                        </div>

                                        @*BusinessOfficeId*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Branch")
                                            @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.BusinessOfficeId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.AuthorizedBusinessOfficeDropdownList, "Value", "Text", 0), new { @class = "form-control mandatory-mark" })
                                            @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.BusinessOfficeId)
                                        </div>

                                        @*GeneralLedgerId*@
                                        <div class="form-group">
                                            @Html.CustomLabel("General Ledger")
                                            @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.GeneralLedgerId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.GeneralLedgerDropdownList, "Value", "Text"), new { @id = "general-ledger-id", @class = "form-control mandatory-mark" })
                                            @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.GeneralLedgerId)
                                        </div>

                                        @*SchemeId*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Scheme")
                                            @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.SchemeId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.SharesCapitalSchemeDropdownList, "Value", "Text"), new { @id = "scheme-id", @class = "form-control mandatory-mark"})
                                            @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.SchemeId)
                                        </div>

                                        @*MinuteOfMeetingAgenda*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Minute Of Meeting Agenda")
                                            @Html.DropDownListFor(m => m.CustomerSharesCapitalAccountViewModel.MinuteOfMeetingAgendaId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.MinuteOfMeetingAgendaDropdownList, "Value", "Text"), new { @id = "minute-of-meeting-agenda-id", @class = "form-control mandatory-mark", required = "required" })
                                            @Html.ValidationMessageFor(m => m.CustomerSharesCapitalAccountViewModel.MinuteOfMeetingAgendaId)
                                        </div>

                                        @*PersonId*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Customer Name")
                                            @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.PersonId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.PersonDropdownList, "Value", "Text"), new { @id = "person-id", @class = "form-control mandatory-mark"})
                                            @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.PersonId)
                                        </div>

                                        @*CurrencyId*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Currency")
                                            @Html.DropDownListFor(m => m.CustomerAccountDetailViewModel.CurrencyId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.CurrencyDropdownList, "Value", "Text", 0), new { @id = "currency-id", @class = "form-control mandatory-mark" })
                                            @Html.ValidationMessageFor(m => m.CustomerAccountDetailViewModel.CurrencyId)
                                        </div>

                                        @*AccountOpeningDate*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Account Opening Date")
                                            @Html.TextBoxFor(m => m.AccountOpeningDate, "{0:yyyy-MM-dd}", new { @id = "account-opening-date", @class = "form-control mandatory-mark deny-future-date", @placeholder = "Enter Account Opening Date", required = "required", @Type = "date", onclick = "DenyFutureDate()" })
                                            @Html.ValidationMessageFor(m => m.AccountOpeningDate)
                                        </div>

                                        @*AccountNumber*@
                                        <div id="account-number-div" class="form-group">
                                            @Html.CustomLabel("Account Number")
                                            @Html.TextBoxFor(m => m.AccountNumber, new { @id = "account-number", @class = "form-control mandatory-mark" })
                                            @Html.ValidationMessageFor(m => m.AccountNumber)
                                        </div>

                                        @*AlternateAccountNumber1*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Alternate Account Number 1")
                                            @Html.TextBoxFor(m => m.AlternateAccountNumber1, new { @id = "alternate-account-number-1", @class = "form-control mandatory-mark" })
                                            @Html.ValidationMessageFor(m => m.AlternateAccountNumber1)
                                        </div>

                                        @*AlternateAccountNumber2*@
                                        <div id="account-number2" class="form-group">
                                            @Html.CustomLabel("Alternate Account Number 2")
                                            @Html.TextBoxFor(m => m.AlternateAccountNumber2, new { @id = "alternate-account-number-2", @class = "form-control mandatory-mark" })
                                            @Html.ValidationMessageFor(m => m.AlternateAccountNumber2)
                                        </div>

                                        @*AlternateAccountNumber3*@
                                        <div id="account-number3" class="form-group ">
                                            @Html.CustomLabel("Alternate Account Number 3")
                                            @Html.TextBoxFor(m => m.AlternateAccountNumber3, new { @id = "alternate-account-number-3", @class = "form-control mandatory-mark" })
                                            @Html.ValidationMessageFor(m => m.AlternateAccountNumber3)
                                        </div>

                                        @*MemberNumber*@
                                        <div id="member-number-grp" class="form-group">
                                            @Html.CustomLabel("Member Number")
                                            @Html.TextBoxFor(m => m.CustomerSharesCapitalAccountViewModel.MemberNumber, new { @id = "member-number", @class = "form-control mandatory-mark" })
                                            @Html.ValidationMessageFor(m => m.CustomerSharesCapitalAccountViewModel.MemberNumber)
                                        </div>

                                        @*IsPrivateCustomer*@
                                        <div class="row form-group mt-3">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Private Customer")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.IsPrivateCustomer, new { @id = "is-private-customer", @class = "form-control switch-input" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        
                                        @*IsOtherSocietyMember*@
                                        <div class="row form-group mt-4">
                                            <div class="col-10">
                                                @Html.CustomLabel("Is Other Society Member")
                                            </div>
                                            <div class="col-2">
                                                <div class="text-left">
                                                    <label class="switch switch-left-right">
                                                        @Html.CheckBoxFor(m => m.CustomerSharesCapitalAccountViewModel.IsOtherSocietyMember, new { @id = "is-other-society-member", @class = "form-control switch-input" })
                                                        <span class="slider round">
                                                            <span class="on"><b>True</b></span><span class="off"><b>False</b></span>
                                                        </span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                        @*Note*@
                                        <div class="form-group">
                                            @Html.CustomLabel("Note")
                                            @Html.TextAreaFor(m => m.Note, new { @id = "note-scheme", @class = "form-control", @placeholder = "Enter Note", required = "required", @maxlength = "1500", @autocomplete = "off", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                            @Html.ValidationMessageFor(m => m.Note)
                                        </div>
                                    </div>
                                </div>

                                @*======= Fourth Row - Accordians =======*@
                                <div class="row mt-3">
                                    <div class="col-12">
                                        <div id="accordion" class="accordion md-accordion accordion-blocks">

                                            @*Accordian 1 - Address Details*@
                                            <div id="heading-address-details" class="card mt-3">
                                                <div class="card-header p-3" id="collapse-address-details-validations" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-address-details" data-target="#collapse-address-details">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <a class="card-title" href="#">
                                                                <b>Address Details</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <span></span>
                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-address-details" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-address-details" data-target="#collapse-address-details">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div id="collapse-address-details" class="collapse" aria-labelledby="heading-address-details" data-parent="#accordion">
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                <div class="modal fade" id="person-address" tabindex="-1" data-backdrop="static" role="dialog" aria-labelledby="modal-new-event" aria-hidden="true">
                                                                    <div class="modal-dialog modal-dialog-centered modal-dialog-zoom modal-lg" role="document">
                                                                        <div class="modal-content">
                                                                            <div class="modal-header">
                                                                                <h4 class="modal-title text-center" id="person-address-text">Add</h4>
                                                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                                                    <span aria-hidden="true">&times;</span>
                                                                                </button>
                                                                            </div>
                                                                            <div class="modal-body">
                                                                                <div class="person-address-details">
                                                                                    @*AddressTypePrmKey*@
                                                                                    <div class="form-group">
                                                                                        @Html.CustomLabel("Address Type")
                                                                                        @Html.DropDownListFor(m => m.PersonAddressViewModel.AddressTypeId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.AddressTypeDropdownList, "Value", "Text", 0), "-- Please Select Address Type --", new { @id = "address-type-id", @class = "form-control mandatory-mark ad", required = "required" })
                                                                                        @Html.ValidationMessageFor(m => m.PersonAddressViewModel.AddressTypeId)
                                                                                    </div>

                                                                                    @*FlatDoorNo*@
                                                                                    <div class="form-group">
                                                                                        @Html.CustomLabel("Flat Door No")
                                                                                        @Html.TextBoxFor(m => m.PersonAddressViewModel.FlatDoorNo, new { @id = "flat-door-no", @class = "form-control mandatory-mark ad", @placeholder = "Enter Flat Door No", @maxlength = "50", @autocomplete = "off", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()", required = "required" })
                                                                                        @Html.ValidationMessageFor(m => m.PersonAddressViewModel.FlatDoorNo)
                                                                                    </div>

                                                                                    @*TransFlatDoorNo*@
                                                                                    <div class="form-group">
                                                                                        @Html.CustomLabel(sharesCapitalCustomerAccountDetailViewModel.FlatDoorNoInRegionalLanguage)
                                                                                        @Html.TextBoxFor(m => m.PersonAddressViewModel.TransFlatDoorNo, null, new { @id = "trans-flat-door-no", @class = "form-control mandatory-mark ad", @placeholder = sharesCapitalCustomerAccountDetailViewModel.FlatDoorNoPlaceHolderInRegionalLanguage, @maxlength = "50", @autocomplete = "off", required = "required" })
                                                                                        @Html.ValidationMessageFor(m => m.PersonAddressViewModel.TransFlatDoorNo)
                                                                                    </div>

                                                                                    @*NameOfBuilding*@
                                                                                    <div class="form-group">
                                                                                        @Html.CustomLabel("Name Of Building")
                                                                                        @Html.TextBoxFor(m => m.PersonAddressViewModel.NameOfBuilding, new { @id = "building-name", @class = "form-control mandatory-mark ad", @placeholder = "Enter Name Of Building", @maxlength = "100", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()", required = "required" })
                                                                                        @Html.ValidationMessageFor(m => m.PersonAddressViewModel.NameOfBuilding)
                                                                                    </div>

                                                                                    @*TransNameOfBuilding*@
                                                                                    <div class="form-group">
                                                                                        @Html.CustomLabel(sharesCapitalCustomerAccountDetailViewModel.NameOfBuildingInRegionalLanguage)
                                                                                        @Html.TextBoxFor(m => m.PersonAddressViewModel.TransNameOfBuilding, new { @id = "trans-building-name", @class = "form-control mandatory-mark ad", @placeholder = sharesCapitalCustomerAccountDetailViewModel.NameOfBuildingPlaceHolderInRegionalLanguage, @maxlength = "100", required = "required" })
                                                                                        @Html.ValidationMessageFor(m => m.PersonAddressViewModel.TransNameOfBuilding)
                                                                                    </div>

                                                                                    @*NameOfRoad*@
                                                                                    <div class="form-group">
                                                                                        @Html.CustomLabel("Name Of Road")
                                                                                        @Html.TextBoxFor(m => m.PersonAddressViewModel.NameOfRoad, new { @id = "road-name", @class = "form-control mandatory-mark ad", @placeholder = "Enter Name Of Road", @maxlength = "100", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()", required = "required" })
                                                                                        @Html.ValidationMessageFor(m => m.PersonAddressViewModel.NameOfRoad)
                                                                                    </div>

                                                                                    @*TransNameOfRoad*@
                                                                                    <div class="form-group">
                                                                                        @Html.CustomLabel(sharesCapitalCustomerAccountDetailViewModel.NameOfRoadInRegionalLanguage)
                                                                                        @Html.TextBoxFor(m => m.PersonAddressViewModel.TransNameOfRoad, new { @id = "trans-road-name", @class = "form-control mandatory-mark ad", @placeholder = sharesCapitalCustomerAccountDetailViewModel.NameOfRoadPlaceHolderInRegionalLanguage, @maxlength = "100", required = "required" })
                                                                                        @Html.ValidationMessageFor(m => m.PersonAddressViewModel.TransNameOfRoad)
                                                                                    </div>

                                                                                    @*NameOfArea*@
                                                                                    <div class="form-group">
                                                                                        @Html.CustomLabel("Name Of Area")
                                                                                        @Html.TextBoxFor(m => m.PersonAddressViewModel.NameOfArea, new { @id = "area-name", @class = "form-control mandatory-mark ad", @placeholder = "Enter Name Of Area", @maxlength = "100", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()", required = "required" })
                                                                                        @Html.ValidationMessageFor(m => m.PersonAddressViewModel.NameOfArea)
                                                                                    </div>

                                                                                    @*TransNameOfArea*@
                                                                                    <div class="form-group">
                                                                                        @Html.CustomLabel(sharesCapitalCustomerAccountDetailViewModel.NameOfAreaInRegionalLanguage)
                                                                                        @Html.TextBoxFor(m => m.PersonAddressViewModel.TransNameOfArea, new { @id = "trans-area-name", @class = "form-control mandatory-mark ad", @placeholder = sharesCapitalCustomerAccountDetailViewModel.NameOfAreaPlaceHolderInRegionalLanguage, @maxlength = "100", required = "required" })
                                                                                        @Html.ValidationMessageFor(m => m.PersonAddressViewModel.TransNameOfArea)
                                                                                    </div>

                                                                                    @*City*@
                                                                                    <div class="form-group">
                                                                                        @Html.CustomLabel("City")
                                                                                        @Html.DropDownListFor(m => m.PersonAddressViewModel.CityId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.CityDropdownList, "Value", "Text", 0), "--Please Select City---", new { @id = "city-id", @class = "form-control mandatory-mark ad", required = "required" })
                                                                                        @Html.ValidationMessageFor(m => m.PersonAddressViewModel.CityId)
                                                                                    </div>

                                                                                    @*ResidenceType*@
                                                                                    <div class="form-group">
                                                                                        @Html.CustomLabel("Residence Type")
                                                                                        @Html.DropDownListFor(m => m.PersonAddressViewModel.ResidenceTypeId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.ResidenceTypeDropdownList, "Value", "Text", 0), "-- Please select Residence Type--", new { @id = "residence-type-id", @class = "form-control mandatory-mark ad", required = "required" })
                                                                                        @Html.ValidationMessageFor(m => m.PersonAddressViewModel.ResidenceTypeId)
                                                                                    </div>

                                                                                    @*ResidenceOwnership*@
                                                                                    <div class="form-group">
                                                                                        @Html.CustomLabel("Ownership Type")
                                                                                        @Html.DropDownListFor(m => m.PersonAddressViewModel.OwnershipTypeId, new SelectList(sharesCapitalCustomerAccountDetailViewModel.OwnershipTypeDropdownList, "Value", "Text", 0), " -- Please select Ownership Type--", new { @id = "residence-ownership-id", @class = "form-control mandatory-mark ad", required = "required" })
                                                                                        @Html.ValidationMessageFor(m => m.PersonAddressViewModel.OwnershipTypeId)
                                                                                    </div>

                                                                                    @*Note*@
                                                                                    <div class="form-group">
                                                                                        @Html.CustomLabel("Note")
                                                                                        @Html.TextAreaFor(m => m.PersonAddressViewModel.Note, new { @id = "note-address", @class = "form-control ad", @placeholder = "Enter Note", @maxlength = "1500", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                                                                        @Html.ValidationMessageFor(m => m.PersonAddressViewModel.Note)
                                                                                    </div>

                                                                                    @*TransNote*@
                                                                                    <div class="form-group">
                                                                                        @Html.CustomLabel(sharesCapitalCustomerAccountDetailViewModel.NoteInRegionalLanguage)
                                                                                        @Html.TextAreaFor(m => m.PersonAddressViewModel.TransNote, new { @id = "trans-note-address", @class = "form-control ad", @placeholder = sharesCapitalCustomerAccountDetailViewModel.NotePlaceHolderInRegionalLanguage, @maxlength = "1500" })
                                                                                        @Html.ValidationMessageFor(m => m.PersonAddressViewModel.TransNote)
                                                                                    </div>

                                                                                    @*ReasonForModification*@
                                                                                    <div id="address-div" class="form-group d-none">
                                                                                        @Html.CustomLabel("Reason For Modification")
                                                                                        @Html.TextAreaFor(m => m.PersonAddressViewModel.ReasonForModification, new { @id = "reason-for-modification-address", @class = "form-control ad", @placeholder = "Enter Reason For Modification", @maxlength = "1500", onkeydown = "Maketranslation()", onkeyup = "ToTextCapitalize()" })
                                                                                        @Html.ValidationMessageFor(m => m.PersonAddressViewModel.ReasonForModification)
                                                                                    </div>
                                                                                </div>

                                                                                @*IsVerified*@
                                                                                <div class="row form-group mt-4">
                                                                                    <div class="col-8">
                                                                                        @Html.CustomLabel("IsVerified")
                                                                                    </div>
                                                                                    <div class="col-4">
                                                                                        <div class="text-right">
                                                                                            <label class="switch switch-left-right">
                                                                                                @Html.CheckBoxFor(m => m.PersonAddressViewModel.IsVerified, new { @class = "form-control switch-input address-detail-new ad", @id = "is-verified-address" })
                                                                                                <span class="slider round">
                                                                                                    <span class="on"><b>Yes</b></span><span class="off"><b class="mr-3">No</b></span>
                                                                                                </span>
                                                                                            </label>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                            </div>

                                                                            <div class="modal-footer">
                                                                                <button type="button" id="btn-add-address" class="btn btn-success ">Add</button>
                                                                                <button type="button" id="btn-update-address" class="btn btn-primary">Update</button>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                                <div class="card-body">
                                                                    <div class="table-responsive" style="min-height:200px;">
                                                                        <table id="person-address-data-table" class="table compact table-striped table-bordered" cellspacing="0" width="100">
                                                                            <thead class="text-center">
                                                                                <tr>
                                                                                    <th><input type="checkbox" name="check_all" id="selectAll" style="margin-left:11px;" /></th>
                                                                                    <th class="d-none">AddressTypeID</th>
                                                                                    <th>AddressType</th>
                                                                                    <th>FlatDoorNo</th>
                                                                                    <th>Reg.FlatDoorNo</th>
                                                                                    <th>NameOfBuilding</th>
                                                                                    <th>Reg.NameOfBuilding</th>
                                                                                    <th>NameOfRoad</th>
                                                                                    <th>Reg.NameOfRoad</th>
                                                                                    <th>NameOfArea</th>
                                                                                    <th>Reg.NameOfArea</th>
                                                                                    <th class="d-none">CityID</th>
                                                                                    <th>City</th>
                                                                                    <th class="d-none">ResidenceTypeID</th>
                                                                                    <th>ResidenceType</th>
                                                                                    <th class="d-none">OwnershipTypeId</th>
                                                                                    <th>NameOfOwnershipType</th>
                                                                                    <th>IsVerified</th>
                                                                                    <th>Note</th>
                                                                                    <th>Reg.Note</th>
                                                                                    <th class="d-none">ReasonForModification</th>
                                                                                    <th class="d-none">PersonAddressPrmKey</th>
                                                                                </tr>
                                                                            </thead>
                                                                            <tbody class="text-center">
                                                                                @{
                                                                                    IEnumerable<PersonAddressViewModel> personAddressViewModelList = (IEnumerable<PersonAddressViewModel>)HttpContext.Current.Session["CustomerLoanAccountAddressDetail"];

                                                                                    foreach (PersonAddressViewModel viewModel in personAddressViewModelList)
                                                                                    {
                                                                                        <tr>
                                                                                            <td><input type="checkbox" name="check_all" class="checks" /></td>
                                                                                            <td class="d-none">@viewModel.AddressTypeId</td>
                                                                                            <td>@viewModel.NameOfAddressType</td>
                                                                                            <td>@viewModel.FlatDoorNo</td>
                                                                                            <td>@viewModel.TransFlatDoorNo</td>
                                                                                            <td>@viewModel.NameOfBuilding</td>
                                                                                            <td>@viewModel.TransNameOfBuilding</td>
                                                                                            <td>@viewModel.NameOfRoad</td>
                                                                                            <td>@viewModel.TransNameOfRoad</td>
                                                                                            <td>@viewModel.NameOfArea</td>
                                                                                            <td>@viewModel.TransNameOfArea</td>
                                                                                            <td class="d-none">@viewModel.CityId</td>
                                                                                            <td>@viewModel.NameOfCenter</td>
                                                                                            <td class="d-none">@viewModel.ResidenceTypeId</td>
                                                                                            <td>@viewModel.NameOfResidenceType</td>
                                                                                            <td class="d-none">@viewModel.OwnershipTypeId</td>
                                                                                            <td>@viewModel.NameOfOwnershipType</td>
                                                                                            <td>@viewModel.IsVerified</td>
                                                                                            <td>@viewModel.Note</td>
                                                                                            <td>@viewModel.TransNote</td>
                                                                                            <td class="d-none">@viewModel.ReasonForModification</td>
                                                                                            <td class="d-none">@viewModel.PersonAddressPrmKey</td>
                                                                                        </tr>
                                                                                    }
                                                                                }
                                                                            </tbody>
                                                                        </table>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            @*Accordian 2 - Person Contact Detail*@
                                            <div id="heading-contact-details" class="card mt-3">
                                                <div class="card-header p-3" id="collapse-contact-details-validations" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-contact-details" data-target="#collapse-contact-details">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <a class="card-title" href="#">
                                                                <b>Contact Detail</b>
                                                            </a>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <span></span>
                                                        </div>
                                                        <div class="col-2">
                                                            <a href="#collapse-contact-details" class="float-right" data-toggle="collapse" aria-expanded="false" aria-controls="collapse-contact-details" data-target="#collapse-contact-details">
                                                                <i class="fa fa-angle-down toggle-icon"></i>
                                                            </a>
